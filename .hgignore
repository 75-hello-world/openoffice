syntax: regexp

# source additions:
^external/unowinreg/unowinreg.dll$

# configure output:
^Makefile$
^[0-9A-Za-z-]+Env\.Set(\.sh)?$
^bootstrap$
^config\.log$
^config\.parms$
^config\.status$
^makefile\.mk$
^set_soenv$
^visibility\.cxx$
^warn$

# bootstrap output:
^dmake/(.+/)?Makefile$
^dmake/(.+/)?\.deps$
^dmake/(.+/)?\.dirstamp$
^dmake/.+\.o$
^dmake/config\.h$
^dmake/config\.log$
^dmake/config\.status$
^dmake/dmake$
^dmake/dmake\.exe$
^dmake/dmakeroot\.h$
^dmake/stamp-h1$
^dmake/startup/config\.mk$

# build output:
^[^/]+/common(\.pro)?$
^[^/]+/unxfbsd(\.pro)?$
^[^/]+/unxlngi\d+(\.pro)?$
^[^/]+/unxlngx\d+(\.pro)?$
^[^/]+/unxmacxi(\.pro)?$
^[^/]+/unxsoli\d+(\.pro)?$
^[^/]+/unxsols\d+(\.pro)?$
^[^/]+/wntgcci(\.pro)?$
^[^/]+/wntmsci\d+(\.pro)?$
^solver$

# Emacs backups (XXX~, #XXX#, .#XXX):
^(.*/)?[^/]+~$
^(.*/)?\#[^/]+\#$
^(.*/)?\.\#[^/]+$

# VIM swap files (.XXX.swp):
^(.*/)?(\.[^/]+)?\.swp$

^(.*/)?\.DS_Store$
