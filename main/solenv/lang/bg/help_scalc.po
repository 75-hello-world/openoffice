msgid  "0.5 Million"
msgstr "0.5 милиона"

#: source/text/scalc/guide/format_value_userdef.xhp#par_id3146114.21
msgid  "1 Million"
msgstr "1 милион"

#: source/text/scalc/guide/format_value_userdef.xhp#par_id3155810.22
msgid  "100000000"
msgstr "100000000"

#: source/text/scalc/guide/format_value_userdef.xhp#par_id3153818.23
msgid  "100. Million"
