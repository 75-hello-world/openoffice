msgid  "Gi gibi 1073741824"
msgstr "Gi гиби 1073741824"

#: source/text/scalc/01/04060116.xhp#par_id0908200903090975
msgid  "Ti tebi 1099511627776"
msgstr "Ti теби 1099511627776"

