msgid  "\<image id=\"img_id3157976\" src=\"res/commandimagelist/sc_prevrecord.png\" width=\"0.222inch\" height=\"0.222inch\"\>\<alt id=\"alt_id3157976\"\>Icon\</alt\>\</image\>"
