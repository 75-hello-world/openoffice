msgid  "sExpr = \"123457896\""
msgstr "sExpr = \"123457896\""

#: source/text/sbasic/shared/03120308.xhp#par_id3153144.27
msgid  "RSet sVar = sExpr"
msgstr "RSet sVar = sExpr"

#: source/text/sbasic/shared/03120308.xhp#par_id3150116.28
