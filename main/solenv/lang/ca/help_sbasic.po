msgstr "LSet sVar = sExpr"

#: source/text/sbasic/shared/03120308.xhp#par_id3154511.34
msgid  "Print \">\"; sVar; \"<\""
msgstr "Print \">\"; sVar; \"<\""

#: source/text/sbasic/shared/03120308.xhp#par_id3153839.36
