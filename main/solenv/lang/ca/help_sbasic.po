msgstr "sVar = LTrim(Str(iVar))"

#: source/text/sbasic/shared/03120103.xhp#par_id3154924.16
msgid  "Msgbox sVar & chr(13) & Str(iVar)"
msgstr "Msgbox sVar & chr(13) & Str(iVar)"

#: source/text/sbasic/shared/03120103.xhp#par_id3152811.17
