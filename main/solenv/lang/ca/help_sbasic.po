msgstr "Dim sInput As String"

#: source/text/sbasic/shared/03120303.xhp#par_id3154217.14
msgid  "Dim sUS_date As String"
msgstr "Dim sUS_date As String"

#: source/text/sbasic/shared/03120303.xhp#par_id3150448.15
msgid  "sInput = InputBox(\"Please input a date in the international format 'YYYY-MM-DD'\")"
msgstr "sInput = InputBox(\"Introduïu una data en el format internacional 'AAAA-MM-DD'\")"

#: source/text/sbasic/shared/03120303.xhp#par_id3149203.16
msgid  "sUS_date = Mid(sInput, 6, 2)"
msgstr "sUS_date = Mid(sInput, 6, 2)"

#: source/text/sbasic/shared/03120303.xhp#par_id3150439.17
msgid  "sUS_date = sUS_date & \"/\""
msgstr "sUS_date = sUS_date & \"/\""

#: source/text/sbasic/shared/03120303.xhp#par_id3153770.18
msgid  "sUS_date = sUS_date & Right(sInput, 2)"
msgstr "sUS_date = sUS_date & Right(sInput, 2)"

#: source/text/sbasic/shared/03120303.xhp#par_id3161833.19
msgid  "sUS_date = sUS_date & \"/\""
msgstr "sUS_date = sUS_date & \"/\""

#: source/text/sbasic/shared/03120303.xhp#par_id3147215.20
msgid  "sUS_date = sUS_date & Left(sInput, 4)"
msgstr "sUS_date = sUS_date & Left(sInput, 4)"

#: source/text/sbasic/shared/03120303.xhp#par_id3149666.21
msgid  "MsgBox sUS_date"
msgstr "MsgBox sUS_date"

#: source/text/sbasic/shared/03120303.xhp#par_id3153138.22
msgid  "End Sub"
msgstr "End Sub"

#: source/text/sbasic/shared/03120304.xhp#tit
