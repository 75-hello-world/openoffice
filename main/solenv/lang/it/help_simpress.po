#: source/text/simpress/01/05140000.xhp#tit
msgid  "Modify Layer"
msgstr "Modifica livello"

#: source/text/simpress/01/05140000.xhp#bm_id3156329
msgid  "\<bookmark_value\>renaming layers\</bookmark_value\>\<bookmark_value\>layers; renaming\</bookmark_value\>"
msgstr "<bookmark_value>Rinomina;livelli</bookmark_value><bookmark_value>Livello;rinominare</bookmark_value>"

#: source/text/simpress/01/05140000.xhp#hd_id3156329.1
msgid  "Modify Layer"
msgstr "Cambiare livello "

