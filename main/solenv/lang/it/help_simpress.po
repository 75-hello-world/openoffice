msgstr "<image id=\"img_id3154869\" src=\"res/commandimagelist/sc_verticaltextfittosizetool.png\" width=\"0.1335inch\" height=\"0.1335inch\"><alt id=\"alt_id3154869\">Icona</alt></image>"

