#: source/text/sbasic/shared/02/20000000.xhp#par_id3151042.11
msgid  "\<ahelp hid=\".uno:InsertImageControl\"\>Adds a control that displays a graphic.\</ahelp\>"
msgstr "<ahelp hid=\".uno:InsertImageControl\">Adiciona um controlo que apresenta um objecto gráfico.</ahelp>"

#: source/text/sbasic/shared/02/20000000.xhp#hd_id3150447.12
msgid  "Check Box"
msgstr "Caixa de verificação"

