msgid  "\<image id=\"img_id3151102\" src=\"res/commandimagelist/sc_interactivegradient.png\" width=\"5.64mm\" height=\"5.64mm\"\>\<alt id=\"alt_id3151102\"\>Icon\</alt\>\</image\>"
