msgid "Choose <emph><switchinline select=\"sys\"><caseinline select=\"MAC\">%PRODUCTNAME - Preferences</caseinline><defaultinline>Tools - Options</defaultinline></switchinline> - <link href=\"text/shared/optionen/01010200.xhp\" name=\"Load/Save - General\">Load/Save - General</link></emph>."
msgstr ""

#: guide/doc_autosave.xhp#PAR_ID3148474.5
msgid "Mark <emph>Always create backup copy</emph>."
msgstr ""

#: guide/doc_autosave.xhp#PAR_ID3149797.6
msgid "If the <emph>Always create backup copy</emph> option is selected, the old version of the file is saved to the backup directory whenever you save the current version of the file. "
msgstr ""

#: guide/doc_autosave.xhp#PAR_ID3148685.7
msgid "You can change the backup directory by choosing <emph><switchinline select=\"sys\"><caseinline select=\"MAC\">%PRODUCTNAME - Preferences</caseinline><defaultinline>Tools - Options</defaultinline></switchinline> - $[officename] - Paths</emph>, then change the <emph>Backups</emph> path in the dialog."
msgstr ""

#: guide/doc_autosave.xhp#PAR_ID3149415.8
msgid "The backup copy has the same name as the document, but the extension is .BAK. If the backup folder already contains such a file, it will be overwritten without warning."
msgstr ""

#: guide/doc_autosave.xhp#HD_ID3149514.9
msgid "To save recovery information automatically every n minutes"
msgstr ""

#: guide/doc_autosave.xhp#PAR_ID3148563.10
msgid "Choose <emph><switchinline select=\"sys\"><caseinline select=\"MAC\">%PRODUCTNAME - Preferences</caseinline><defaultinline>Tools - Options</defaultinline></switchinline> - <link href=\"text/shared/optionen/01010200.xhp\" name=\"Load/Save - General\">Load/Save - General</link></emph>."
msgstr ""

#: guide/doc_autosave.xhp#PAR_ID3154760.11
msgid "Mark <emph>Save AutoRecovery information every</emph> and select the time interval."
msgstr ""

#: guide/doc_autosave.xhp#PAR_ID3153526.13
msgid "This command saves the information necessary to restore the current document in case of a crash. Additionally, in case of a crash %PRODUCTNAME tries automatically to save AutoRecovery information for all open documents, if possible."
