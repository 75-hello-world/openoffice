msgstr ""

#: 02/12080000.xhp#HD_ID3149031.1
msgid "<link href=\"text/shared/02/12080000.xhp\" name=\"Data to Fields\">Data to Fields</link>"
msgstr ""

#: 02/12080000.xhp#PAR_ID3150476.2
