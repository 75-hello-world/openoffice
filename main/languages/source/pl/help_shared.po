msgstr ""

#: guide/start_parameters.xhp#PAR_ID3153222.66
msgid "Starts with an empty Impress document."
msgstr ""

#: guide/start_parameters.xhp#PAR_ID3155853.67
msgid "-math"
msgstr ""

#: guide/start_parameters.xhp#PAR_ID3146928.68
msgid "Starts with an empty Math document."
msgstr ""

#: guide/start_parameters.xhp#PAR_ID3149960.69
msgid "-global"
msgstr ""

#: guide/start_parameters.xhp#PAR_ID3151075.70
msgid "Starts with an empty Writer master document."
msgstr ""

#: guide/start_parameters.xhp#PAR_ID3154510.71
msgid "-web"
msgstr ""

#: guide/start_parameters.xhp#PAR_ID3148836.72
msgid "Starts with an empty HTML document."
msgstr ""

#: guide/start_parameters.xhp#PAR_ID3149403
msgid "-show {filename.odp}"
msgstr ""

#: guide/start_parameters.xhp#PAR_ID3153838.80
msgid "Starts with the Impress file <emph>{filename.odp}</emph> and starts the presentation. Enters edit mode after the presentation."
msgstr ""

#: guide/start_parameters.xhp#PAR_ID3156276.13
msgid "-minimized"
msgstr ""

#: guide/start_parameters.xhp#PAR_ID3146080.14
msgid "Starts minimized. The splash screen is not displayed."
msgstr ""

#: guide/start_parameters.xhp#PAR_ID3641773
msgid "-maximized"
msgstr ""

#: guide/start_parameters.xhp#PAR_ID8224433
msgid "Starts maximized."
msgstr ""

#: guide/start_parameters.xhp#PAR_ID3145641.15
msgid "-invisible"
msgstr ""

#: guide/start_parameters.xhp#PAR_ID3154756.16
msgid "Starts in invisible mode."
msgstr ""

#: guide/start_parameters.xhp#PAR_ID3148914.17
msgid "Neither the start-up logo nor the initial program window will be visible. However, the $[officename] software can be controlled and documents and dialogs opened via the <link href=\"http://api.openoffice.org\" name=\"API\">API</link>. "
msgstr ""

#: guide/start_parameters.xhp#PAR_ID3147341.18
msgid "When the $[officename] software has been started with this parameter, it can only be ended using the taskmanager (Windows) or the <emph>kill </emph>command (UNIX based systems)."
msgstr ""

#: guide/start_parameters.xhp#PAR_ID3150388.48
msgid "It cannot be used in conjunction with <emph>-quickstart</emph>."
msgstr ""

#: guide/start_parameters.xhp#PAR_ID3145147.19
msgid "More information is found in the <emph>$[officename] Developer's Guide</emph>."
