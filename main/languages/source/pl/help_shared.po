msgstr ""

#: 01/02100001.xhp#HD_ID3146765.203
msgid "<variable id=\"02100001\"><link href=\"text/shared/01/02100001.xhp\">List of Regular Expressions</link></variable>"
msgstr ""

#: 01/02100001.xhp#PAR_ID3149741.17
msgid "Character"
msgstr ""

#: 01/02100001.xhp#PAR_ID3155577.18
msgid "Result/Use"
msgstr ""

#: 01/02100001.xhp#PAR_ID6600543
msgid "Any character"
msgstr ""

#: 01/02100001.xhp#PAR_ID9824518.00000001
msgid "Represents the given character unless otherwise specified."
msgstr ""

#: 01/02100001.xhp#PAR_ID3152427.19
msgid "."
msgstr ""

#: 01/02100001.xhp#PAR_ID3149031.20
msgid "Represents any single character except for a line break or paragraph break. For example, the search term \"sh.rt\" returns both \"shirt\" and \"short\"."
msgstr ""

#: 01/02100001.xhp#PAR_ID3154682.21
msgid "^"
msgstr ""

#: 01/02100001.xhp#PAR_ID3155351.22
msgid "Only finds the search term if the term is at the beginning of a paragraph. Special objects such as empty fields or character-anchored frames, at the beginning of a paragraph are ignored. Example: \"^Peter\"."
msgstr ""

#: 01/02100001.xhp#PAR_ID3159194.23
msgid "$"
msgstr ""

#: 01/02100001.xhp#PAR_ID3152542.24
msgid "Only finds the search term if the term appears at the end of a paragraph. Special objects such as empty fields or character-anchored frames at the end of a paragraph are ignored. Example: \"Peter$\"."
msgstr ""

#: 01/02100001.xhp#PAR_ID3156414.25
msgid "*"
msgstr ""

#: 01/02100001.xhp#PAR_ID3155555.26
msgid "Finds zero or more of the characters in front of the \"*\". For example, \"Ab*c\" finds \"Ac\", \"Abc\", \"Abbc\", \"Abbbc\", and so on."
msgstr ""

#: 01/02100001.xhp#PAR_ID3147399.27
msgid "+"
msgstr ""

#: 01/02100001.xhp#PAR_ID3157958.28
msgid "Finds one or more of the characters in front of the \"+\". For example, \"AX.+4\" finds \"AXx4\", but not \"AX4\"."
msgstr ""

#: 01/02100001.xhp#PAR_ID3145313.207
msgid "The longest possible string that matches this search pattern in a paragraph is always found. If the paragraph contains the string \"AX 4 AX4\", the entire passage is highlighted."
msgstr ""

#: 01/02100001.xhp#PAR_ID3143267.199
msgid "?"
msgstr ""

#: 01/02100001.xhp#PAR_ID3153684.200
msgid "Finds zero or one of the characters in front of the \"?\". For example, \"Texts?\" finds \"Text\" and \"Texts\" and \"x(ab|c)?y\" finds \"xy\", \"xaby\", or \"xcy\"."
msgstr ""

#: 01/02100001.xhp#PAR_ID3166410.158
msgid "\\"
msgstr ""

#: 01/02100001.xhp#PAR_ID3147209.159
msgid "Search interprets the special character that follows the \"\\\" as a normal character and not as a regular expression (except for the combinations \\n, \\t, \\>, and \\<). For example, \"tree\\.\" finds \"tree.\", not \"treed\" or \"trees\"."
msgstr ""

#: 01/02100001.xhp#PAR_ID3152945.166
msgid "\\n"
msgstr ""

#: 01/02100001.xhp#PAR_ID3153700.167
msgid "Represents a line break that was inserted with the Shift+Enter key combination. To change a line break into a paragraph break, enter <emph>\\n</emph> in the <emph>Search for</emph> and <emph>Replace with</emph> boxes, and then perform a search and replace."
msgstr ""

#: 01/02100001.xhp#PAR_ID9262672
msgid "\\n in the <emph>Search for</emph> text box stands for a line break that was inserted with the Shift+Enter key combination."
msgstr ""

#: 01/02100001.xhp#PAR_ID2366100
msgid "\\n in the <emph>Replace with</emph> text box stands for a paragraph break that can be entered with the Enter or Return key."
msgstr ""

#: 01/02100001.xhp#PAR_ID3153258.29
msgid "\\t"
msgstr ""

#: 01/02100001.xhp#PAR_ID3157809.30
msgid "Represents a tab. You can also use this expression in the <emph>Replace with</emph> box."
msgstr ""

#: 01/02100001.xhp#PAR_ID3150670.35
msgid "\\b"
msgstr ""

#: 01/02100001.xhp#PAR_ID3153666.36
msgid "Match a word boundary. For example, \"\\bbook\" finds \"bookmark\" but not \"checkbook\" whereas \"book\\b\" finds \"checkbook\" but not \"bookmark\". The discrete word \"book\" is found by both search terms."
msgstr ""

#: 01/02100001.xhp#PAR_ID3149576.37
msgid "^$"
msgstr ""

#: 01/02100001.xhp#PAR_ID3151245.38
msgid "Finds an empty paragraph."
msgstr ""

#: 01/02100001.xhp#PAR_ID3148550.41
msgid "^."
msgstr ""

#: 01/02100001.xhp#PAR_ID3159413.42
msgid "Finds the first character of a paragraph."
msgstr ""

#: 01/02100001.xhp#PAR_ID3147282.43
msgid "& or $0"
msgstr ""

#: 01/02100001.xhp#PAR_ID3153961.44
msgid "Adds the string that was found by the search criteria in the <emph>Search for</emph> box to the term in the <emph>Replace with</emph> box when you make a replacement."
msgstr ""

#: 01/02100001.xhp#PAR_ID3149650.201
msgid "For example, if you enter \"window\" in the <emph>Search for</emph> box and \"&frame\" in the <emph>Replace with</emph> box, the word \"window\" is replaced with \"windowframe\"."
msgstr ""

#: 01/02100001.xhp#PAR_ID3150543.206
msgid "You can also enter an \"&\" in the <emph>Replace with</emph> box to modify the <emph>Attributes</emph> or the <emph>Format</emph> of the string found by the search criteria."
msgstr ""

#: 01/02100001.xhp#PAR_ID3145419.172
msgid "[abc123]"
msgstr ""

#: 01/02100001.xhp#PAR_ID3154630.173
msgid "Represents one of the characters that are between the brackets."
msgstr ""

#: 01/02100001.xhp#PAR_ID3156293.174
msgid "[a-e]"
msgstr ""

#: 01/02100001.xhp#PAR_ID3149167.175
msgid "Represents any of the characters that are between a and e, including both start and end characters"
msgstr ""

#: 01/02100001.xhp#PAR_ID100520090232005
msgid "The characters are ordered by their code numbers."
msgstr ""

#: 01/02100001.xhp#PAR_ID3155994.176
msgid "[a-eh-x]"
msgstr ""

#: 01/02100001.xhp#PAR_ID3148676.177
msgid "Represents any of the characters that are between a-e and h-x."
msgstr ""

#: 01/02100001.xhp#PAR_ID3145318.178
msgid "[^a-s]"
msgstr ""

#: 01/02100001.xhp#PAR_ID3153351.179
msgid "Represents everything that is not between a and s."
msgstr ""

#: 01/02100001.xhp#PAR_ID3156543.180
msgid "\\xXXXX"
msgstr ""

#: 01/02100001.xhp#PAR_ID3153768.181
msgid "Represents a special character based on its four-digit hexadecimal code (XXXX)."
msgstr ""

#: 01/02100001.xhp#PAR_ID3159252.208
msgid "The code for the special character depends on the font used. You can view the codes by choosing <emph>Insert - Special Character</emph>."
msgstr ""

#: 01/02100001.xhp#PAR_ID3153951.186
msgid "|"
msgstr ""

#: 01/02100001.xhp#PAR_ID3154985.187
msgid "Finds the terms that occur before the \"|\" and also finds the terms that occur after the \"|\". For example, \"this|that\" finds \"this\" and \"that\"."
msgstr ""

#: 01/02100001.xhp#PAR_ID3147376.209
msgid "{2}"
msgstr ""

#: 01/02100001.xhp#PAR_ID3150103.210
msgid "Defines the number of times that the character in front of the opening bracket occurs. For example, \"tre{2}\" finds and selects \"tree\"."
msgstr ""

#: 01/02100001.xhp#PAR_ID3151289.211
msgid "{1,2}"
msgstr ""

#: 01/02100001.xhp#PAR_ID3147317.212
msgid "Defines the minimum and maximum number of times that the character in front of the opening bracket can occur. For example, \"tre{1,2}\" finds and selects \"tre\" and \"tree\"."
msgstr ""

#: 01/02100001.xhp#PAR_ID4870754
msgid "{1,}"
msgstr ""

#: 01/02100001.xhp#PAR_ID843836
msgid "Defines the minimum number of times that the character in front of the opening bracket can occur. For example, \"tre{2,}\" finds \"tree\", \"treee\", and \"treeeee\"."
msgstr ""

#: 01/02100001.xhp#PAR_ID3148616.213
msgid "( )"
msgstr ""

#: 01/02100001.xhp#PAR_ID2701803
msgid "In the <emph>Search for</emph> box:"
msgstr ""

#: 01/02100001.xhp#PAR_ID3153573.214
msgid "Defines the characters inside the parentheses as a reference. You can then refer to the first reference in the current expression with \"\\1\", to the second reference with \"\\2\", and so on."
msgstr ""

#: 01/02100001.xhp#PAR_ID3156061.215
msgid "For example, if your text contains the number 13487889 and you search using the regular expression (8)7\\1\\1, \"8788\" is found."
msgstr ""

#: 01/02100001.xhp#PAR_ID2367931
msgid "You can also use () to group terms, for example, \"a(bc)?d\" finds \"ad\" or \"abcd\"."
msgstr ""

#: 01/02100001.xhp#PAR_ID9200109
msgid "In the <emph>Replace with</emph> box:"
msgstr ""

#: 01/02100001.xhp#PAR_ID5766472
msgid "Use $ (dollar) instead of \\ (backslash) to replace references. Use $0 to replace the whole found string."
msgstr ""

#: 01/02100001.xhp#PAR_ID3154790.226
msgid "[:alpha:]"
msgstr ""

#: 01/02100001.xhp#PAR_ID3147397.227
msgid "Represents an alphabetic character. Use [:alpha:]+ to find one of them."
msgstr ""

#: 01/02100001.xhp#PAR_ID3152885.216
msgid "[:digit:]"
msgstr ""

#: 01/02100001.xhp#PAR_ID3150010.217
msgid "Represents a decimal digit. Use [:digit:]+ to find one of them."
msgstr ""

#: 01/02100001.xhp#PAR_ID3153743.224
msgid "[:alnum:]"
msgstr ""

#: 01/02100001.xhp#PAR_ID3153281.225
msgid "Represents an alphanumeric character ([:alpha:] and [:digit:])."
msgstr ""

#: 01/02100001.xhp#PAR_ID3153726.218
msgid "[:space:]"
msgstr ""

#: 01/02100001.xhp#PAR_ID3150961.219
msgid "Represents a space character (but not other whitespace characters)."
msgstr ""

#: 01/02100001.xhp#PAR_ID3150486.220
msgid "[:print:]"
msgstr ""

#: 01/02100001.xhp#PAR_ID3150872.221
msgid "Represents a printable character."
msgstr ""

#: 01/02100001.xhp#PAR_ID3155854.222
msgid "[:cntrl:]"
msgstr ""

#: 01/02100001.xhp#PAR_ID3152576.223
msgid "Represents a nonprinting character."
msgstr ""

#: 01/02100001.xhp#PAR_ID3149958.228
msgid "[:lower:]"
msgstr ""

#: 01/02100001.xhp#PAR_ID3145730.229
msgid "Represents a lowercase character if <emph>Match case</emph> is selected in <emph>Options</emph>."
msgstr ""

#: 01/02100001.xhp#PAR_ID3148455.230
msgid "[:upper:]"
msgstr ""

#: 01/02100001.xhp#PAR_ID3150092.231
msgid "Represents an uppercase character if <emph>Match case</emph> is selected in <emph>Options.</emph>               "
msgstr ""

#: 01/02100001.xhp#HD_ID5311441
msgid "Examples"
msgstr ""

#: 01/02100001.xhp#PAR_ID956834773
msgid "e([:digit:])?    -- finds 'e' followed by zero or one digit. Note that currently all named character classes like [:digit:] must be enclosed in parentheses."
msgstr ""

#: 01/02100001.xhp#PAR_ID952368773
msgid "^([:digit:])$    -- finds lines or cells with exactly one digit."
msgstr ""

#: 01/02100001.xhp#PAR_ID9568773
msgid "You can combine the search terms to form complex searches."
msgstr ""

#: 01/02100001.xhp#HD_ID71413
msgid "To find three-digit numbers alone in a paragraph"
msgstr ""

#: 01/02100001.xhp#PAR_ID2924283
msgid "^[:digit:]{3}$"
msgstr ""

#: 01/02100001.xhp#PAR_ID5781731
msgid "^ means the match has to be at the start of a paragraph, "
msgstr ""

#: 01/02100001.xhp#PAR_ID6942045
msgid "[:digit:] matches any decimal digit, "
msgstr ""

#: 01/02100001.xhp#PAR_ID4721823
msgid "{3} means there must be exactly 3 copies of \"digit\", "
msgstr ""

#: 01/02100001.xhp#PAR_ID5806756
msgid "$ means the match must end a paragraph. "
msgstr ""

