#: explorer/database/02010100.xhp#PAR_ID3154179.241
msgid "Null"
msgstr ""

#: explorer/database/02010100.xhp#PAR_ID3147035.242
msgid "IS NULL"
msgstr ""

#: explorer/database/02010100.xhp#PAR_ID3159385.243
