msgid "Status"
msgstr ""

#: explorer/database/02010100.xhp#PAR_ID3147481.231
msgid "Query criterion"
msgstr ""

#: explorer/database/02010100.xhp#PAR_ID3155187.232
msgid "Example"
msgstr ""

