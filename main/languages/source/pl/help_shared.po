msgid "CYAN"
msgstr ""

#: 01/05020301.xhp#PAR_ID3148676.30
msgid "GREEN"
msgstr ""

#: 01/05020301.xhp#PAR_ID3154123.31
msgid "BLACK"
msgstr ""

#: 01/05020301.xhp#PAR_ID3149167.32
msgid "BLUE"
msgstr ""

#: 01/05020301.xhp#PAR_ID3158407.33
msgid "MAGENTA"
msgstr ""

#: 01/05020301.xhp#PAR_ID3149560.34
