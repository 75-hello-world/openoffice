msgstr ""

#: explorer/database/02010100.xhp#PAR_ID3146090.256
msgid "{D'YYYY-MM-DD HH:MM:SS'}"
msgstr ""

#: explorer/database/02010100.xhp#PAR_ID3155947.257
msgid "Time"
msgstr ""

#: explorer/database/02010100.xhp#PAR_ID3150964.258
msgid "{D'HH:MM:SS'}"
msgstr ""

#: explorer/database/02010100.xhp#PAR_ID3151220.260
msgid "$[officename] also supports the following <emph>Escape sequences</emph> known from ODBC and JDBC:"
msgstr ""

