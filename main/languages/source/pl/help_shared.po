msgstr ""

#: optionen/01030300.xhp#PAR_ID7021088
msgid "<ahelp hid=\".\" visibility=\"hidden\">Removes all entries from the list.</ahelp>"
msgstr ""

#: optionen/01030300.xhp#PAR_ID1981261
