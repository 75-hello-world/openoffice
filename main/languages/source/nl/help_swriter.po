msgid  "Example: 1 XOR 0 displays 1 (true)"
msgstr "Voorbeeld: 1 XOR 0 geeft -1 (waar)"

#: source/text/swriter/02/14020000.xhp#par_id3152925.79
msgid  "Boolean And"
msgstr "Booleaans En"

