msgid  "Find only in"
msgstr "Etsi vain"

#: source/text/swriter/01/mm_finent.xhp#par_idN10559
msgid  "\<ahelp hid=\".\"\>Restricts the search to one data field. \</ahelp\>"
msgstr "<ahelp hid=\".\">Rajoitetaan haku yhteen tietokenttään.</ahelp>"

#: source/text/swriter/01/mm_finent.xhp#par_idN1055C
msgid  "Select the data field where you want to search for the text."
msgstr "Valitaan tietokenttä, josta tekstiä haetaan."

