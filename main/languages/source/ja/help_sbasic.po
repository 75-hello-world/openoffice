msgid  "Dim sMsg as String"
msgstr "Dim sMsg as String"

#: source/text/sbasic/shared/03020301.xhp#par_id3156344.15
msgid  "aFile = \"c:\\\\data.txt\""
msgstr "aFile = \"c:\\data.txt\""

#: source/text/sbasic/shared/03020301.xhp#par_id3148663.17
msgid  "iNumber = Freefile"
msgstr "iNumber = Freefile"

#: source/text/sbasic/shared/03020301.xhp#par_id3153379.18
msgid  "Open aFile For Output As #iNumber"
msgstr "Open aFile For Output As #iNumber"

#: source/text/sbasic/shared/03020301.xhp#par_id3153360.19
msgid  "Print #iNumber, \"First line of text\""
msgstr "Print #iNumber, \"First line of text\""

#: source/text/sbasic/shared/03020301.xhp#par_id3148797.20
msgid  "Print #iNumber, \"Another line of text\""
msgstr "Print #iNumber, \"Another line of text\""

#: source/text/sbasic/shared/03020301.xhp#par_id3154684.21
msgid  "Close #iNumber"
msgstr "Close #iNumber"

#: source/text/sbasic/shared/03020301.xhp#par_id3153104.25
msgid  "iNumber = Freefile"
msgstr "iNumber = Freefile"

#: source/text/sbasic/shared/03020301.xhp#par_id3144761.26
msgid  "Open aFile For Input As iNumber"
msgstr "Open aFile For Input As iNumber"

#: source/text/sbasic/shared/03020301.xhp#par_id3153193.27
msgid  "While not eof(iNumber)"
msgstr "While not eof(iNumber)"

#: source/text/sbasic/shared/03020301.xhp#par_id3158408.28
msgid  "Line Input #iNumber, sLine"
msgstr "Line Input #iNumber, sLine"

#: source/text/sbasic/shared/03020301.xhp#par_id3149203.29
msgid  "If sLine <>\"\" then"
msgstr "If sLine <>\"\" then"

#: source/text/sbasic/shared/03020301.xhp#par_id3153770.30
msgid  "sMsg = sMsg & sLine & chr(13)"
msgstr "sMsg = sMsg & sLine & chr(13)"

#: source/text/sbasic/shared/03020301.xhp#par_id3153367.32
msgid  "end if"
msgstr "end if"

#: source/text/sbasic/shared/03020301.xhp#par_id3147318.33
msgid  "wend"
msgstr "Wend"

#: source/text/sbasic/shared/03020301.xhp#par_id3152939.34
msgid  "Close #iNumber"
msgstr "Close #iNumber"

#: source/text/sbasic/shared/03020301.xhp#par_id3153726.38
msgid  "Msgbox sMsg"
msgstr "Msgbox sMsg"

#: source/text/sbasic/shared/03020301.xhp#par_id3153092.35
msgid  "End Sub"
msgstr "End Sub"

#: source/text/sbasic/shared/03020302.xhp#tit
msgid  "Loc Function [Runtime]"
msgstr "Loc 関数 [実行時]"

