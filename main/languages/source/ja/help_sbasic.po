msgstr "<image id=\"img_id3150318\" src=\"res/commandimagelist/sc_progressbar.png\" width=\"5.64mm\" height=\"5.64mm\"><alt  id=\"alt_id3150318\" xml-lang=\"ja-JP\" >マーク</alt></image>"

#: source/text/sbasic/shared/02/20000000.xhp#par_id3157979.32
