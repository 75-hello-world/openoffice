msgid  "\<image id=\"img_id3150203\" src=\"res/commandimagelist/sc_vscrollbar.png\" width=\"0.1665inch\" height=\"0.1665inch\"\>\<alt id=\"alt_id3150203\"\>Icon\</alt\>\</image\>"
msgstr "<image id=\"img_id3150203\" src=\"res/commandimagelist/sc_vscrollbar.png\" width=\"4.23mm\" height=\"4.23mm\"><alt id=\"alt_id3150203\" xml-lang=\"ja-JP\">アイコン</alt></image>"

#: source/text/sbasic/shared/02/20000000.xhp#par_id3155376.27
msgid  "\<ahelp hid=\".uno:VScrollbar\"\>Adds a vertical scrollbar to the dialog.\</ahelp\>"
