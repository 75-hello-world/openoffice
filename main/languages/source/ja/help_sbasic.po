msgstr "11 : 明るいシアン"

#: source/text/sbasic/shared/03010304.xhp#par_id3154943.22
msgid  "12 : Light Red"
msgstr "12 : 明るい赤"

#: source/text/sbasic/shared/03010304.xhp#par_id3150715.23
msgid  "13 : Light Magenta"
msgstr "13 : 明るいマゼンタ"

