msgstr "REM 40 文字長の文字列変数中に「SBX」を左揃えで格納します"

#: source/text/sbasic/shared/03120304.xhp#par_id3147126.31
msgid  "LSet sVar = sExpr"
msgstr "LSet sVar = sExpr"

#: source/text/sbasic/shared/03120304.xhp#par_id3154792.32
msgid  "Print \">\"; sVar; \"<\""
msgstr "Print \">\"; sVar; \"<\""

