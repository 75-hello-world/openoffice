msgid  "\<link href=\"text/sbasic/shared/03120103.xhp\" name=\"Str Function [Runtime]\"\>Str Function [Runtime]\</link\>"
msgstr "<link href=\"text/sbasic/shared/03120103.xhp\" name=\"Str 関数 [実行時]\">Str 関数 [実行時]</link>"

#: source/text/sbasic/shared/03120103.xhp#par_id3155100.2
msgid  "Converts a numeric expression into a string."
msgstr "数値表式を文字列に変換します。"

#: source/text/sbasic/shared/03120103.xhp#hd_id3109850.3
msgid  "Syntax:"
msgstr "構文:"

#: source/text/sbasic/shared/03120103.xhp#par_id3149497.4
msgid  "Str (Expression)"
msgstr "Str (Expression)"

#: source/text/sbasic/shared/03120103.xhp#hd_id3150040.5
msgid  "Return value:"
msgstr "戻り値:"

#: source/text/sbasic/shared/03120103.xhp#par_id3146117.6
msgid  "String"
msgstr "文字列"

#: source/text/sbasic/shared/03120103.xhp#hd_id3155805.7
msgid  "Parameters:"
msgstr "パラメータ:"

#: source/text/sbasic/shared/03120103.xhp#par_id3149178.8
msgid  "\<emph\>Expression: \</emph\>Any numeric expression."
msgstr "<emph>Expression:</emph>任意の数値表式。"

#: source/text/sbasic/shared/03120103.xhp#par_id3146958.9
msgid  "The \<emph\>Str\</emph\> function converts a numeric variable, or the result of a calculation into a string. Negative numbers are preceded by a minus sign. Positive numbers are preceded by a space (instead of the plus sign)."
msgstr "<emph>Str</emph> 関数は、数値変数および数値演算の結果を、文字列に変換します。負の値にはマイナス記号が付きます。正の値にはスペース記号が付きます (プラス記号は付きません)。"

#: source/text/sbasic/shared/03120103.xhp#hd_id3155419.10
msgid  "Example:"
msgstr "例:"

#: source/text/sbasic/shared/03120103.xhp#par_id3149514.11
msgid  "Sub ExampleStr"
msgstr "Sub ExampleStr"

#: source/text/sbasic/shared/03120103.xhp#par_id3150771.12
msgid  "Dim iVar As Single"
msgstr "Dim iVar As Single"

#: source/text/sbasic/shared/03120103.xhp#par_id3153626.13
msgid  "Dim sVar As String"
msgstr "Dim sVar As String"

#: source/text/sbasic/shared/03120103.xhp#par_id3145069.14
msgid  "iVar = 123.123"
msgstr "iVar = 123.123"

#: source/text/sbasic/shared/03120103.xhp#par_id3153897.15
msgid  "sVar = LTrim(Str(iVar))"
msgstr "sVar = LTrim(Str(iVar))"

#: source/text/sbasic/shared/03120103.xhp#par_id3154924.16
msgid  "Msgbox sVar & chr(13) & Str(iVar)"
msgstr "Msgbox sVar & chr(13) & Str(iVar)"

#: source/text/sbasic/shared/03120103.xhp#par_id3152811.17
msgid  "end sub"
msgstr "end sub"

#: source/text/sbasic/shared/03120104.xhp#tit
msgid  "Val Function [Runtime]"
msgstr "Val 関数 [実行時]"

