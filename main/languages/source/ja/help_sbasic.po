msgid  "Cbyte( expression )"
msgstr "Cbyte( expression )"

#: source/text/sbasic/shared/03120105.xhp#hd_id3145315.5
msgid  "Return value:"
msgstr "戻り値:"

#: source/text/sbasic/shared/03120105.xhp#par_id3148473.6
msgid  "Byte"
msgstr "バイト型"

#: source/text/sbasic/shared/03120105.xhp#hd_id3147530.7
msgid  "Parameters:"
msgstr "パラメータ:"

#: source/text/sbasic/shared/03120105.xhp#par_id3145068.8
msgid  "\<emph\>Expression:\</emph\> A string or a numeric expression."
msgstr "<emph>Expression:</emph> 文字列ないし数値表式。"

