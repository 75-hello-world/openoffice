msgstr "<image id=\"img_id3153345\" src=\"res/commandimagelist/sc_basicstepinto.png\" width=\"0.222inch\" height=\"0.222inch\"><alt id=\"alt_id3153345\">アイコン</alt></image>"

#: source/text/sbasic/shared/02/11050000.xhp#par_id3147573.3
msgid  "Single Step"
msgstr "シングルステップ"

#: source/text/sbasic/shared/02/11050000.xhp#par_id3149235.6
