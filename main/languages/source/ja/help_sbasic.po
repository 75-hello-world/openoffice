msgid  "MsgBox stext,0,\"Color \" & iColor"
msgstr "MsgBox stext,0,\"Color \" & iColor"

#: source/text/sbasic/shared/03010304.xhp#par_id3154705.34
msgid  "End Sub"
msgstr "End Sub"

