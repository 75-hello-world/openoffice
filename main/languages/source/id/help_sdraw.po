msgid  "Opens the \<emph\>Position and Size\</emph\> dialog."
msgstr "Membuka dialog <emph>Posisi dan Ukuran</emph>."

#: source/text/sdraw/04/01020000.xhp#hd_id3148870.21
msgid  "F5"
msgstr "F5"

#: source/text/sdraw/04/01020000.xhp#par_id3153917.22
msgid  "Opens the \<emph\>Navigator\</emph\>."
msgstr "Membuka <emph>Navigator</emph>."

#: source/text/sdraw/04/01020000.xhp#hd_id3157982.25
msgid  "F7"
msgstr "F7"

