#: ext/adabas/AdabasNewDb.src#DLG_ADABAS_NEWDB.FT_TRANSACTIONLOG_SIZE.FIXEDTEXT.TEXT
msgid  "Transaction ~file (MB)"
msgstr "Трансакциона ~датотека (МБ)"

#: ext/adabas/AdabasNewDb.src#DLG_ADABAS_NEWDB.FT_DATADEVSPACE_SIZE.FIXEDTEXT.TEXT
msgid  "Data~base size (MB)"
msgstr "Величина базе по~датака (МБ)"

#: ext/adabas/AdabasNewDb.src#DLG_ADABAS_NEWDB.FT_CACHE_SIZE.FIXEDTEXT.TEXT
msgid  "Data ~buffer size (MB)"
msgstr "Величина ~бафера податка (МБ)"

#: ext/adabas/AdabasNewDb.src#DLG_ADABAS_NEWDB.STR_BACKUPFILE_LOAD.STRING.TEXT
msgid  "Backup file information"
msgstr "Подаци о резервној датотеци "

#: ext/adabas/AdabasNewDb.src#DLG_ADABAS_NEWDB.STRING.TEXT
msgid  "Create New Adabas D Database"
msgstr "Направите нову Адабас Д базу података"

#: ext/adabas/AdabasNewDb.src#STR_ADABAS_NO_PARAM_SET.STRING.TEXT
msgid  "No entry could be written in the configuration file."
msgstr "Ниједан унос се не може уписати у датотеку за конфигурацију."

#: ext/adabas/AdabasNewDb.src#STR_ADABAS_NO_DB_CREATED.STRING.TEXT
msgid  "An error occurred as the DB files were being created."
msgstr "Грешка током израде датотека базе података."

#: ext/adabas/AdabasNewDb.src#STR_ADABAS_NO_SYSTEM_TABLES.STRING.TEXT
msgid  "An error occurred as the system tables were being created."
msgstr "Грешка током израде системских табела."

#: ext/adabas/AdabasNewDb.src#STR_ADABAS_NO_BACKUP_FILE.STRING.TEXT
msgid  "The backup file entered does not exist."
msgstr "Унета резервна копија датотеке не постоји."

#: ext/adabas/AdabasNewDb.src#INFO_STR_MAX_FILE_LENGTH.INFOBOX.MESSAGE
msgid  "The fully qualified file cannot be longer than \\'#\\' characters."
msgstr "Пуно квалификовано име датотеке не може бити дуже од \\'#\\' карактера."

#: ext/adabas/AdabasNewDb.src#STR_NO_SAME_USER.STRING.TEXT
msgid  "The administrator and the control user cannot have the same name!"
msgstr "Администратор и надзорник не могу имати исто име!"

#: ext/adabas/AdabasNewDb.src#STR_DEVSPACE_NO_FILE.STRING.TEXT
msgid  "No name has been entered for the $name$ file!"
msgstr "Није унети ниједно име за датотеку $name$!"

#: ext/adabas/AdabasNewDb.src#STR_NO_PERMISSION.STRING.TEXT
msgid  "You have to have administrator rights to create a new database!"
msgstr "Морате имати администраторска права да бисте направили нову базу!"

#: ext/adabas/AdabasNewDb.src#STR_FILE_OR_DIR_UNKNOWN.STRING.TEXT
msgid  "The specified file name or directory does not exist."
msgstr "Наведено име датотеке или директоријума не постоји."

#: ext/adabas/AdabasNewDb.src#STR_ERROR_IN_ADABAS_INSTALLATION.STRING.TEXT
msgid  "No new database can be created.\nPlease check your Adabas D installation, especially the variables \"DBROOT\" and \"DBWORK\"."
msgstr "Није могуће направити нову базу података.\nПроверите Адабас Д инсталацију, посебно променљиве „DBROOT“ и „DBWORK“."

#: ext/adabas/AdabasNewDb.src#STR_STAT_WARNING.STRING.TEXT
msgid  "Warning!"
msgstr "Упозорење!"

#: ext/adabas/AdabasNewDb.src#STR_ADABAS_CREATEFILES.STRING.TEXT
msgid  "Creating database files."
msgstr "Правим датотеке базе података."

#: ext/adabas/AdabasNewDb.src#STR_ADABAS_CREATEPARAM_FILE.STRING.TEXT
msgid  "Creating database configuration file."
msgstr "Правим датотеку базе података за конфигурацију."

#: ext/adabas/AdabasNewDb.src#STR_ADABAS_START_DATABASE.STRING.TEXT
msgid  "Starting database for the first time."
msgstr "Покрећем базу података први пут."

#: ext/adabas/AdabasNewDb.src#STR_ADABAS_DB_EXISTS.STRING.TEXT
msgid  "The name entered already exists."
msgstr "Унето име већ постоји."

#: ext/adabas/AdabasNewDb.src#STR_ADABAS_NO_DEFAULTNAME.STRING.TEXT
msgid  "No default names could be set for the database files. The total length is more than 40 characters."
msgstr "Нема подразумеваних имена која се могу поставити за датотеке базе података. Укупна дужина је већа од 40 карактера."

#: ext/adabas/AdabasNewDb.src#STR_ADABAS_NO_ACCESSRIGHTS.STRING.TEXT
msgid  "No default names could be set for the database files. There is no write access."
msgstr "Нема подразумеваних имена која се могу поставити за датотеке базе података. Не постоји одобрење уписа."

#: ext/adabas/AdabasNewDb.src#STR_ADABAS_CANNOT_CONVERT.STRING.TEXT
msgid  "The current database need to be converted. Please insert control user  and password."
msgstr "Тренутна база података мора да се конвертује. Упишите име надзорника и лозинку."

#: ext/adabas/AdabasNewDb.src#STR_ADABAS_PASSWORD_ILLEGAL.STRING.TEXT
