#: ui/src/scfuncs.src#RID_SC_FUNCTION_DESCRIPTIONS2.SC_OPCODE_EUROCONVERT.10.STRING.TEXT
msgid  "triangulation_precision"
msgstr "háromszögelési_pontosság"

