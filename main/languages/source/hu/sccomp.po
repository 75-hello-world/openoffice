#: solver/solver.src#RID_SOLVER_COMPONENT.STRING.TEXT
msgid "%PRODUCTNAME Linear Solver"
msgstr "%PRODUCTNAME lineáris megoldó"

#: solver/solver.src#RID_PROPERTY_NONNEGATIVE.STRING.TEXT
msgid "Assume variables as non-negative"
msgstr "A változók nem negatívak"

#: solver/solver.src#RID_PROPERTY_INTEGER.STRING.TEXT
msgid "Assume variables as integer"
msgstr "A változók egész számok"

#: solver/solver.src#RID_PROPERTY_TIMEOUT.STRING.TEXT
msgid "Solving time limit (seconds)"
msgstr "Megoldási időkorlát (másodperc)"

#: solver/solver.src#RID_PROPERTY_EPSILONLEVEL.STRING.TEXT
msgid "Epsilon level (0-3)"
msgstr "Epszilon szint (0-3)"

#: solver/solver.src#RID_PROPERTY_LIMITBBDEPTH.STRING.TEXT
msgid "Limit branch-and-bound depth"
msgstr "Elágazási és kötési mélység korlátozása"

#: solver/solver.src#RID_ERROR_NONLINEAR.STRING.TEXT
msgid "The model is not linear."
msgstr "A modell nem lineáris."

#: solver/solver.src#RID_ERROR_EPSILONLEVEL.STRING.TEXT
msgid "The epsilon level is invalid."
msgstr "Az epszilon szint érvénytelen."

#: solver/solver.src#RID_ERROR_INFEASIBLE.STRING.TEXT
msgid "The model is infeasible. Check limiting conditions."
msgstr "A modell megvalósíthatatlan. Ellenőrizze a korlátozó feltételeket."

#: solver/solver.src#RID_ERROR_UNBOUNDED.STRING.TEXT
msgid "The model is unbounded."
msgstr "A modell nem kötött."

#: solver/solver.src#RID_ERROR_TIMEOUT.STRING.TEXT
msgid "The time limit was reached."
msgstr "A számítási idő túllépte az időkorlátot."
