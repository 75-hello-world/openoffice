msgstr "<link href=\"text/smath/main0101.xhp\" name=\"Archivo\">Archivo</link>"

#: source/text/smath/main0101.xhp#par_id3155959.2
msgid  "This menu contains the general commands for working with formula documents, such as open, save and print."
msgstr "Este menú contiene los comandos generales para trabajar con documentos de fórmula, como por ejemplo, abrir, guardar e imprimir los documentos."

#: source/text/smath/main0101.xhp#hd_id3148489.3
