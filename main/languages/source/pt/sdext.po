msgstr "Eliminar áreas ~de imagem cortadas"

#: minimizer/minimizer.src#STR_IMAGE_RESOLUTION.STRING.TEXT
msgid "Reduce ~image resolution"
msgstr "Reduzir resolução de ~imagem"

#: minimizer/minimizer.src#STR_IMAGE_RESOLUTION_0.STRING.TEXT
msgid "<no change>"
msgstr "<inalterada>"

#: minimizer/minimizer.src#STR_IMAGE_RESOLUTION_1.STRING.TEXT
