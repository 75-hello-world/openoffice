msgid  "\<ahelp hid=\".uno:AdvancedMode\"\>Modifies the shape, orientation or fill of the selected object(s).\</ahelp\>"
msgstr "<ahelp hid=\".uno:AdvancedMode\">Modifica a forma, a orientação ou o preenchimento do(s) objeto(s) selecionado(s).</ahelp>"

#: source/text/simpress/02/10030000.xhp#par_id3149665
msgid  "\<image id=\"img_id3154490\" src=\"res/commandimagelist/sc_toggleobjectrotatemode.png\" width=\"5.64mm\" height=\"5.64mm\"\>\<alt id=\"alt_id3154490\"\>Icon\</alt\>\</image\>"
