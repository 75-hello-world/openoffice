msgid  "Mozilla / Netscape"
msgstr "Адресная книга Mozilla / Netscape"

#: abpilot/abspilot.src#RID_PAGE_SELECTABTYPE.RB_THUNDERBIRD.RADIOBUTTON.TEXT
msgid  "Thunderbird"
msgstr "Адресная книга Thunderbird"

#: abpilot/abspilot.src#RID_PAGE_SELECTABTYPE.RB_KAB.RADIOBUTTON.TEXT
