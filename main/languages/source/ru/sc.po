msgid  "invest"
msgstr "Инвестиция"

#: ui/src/scfuncs.src#RID_SC_FUNCTION_DESCRIPTIONS1.SC_OPCODE_ISPMT.9.STRING.TEXT
msgid  "Amount of the investment."
msgstr "Сумма инвестиции."

#: ui/src/scfuncs.src#RID_SC_FUNCTION_DESCRIPTIONS1.SC_OPCODE_LAUFZ.1.STRING.TEXT
