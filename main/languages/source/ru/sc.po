#: ui/src/scfuncs.src#RID_SC_FUNCTION_DESCRIPTIONS1.SC_OPCODE_ISPMT.2.STRING.TEXT
msgid  "rate"
msgstr "Процент"

#: ui/src/scfuncs.src#RID_SC_FUNCTION_DESCRIPTIONS1.SC_OPCODE_ISPMT.3.STRING.TEXT
msgid  "Interest rate for a single amortization rate."
