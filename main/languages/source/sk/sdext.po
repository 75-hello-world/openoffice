msgid  "~Choose settings for Presentation Minimizer"
msgstr "Vyberte nastavenia pre Presentation Minimizer"

#: minimizer/minimizer.src#STR_REMOVE.STRING.TEXT
msgid  "~Delete"
msgstr "Vymazať"

