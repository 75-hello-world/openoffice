msgid  "\<link href=\"text/swriter/01/04120225.xhp\" name=\"User-Defined\"\>User-Defined\</link\>"
msgstr "<link href=\"text/swriter/01/04120225.xhp\" name=\"Brugerdefineret\">Brugerdefineret</link>"

