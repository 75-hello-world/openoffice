#: core/resource/strings.src#RID_STR_TRIED_OPEN_TABLE.STRING.TEXT
msgid  "Tried to open the table $name$."
msgstr "Rinneadh iarracht tábla $name$ a oscailt."

#: core/resource/strings.src#RID_STR_CONNECTION_INVALID.STRING.TEXT
msgid  "No connection could be established."
msgstr "Níorbh fhéidir ceangal a bhunú."

#: core/resource/strings.src#RID_STR_TABLE_IS_FILTERED.STRING.TEXT
msgid  "The table $name$ already exists. It is not visible because it has been filtered out."
msgstr "Tá tábla $name$ ann cheana. Níl sé le feiceáil mar gur scagadh amach é."

#: core/resource/strings.src#RID_STR_NEED_CONFIG_WRITE_ACCESS.STRING.TEXT
msgid  "You have no write access to the configuration data the object is based on."
msgstr "Níl cead agat scríobh ar na sonraí cumraíochta ar a mbunaítear an réad."

#: core/resource/strings.src#RID_STR_COULDNOTCONNECT_UNSPECIFIED.STRING.TEXT
msgid  "The connection to the external data source could not be established. An unknown error occurred. The driver is probably defective."
msgstr "Níorbh fhéidir ceangal a bhunú leis an bhfoinse sheachtrach sonraí. Tharla earráid anaithnid. Is dócha go bhfuil an tiománaí lochtach."

#: core/resource/strings.src#RID_STR_COULDNOTCONNECT_NODRIVER.STRING.TEXT
msgid  "The connection to the external data source could not be established. No SDBC driver was found for the given URL."
msgstr "Níorbh fhéidir ceangal a bhunú leis an bhfoinse sheachtrach sonraí. Níor aimsíodh tiománaí SDBC i gcomhair an URL sonraithe."

#: core/resource/strings.src#RID_STR_COULDNOTLOAD_MANAGER.STRING.TEXT
msgid  "The connection to the external data source could not be established. The SDBC driver manager could not be loaded."
msgstr "Níorbh fhéidir ceangal a bhunú leis an bhfoinse sheachtrach sonraí. Níorbh fhéidir bainisteoir na dtiománaithe SDBC a luchtú."

#: core/resource/strings.src#RID_STR_FORM.STRING.TEXT
msgid  "Form"
msgstr "Foirm"

#: core/resource/strings.src#RID_STR_REPORT.STRING.TEXT
msgid  "Report"
msgstr "Tuairisc"

#: core/resource/strings.src#RID_STR_DATASOURCE_NOT_STORED.STRING.TEXT
msgid  "The data source was not saved. Please use the interface XStorable to save the data source."
msgstr "Níor sábháladh an fhoinse sonraí. Úsáid an comhéadan XStorable chun an fhoinse sonraí a shábháil."

#: core/resource/strings.src#RID_STR_ONLY_QUERY.STRING.TEXT
msgid  "The given command is not a SELECT statement.\nOnly queries are allowed."
msgstr "Ní ráiteas SELECT é an t-ordú a tugadh.\nNí cheadaítear ach iarratais."

#: core/resource/strings.src#RID_STR_NO_VALUE_CHANGED.STRING.TEXT
msgid  "No values were modified."
msgstr "Níor mionathraíodh aon luach."

#: core/resource/strings.src#RID_STR_NO_XROWUPDATE.STRING.TEXT
msgid  "Values could not be inserted. The XRowUpdate interface is not supported by ResultSet."
msgstr "Ní fhéadfaí luachanna a chur isteach. Ní thacaíonn ResultSet leis an gcomhéadan XRowUpdate."

#: core/resource/strings.src#RID_STR_NO_XRESULTSETUPDATE.STRING.TEXT
msgid  "Values could not be inserted. The XResultSetUpdate interface is not supported by ResultSet."
msgstr "Ní fhéadfaí luachanna a chur isteach. Ní thacaíonn ResultSet leis an gcomhéadan XResultSetUpdate."

#: core/resource/strings.src#RID_STR_NO_UPDATE_MISSING_CONDITION.STRING.TEXT
msgid  "Values could not be modified, due to a missing condition statement."
msgstr "Ní fhéadfaí luachanna a mhionathrú, mar go raibh ráiteas coinníollach in easnamh."

#: core/resource/strings.src#RID_STR_NO_COLUMN_ADD.STRING.TEXT
msgid  "The adding of columns is not supported."
msgstr "Ní thacaítear le suimiú colún."

#: core/resource/strings.src#RID_STR_NO_COLUMN_DROP.STRING.TEXT
msgid  "The dropping of columns is not supported."
msgstr "Ní thacaítear le baint colún."

#: core/resource/strings.src#RID_STR_NO_CONDITION_FOR_PK.STRING.TEXT
msgid  "The WHERE condition could not be created for the primary key."
msgstr "Ní fhéadfaí an coinníoll WHERE a chruthú don phríomheochair."

#: core/resource/strings.src#RID_STR_COLUMN_UNKNOWN_PROP.STRING.TEXT
msgid  "The column does not support the property '%value'."
msgstr "Ní thacaíonn an colún leis an airí '%value'."

#: core/resource/strings.src#RID_STR_COLUMN_NOT_SEARCHABLE.STRING.TEXT
msgid  "The column is not searchable!"
msgstr "Ní féidir an colún a chuardach!"

#: core/resource/strings.src#RID_STR_NOT_SEQUENCE_INT8.STRING.TEXT
msgid  "The value of the columns is not of the type Sequence<sal_Int8>."
msgstr "Níl luach na gcolún den chineál Sequence<sal_Int8>."

#: core/resource/strings.src#RID_STR_COLUMN_NOT_VALID.STRING.TEXT
msgid  "The column is not valid."
msgstr "Níl an colún bailí."

#: core/resource/strings.src#RID_STR_COLUMN_MUST_VISIBLE.STRING.TEXT
msgid  "The column '%name' must be visible as a column."
msgstr "Caithfidh colún '%name' a bheith le feiceáil mar cholún."

#: core/resource/strings.src#RID_STR_NO_XQUERIESSUPPLIER.STRING.TEXT
msgid  "The interface XQueriesSupplier is not available."
msgstr "Níl an comhéadan XQueriesSupplier ar fáil."

#: core/resource/strings.src#RID_STR_NOT_SUPPORTED_BY_DRIVER.STRING.TEXT
msgid  "The driver does not support this function."
msgstr "Ní thacaíonn an tiománaí leis an bhfeidhm."

#: core/resource/strings.src#RID_STR_NO_ABS_ZERO.STRING.TEXT
msgid  "An 'absolute(0)' call is not allowed."
msgstr "Ní cheadaítear glao 'absolute(0)'."

#: core/resource/strings.src#RID_STR_NO_RELATIVE.STRING.TEXT
msgid  "Relative positioning is not allowed in this state."
msgstr "Níl suíomh coibhneasta ceadaithe sa staid seo."

#: core/resource/strings.src#RID_STR_NO_REFESH_AFTERLAST.STRING.TEXT
msgid  "A row cannot be refreshed when the ResultSet is positioned after the last row."
msgstr "Ní féidir ró a athnuachan nuair atá an ResultSet suite tar éis an ró deiridh."

#: core/resource/strings.src#RID_STR_NO_MOVETOINSERTROW_CALLED.STRING.TEXT
msgid  "A new row cannot be inserted when the ResultSet is not first moved to the insert row."
msgstr "Ní féidir ró nua a ionsá mura n-aistrítear an ResultSet ar dtús chuig an ró ionsá."

#: core/resource/strings.src#RID_STR_NO_UPDATEROW.STRING.TEXT
msgid  "A row cannot be modified in this state"
msgstr "Ní féidir ró a mhionathrú sa staid seo"

#: core/resource/strings.src#RID_STR_NO_DELETEROW.STRING.TEXT
msgid  "A row cannot be deleted in this state."
msgstr "Ní féidir ró a scriosadh sa staid seo."

#: core/resource/strings.src#RID_STR_NO_TABLE_RENAME.STRING.TEXT
msgid  "The driver does not support table renaming."
msgstr "Ní thacaíonn an tiománaí le hathainmniú comhad."

#: core/resource/strings.src#RID_STR_NO_ALTER_COLUMN_DEF.STRING.TEXT
msgid  "The driver does not support the modification of column descriptions."
msgstr "Ní thacaíonn an tiománaí le mionathrú ar thuairisc na gcolún."

#: core/resource/strings.src#RID_STR_COLUMN_ALTER_BY_NAME.STRING.TEXT
msgid  "The driver does not support the modification of column descriptions by changing the name."
msgstr "Ní thacaíonn an tiománaí le mionathrú na dtuairiscí colún trí ainm a athrú."

#: core/resource/strings.src#RID_STR_COLUMN_ALTER_BY_INDEX.STRING.TEXT
msgid  "The driver does not support the modification of column descriptions by changing the index."
msgstr "Ní thacaíonn an tiománaí le mionathrú ar thuairisc na gcolún trí innéacs a athrú."

#: core/resource/strings.src#RID_STR_FILE_DOES_NOT_EXIST.STRING.TEXT
msgid  "The file \"$file$\" does not exist."
msgstr "Níl an comhad \"$file$\" ann."

#: core/resource/strings.src#RID_STR_TABLE_DOES_NOT_EXIST.STRING.TEXT
msgid  "There exists no table named \"$table$\"."
msgstr "Níl aon tábla darb ainm \"$table$\" ann."

#: core/resource/strings.src#RID_STR_QUERY_DOES_NOT_EXIST.STRING.TEXT
msgid  "There exists no query named \"$table$\"."
msgstr "Níl aon iarratas darb ainm \"$table$\" ann."

#: core/resource/strings.src#RID_STR_CONFLICTING_NAMES.STRING.TEXT
msgid  "There are tables in the database whose names conflict with the names of existing queries. To make full use of all queries and tables, make sure they have distinct names."
msgstr "Tá táblaí sa bhunachar sonraí a bhfuil a n-ainmneacha i gcoinbhleacht le hainmneacha na n-iarratas atá ann cheana. Chun lánúsáid a bhaint as gach iarratas agus tábla, déan cinnte go bhfuil ainmneacha ar leith acu."

#: core/resource/strings.src#RID_STR_COMMAND_LEADING_TO_ERROR.STRING.TEXT
msgid  "The SQL command leading to this error is:\n\n$command$"
msgstr "An t-ordú SQL a threoraíonn chuig an earráid seo:\n\n$command$"

#: core/resource/strings.src#RID_STR_STATEMENT_WITHOUT_RESULT_SET.STRING.TEXT
msgid  "The SQL command does not describe a result set."
msgstr "Ní dhéanann an t-ordú SQL tagairt do thacar toraidh."

#: core/resource/strings.src#RID_STR_NAME_MUST_NOT_BE_EMPTY.STRING.TEXT
msgid  "The name must not be empty."
msgstr "Ní cheadaítear ainm folamh."

#: core/resource/strings.src#RID_STR_NO_NULL_OBJECTS_IN_CONTAINER.STRING.TEXT
msgid  "The container cannot contain NULL objects."
msgstr "Ní cheadaítear réada NULL sa choimeádán."

#: core/resource/strings.src#RID_STR_NAME_ALREADY_USED.STRING.TEXT
msgid  "There already is an object with the given name."
msgstr "Tá réad ann cheana leis an ainm seo air."

#: core/resource/strings.src#RID_STR_OBJECT_CONTAINER_MISMATCH.STRING.TEXT
msgid  "This object cannot be part of this container."
msgstr "Ní cheadaítear an réad seo mar chuid den choimeádán seo."

#: core/resource/strings.src#RID_STR_OBJECT_ALREADY_CONTAINED.STRING.TEXT
msgid  "The object already is, with a different name, part of the container."
msgstr "Tá an réad sa choimeádán cheana, le hainm difriúil."

#: core/resource/strings.src#RID_STR_NAME_NOT_FOUND.STRING.TEXT
msgid  "Unable to find the document '$name$'."
msgstr "Ní féidir cáipéis '$name$' a aimsiú."

#: core/resource/strings.src#RID_STR_ERROR_WHILE_SAVING.STRING.TEXT
msgid  "Could not save the document to $location$:\n$message$"
msgstr "Níorbh fhéidir an cháipéis a shábháil i $location$:\n$message$"

#: core/resource/strings.src#RID_NO_SUCH_DATA_SOURCE.STRING.TEXT
msgid  "Error accessing data source '$name$':\n$error$"
msgstr "Earráid agus foinse sonraí '$name$' á rochtain:\n$error$"

#: core/resource/strings.src#RID_STR_NO_SUB_FOLDER.STRING.TEXT
msgid  "There exists no folder named \"$folder$\"."
msgstr "Níl aon fhillteán darb ainm \"$folder$\" ann."

#: core/resource/strings.src#RID_STR_NO_DELETE_BEFORE_AFTER.STRING.TEXT
msgid  "Cannot delete the before-first or after-last row."
msgstr "Ní féidir an ró 'roimh-thosach' nó 'tar-éis-deiridh' a scriosadh."

#: core/resource/strings.src#RID_STR_NO_DELETE_INSERT_ROW.STRING.TEXT
msgid  "Cannot delete the insert-row."
msgstr "Ní féidir an ró ionsáite a scriosadh."

#: core/resource/strings.src#RID_STR_RESULT_IS_READONLY.STRING.TEXT
msgid  "Result set is read only."
msgstr "Tá na torthaí inléite amháin."

#: core/resource/strings.src#RID_STR_NO_DELETE_PRIVILEGE.STRING.TEXT
msgid  "DELETE privilege not available."
msgstr "Níl cead SCRIOS ar fáil."

#: core/resource/strings.src#RID_STR_ROW_ALREADY_DELETED.STRING.TEXT
msgid  "Current row is already deleted."
msgstr "Tá an ró reatha scriosta cheana."

#: core/resource/strings.src#RID_STR_UPDATE_FAILED.STRING.TEXT
#, fuzzy
msgid  "Current row could not be updated."
msgstr "B'fhéidir an ró reatha a nuashonrú."

#: core/resource/strings.src#RID_STR_NO_INSERT_PRIVILEGE.STRING.TEXT
#, fuzzy
msgid  "INSERT privilege not available."
msgstr "Níl cead IONSÁIGH ar fáil."

#: core/resource/strings.src#RID_STR_INTERNAL_ERROR.STRING.TEXT
#, fuzzy
msgid  "Internal error: no statement object provided by the database driver."
