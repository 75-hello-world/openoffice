msgstr "sDatei = Dir$"

#: source/text/sbasic/shared/03110100.xhp#par_id3149664.25
msgid  "Loop Until sFile = \"\""
msgstr "Loop Until sDatei = \"\""

#: source/text/sbasic/shared/03110100.xhp#par_id3146986.26
