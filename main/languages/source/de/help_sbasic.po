msgid  "\<emph\>Omitted:\</emph\> Returns the next random number in the sequence."
msgstr "<emph>Keine Angabe:</emph> Gibt die nächste Zufallszahl in der Folge zurück."

#: source/text/sbasic/shared/03080302.xhp#par_id3147318.14
msgid  "The \<emph\>Rnd\</emph\> function only returns values ranging from 0 to 1. To generate random integers in a given range, use the formula in the following example:"
msgstr "Die <emph>Rnd</emph>-Funktion liefert grundsätzlich nur Werte zwischen 0 und 1. Um Zufallszahlen ohne Nachkommastellen in bestimmten Zahlenbereichen zu generieren, können Sie sich folgender Formel bedienen:"

#: source/text/sbasic/shared/03080302.xhp#hd_id3151118.15
msgid  "Example:"
msgstr "Beispiel:"

#: source/text/sbasic/shared/03080302.xhp#par_id3145365.16
msgid  "Sub ExampleRandomSelect"
msgstr "Sub ExampleRandomSelect"

#: source/text/sbasic/shared/03080302.xhp#par_id3147426.17
msgid  "Dim iVar As Integer"
msgstr "Dim iVar As Integer"

#: source/text/sbasic/shared/03080302.xhp#par_id3150011.18
msgid  "iVar = Int((15 * Rnd) -2)"
msgstr "iVar = Int((15 * Rnd) -2)"

#: source/text/sbasic/shared/03080302.xhp#par_id3148575.19
msgid  "Select Case iVar"
msgstr "Select Case iVar"

#: source/text/sbasic/shared/03080302.xhp#par_id3154097.20
msgid  "Case 1 To 5"
msgstr "Case 1 To 5"

#: source/text/sbasic/shared/03080302.xhp#par_id3147124.21
msgid  "Print \"Number from 1 to 5\""
msgstr "Print \"Zahl von 1 bis 5\""

#: source/text/sbasic/shared/03080302.xhp#par_id3155418.22
msgid  "Case 6, 7, 8"
msgstr "Case 6, 7, 8"

#: source/text/sbasic/shared/03080302.xhp#par_id3154943.23
msgid  "Print \"Number from 6 to 8\""
msgstr "Print \"Zahl von 6,7, oder  8\""

#: source/text/sbasic/shared/03080302.xhp#par_id3145800.24
msgid  "Case Is > 8 And iVar < 11"
msgstr "Case Is > 8 And iVar < 11"

#: source/text/sbasic/shared/03080302.xhp#par_id3151074.25
msgid  "Print \"Greater than 8\""
msgstr "Print \"Größer als 8\""

#: source/text/sbasic/shared/03080302.xhp#par_id3154016.26
msgid  "Case Else"
msgstr "Case Else"

#: source/text/sbasic/shared/03080302.xhp#par_id3155602.27
msgid  "Print \"Outside range 1 to 10\""
msgstr "Print \"Außerhalb von 1 bis 10\""

#: source/text/sbasic/shared/03080302.xhp#par_id3150328.28
msgid  "End Select"
msgstr "End Select"

#: source/text/sbasic/shared/03080302.xhp#par_id3154479.29
msgid  "end sub"
msgstr "end sub"

