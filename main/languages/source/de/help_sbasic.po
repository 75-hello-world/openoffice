msgid  "case 4"
msgstr "case 4"

#: source/text/sbasic/shared/03030105.xhp#par_id3154942.22
msgid  "sDay=\"Wednesday\""
msgstr "sDay=\"Mittwoch\""

#: source/text/sbasic/shared/03030105.xhp#par_id3145799.23
msgid  "case 5"
msgstr "case 5"

#: source/text/sbasic/shared/03030105.xhp#par_id3155416.24
msgid  "sDay=\"Thursday\""
msgstr "sDay=\"Donnerstag\""

#: source/text/sbasic/shared/03030105.xhp#par_id3150716.25
msgid  "case 6"
msgstr "case 6"

#: source/text/sbasic/shared/03030105.xhp#par_id3154015.26
msgid  "sDay=\"Friday\""
msgstr "sDay=\"Freitag\""

#: source/text/sbasic/shared/03030105.xhp#par_id3146971.27
msgid  "case 7"
msgstr "case 7"

#: source/text/sbasic/shared/03030105.xhp#par_id3153707.28
