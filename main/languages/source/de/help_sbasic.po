msgstr "sVar = \"Las Vegas\""

#: source/text/sbasic/shared/03120302.xhp#par_id3146121.14
msgid  "Print LCase(sVar) REM Returns \"las vegas\""
msgstr "print LCase(sVar) REM liefert \"las vegas\""

#: source/text/sbasic/shared/03120302.xhp#par_id3146986.15
msgid  "Print UCase(sVar) REM Returns \"LAS VEGAS\""
msgstr "print UCase(sVar) REM liefert \"LAS VEGAS\""

#: source/text/sbasic/shared/03120302.xhp#par_id3153575.16
msgid  "end Sub"
msgstr "end Sub"

#: source/text/sbasic/shared/03120303.xhp#tit
