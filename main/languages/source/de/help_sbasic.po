msgstr "Print \"Zahl von 1 bis 5\""

#: source/text/sbasic/shared/03090102.xhp#par_id3147428.15
msgid  "Case 6, 7, 8"
msgstr "Case 6, 7, 8"

#: source/text/sbasic/shared/03090102.xhp#par_id3147349.16
msgid  "Print \"Number from 6 to 8\""
msgstr "Print \"Zahl von 6,7, oder  8\""

#: source/text/sbasic/shared/03090102.xhp#par_id3153729.17
msgid  "Case 8 To 10"
msgstr "Case 8 To 10"

#: source/text/sbasic/shared/03090102.xhp#par_id3152886.18
msgid  "Print \"Greater than 8\""
msgstr "Print \"Größer als 8\""

#: source/text/sbasic/shared/03090102.xhp#par_id3155414.19
msgid  "Case Else"
msgstr "Case Else"

#: source/text/sbasic/shared/03090102.xhp#par_id3146975.20
msgid  "Print \"Out of range 1 to 10\""
msgstr "Print \"Außerhalb von 1 bis 10\""

#: source/text/sbasic/shared/03090102.xhp#par_id3150419.21
msgid  "End Select"
msgstr "End Select"

#: source/text/sbasic/shared/03090102.xhp#par_id3154943.22
msgid  "end sub"
msgstr "end sub"

