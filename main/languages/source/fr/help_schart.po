msgstr "<ahelp hid=\".\" visibility=\"hidden\">Insère une grille secondaire.</ahelp>"

#: source/text/schart/main0000.xhp#par_id0810200904233111
msgid  "\<ahelp hid=\".\" visibility=\"hidden\"\>Inserts data labels.\</ahelp\>"
