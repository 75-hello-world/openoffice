msgid  "\<link href=\"text/schart/01/05020100.xhp\" name=\"Title\"\>Title\</link\>"
msgstr "<link href=\"text/schart/01/05020100.xhp\" name=\"Titre\">Titre</link>"

#: source/text/schart/01/05010000.xhp#hd_id3150297.7
