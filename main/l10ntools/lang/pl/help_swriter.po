msgstr "Wiersze"

#: 10070000.xhp#par_id3149483.7.help.text
msgid ""
"<ahelp hid=\"SW:NUMERICFIELD:DLG_PAGEPREVIEW_ZOOM:ED_ROW\">Defines the number "
"of rows of pages.</ahelp>"
msgstr ""
"<ahelp hid=\"SW:NUMERICFIELD:DLG_PAGEPREVIEW_ZOOM:ED_ROW\">Określa liczbę "
