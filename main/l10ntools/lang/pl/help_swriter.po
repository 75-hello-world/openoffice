msgstr ""
"<ahelp hid=\"SW_NUMERICFIELD_TP_TEXTGRID_PAGE_NF_LINESPERPAGE\">Określa "
"maksymalną liczbę wierszy wyświetlanych na stronie.</ahelp>"

#: 05040800.xhp#hd_id3152957.10.help.text
msgid "Characters per line"
msgstr "Znaków w wierszu"

#: 05040800.xhp#par_id3153674.9.help.text
msgid ""
"<ahelp hid=\"SW_NUMERICFIELD_TP_TEXTGRID_PAGE_NF_CHARSPERLINE\">Enter the "
"maximum number of characters that you want on a line.</ahelp>"
msgstr ""
"<ahelp hid=\"SW_NUMERICFIELD_TP_TEXTGRID_PAGE_NF_CHARSPERLINE\">Określa "
"maksymalną liczbę znaków wyświetlanych w wierszu.</ahelp>"

#: 05040800.xhp#hd_id3149684.8.help.text
msgid "Max. base text size"
msgstr "Rozmiar tekstu podstawowego"

#: 05040800.xhp#par_id3154193.7.help.text
msgid ""
"<ahelp hid=\"SW_METRICFIELD_TP_TEXTGRID_PAGE_MF_TEXTSIZE\">Enter the maximum "
"base text size. A large value results in less characters per line.</ahelp>"
msgstr ""
"<ahelp hid=\"SW_METRICFIELD_TP_TEXTGRID_PAGE_MF_TEXTSIZE\">Określa maksymalny "
"rozmiar tekstu podstawowego. Duża wartość skutkuje zmniejszeniem liczby "
"znaków na wiersz.</ahelp>"

#: 05040800.xhp#hd_id3155182.6.help.text
msgid "Max. Ruby text size"
msgstr "Rozmiar dopisku fonetycznego"

#: 05040800.xhp#par_id3143283.5.help.text
