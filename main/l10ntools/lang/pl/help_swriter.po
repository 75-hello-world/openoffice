msgstr ""
"<ahelp hid=\"SW_METRICFIELD_TP_TEXTGRID_PAGE_MF_RUBYSIZE\">Określa maksymalny "
"rozmiar czcionki dla dopisku fonetycznego.</ahelp>"

#: 05040800.xhp#hd_id3149496.4.help.text
msgid "Ruby text below/left from base text"
msgstr "Dopisek fonetyczny poniżej lub z lewej strony tekstu bazowego"

#: 05040800.xhp#par_id3149816.3.help.text
msgid ""
"<ahelp hid=\"SW_CHECKBOX_TP_TEXTGRID_PAGE_CB_RUBYBELOW\">Displays Ruby text to "
"the left of or below the base text.</ahelp>"
msgstr ""
"<ahelp hid=\"SW_CHECKBOX_TP_TEXTGRID_PAGE_CB_RUBYBELOW\">Wyświetla tekst "
"fonetyczny poniżej lub z lewej strony tekstu bazowego.</ahelp>"

