msgid  "Error reading data from database"
msgstr "Klaida skaitant duomenis iš duomenų bazės"

#: source/resource/strings.src#RID_STR_CONNECTERROR.String.Text
msgid  "Connection failed"
msgstr "Ryšys nutrūko"

