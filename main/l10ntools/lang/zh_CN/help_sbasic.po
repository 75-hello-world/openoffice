msgstr ""

#: source/text/sbasic/shared/03102200.xhp#par_id3159413.2
msgid  "Determines if a variable is a data field in an array."
msgstr "确定变量是否是数组中的数据字段。"

#: source/text/sbasic/shared/03102200.xhp#hd_id3150792.3
msgid  "Syntax:"
msgstr "语法："

#: source/text/sbasic/shared/03102200.xhp#par_id3153379.4
msgid  "IsArray (Var)"
msgstr "IsArray (Var)"

#: source/text/sbasic/shared/03102200.xhp#hd_id3154365.5
msgid  "Return value:"
msgstr "返回值："

#: source/text/sbasic/shared/03102200.xhp#par_id3154685.6
msgid  "Bool"
msgstr "布尔"

#: source/text/sbasic/shared/03102200.xhp#hd_id3153969.7
msgid  "Parameters:"
msgstr "参数："

#: source/text/sbasic/shared/03102200.xhp#par_id3145172.8
msgid  "\<emph\>Var:\</emph\> Any variable that you want to test if it is declared as an array. If the variable is an array, then the function returns \<emph\>True\</emph\>, otherwise \<emph\>False \</emph\>is returned."
msgstr ""

#: source/text/sbasic/shared/03102200.xhp#hd_id3155131.9
msgid  "Example:"
msgstr "示例："

#: source/text/sbasic/shared/03102200.xhp#par_id3153365.10
msgid  "Sub ExampleIsArray"
msgstr "Sub ExampleIsArray"

#: source/text/sbasic/shared/03102200.xhp#par_id3150487.11
