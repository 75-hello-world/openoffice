msgid  "LSet sVar = sExpr"
msgstr "LSet sVar = sExpr"

#: source/text/sbasic/shared/03120304.xhp#par_id3146969.37
msgid  "Print \">\"; sVar; \"<\""
msgstr "Print \">\"; sVar; \"<\""

