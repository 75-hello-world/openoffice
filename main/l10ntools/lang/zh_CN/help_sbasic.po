msgstr "Print Second(TimeSerial(12,30,41))"

#: source/text/sbasic/shared/03030204.xhp#par_id3151117.11
msgid  "returns the value 41."
msgstr "返回值为 41。"

#: source/text/sbasic/shared/03030204.xhp#hd_id3147426.12
msgid  "Example:"
msgstr "示例："

#: source/text/sbasic/shared/03030204.xhp#par_id3154012.13
msgid  "Sub ExampleSecond"
msgstr "Sub ExampleSecond"

#: source/text/sbasic/shared/03030204.xhp#par_id3156441.14
