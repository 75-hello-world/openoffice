msgid  "Dim dValue as Double"
msgstr "Dim dValue as Double"

#: source/text/sbasic/shared/03080201.xhp#par_id3145172.12
msgid  "const b1=12.345e12"
msgstr "const b1=12.345e12"

#: source/text/sbasic/shared/03080201.xhp#par_id3159254.13
msgid  "const b2=1.345e34"
msgstr "const b2=1.345e34"

#: source/text/sbasic/shared/03080201.xhp#par_id3147287.14
msgid  "dValue=Exp( Log(b1)+Log(b2) )"
msgstr "dValue=Exp( Log(b1)+Log(b2) )"

#: source/text/sbasic/shared/03080201.xhp#par_id3161832.15
msgid  "MsgBox \"\" & dValue & chr(13) & (b1*b2) ,0,\"Multiplication by logarithm\""
msgstr "MsgBox \"\" & dValue & chr(13) & (b1*b2) ,0,\"Multiplication by logarithm\""

#: source/text/sbasic/shared/03080201.xhp#par_id3151112.16
