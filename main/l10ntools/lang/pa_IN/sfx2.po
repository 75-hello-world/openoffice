"ਆਪਣੀ ਪਰਿੰਟਰ ਸੰਰਚਨਾ ਚੈੱਕ ਕਰੋ ਜੀ।"

#: view.src#STR_PAGE.string.text
msgid "Page "
msgstr "ਪੇਜ਼ "

#: view.src#STR_PREVIEW_DOCINFO.string.text
msgid ""
"<html><body BGCOLOR=\"#c0c0c0\"><font FACE=\"Arial\"><dl><dt><b>Title:</b><dd>$("
"TITEL)<dt><b>Subject:</b><dd>$(THEME)<dt><b>Keywords:</b><dd>$(KEYWORDS)<dt>"
"<b>Description:</b><dd>$(TEXT)</dl></font></body></html>"
msgstr ""
"<html><body BGCOLOR=\"#c0c0c0\"><font FACE=\"Arial\"><dl><dt><b>ਟਾਈਟਲ:</b><dd>$("
"TITEL)<dt><b>ਵਿਸ਼ਾ:</b><dd>$(THEME)<dt><b>ਸ਼ਬਦ:</b><dd>$(KEYWORDS)<dt><b>ਵੇਰ"
"ਵਾ:</b><dd>$(TEXT)</dl></font></body></html>"

#: view.src#STR_PREVIEW_NODOCINFO.string.text
