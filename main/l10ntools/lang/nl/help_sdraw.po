msgid  "F2"
msgstr "F2"

#: source/text/sdraw/04/01020000.xhp#par_id3152346.8
msgid  "Add or edit text."
msgstr "Tekst toevoegen of bewerken."

#: source/text/sdraw/04/01020000.xhp#hd_id3154705.9
msgid  "F3"
msgstr "F3"

#: source/text/sdraw/04/01020000.xhp#par_id3147004.10
msgid  "Opens group to edit individual objects."
msgstr "Opent een groep zodat individuele objecten bewerkt kunnen worden."

#: source/text/sdraw/04/01020000.xhp#hd_id3155113.11
msgid  "\<switchinline select=\"sys\"\>\<caseinline select=\"MAC\"\>Command\</caseinline\>\<defaultinline\>Ctrl\</defaultinline\>\</switchinline\>+F3"
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+F3"

#: source/text/sdraw/04/01020000.xhp#par_id3159238.12
msgid  "Close group editor."
msgstr "Groep-editor sluiten."

#: source/text/sdraw/04/01020000.xhp#hd_id3150199.13
msgid  "Shift+F3"
msgstr "Shift+F3"

#: source/text/sdraw/04/01020000.xhp#par_id3152994.14
msgid  "Opens the \<emph\>Duplicate\</emph\> dialog."
msgstr "Opent het dialoogvenster <emph>Dupliceren</emph>."

#: source/text/sdraw/04/01020000.xhp#hd_id3154488.15
msgid  "F4"
msgstr "F4"

#: source/text/sdraw/04/01020000.xhp#par_id3149406.16
msgid  "Opens the \<emph\>Position and Size\</emph\> dialog."
msgstr "Opent het dialoogvenster <emph>Positie en grootte</emph>."

#: source/text/sdraw/04/01020000.xhp#hd_id3148870.21
msgid  "F5"
msgstr "F5"

#: source/text/sdraw/04/01020000.xhp#par_id3153917.22
msgid  "Opens the \<emph\>Navigator\</emph\>."
msgstr "Opent de <emph>Navigator</emph>."

#: source/text/sdraw/04/01020000.xhp#hd_id3157982.25
msgid  "F7"
msgstr "F7"

#: source/text/sdraw/04/01020000.xhp#par_id3154649.26
msgid  "Checks spelling."
msgstr "Hiermee controleert u de spelling."

#: source/text/sdraw/04/01020000.xhp#hd_id3152869.27
msgid  "\<switchinline select=\"sys\"\>\<caseinline select=\"MAC\"\>Command\</caseinline\>\<defaultinline\>Ctrl\</defaultinline\>\</switchinline\>+F7"
