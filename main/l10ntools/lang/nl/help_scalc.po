#: source/text/scalc/01/04060116.xhp#bm_id3154959
msgid  "\<bookmark_value\>IMABS function\</bookmark_value\>"
msgstr "<bookmark_value>C.ABS-functie</bookmark_value>"

#: source/text/scalc/01/04060116.xhp#hd_id3154959.44
msgid  "IMABS"
msgstr "C.ABS"

#: source/text/scalc/01/04060116.xhp#par_id3149895.45
msgid  "\<ahelp hid=\"HID_AAI_FUNC_IMABS\"\>The result is the absolute value of a complex number.\</ahelp\>"
msgstr "<ahelp hid=\"HID_AAI_FUNC_IMABS\">Het resultaat is de absolute waarde van een complex getal.</ahelp>"

#: source/text/scalc/01/04060116.xhp#hd_id3155382.46
msgid  "Syntax"
msgstr "Syntaxis"

#: source/text/scalc/01/04060116.xhp#par_id3151302.47
msgid  "IMABS(\"ComplexNumber\")"
msgstr "C.ABS(\"ComplexGetal\")"

#: source/text/scalc/01/04060116.xhp#par_id3153974.48
msgid  "\<variable id=\"complex\"\>\<emph\>ComplexNumber\</emph\> is a complex number that is entered in the form \"x+yi\" or \"x+yj\".\</variable\>"
msgstr "<variable id=\"complex\"><emph>ComplexGetal</emph> is een complex getal dat wordt ingevoerd in de vorm \"x+yi\" of \"x+yj\". </variable>"

#: source/text/scalc/01/04060116.xhp#hd_id3149697.49
msgid  "Example"
msgstr "Voorbeeld"

#: source/text/scalc/01/04060116.xhp#par_id3143222.50
msgid  "            \<item type=\"input\"\>=IMABS(\"5+12j\")\</item\> returns 13."
msgstr "            <item type=\"input\">=C.ABS(\"5+12j\")</item> geeft 13 terug."

#: source/text/scalc/01/04060116.xhp#bm_id3145357
msgid  "\<bookmark_value\>IMAGINARY function\</bookmark_value\>"
msgstr "<bookmark_value>C.IM.DEEL-functie</bookmark_value>"

#: source/text/scalc/01/04060116.xhp#hd_id3145357.51
msgid  "IMAGINARY"
msgstr "C.IM.DEEL"

#: source/text/scalc/01/04060116.xhp#par_id3146965.52
msgid  "\<ahelp hid=\"HID_AAI_FUNC_IMAGINARY\"\>The result is the imaginary coefficient of a complex number.\</ahelp\>"
msgstr "<ahelp hid=\"HID_AAI_FUNC_IMAGINARY\">Het resultaat is de imaginaire coëfficiënt van een complex getal.</ahelp>"

#: source/text/scalc/01/04060116.xhp#hd_id3153555.53
msgid  "Syntax"
msgstr "Syntaxis"

#: source/text/scalc/01/04060116.xhp#par_id3155522.54
msgid  "IMAGINARY(\"ComplexNumber\")"
msgstr "C.IM.DEEL(\"ComplexGetal\")"

#: source/text/scalc/01/04060116.xhp#hd_id3151193.56
msgid  "Example"
msgstr "Voorbeeld"

#: source/text/scalc/01/04060116.xhp#par_id3155592.57
msgid  "            \<item type=\"input\"\>=IMAGINARY(\"4+3j\")\</item\> returns 3."
msgstr "            <item type=\"input\">=C.IM.DEEL(\"4+3j\")</item> geeft 3 terug."

#: source/text/scalc/01/04060116.xhp#bm_id3146106
msgid  "\<bookmark_value\>IMPOWER function\</bookmark_value\>"
msgstr "<bookmark_value>functie C.MACHT</bookmark_value>"

#: source/text/scalc/01/04060116.xhp#hd_id3146106.58
msgid  "IMPOWER"
msgstr "C.MACHT"

#: source/text/scalc/01/04060116.xhp#par_id3147245.59
msgid  "\<ahelp hid=\"HID_AAI_FUNC_IMPOWER\"\>The result is the integer power of a complex number.\</ahelp\>"
msgstr "<ahelp hid=\"HID_AAI_FUNC_IMPOWER\">Het resultaat is de macht (geheel getal) van een complex getal.</ahelp>"

#: source/text/scalc/01/04060116.xhp#hd_id3150954.60
msgid  "Syntax"
msgstr "Syntaxis"

#: source/text/scalc/01/04060116.xhp#par_id3147501.61
msgid  "IMPOWER(\"ComplexNumber\"; Number)"
msgstr "C.MACHT(\"ComplexGetal\"; Getal)"

#: source/text/scalc/01/04060116.xhp#par_id3155743.63
msgid  "            \<emph\>Number\</emph\> is the exponent."
msgstr "            <emph>Getal</emph> is de exponent."

#: source/text/scalc/01/04060116.xhp#hd_id3149048.64
msgid  "Example"
msgstr "Voorbeeld"

#: source/text/scalc/01/04060116.xhp#par_id3151393.65
msgid  "            \<item type=\"input\"\>=IMPOWER(\"2+3i\";2)\</item\> returns -5+12i."
msgstr "            <item type=\"input\">=C.MACHT(\"2+3i\";2)</item> geeft -5+12i terug."

#: source/text/scalc/01/04060116.xhp#bm_id3148748
msgid  "\<bookmark_value\>IMARGUMENT function\</bookmark_value\>"
msgstr "<bookmark_value>C.ARGUMENT-functie</bookmark_value>"

#: source/text/scalc/01/04060116.xhp#hd_id3148748.66
