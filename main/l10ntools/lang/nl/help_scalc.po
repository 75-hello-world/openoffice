msgid  "\<item type=\"input\"\>Name\</item\>"
msgstr "<item type=\"input\">Naam</item>"

#: source/text/scalc/01/04060101.xhp#par_id3155532.11
msgid  "\<item type=\"input\"\>Grade\</item\>"
msgstr "<item type=\"input\">Groep</item>"

#: source/text/scalc/01/04060101.xhp#par_id3156448.12
msgid  "\<item type=\"input\"\>Age\</item\>"
msgstr "<item type=\"input\">Leeftijd</item>"

#: source/text/scalc/01/04060101.xhp#par_id3154486.13
msgid  "\<item type=\"input\"\>Distance to School\</item\>"
msgstr "<item type=\"input\">Afstand naar school</item>"

#: source/text/scalc/01/04060101.xhp#par_id3152899.14
msgid  "\<item type=\"input\"\>Weight\</item\>"
msgstr "<item type=\"input\">Gewicht</item>"

