#: source/text/scalc/01/04060116.xhp#par_id3149710.146
msgid  "IMSUB(\"ComplexNumber1\"; \"ComplexNumber2\")"
msgstr "C.VERSCHIL(\"ComplexGetal1\"; \"ComplexGetal2\")"

#: source/text/scalc/01/04060116.xhp#hd_id3155833.148
msgid  "Example"
msgstr "Voorbeeld"

#: source/text/scalc/01/04060116.xhp#par_id3150963.149
msgid  "            \<item type=\"input\"\>=IMSUB(\"13+4j\";\"5+3j\")\</item\> returns 8+j."
msgstr "            <item type=\"input\">=C.SUB(\"13+4j\";\"5+3j\")</item> geeft 8+j terug."

#: source/text/scalc/01/04060116.xhp#bm_id3156312
msgid  "\<bookmark_value\>IMSUM function\</bookmark_value\>"
msgstr "<bookmark_value>functie C.SOM</bookmark_value>"

#: source/text/scalc/01/04060116.xhp#hd_id3156312.150
msgid  "IMSUM"
msgstr "C.SOM"

#: source/text/scalc/01/04060116.xhp#par_id3153215.151
msgid  "\<ahelp hid=\"HID_AAI_FUNC_IMSUM\"\>The result is the sum of up to 29 complex numbers.\</ahelp\>"
msgstr "<ahelp hid=\"HID_AAI_FUNC_IMSUM\">Het resultaat is de som van tot 29 complexe getallen.</ahelp>"

#: source/text/scalc/01/04060116.xhp#hd_id3156095.152
msgid  "Syntax"
msgstr "Syntaxis"

#: source/text/scalc/01/04060116.xhp#par_id3152930.153
msgid  "IMSUM(\"ComplexNumber1\"; \"ComplexNumber2\"; ...)"
msgstr "C.SOM(\"ComplexGetal1\"; \"ComplexGetal2\"; ...)"

#: source/text/scalc/01/04060116.xhp#hd_id3154640.155
msgid  "Example"
msgstr ""

#: source/text/scalc/01/04060116.xhp#par_id3147081.156
msgid  "            \<item type=\"input\"\>=IMSUM(\"13+4j\";\"5+3j\")\</item\> returns 18+7j."
msgstr "            <item type=\"input\">=C.SOM(\"13+4j\";\"5+3j\")</item> geeft 18+7j terug."

#: source/text/scalc/01/04060116.xhp#bm_id3147570
msgid  "\<bookmark_value\>IMSQRT function\</bookmark_value\>"
msgstr "<bookmark_value>functie C.WORTEL</bookmark_value>"

#: source/text/scalc/01/04060116.xhp#hd_id3147570.167
msgid  "IMSQRT"
msgstr "C.WORTEL"

#: source/text/scalc/01/04060116.xhp#par_id3156131.168
msgid  "\<ahelp hid=\"HID_AAI_FUNC_IMSQRT\"\>The result is the square root of a complex number.\</ahelp\>"
msgstr "<ahelp hid=\"HID_AAI_FUNC_IMSQRT\">Het resultaat is de vierkantswortel van een complex getal.</ahelp>"

#: source/text/scalc/01/04060116.xhp#hd_id3145202.169
msgid  "Syntax"
msgstr ""

#: source/text/scalc/01/04060116.xhp#par_id3150760.170
msgid  "IMSQRT(\"ComplexNumber\")"
msgstr "C.WORTEL(\"ComplexGetal\")"

#: source/text/scalc/01/04060116.xhp#hd_id3147268.172
msgid  "Example"
msgstr ""

#: source/text/scalc/01/04060116.xhp#par_id3152807.173
msgid  "            \<item type=\"input\"\>=IMSQRT(\"3+4i\")\</item\> returns 2+1i."
msgstr "            <item type=\"input\">=C.WORTEL(\"3+4i\")</item> geeft 2+1i terug."

