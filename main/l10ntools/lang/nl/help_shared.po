msgid  "\<ahelp hid=\"SVX_RADIOBUTTON_RID_SVXPAGE_CHAR_POSITION_RB_NORMALPOS\"\>Removes superscript or subscript formatting.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05020500.xhp#hd_id3150465.9
msgid  "Subscript"
msgstr "Subscript"

#: source/text/shared/01/05020500.xhp#par_id3155420.10
msgid  "\<variable id=\"tieftext\"\>\<ahelp hid=\"SVX_RADIOBUTTON_RID_SVXPAGE_CHAR_POSITION_RB_LOWPOS\"\>Reduces the font size of the selected text and lowers the text below the baseline.\</ahelp\>\</variable\>"
msgstr ""

#: source/text/shared/01/05020500.xhp#hd_id3148992.11
msgid  "Raise/lower by"
msgstr "Hoger/lager met"

#: source/text/shared/01/05020500.xhp#par_id3150275.12
msgid  "\<ahelp hid=\"SVX_METRICFIELD_RID_SVXPAGE_CHAR_POSITION_ED_HIGHLOW\"\>Enter the amount by which you want to raise or to lower the selected text in relation to the baseline. One hundred percent is equal to the height of the font.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05020500.xhp#hd_id3150670.13
msgid  "Relative font size"
msgstr "Relatieve tekengrootte"

#: source/text/shared/01/05020500.xhp#par_id3153126.14
msgid  "\<ahelp hid=\"SVX_METRICFIELD_RID_SVXPAGE_CHAR_POSITION_ED_FONTSIZE\"\>Enter the amount by which you want to reduce the font size of the selected text.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05020500.xhp#hd_id3153349.15
msgid  "Automatic"
msgstr "Automatisch"

#: source/text/shared/01/05020500.xhp#par_id3153061.16
msgid  "\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_CHAR_POSITION_CB_HIGHLOW\"\>Automatically sets the amount by which the selected text is raised or lowered in relation to the baseline.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05020500.xhp#hd_id3154905.30
msgid  "Rotation / scaling"
msgstr "Rotatie/schaal"

#: source/text/shared/01/05020500.xhp#par_id3154923.36
msgid  "Set the rotation and the scaling options for the selected text."
msgstr "Stel de rotatie en schaalopties voor de geselecteerde tekst in."

#: source/text/shared/01/05020500.xhp#hd_id3154280.31
msgid  "0 degrees"
msgstr "0 graden"

#: source/text/shared/01/05020500.xhp#par_id3149045.37
msgid  "\<ahelp hid=\"SVX_RADIOBUTTON_RID_SVXPAGE_CHAR_POSITION_RB_0_DEG\"\>Does not rotate the selected text.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05020500.xhp#hd_id3156434.32
msgid  "90 degrees"
msgstr "90 graden"

#: source/text/shared/01/05020500.xhp#par_id3148739.38
msgid  "\<ahelp hid=\"SVX_RADIOBUTTON_RID_SVXPAGE_CHAR_POSITION_RB_90_DEG\"\>Rotates the selected text to the left by 90 degrees.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05020500.xhp#hd_id3150398.33
msgid  "270 degrees"
msgstr "270 graden"

#: source/text/shared/01/05020500.xhp#par_id3153778.39
msgid  "\<ahelp hid=\"SVX_RADIOBUTTON_RID_SVXPAGE_CHAR_POSITION_RB_270_DEG\"\>Rotates the selected text to the right by 90 degrees.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05020500.xhp#hd_id3147228.34
msgid  "Fit to line"
msgstr "Aanpassen aan regel"

#: source/text/shared/01/05020500.xhp#par_id3150288.40
msgid  "\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_CHAR_POSITION_CB_FIT_TO_LINE\"\>Stretches or compresses the selected text so that it fits between the line that is above the text and the line that is below the text.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05020500.xhp#hd_id3155994.35
msgid  "Scale width"
msgstr "Breedte schalen"

#: source/text/shared/01/05020500.xhp#par_id3145171.41
msgid  "\<ahelp hid=\"SVX_METRICFIELD_RID_SVXPAGE_CHAR_POSITION_MF_SCALE_WIDTH\"\>Enter the percentage of the font width by which to horizontally stretch or compress the selected text.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05020500.xhp#hd_id3149807.17
msgid  "Spacing"
msgstr "Afstand"

#: source/text/shared/01/05020500.xhp#par_id3156212.18
msgid  "Specify the spacing between individual characters."
msgstr "Specificeer de afstand tussen individuele tekens."

#: source/text/shared/01/05020500.xhp#hd_id3125865.19
msgid  "Spacing"
msgstr "Afstand"

#: source/text/shared/01/05020500.xhp#par_id3153178.20
msgid  "\<ahelp hid=\"SVX_LISTBOX_RID_SVXPAGE_CHAR_POSITION_LB_KERNING2\"\>Specifies the spacing between the characters of the selected text. For expanded or condensed spacing, enter the amount that you want to expand or condense the text in the \<emph\>by \</emph\>box.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05020500.xhp#par_id3154908.21
msgid  "\<emph\>Default\</emph\> - uses the character spacing specified in the font type"
msgstr ""

#: source/text/shared/01/05020500.xhp#par_id3156543.22
msgid  "\<emph\>Expanded\</emph\> - increases the character spacing"
msgstr ""

#: source/text/shared/01/05020500.xhp#par_id3154297.23
msgid  "\<emph\>Condensed\</emph\> - decreases the character spacing"
msgstr ""

#: source/text/shared/01/05020500.xhp#hd_id3157870.25
msgid  "by"
msgstr "met"

#: source/text/shared/01/05020500.xhp#par_id3146974.26
msgid  "\<ahelp hid=\"SVX_METRICFIELD_RID_SVXPAGE_CHAR_POSITION_ED_KERNING2\"\>Enter the amount by which you want to expand or condense the character spacing for the selected text.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05020500.xhp#hd_id3154127.27
msgid  "\<link href=\"text/shared/00/00000005.xhp#kerning\" name=\"Pair kerning\"\>Pair kerning\</link\>"
msgstr ""

#: source/text/shared/01/05020500.xhp#par_id3148616.28
msgid  "\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_CHAR_POSITION_CB_PAIRKERNING\"\>Automatically adjust the character spacing for specific letter combinations.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05020500.xhp#par_id3150010.29
msgid  "Kerning is only available for certain font types and requires that your printer support this option."
msgstr "Kerning is alleen voor bepaalde lettertypen beschikbaar en moet door uw printer ondersteund worden."

#: source/text/shared/01/05020600.xhp#tit
msgid  "Asian Layout"
msgstr "Aziatische lay-out"

#: source/text/shared/01/05020600.xhp#bm_id3156053
msgid  "\<bookmark_value\>double-line writing in Asian layout\</bookmark_value\>\<bookmark_value\>formats; Asian layout\</bookmark_value\>\<bookmark_value\>characters; Asian layout\</bookmark_value\>\<bookmark_value\>text; Asian layout\</bookmark_value\>"
msgstr ""

#: source/text/shared/01/05020600.xhp#hd_id3156053.1
msgid  "\<link href=\"text/shared/01/05020600.xhp\" name=\"Asian Layout\"\>Asian Layout\</link\>"
msgstr ""

#: source/text/shared/01/05020600.xhp#par_id3155351.2
msgid  "\<ahelp hid=\"\"\>Sets the options for double-line writing for Asian languages. Select the characters in your text, and then choose this command.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05020600.xhp#hd_id3152552.3
msgid  "Double-lined"
msgstr "Tweeregelig"

#: source/text/shared/01/05020600.xhp#par_id3155338.4
msgid  "Set the double-line options for the selected text."
msgstr "Kies hier of u de geselecteerde tekens tweeregelig wenst te schrijven."

#: source/text/shared/01/05020600.xhp#hd_id3147089.5
msgid  "Write in double lines"
msgstr "Twee regels per rij"

#: source/text/shared/01/05020600.xhp#par_id3150693.6
msgid  "\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_CHAR_TWOLINES_CB_TWOLINES\"\>Allows you to write in double lines in the area that you selected in the current document.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05020600.xhp#hd_id3157959.7
msgid  "Enclosing characters"
msgstr "Omhullend teken"

#: source/text/shared/01/05020600.xhp#par_id3154749.8
msgid  "Specify the characters to enclose the double-lined area."
msgstr "Definieer hier de begin- en eindtekens die het tweeregelige gebied. omsluiten."

#: source/text/shared/01/05020600.xhp#hd_id3148539.9
msgid  "Initial character"
msgstr "Eerste teken"

#: source/text/shared/01/05020600.xhp#par_id3150504.10
msgid  "\<ahelp hid=\"SVX_LISTBOX_RID_SVXPAGE_CHAR_TWOLINES_ED_STARTBRACKET\"\>Select the character to define the start of the double-lined area. If you want to choose a custom character, select \<emph\>Other Characters\</emph\>.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05020600.xhp#hd_id3159115.11
msgid  "Final character"
msgstr "Laatste teken"

#: source/text/shared/01/05020600.xhp#par_id3149191.12
msgid  "\<ahelp hid=\"SVX_LISTBOX_RID_SVXPAGE_CHAR_TWOLINES_ED_ENDBRACKET\"\>Select the character to define the end of the double-lined area. If you want to choose a custom character, select \<emph\>Other Characters\</emph\>.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05020700.xhp#tit
msgid  "Asian Typography"
msgstr "Aziatische typografie"

#: source/text/shared/01/05020700.xhp#bm_id3155620
msgid  "\<bookmark_value\>Asian typography\</bookmark_value\>\<bookmark_value\>formatting; Asian typography\</bookmark_value\>\<bookmark_value\>paragraphs; Asian typography\</bookmark_value\>\<bookmark_value\>typography; Asian\</bookmark_value\>"
msgstr ""

#: source/text/shared/01/05020700.xhp#hd_id3155620.1
msgid  "\<link href=\"text/shared/01/05020700.xhp\" name=\"Asian Typography\"\>Asian Typography\</link\>"
msgstr ""

#: source/text/shared/01/05020700.xhp#par_id3153124.2
msgid  "\<ahelp hid=\".\"\>Set the typographic options for cells or paragraphs in Asian language files. To enable Asian language support, choose \<emph\>Language Settings - Languages\</emph\> in the Options dialog box, and then select the \<emph\>Enabled\</emph\> box in the \<emph\>Asian language support\</emph\> area.\</ahelp\> The Asian typography options are ignored in HTML documents."
msgstr ""

#: source/text/shared/01/05020700.xhp#hd_id3147571.3
msgid  "Line change"
msgstr "Nieuwe regel"

#: source/text/shared/01/05020700.xhp#par_id3147834.4
msgid  "Set the options for line breaks in Asian language documents."
msgstr "Stel de opties voor regeleinden in Aziatische taaldocumenten in."

#: source/text/shared/01/05020700.xhp#hd_id3145072.9
msgid  "Apply list of forbidden characters to the beginning and end of line"
msgstr "Lijst met verboden tekens aan regelbegin en -einde toepassen"

#: source/text/shared/01/05020700.xhp#par_id3153683.10
msgid  "\<ahelp hid=\"SVX_TRISTATEBOX_RID_SVXPAGE_PARA_ASIAN_CB_AS_FORBIDDEN\"\>Prevents the characters in the list from starting or ending a line. The characters are relocated to either the previous or the next line.\</ahelp\> To edit the list of restricted characters, choose \<emph\>\<switchinline select=\"sys\"\>\<caseinline select=\"MAC\"\>%PRODUCTNAME - Preferences\</caseinline\>\<defaultinline\>Tools - Options\</defaultinline\>\</switchinline\> - Language Settings - \<link href=\"text/shared/optionen/01150100.xhp\" name=\"Asian Layout\"\>Asian Layout\</link\>\</emph\>."
msgstr ""

#: source/text/shared/01/05020700.xhp#hd_id3149751.5
msgid  "Allow hanging punctuation"
msgstr "Hangende interpunctie toestaan"

#: source/text/shared/01/05020700.xhp#par_id3149096.6
msgid  "\<ahelp hid=\"SVX_TRISTATEBOX_RID_SVXPAGE_PARA_ASIAN_CB_AS_HANG_PUNC\"\>Prevents commas and periods from breaking the line. Instead, these characters are added to the end of the line, even in the page margin.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05020700.xhp#par_id3147275.7
msgid  "\<emph\>Apply spacing between Asian, Latin and Complex text\</emph\>"
msgstr ""

#: source/text/shared/01/05020700.xhp#par_id3148539.8
msgid  "\<ahelp hid=\"SVX_TRISTATEBOX_RID_SVXPAGE_PARA_ASIAN_CB_AS_SCRIPT_SPACE\"\>Inserts a space between Asian, Latin and complex characters.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05020700.xhp#par_id3153665
msgid  "\<link href=\"text/shared/optionen/01140000.xhp\" name=\"Enabling Asian language support\"\>Enabling Asian language support\</link\>"
msgstr ""

#: source/text/shared/01/05030000.xhp#tit
msgid  "Paragraph"
msgstr "Alinea"

#: source/text/shared/01/05030000.xhp#hd_id3150467.1
msgid  "Paragraph"
msgstr "Alinea"

#: source/text/shared/01/05030000.xhp#par_id3148668.2
msgid  "\<variable id=\"absatztext\"\>\<ahelp hid=\".uno:EditStyle\"\>Modifies the format of the current paragraph, such as indents and alignment.\</ahelp\>\</variable\> To modify the font of the current paragraph, select the entire paragraph, choose Format - Character, and then click on the Font tab."
msgstr ""

#: source/text/shared/01/05030000.xhp#par_id3156042.3
msgid  "\<switchinline select=\"appl\"\>\<caseinline select=\"WRITER\"\>The paragraph style for the current paragraph is displayed at the \<emph\>Formatting\</emph\> toolbar, and is highlighted in the \<link href=\"text/swriter/01/05140000.xhp\" name=\"Styles\"\>Styles and Formatting window\</link\>. \</caseinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#tit
msgid  "Indents and Spacing"
msgstr "Inspringingen en afstanden"

#: source/text/shared/01/05030100.xhp#bm_id3154689
msgid  "\<bookmark_value\>spacing; between paragraphs in footnotes\</bookmark_value\>         \<bookmark_value\>line spacing; paragraph\</bookmark_value\>         \<bookmark_value\>spacing; lines and paragraphs\</bookmark_value\>         \<bookmark_value\>single-line spacing in text\</bookmark_value\>         \<bookmark_value\>one and a half line spacing in text\</bookmark_value\>         \<bookmark_value\>double-line spacing in paragraphs\</bookmark_value\>         \<bookmark_value\>leading between paragraphs\</bookmark_value\>         \<bookmark_value\>paragraphs;spacing\</bookmark_value\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#hd_id3154689.1
msgid  "\<link href=\"text/shared/01/05030100.xhp\" name=\"Indents and Spacing\"\>Indents and Spacing\</link\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#par_id3155069.2
msgid  "\<ahelp hid=\"HID_FORMAT_PARAGRAPH_STD\"\>Sets the indenting and the spacing options for the paragraph.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#par_id3153910.64
msgid  "To change the measurement units used in this dialog, choose \<switchinline select=\"sys\"\>\<caseinline select=\"MAC\"\>%PRODUCTNAME - Preferences\</caseinline\>\<defaultinline\>Tools - Options\</defaultinline\>\</switchinline\> - %PRODUCTNAME Writer - General, and then select a new measurement unit in the Settings area. "
msgstr ""

#: source/text/shared/01/05030100.xhp#par_id3154823.11
msgid  "You can also \<link href=\"text/swriter/guide/ruler.xhp\" name=\"ruler\"\>set indents using the ruler\</link\>. To display the ruler, choose \<emph\>View - Ruler\</emph\>."
msgstr ""

#: source/text/shared/01/05030100.xhp#hd_id3158430.3
msgid  "Indent"
msgstr "Inspringing"

#: source/text/shared/01/05030100.xhp#par_id3155419.4
msgid  "Specify the amount of space to leave between the left and the right page margins and the paragraph."
msgstr "Specificeer de hoeveelheid ruimte die u tussen de linker- en rechterpaginamarges en de alinea wilt vrijlaten."

#: source/text/shared/01/05030100.xhp#hd_id3153698.5
msgid  "Before text"
msgstr "Vóór tekst"

#: source/text/shared/01/05030100.xhp#par_id3148990.6
msgid  "\<ahelp hid=\"SVX:METRICFIELD:RID_SVXPAGE_STD_PARAGRAPH:ED_LEFTINDENT\"\>Enter the amount of space that you want to indent the paragraph from the page margin. If you want the paragraph to extend into the page margin, enter a negative number. In Left-to-Right languages, the left edge of the paragraph is indented with respect to the left page margin. In Right-to-Left languages, the right edge of the paragraph is indented with respect to the right page margin.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#hd_id3152361.9
msgid  "After text"
msgstr "Na tekst"

#: source/text/shared/01/05030100.xhp#par_id3154390.10
msgid  "\<ahelp hid=\"SVX:METRICFIELD:RID_SVXPAGE_STD_PARAGRAPH:ED_RIGHTINDENT\"\>Enter the amount of space that you want to indent the paragraph from the page margin. If you want the paragraph to extend into the page margin, enter a negative number. In Left-to-Right languages, the right edge of the paragraph is indented with respect to the right page margin. In Right-to-Left languages, the left edge of the paragraph is indented with respect to the left page margin.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#hd_id3149169.7
msgid  "First line"
msgstr "Eerste regel"

#: source/text/shared/01/05030100.xhp#par_id3150651.8
msgid  "\<ahelp hid=\"SVX:METRICFIELD:RID_SVXPAGE_STD_PARAGRAPH:ED_FLINEINDENT\"\>Indents the first line of a paragraph by the amount that you enter. To create a hanging indent enter a positive value for \"Before text\" and a negative value for \"First line\". To indent the first line of a paragraph that uses numbering or bullets, choose \"\<link href=\"text/shared/01/06050600.xhp\"\>Format - Bullets and Numbering - Position\</link\>\".\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#hd_id3150288.52
msgid  "\<switchinline select=\"appl\"\>\<caseinline select=\"WRITER\"\>Automatic \</caseinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#par_id3151041.53
msgid  "\<switchinline select=\"appl\"\>\<caseinline select=\"WRITER\"\>\<ahelp hid=\"SVX:CHECKBOX:RID_SVXPAGE_STD_PARAGRAPH:CB_AUTO\"\>Automatically indents a paragraph according to the font size and the line spacing. The setting in the \<emph\>First Line \</emph\>box is ignored.\</ahelp\>\</caseinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#hd_id3157894.22
msgid  "Spacing"
msgstr "Afstand"

#: source/text/shared/01/05030100.xhp#par_id3152462.23
msgid  "Specify the amount of space to leave between selected paragraphs."
msgstr "Specificeer de hoeveelheid ruimte die u tussen geselecteerde alinea's wilt vrijlaten."

#: source/text/shared/01/05030100.xhp#hd_id3147216.24
msgid  "Above paragraph"
msgstr "Boven alinea"

#: source/text/shared/01/05030100.xhp#par_id3146148.25
msgid  "\<ahelp hid=\"SVX:METRICFIELD:RID_SVXPAGE_STD_PARAGRAPH:ED_TOPDIST\"\>Enter the amount of space that you want to leave above the selected paragraph(s).\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#hd_id3145590.26
msgid  "Below paragraph"
msgstr "Onder alinea"

#: source/text/shared/01/05030100.xhp#par_id3163822.27
msgid  "\<ahelp hid=\"SVX:METRICFIELD:RID_SVXPAGE_STD_PARAGRAPH:ED_BOTTOMDIST\"\>Enter the amount of space that you want to leave below the selected paragraph(s).\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#hd_id3156441.28
msgid  "Line spacing"
msgstr "Regelafstand"

#: source/text/shared/01/05030100.xhp#par_id3146985.29
msgid  "\<ahelp hid=\"SVX:LISTBOX:RID_SVXPAGE_STD_PARAGRAPH:LB_LINEDIST\"\>Specify the amount of space to leave between lines of text in a paragraph.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#hd_id3146923.30
msgid  "Single"
msgstr "Enkel"

#: source/text/shared/01/05030100.xhp#par_id3150011.31
msgid  "\<variable id=\"einzeiligtext\"\>Applies single line spacing to the current paragraph. This is the default setting. \</variable\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#hd_id3148500.33
msgid  "1.5 lines"
msgstr "Anderhalf"

#: source/text/shared/01/05030100.xhp#par_id3150094.34
msgid  "\<variable id=\"eineinhalbzeiligtext\"\>Sets the line spacing to 1.5 lines. \</variable\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#hd_id3149378.36
msgid  "Double"
msgstr "Dubbel"

#: source/text/shared/01/05030100.xhp#par_id3154512.37
msgid  "\<variable id=\"zweizeiligtext\"\>Sets the line spacing to two lines. \</variable\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#hd_id3151206.39
msgid  "Proportional"
msgstr "Proportioneel"

#: source/text/shared/01/05030100.xhp#par_id3147494.40
msgid  "Select this option and then enter a percentage value in the box, where 100% corresponds to single line spacing."
msgstr "Selecteer deze optie en voer een percentagewaarde in het vakje in, waarbij 100% overeenkomt met een afstand van één regel."

#: source/text/shared/01/05030100.xhp#hd_id3156332.41
msgid  "At Least"
msgstr "Minstens"

#: source/text/shared/01/05030100.xhp#par_id3157965.42
msgid  "Sets the minimum line spacing to the value that you enter in the box."
msgstr "Stelt de minimumregelafstand in op de waarde die u in het vakje invoert."

#: source/text/shared/01/05030100.xhp#par_id3150744.47
msgid  "If you use different font sizes within a paragraph, the line spacing is automatically adjusted to the largest font size. If you prefer to have identical spacing for all lines, specify a value in \<emph\>At least\</emph\> that corresponds to the largest font size."
msgstr ""

#: source/text/shared/01/05030100.xhp#hd_id3153927.43
msgid  "Leading"
msgstr "Regelafstand"

#: source/text/shared/01/05030100.xhp#par_id3153354.44
msgid  "Sets the height of the vertical space that is inserted between two lines."
msgstr "Stelt de hoogte in van de verticale ruimte die tussen twee regels is ingevoegd."

#: source/text/shared/01/05030100.xhp#hd_id3155443.54
msgid  "\<switchinline select=\"appl\"\>\<caseinline select=\"WRITER\"\>Fixed \</caseinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#par_id3153711.55
msgid  "\<switchinline select=\"appl\"\>\<caseinline select=\"WRITER\"\>Sets the line spacing to exactly match the value that you enter in the box. This can result in cropped characters. \</caseinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#hd_id3156383.45
msgid  "of"
msgstr "van"

#: source/text/shared/01/05030100.xhp#par_id3154304.46
msgid  "\<ahelp hid=\"SVX:METRICFIELD:RID_SVXPAGE_STD_PARAGRAPH:ED_LINEDISTMETRIC\"\>Enter the value to use for the line spacing.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#hd_id3154965.48
msgid  "\<switchinline select=\"appl\"\>\<caseinline select=\"WRITER\"\>Register-true \</caseinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#hd_id3146316.50
msgid  "\<switchinline select=\"appl\"\>\<caseinline select=\"WRITER\"\>Activate \</caseinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#par_id3156315.51
msgid  "\<switchinline select=\"appl\"\>\<caseinline select=\"WRITER\"\>\<ahelp hid=\"SVX:CHECKBOX:RID_SVXPAGE_STD_PARAGRAPH:CB_REGISTER\"\>Aligns the baseline of each line of text to a vertical document grid, so that each line is the same height. To use this feature, you must first activate the \<emph\>Register-true \</emph\>option for the current page style. To do this, choose \<emph\>Format - Page\</emph\>, click on the \<emph\>Page \</emph\>tab, and then select the\<emph\> Register-true\</emph\> box in the\<emph\> Layout settings\</emph\> area.\</ahelp\>\</caseinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/01/05030100.xhp#par_id9267250
msgid  "\<link href=\"text/swriter/guide/registertrue.xhp\" name=\"Writing Register-true\"\>Writing Register-true\</link\>"
msgstr ""

#: source/text/shared/01/05030300.xhp#tit
msgid  "Tabs"
msgstr "Tabulator"

#: source/text/shared/01/05030300.xhp#bm_id3156027
msgid  "\<bookmark_value\>formats; tabulators\</bookmark_value\>\<bookmark_value\>fill characters with tabulators\</bookmark_value\>\<bookmark_value\>tab stops;settings\</bookmark_value\>"
msgstr ""

#: source/text/shared/01/05030300.xhp#hd_id3156027.1
msgid  "\<link href=\"text/shared/01/05030300.xhp\" name=\"Tabs\"\>Tabs\</link\>"
msgstr ""

#: source/text/shared/01/05030300.xhp#par_id3153577.2
msgid  "\<ahelp hid=\"HID_TABULATOR\"\>Set the position of a tab stop in a paragraph.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030300.xhp#par_id3147653.40
msgid  "If you want, you can also use the ruler to set the tab positions."
msgstr "U kunt desgewenst ook de liniaal gebruiken om de tabposities in te stellen."

#: source/text/shared/01/05030300.xhp#hd_id3154897.3
msgid  "Position"
msgstr "Positie"

#: source/text/shared/01/05030300.xhp#par_id3153331.4
msgid  "\<ahelp hid=\"SVX:METRICBOX:RID_SVXPAGE_TABULATOR:ED_TABPOS\"\>Select a tab stop type, enter a new measurement, and then click \<emph\>New\</emph\>. If you want, you can also specify the measurement units to use for the tab (cm for centimeter, or \" for inches). Existing tabs to the left of the first tab that you set are removed.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030300.xhp#hd_id3155180.9
msgid  "Type"
msgstr "Type"

#: source/text/shared/01/05030300.xhp#par_id3149514.10
msgid  "Select the type of tab stop that you want to modify."
msgstr "Selecteer het tabstop-type dat u wilt wijzigen."

#: source/text/shared/01/05030300.xhp#hd_id3157910.11
msgid  "Left"
msgstr "Links"

#: source/text/shared/01/05030300.xhp#par_id3146847.41
msgid  "The name of this tab stop is \<emph\>Left/Top\</emph\> if Asian language support is enabled."
msgstr ""

#: source/text/shared/01/05030300.xhp#par_id3153698.12
msgid  "\<ahelp hid=\"SVX:RADIOBUTTON:RID_SVXPAGE_TABULATOR:BTN_TABTYPE_LEFT\"\>Aligns the left edge of the text to the tab stop and extends the text to the right.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030300.xhp#hd_id3149763.13
msgid  "Right"
msgstr "Rechts"

#: source/text/shared/01/05030300.xhp#par_id3148491.42
msgid  "This name of this tab stop is \<emph\>Right/Bottom\</emph\> if Asian language support is enabled."
msgstr ""

#: source/text/shared/01/05030300.xhp#par_id3151384.14
msgid  "\<ahelp hid=\"SVX:RADIOBUTTON:RID_SVXPAGE_TABULATOR:BTN_TABTYPE_RIGHT\"\>Aligns the right edge of the text to the tab stop and extends the text to the left of the tab stop.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030300.xhp#hd_id3153628.15
msgid  "Center"
msgstr "Gecentreerd"

#: source/text/shared/01/05030300.xhp#par_id3154347.16
msgid  "\<ahelp hid=\"SVX:RADIOBUTTON:RID_SVXPAGE_TABULATOR:BTN_TABTYPE_CENTER\"\>Aligns the center of the text to the tab stop.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030300.xhp#hd_id3148552.17
msgid  "Decimal"
msgstr "Decimaal"

#: source/text/shared/01/05030300.xhp#par_id3144422.18
msgid  "\<ahelp hid=\"SVX:RADIOBUTTON:RID_SVXPAGE_TABULATOR:BTN_TABTYPE_DECIMAL\"\>Aligns the decimal point of a number to the center of the tab stop and text to the left of the tab.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030300.xhp#par_id3154388.19
msgid  "\<switchinline select=\"sys\"\>\<caseinline select=\"WIN\"\>The character that is used as a decimal separator depends on the regional setting of your operating system. \</caseinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/01/05030300.xhp#hd_id3153380.20
msgid  "Character"
msgstr "Teken"

#: source/text/shared/01/05030300.xhp#par_id3153778.21
msgid  "\<ahelp hid=\"SVX:EDIT:RID_SVXPAGE_TABULATOR:ED_TABTYPE_DECCHAR\"\>Enter a character that you want the decimal tab to use as a decimal separator.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030300.xhp#hd_id3159151.22
msgid  "Fill Character"
msgstr "Opvulteken"

#: source/text/shared/01/05030300.xhp#par_id3154153.23
msgid  "Specify the characters to use as leader to the left of the tab stop."
msgstr "Specificeer de tekens die moeten worden gebruikt als opvulteken links van de tabstop."

#: source/text/shared/01/05030300.xhp#hd_id3144760.24
msgid  "None"
msgstr "Geen"

#: source/text/shared/01/05030300.xhp#par_id3143231.25
msgid  "\<ahelp hid=\"SVX:RADIOBUTTON:RID_SVXPAGE_TABULATOR:BTN_FILLCHAR_NO\"\>Inserts no fill characters, or removes existing fill characters to the left of the tab stop.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030300.xhp#hd_id3152933.26
msgid  "......."
msgstr "......."

#: source/text/shared/01/05030300.xhp#par_id3153192.27
msgid  "\<ahelp hid=\"SVX:RADIOBUTTON:RID_SVXPAGE_TABULATOR:BTN_FILLCHAR_POINTS\"\>Fills the empty space to the left of the tab stop with dots.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030300.xhp#hd_id3156280.28
msgid  "------"
msgstr "------"

#: source/text/shared/01/05030300.xhp#par_id3156212.29
msgid  "\<ahelp hid=\"SVX:RADIOBUTTON:RID_SVXPAGE_TABULATOR:BTN_FILLCHAR_DASHLINE\"\>Fills the empty space to the left of the tab stop with dashes.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030300.xhp#hd_id3157960.30
msgid  "______"
msgstr "______"

#: source/text/shared/01/05030300.xhp#par_id3151043.31
msgid  "\<ahelp hid=\"SVX:RADIOBUTTON:RID_SVXPAGE_TABULATOR:BTN_FILLCHAR_UNDERSCORE\"\>Draws a line to fill the empty space to the left of the tab stop.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030300.xhp#hd_id3153770.32
msgid  "Character"
msgstr "Teken"

#: source/text/shared/01/05030300.xhp#par_id3150441.33
msgid  "\<ahelp hid=\"SVX:RADIOBUTTON:RID_SVXPAGE_TABULATOR:BTN_FILLCHAR_OTHER\"\>Allows you to specify a character to fill the empty space to the left of the tab stop.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030300.xhp#hd_id3152596.36
msgid  "New"
msgstr "Nieuw"

#: source/text/shared/01/05030300.xhp#par_id3163717.37
msgid  "\<ahelp hid=\"SVX:PUSHBUTTON:RID_SVXPAGE_TABULATOR:BTN_NEW\"\>Adds the tab stop that you defined to the current paragraph.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030300.xhp#hd_id3153945.38
msgid  "Clear All"
msgstr "Alles wissen"

#: source/text/shared/01/05030300.xhp#par_id3145660.39
msgid  "\<ahelp hid=\"SVX:PUSHBUTTON:RID_SVXPAGE_TABULATOR:BTN_DELALL\"\>Removes all of the tab stops that you defined under \<emph\>Position\</emph\>. Sets \<emph\>Left\</emph\> tab stops at regular intervals as the default tab stops.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030500.xhp#tit
msgid  "Borders"
msgstr "Randen"

#: source/text/shared/01/05030500.xhp#hd_id3154812.1
msgid  "\<link href=\"text/shared/01/05030500.xhp\" name=\"Borders\"\>Borders\</link\>"
msgstr ""

#: source/text/shared/01/05030500.xhp#par_id3151097.2
msgid  "\<ahelp hid=\"HID_BORDER\"\>Sets the border options for the selected objects in Writer or Calc.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030500.xhp#par_id3155351.44
msgid  "You can specify the border position, size, and style in Writer or Calc. \<switchinline select=\"appl\"\>\<caseinline select=\"WRITER\"\>In $[officename] Writer, you can add borders to pages, frames, graphics, tables, paragraphs, and to embedded objects. \</caseinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/01/05030500.xhp#par_id3152997.40
msgid  "To modify the border of an entire table, place the cursor in a table cell, right-click, choose \<emph\>Table\</emph\>, and then click the \<emph\>Borders\</emph\> tab. To modify the border of a table cell, select the cell, right-click, choose \<emph\>Table\</emph\>, and then click the \<emph\>Borders\</emph\> tab."
msgstr ""

#: source/text/shared/01/05030500.xhp#hd_id3145417.3
msgid  "Line arrangement"
msgstr "Lijnschikking"

#: source/text/shared/01/05030500.xhp#par_id3153332.4
msgid  "\<ahelp hid=\"HID_BORDER_CTL_PRESETS\"\>Select a predefined border style to apply.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030500.xhp#par_id3148643.5
msgid  "If you are in a table or spreadsheet, you can also add or remove predefined borders. Use the \<emph\>Borders\</emph\> icon on the \<emph\>Table Bar\</emph\>."
msgstr ""

#: source/text/shared/01/05030500.xhp#hd_id3149575.23
msgid  "Line"
msgstr "Lijn"

#: source/text/shared/01/05030500.xhp#par_id3152360.24
msgid  "\<ahelp hid=\"SVX:LISTBOX:RID_SVXPAGE_BORDER:LB_LINESTYLE\"\>Click the border style that you want to apply. The style is applied to the borders selected in the preview.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030500.xhp#par_id3154938.29
msgid  "\<ahelp hid=\"SVX:LISTBOX:RID_SVXPAGE_BORDER:LB_LINECOLOR\"\>Select the line color that you want to use for the selected border(s).\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030500.xhp#hd_id3150359.21
msgid  "Spacing to contents"
msgstr "Afstand tot inhoud"

#: source/text/shared/01/05030500.xhp#par_id3154365.22
msgid  "Specify the amount of space that you want to leave between the border and the contents of the selection."
msgstr "Geef de hoeveelheid ruimte aan die tussen de rand en de inhoud van de selectie moet worden opengelaten."

#: source/text/shared/01/05030500.xhp#hd_id3147084.45
msgid  "Left"
msgstr "Links"

#: source/text/shared/01/05030500.xhp#par_id3151176.46
msgid  "\<ahelp hid=\"SVX_METRICFIELD_RID_SVXPAGE_BORDER_MF_LEFT\"\>Enter the distance that you want to have between the left border and the contents of the selection.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030500.xhp#hd_id3150650.47
msgid  "Right"
msgstr "Rechts"

#: source/text/shared/01/05030500.xhp#par_id3153104.48
msgid  "\<ahelp hid=\"SVX_METRICFIELD_RID_SVXPAGE_BORDER_MF_RIGHT\"\>Enter the distance that you want to have between the right border and the contents of the selection.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030500.xhp#hd_id3150495.49
msgid  "Top"
msgstr "Boven"

#: source/text/shared/01/05030500.xhp#par_id3156212.50
msgid  "\<ahelp hid=\"SVX_METRICFIELD_RID_SVXPAGE_BORDER_MF_TOP\"\>Enter the distance that you want to have between the top border and the contents of the selection.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030500.xhp#hd_id3150767.51
msgid  "Bottom"
msgstr "Beneden"

#: source/text/shared/01/05030500.xhp#par_id3158410.52
msgid  "\<ahelp hid=\"SVX_METRICFIELD_RID_SVXPAGE_BORDER_MF_BOTTOM\"\>Enter the distance that you want to have between the bottom border and the contents of the selection.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030500.xhp#hd_id3155429.53
msgid  "Synchronize"
msgstr "Synchroniseren"

#: source/text/shared/01/05030500.xhp#par_id3154299.54
msgid  "\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_BORDER_CB_SYNC\"\>Applies the same \<emph\>spacing to contents\</emph\> setting to all four borders when you enter a new distance.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030500.xhp#bm_id3155855
msgid  "\<bookmark_value\>shadows; borders\</bookmark_value\>\<bookmark_value\>borders; shadows\</bookmark_value\>\<bookmark_value\>margins; shadows\</bookmark_value\>"
msgstr ""

#: source/text/shared/01/05030500.xhp#hd_id3155855.31
msgid  "Shadow style"
msgstr "Schaduwstijl"

#: source/text/shared/01/05030500.xhp#par_id3146975.32
msgid  "You can also apply a shadow effect to borders. For the best results, only apply this effect when all four borders are visible."
msgstr "U kunt ook een schaduweffect op randen toepassen. Voor de beste resultaten kunt u dit effect het beste toepassen wanneer alle vier de randen zichtbaar zijn."

#: source/text/shared/01/05030500.xhp#par_id3157309.43
msgid  "Graphics or objects that are anchored to a frame in the document cannot exceed the size of the frame. If you apply a shadow to the borders of an object that fills an entire frame, the size of the object is reduced to display the shadows."
msgstr "Afbeeldingen of objecten die aan een frame in het document zijn verankerd, kunnen niet groter zijn dan het formaat van het frame. Als u op de randen van een object een schaduw toepast waarmee een frame volledig wordt gevuld, wordt de grootte van het object verkleind zodat de schaduw kan worden weergegeven."

#: source/text/shared/01/05030500.xhp#hd_id3153728.33
msgid  "Position"
msgstr "Positie"

#: source/text/shared/01/05030500.xhp#par_id3153364.34
msgid  "\<ahelp hid=\"HID_BORDER_CTL_SHADOWS\"\>Click a shadow style for the selected borders.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030500.xhp#hd_id3156444.35
msgid  "Distance"
msgstr "Afstand"

#: source/text/shared/01/05030500.xhp#par_id3156060.36
msgid  "\<ahelp hid=\"SVX:METRICFIELD:RID_SVXPAGE_BORDER:ED_SHADOWSIZE\"\>Enter the width of the shadow.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030500.xhp#hd_id3155307.37
msgid  "Color"
msgstr "Kleur"

#: source/text/shared/01/05030500.xhp#par_id3146147.38
msgid  "\<ahelp hid=\"SVX:LISTBOX:RID_SVXPAGE_BORDER:LB_SHADOWCOLOR\"\>Select a color for the shadow.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030500.xhp#par_idN10A2B
msgid  "Properties"
msgstr "Eigenschappen"

#: source/text/shared/01/05030500.xhp#par_idN10A2F
msgid  "Specifies the properties for the current paragraph or the selected paragraphs."
msgstr "Specificeert de eigenschappen voor de huidige alinea of de geselecteerde alinea's."

#: source/text/shared/01/05030500.xhp#par_idN10A3A
msgid  "Merge with next paragraph"
msgstr "Met volgende alinea samenvoegen"

#: source/text/shared/01/05030500.xhp#par_idN10A3E
msgid  "\<ahelp hid=\"svx:CheckBox:RID_SVXPAGE_BORDER:CB_MERGEWITHNEXT\"\>Merges the border style and the shadow style of the current paragraph with the next paragraph.\</ahelp\> These styles are only merged if the indent, border, and shadow styles of the next paragraph are the same as the current paragraph. This option is also available for Paragraph Styles."
msgstr ""

#: source/text/shared/01/05030500.xhp#par_idN109BA
msgid  "Merge adjacent line styles"
msgstr "Aangrenzende lijnstijlen samenvoegen"

#: source/text/shared/01/05030500.xhp#par_idN109BE
msgid  "\<ahelp hid=\"700793922\"\>Merges two different border styles of adjacent cells in a Writer table into one border style. This property is valid for a whole table in a Writer document.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030500.xhp#par_idN109C1
msgid  "The rules can be condensed to the statement that the stronger attribute wins. If, for example, one cell has a red border of 2 point width, and the adjacent cell has a blue border of 3 point width, then the common border between these two cells will be blue with 3 point width."
msgstr "De regels kunnen worden gereduceerd tot de instructie dat het sterkere attribuut wint. Als één cel bijvoorbeeld een rode rand met 2-punts dikte heeft, en de aangrenzende cel een blauwe rand met 3-punts dikte, dan is de gemeenschappelijke rand tussen deze twee cellen blauw met 3-punts dikte."

#: source/text/shared/01/05030600.xhp#tit
msgid  "Background"
msgstr "Achtergrond"

#: source/text/shared/01/05030600.xhp#bm_id3151097
msgid  "\<bookmark_value\>frames; backgrounds\</bookmark_value\>\<bookmark_value\>backgrounds; frames/sections/indexes\</bookmark_value\>\<bookmark_value\>sections; backgrounds\</bookmark_value\>\<bookmark_value\>indexes; backgrounds\</bookmark_value\>\<bookmark_value\>footers;backgrounds\</bookmark_value\>\<bookmark_value\>headers;backgrounds\</bookmark_value\>"
msgstr ""

#: source/text/shared/01/05030600.xhp#hd_id3151097.1
msgid  "\<link href=\"text/shared/01/05030600.xhp\" name=\"Background\"\>Background\</link\>"
msgstr ""

#: source/text/shared/01/05030600.xhp#par_id3153748.2
msgid  "\<ahelp hid=\"HID_BACKGROUND\"\>Set the background color or graphic.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030600.xhp#par_id3147653.34
msgid  "You can specify the background for \<switchinline select=\"appl\"\>\<caseinline select=\"WRITER\"\>paragraphs, pages, headers, footers, text frames, tables, table cells, sections, and indexes.\</caseinline\>\<caseinline select=\"CALC\"\>cells and pages.\</caseinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/01/05030600.xhp#hd_id3154514.4
msgid  "As"
msgstr "Als"

#: source/text/shared/01/05030600.xhp#par_id3154380.5
msgid  "\<ahelp hid=\"SVX:LISTBOX:RID_SVXPAGE_BACKGROUND:LB_SELECTOR\"\>Select the type of background that you want to apply.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030600.xhp#hd_id3151245.7
msgid  "Using a Color as a Background"
msgstr "Een kleur als achtergrond gebruiken"

#: source/text/shared/01/05030600.xhp#hd_id3148946.8
msgid  "Color Background"
msgstr "Achtergrondkleur"

#: source/text/shared/01/05030600.xhp#par_id3152361.9
msgid  "\<ahelp hid=\"HID_BACKGROUND_CTL_BGDCOLORSET\"\>Click the color that you want to use as a background. To remove a background color, click \<emph\>No Fill\</emph\>.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030600.xhp#hd_id3153524.37
msgid  "\<switchinline select=\"appl\"\>\<caseinline select=\"WRITER\"\>Transparency\</caseinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/01/05030600.xhp#par_idN107A4
msgid  "Background transparency can be set only for frames."
msgstr "Achtergrondtransparantie kan alleen voor frames ingesteld worden."

#: source/text/shared/01/05030600.xhp#par_id3150358.38
msgid  "\<switchinline select=\"appl\"\>\<caseinline select=\"WRITER\"\>\<ahelp hid=\"SVX_METRICFIELD_RID_SVXPAGE_BACKGROUND_MF_COL_TRANS\"\>Set the transparency for the background color or graphic of a frame, where 100% is completely transparent and 0% is opaque. When you increase the transparency of the background, the underlying text or objects become visible through the background of the frame.\</ahelp\>\</caseinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/01/05030600.xhp#hd_id3154216.11
msgid  "\<switchinline select=\"appl\"\>\<caseinline select=\"WRITER\"\>For\</caseinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/01/05030600.xhp#par_id3145419.12
msgid  "\<switchinline select=\"appl\"\>\<caseinline select=\"WRITER\"\>\<ahelp hid=\"SVX:LISTBOX:RID_SVXPAGE_BACKGROUND:LB_TBL_BOX\"\>Select the area that you want to apply the background color to.\</ahelp\> For example, when you define the background color for a table, you can choose to apply it to the table, the active cell, the row, or the column.\</caseinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/01/05030600.xhp#par_id3150497.13
msgid  "This option is only available when you edit the background of a table or a paragraph style."
msgstr "Deze optie is alleen beschikbaar wanneer u de achtergrond van een tabel of een alinea-opmaakprofiel bewerkt."

#: source/text/shared/01/05030600.xhp#hd_id3153056.14
msgid  "Using a Graphic as a Background"
msgstr "Een afbeelding als achtergrond gebruiken"

#: source/text/shared/01/05030600.xhp#hd_id3149983.15
msgid  "File"
msgstr "Bestand"

#: source/text/shared/01/05030600.xhp#par_id3152462.16
msgid  "Contains information about the graphic file."
msgstr "Bevat informatie over het afbeeldingsbestand."

#: source/text/shared/01/05030600.xhp#hd_id3145592.17
msgid  "Display field"
msgstr "Weergaveveld"

#: source/text/shared/01/05030600.xhp#par_id3154920.18
msgid  "Shows the path for the graphic file."
msgstr "Toont het pad voor het afbeeldingsbestand."

#: source/text/shared/01/05030600.xhp#hd_id3145272.19
msgid  "Link"
msgstr "Koppeling"

#: source/text/shared/01/05030600.xhp#par_id3154150.20
msgid  "\<ahelp hid=\"SVX:CHECKBOX:RID_SVXPAGE_BACKGROUND:BTN_LINK\"\>Links to or embeds the graphic file in the current file.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030600.xhp#hd_id3155366.21
msgid  "Preview"
msgstr "Voorbeeld"

#: source/text/shared/01/05030600.xhp#par_id3147426.22
msgid  "\<ahelp hid=\"SVX:CHECKBOX:RID_SVXPAGE_BACKGROUND:BTN_PREVIEW\"\>Displays or hides a preview of the selected graphic.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030600.xhp#hd_id3154472.23
msgid  "Browse"
msgstr "Bladeren"

#: source/text/shared/01/05030600.xhp#par_id3153951.24
msgid  "\<ahelp hid=\"SVX:PUSHBUTTON:RID_SVXPAGE_BACKGROUND:BTN_BROWSE\"\>Locate the graphic file that you want to use as a background, and then click \<emph\>Open\</emph\>.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030600.xhp#hd_id3153726.25
msgid  "Type"
msgstr "Type"

#: source/text/shared/01/05030600.xhp#par_id3147442.26
msgid  "Specify the way that you want to display the background graphic."
msgstr "Geef aan op welke manier de achtergrondafbeelding wordt weergegeven."

#: source/text/shared/01/05030600.xhp#hd_id3153366.27
msgid  "Position"
msgstr "Positie"

#: source/text/shared/01/05030600.xhp#par_id3153741.28
msgid  "\<ahelp hid=\"SVX:RADIOBUTTON:RID_SVXPAGE_BACKGROUND:BTN_POSITION\"\>Select this option, and then click a location in the position grid.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030600.xhp#hd_id3156005.29
msgid  "Area"
msgstr "Gebied"

#: source/text/shared/01/05030600.xhp#par_id3152596.30
msgid  "\<ahelp hid=\"SVX:RADIOBUTTON:RID_SVXPAGE_BACKGROUND:BTN_AREA\"\>Stretches the graphic to fill the entire background of the selected object.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030600.xhp#hd_id3145663.32
msgid  "Tile"
msgstr "Naast elkaar"

#: source/text/shared/01/05030600.xhp#par_id3149481.33
msgid  "\<ahelp hid=\"SVX:RADIOBUTTON:RID_SVXPAGE_BACKGROUND:BTN_TILE\"\>Repeats the graphic so that it covers the entire background of the selected object.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030600.xhp#par_id3151114.35
msgid  "\<ahelp hid=\".\" visibility=\"hidden\"\>Click a color. Click No Fill to remove a background or highlighting color. Click Automatic to reset a font color.\</ahelp\>"
msgstr ""

#: source/text/shared/01/05030700.xhp#tit
msgid  "Alignment"
msgstr "Uitlijning"

#: source/text/shared/01/05030700.xhp#bm_id3150008
msgid  "\<bookmark_value\>aligning; paragraphs\</bookmark_value\>\<bookmark_value\>paragraphs; alignment\</bookmark_value\>\<bookmark_value\>lines of text; alignment\</bookmark_value\>\<bookmark_value\>left alignment of paragraphs\</bookmark_value\>\<bookmark_value\>right alignment of paragraphs\</bookmark_value\>\<bookmark_value\>centered text\</bookmark_value\>\<bookmark_value\>justifying text\</bookmark_value\>"
msgstr ""

#: source/text/shared/01/05030700.xhp#hd_id3150008.1
msgid  "\<link href=\"text/shared/01/05030700.xhp\" name=\"Alignment\"\>Alignment\</link\>"
msgstr ""

