#: source/text/shared/optionen/01060100.xhp#par_id3153988.2
msgid  "\<ahelp hid=\"HID_SCPAGE_CONTENT\"\>Defines which elements of the \<item type=\"productname\"\>%PRODUCTNAME\</item\> Calc main window are displayed. You can also show or hide highlighting of values in tables.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3153682.28
msgid  "Visual aids"
msgstr "Visuele hulpmiddelen"

#: source/text/shared/optionen/01060100.xhp#par_id3153311.29
msgid  "Specifies which lines are displayed."
msgstr "Dit gedeelte wordt gebruikt om te specificeren welke lijnen moeten worden getoond."

#: source/text/shared/optionen/01060100.xhp#hd_id3147242.30
msgid  "Grid lines"
msgstr "Rasterlijnen"

#: source/text/shared/optionen/01060100.xhp#par_id3153088.31
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CONTENT_CB_GRID\"\>Specifies whether to display grid lines between the cells.\</ahelp\>\<switchinline select=\"appl\"\>\<caseinline select=\"CALC\"\>For printing, choose \<emph\>Format - Page - \</emph\>\<link href=\"text/scalc/01/05070500.xhp\" name=\"Sheet\"\>\<emph\>Sheet\</emph\>\</link\> and mark the \<emph\>Grid\</emph\> check box.\</caseinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3156326.32
msgid  "Color"
msgstr "Kleur"

#: source/text/shared/optionen/01060100.xhp#par_id3154286.33
msgid  "\<ahelp hid=\"SC_LISTBOX_RID_SCPAGE_CONTENT_LB_COLOR\"\>Specifies a color for the grid lines in the current document.\</ahelp\> To see the grid line color that was saved with the document, go to \<emph\>\<switchinline select=\"sys\"\>\<caseinline select=\"MAC\"\>%PRODUCTNAME - Preferences\</caseinline\>\<defaultinline\>Tools - Options\</defaultinline\>\</switchinline\> - %PRODUCTNAME - Appearance\</emph\>, under \<emph\>Scheme\</emph\> find the entry \<emph\>Spreadsheet - Grid lines\</emph\> and set the color to \"Automatic\"."
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3152349.34
msgid  "Page breaks"
msgstr "Pagina-einde"

#: source/text/shared/optionen/01060100.xhp#par_id3151245.35
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CONTENT_CB_PAGEBREAKS\"\>Specifies whether to view the page breaks within a defined print area.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3149669.36
msgid  "Guides while moving"
msgstr "hulplijnen bij verplaatsen"

#: source/text/shared/optionen/01060100.xhp#par_id3148550.37
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CONTENT_CB_GUIDELINE\"\>Specifies whether to view guides when moving drawings, frames, graphics and other objects.\</ahelp\> These guides help you align objects."
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3150358.38
msgid  "Simple handles"
msgstr "eenvoudige handvatten"

#: source/text/shared/optionen/01060100.xhp#par_id3154140.39
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CONTENT_CB_HANDLES\"\>Specifies whether to display the handles (the eight points on a selection box) as simple squares without 3D effect.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3149202.40
msgid  "Large handles"
msgstr "grote handvatten"

#: source/text/shared/optionen/01060100.xhp#par_id3154123.41
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CONTENT_CB_BIGHANDLES\"\>Specifies that larger than normal handles (the eight points on a selection box) are displayed.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3152920.3
msgid  "Display"
msgstr "Weergeven"

#: source/text/shared/optionen/01060100.xhp#par_id3125864.4
msgid  "Select various options for the screen display."
msgstr "Selecteer verschillende opties voor de schermweergave."

#: source/text/shared/optionen/01060100.xhp#hd_id3154218.5
msgid  "Formulas"
msgstr "Formules"

#: source/text/shared/optionen/01060100.xhp#par_id3150440.6
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CONTENT_CB_FORMULA\"\>Specifies whether to show formulas instead of results in the cells.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3155132.7
msgid  "Zero values"
msgstr "Nulwaarden"

#: source/text/shared/optionen/01060100.xhp#par_id3147318.8
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CONTENT_CB_NIL\"\>Specifies whether to show numbers with the value of 0.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3147348.9
msgid  "Comment indicator"
msgstr "Indicatie voor notitie"

#: source/text/shared/optionen/01060100.xhp#par_id3146974.10
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CONTENT_CB_ANNOT\"\>Specifies that a small rectangle in the top right corner of the cell indicates that a comment exists. The comment will be shown only when you enable tips under \<emph\>%PRODUCTNAME - General\</emph\> in the Options dialog box.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060100.xhp#par_id3150487.24
msgid  "\<ahelp hid=\".uno:NoteVisible\"\>To display a comment permanently, select the \<emph\>Show comment\</emph\> command from the cell's context menu.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060100.xhp#par_id3149667.25
msgid  "You can type and edit comments with the \<link href=\"text/shared/01/04050000.xhp\" name=\"Insert - Comment\"\>\<emph\>Insert - Comment\</emph\>\</link\> command. Comments that are permanently displayed can be edited by clicking the comment box. Click the Navigator and under the \<emph\>Comments\</emph\> entry you can view all comments in the current document. By double clicking a comment in Navigator, the cursor will jump to the corresponding cell containing the comment."
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3150872.11
msgid  "Value highlighting"
msgstr "Waardenmarkering"

#: source/text/shared/optionen/01060100.xhp#par_id3154792.12
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CONTENT_CB_VALUE\"\>Mark the \<emph\>Value highlighting\</emph\> box to show the cell contents in different colors, depending on type. Text cells are formatted in black, formulas in green, and number cells in blue, no matter how their display is formatted.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060100.xhp#par_id3151319.13
msgid  "When this command is active, any colors assigned in the document will not be displayed until the function is deactivated."
msgstr "Wanneer deze opdracht actief is, zullen kleuren die toegekend zijn in het document, niet getoond worden totdat de functie is gedeactiveerd."

#: source/text/shared/optionen/01060100.xhp#hd_id3157846.14
msgid  "Anchor"
msgstr "Anker"

#: source/text/shared/optionen/01060100.xhp#par_id3147494.15
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CONTENT_CB_ANCHOR\"\>Specifies whether the anchor icon is displayed when an inserted object, such as a graphic, is selected.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3146898.26
msgid  "Text overflow"
msgstr "Tekstoverloop"

#: source/text/shared/optionen/01060100.xhp#par_id3153707.27
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CONTENT_CB_CLIP\"\>If a cell contains text that is wider than the width of the cell, the text is displayed over empty neighboring cells in the same row. If there is no empty neighboring cell, a small triangle at the cell border indicates that the text continues.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3150327.54
msgid  "Show references in color"
msgstr "Verwijzingen in kleur weergeven"

#: source/text/shared/optionen/01060100.xhp#par_id3153766.55
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CONTENT_CB_RFIND\"\>Specifies that each reference is highlighted in color in the formula. The cell range is also enclosed by a colored border as soon as the cell containing the reference is selected for editing.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3155444.16
msgid  "Objects"
msgstr "Objecten"

#: source/text/shared/optionen/01060100.xhp#par_id3148405.17
msgid  "Defines whether to display or hide objects for up to three object groups."
msgstr "Hier kunt u tot drie objectgroepen definiëren om diagrammen weer te geven of te verbergen."

#: source/text/shared/optionen/01060100.xhp#hd_id3150043.18
msgid  "Objects/Graphics"
msgstr "Objecten/Afbeeldingen"

#: source/text/shared/optionen/01060100.xhp#par_id3163549.19
msgid  "\<ahelp hid=\"SC_LISTBOX_RID_SCPAGE_CONTENT_LB_OBJGRF\"\>Defines if objects and graphics are shown or hidden.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3151249.20
msgid  "Charts"
msgstr "Diagrammen"

#: source/text/shared/optionen/01060100.xhp#par_id3149106.21
msgid  "\<ahelp hid=\"SC_LISTBOX_RID_SCPAGE_CONTENT_LB_DIAGRAM\"\>Defines if charts in your document are shown or hidden.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3154703.22
msgid  "Drawing objects"
msgstr "Tekenobjecten"

#: source/text/shared/optionen/01060100.xhp#par_id3155959.23
msgid  "\<ahelp hid=\"SC_LISTBOX_RID_SCPAGE_CONTENT_LB_DRAW\"\>Defines if drawing objects in your document are shown or hidden.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id0909200810585828
msgid  "Zoom"
msgstr "In-/Uitzoomen"

#: source/text/shared/optionen/01060100.xhp#hd_id0909200810585881
msgid  "Synchronize sheets"
msgstr "Bladen synchroniseren"

#: source/text/shared/optionen/01060100.xhp#par_id0909200810585870
msgid  "\<ahelp hid=\".\"\>If checked, all sheets are shown with the same zoom factor. If not checked, each sheet can have its own zoom factor.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3153920.42
msgid  "Window"
msgstr "Venster"

#: source/text/shared/optionen/01060100.xhp#par_id3154661.43
msgid  "Specifies whether some Help elements will or will not appear in the table."
msgstr "Dit gedeelte wordt gebruikt om te specificeren of enkele hulpelementen wel of niet verschijnen in de tabel."

#: source/text/shared/optionen/01060100.xhp#hd_id3149923.44
msgid  "Column/Row headers"
msgstr "Kolom-/Rijkoppen"

#: source/text/shared/optionen/01060100.xhp#par_id3149816.45
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CONTENT_CB_ROWCOLHEADER\"\>Specifies whether to display row and column headers.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3154205.46
msgid  "Horizontal scrollbar"
msgstr "Horizontale schuifbalk"

#: source/text/shared/optionen/01060100.xhp#par_id3155578.47
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CONTENT_CB_HSCROLL\"\>Specifies whether to display a horizontal scrollbar at the bottom of the document window.\</ahelp\> Note that there is a slider between the horizontal scrollbar and the sheet tabs that may be set to one end."
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3148422.48
msgid  "Vertical scrollbar"
msgstr "Verticale schuifbalk"

#: source/text/shared/optionen/01060100.xhp#par_id3147128.49
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CONTENT_CB_VSCROLL\"\>Specifies whether to display a vertical scrollbar at the right of the document window.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3150826.50
msgid  "Sheet tabs"
msgstr "Bladtabs"

#: source/text/shared/optionen/01060100.xhp#par_id3154658.51
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CONTENT_CB_TBLREG\"\>Specifies whether to display the sheet tabs at the bottom of the spreadsheet document.\</ahelp\> If this box is not checked, you will only be able to switch between the sheets through the \<switchinline select=\"appl\"\>\<caseinline select=\"CALC\"\>\<link href=\"text/scalc/01/02110000.xhp\" name=\"Navigator\"\>Navigator\</link\>\</caseinline\>\<defaultinline\>Navigator\</defaultinline\>\</switchinline\>. Note that there is a slider between the horizontal scrollbar and the sheet tabs that may be set to one end."
msgstr ""

#: source/text/shared/optionen/01060100.xhp#hd_id3152584.52
msgid  "Outline symbols"
msgstr "Overzichtstekens"

#: source/text/shared/optionen/01060100.xhp#par_id3145135.53
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CONTENT_CB_OUTLINE\"\>If you have defined an \<switchinline select=\"appl\"\>\<caseinline select=\"CALC\"\>\<link href=\"text/scalc/01/12080000.xhp\" name=\"outline\"\>outline\</link\>\</caseinline\>\<defaultinline\>outline\</defaultinline\>\</switchinline\>, the \<emph\>Outline symbols\</emph\> option specifies whether to view the outline symbols at the border of the sheet.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060300.xhp#tit
msgid  "General"
msgstr "Algemeen"

#: source/text/shared/optionen/01060300.xhp#bm_id3151110
msgid  "\<bookmark_value\>metrics;in sheets\</bookmark_value\>\<bookmark_value\>tab stops; setting in sheets\</bookmark_value\>\<bookmark_value\>cells; cursor positions after input (Calc)\</bookmark_value\>\<bookmark_value\>edit mode; through Enter key (Calc)\</bookmark_value\>\<bookmark_value\>formatting; expanding (Calc)\</bookmark_value\>\<bookmark_value\>expanding formatting (Calc)\</bookmark_value\>\<bookmark_value\>references; expanding (Calc)\</bookmark_value\>\<bookmark_value\>column headers; highlighting (Calc)\</bookmark_value\>\<bookmark_value\>row headers; highlighting (Calc)\</bookmark_value\>"
msgstr ""

#: source/text/shared/optionen/01060300.xhp#hd_id3153311.1
msgid  "\<link href=\"text/shared/optionen/01060300.xhp\" name=\"General\"\>General\</link\>"
msgstr ""

#: source/text/shared/optionen/01060300.xhp#par_id3156410.2
msgid  "Defines general settings for spreadsheet documents."
msgstr "Definieert algemene instellingen voor werkbladen."

#: source/text/shared/optionen/01060300.xhp#hd_id3155338.24
msgid  "Metrics"
msgstr "Metrieken"

#: source/text/shared/optionen/01060300.xhp#hd_id3151110.20
msgid  "Measurement unit"
msgstr "Maateenheid"

#: source/text/shared/optionen/01060300.xhp#par_id3150444.21
msgid  "\<ahelp hid=\"SC:LISTBOX:RID_SCPAGE_LAYOUT:LB_UNIT\"\>Defines the unit of measure in spreadsheets.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060300.xhp#hd_id3149795.22
msgid  "Tab stops"
msgstr "Tabstops"

#: source/text/shared/optionen/01060300.xhp#par_id3150084.23
msgid  "\<ahelp hid=\"SC:METRICFIELD:RID_SCPAGE_LAYOUT:MF_TAB\"\>Defines the tab stops distance.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060300.xhp#hd_id3155135.3
msgid  "Input settings"
msgstr "Invoerinstellingen"

#: source/text/shared/optionen/01060300.xhp#hd_id3148491.5
msgid  "Press Enter to move selection"
msgstr "Druk op Enter om selectie te verplaatsen"

#: source/text/shared/optionen/01060300.xhp#par_id3145119.6
msgid  "\<ahelp hid=\"SC_LISTBOX_RID_SCPAGE_LAYOUT_LB_ALIGN\"\>Determines the direction that the cursor in the spreadsheet will move after you press the Enter key.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060300.xhp#hd_id3154307.8
msgid  "Press Enter to switch to edit mode"
msgstr "Druk op Enter om naar bewerkingsmodus te gaan"

#: source/text/shared/optionen/01060300.xhp#par_id3148943.9
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_LAYOUT_CB_EDITMODE\"\>Allows you to immediately edit the selected cell after pressing the Enter key.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060300.xhp#hd_id3153896.10
msgid  "Expand formatting"
msgstr "Opmaak uitbreiden"

#: source/text/shared/optionen/01060300.xhp#par_id3150400.11
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_LAYOUT_CB_FORMAT\"\>Specifies whether to automatically apply the formatting attributes of the selected cell to the empty adjacent cells.\</ahelp\> If, for example, the contents of the selected cell have the bold attribute, this bold attribute will also apply to adjacent cells. Cells that already have a special format will not be modified by this function. You can see the range in question by pressing the \<switchinline select=\"sys\"\>\<caseinline select=\"MAC\"\>Command \</caseinline\>\<defaultinline\>Ctrl\</defaultinline\>\</switchinline\> + * (multiplication sign on the number pad) shortcut. This format also applies to all new values inserted within this range. The normal default settings apply to cells outside this range."
msgstr ""

#: source/text/shared/optionen/01060300.xhp#hd_id3148451.14
msgid  "Expand references when new columns/rows are inserted"
msgstr "Verwijzingen bij invoegen van kolommen/rijen aan de randen uitbreiden"

#: source/text/shared/optionen/01060300.xhp#par_id3154684.15
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_LAYOUT_CB_EXPREF\"\>Specifies whether to expand references when inserting columns or rows adjacent to the reference range. This is only possible if the reference range, where the column or row is inserted, originally spanned at least two cells in the desired direction.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060300.xhp#par_id3153194.16
msgid  "\<emph\>Example:\</emph\> If the range A1:B1 is referenced in a formula and you insert a new column after column B, the reference is expanded to A1:C1. If the range A1:B1 is referenced and a new row is inserted under row 1, the reference is not expanded, since there is only a single cell in the vertical direction."
msgstr ""

#: source/text/shared/optionen/01060300.xhp#par_id3150449.17
msgid  "If you insert rows or columns in the middle of a reference area, the reference is always expanded."
msgstr "Als u rijen of kolommen midden in een verwijzingsgebied invoegt, wordt de verwijzing altijd vergroot."

#: source/text/shared/optionen/01060300.xhp#hd_id3151176.18
msgid  "Highlight selection in column/row headings"
msgstr "Selectie in kolom-/rijkoppen accentueren"

#: source/text/shared/optionen/01060300.xhp#par_id3145171.19
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_LAYOUT_CB_MARKHDR\"\>Specifies whether to highlight column and row headers in the selected columns or rows.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060300.xhp#hd_id3159252.25
msgid  "Use printer metrics for text formatting"
msgstr "Printermetrieken voor tekstopmaak gebruiken"

#: source/text/shared/optionen/01060300.xhp#par_id3145366.26
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_LAYOUT_CB_TEXTFMT\"\>Specifies that printer metrics are applied for printing and also for formatting the display on the screen.\</ahelp\> If this box is not checked, a printer independent layout will be used for screen display and printing."
msgstr ""

#: source/text/shared/optionen/01060300.xhp#hd_id3146146.27
msgid  "Show overwrite warning when pasting data"
msgstr "Waarschuwen voor overschrijven bij plakken van gegevens"

#: source/text/shared/optionen/01060300.xhp#par_id3150872.28
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_LAYOUT_CB_REPLWARN\"\>Specifies that, when you paste cells from the clipboard to a cell range that is not empty, a warning appears.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060400.xhp#tit
msgid  "Sort Lists"
msgstr "Sorteerlijsten"

#: source/text/shared/optionen/01060400.xhp#hd_id3145382.1
msgid  "\<link href=\"text/shared/optionen/01060400.xhp\" name=\"Sort Lists\"\>Sort Lists\</link\>"
msgstr ""

#: source/text/shared/optionen/01060400.xhp#par_id3153825.2
msgid  "\<ahelp hid=\"HID_SCPAGE_USERLISTS\"\>All user-defined lists are displayed in the\<emph\> Sort Lists \</emph\>dialog. You can also define and edit your own lists. Only text can be used as sort lists, no numbers.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060400.xhp#hd_id3149416.3
msgid  "Lists"
msgstr "Lijsten"

#: source/text/shared/optionen/01060400.xhp#par_id3150503.4
msgid  "\<ahelp hid=\"SC:LISTBOX:RID_SCPAGE_USERLISTS:LB_LISTS\"\>Displays all the available lists. These lists can be selected for editing.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060400.xhp#hd_id3147531.5
msgid  "Entries"
msgstr "Items"

#: source/text/shared/optionen/01060400.xhp#par_id3149669.6
msgid  "\<ahelp hid=\"SC:MULTILINEEDIT:RID_SCPAGE_USERLISTS:ED_ENTRIES\"\>Displays the content of the currently selected list. This content can be edited.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060400.xhp#hd_id3145069.7
msgid  "Copy list from"
msgstr "Lijst kopiëren van"

#: source/text/shared/optionen/01060400.xhp#par_id3149457.8
msgid  "\<ahelp hid=\"SC:EDIT:RID_SCPAGE_USERLISTS:ED_COPYFROM\"\>Defines the spreadsheet and the cells to copy, in order to include them in the \<emph\>Lists\</emph\> box. The currently selected range in the spreadsheet is the default.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060400.xhp#hd_id3151211.13
msgid  "Copy"
msgstr "Kopiëren"

#: source/text/shared/optionen/01060400.xhp#par_id3158409.14
msgid  "\<ahelp hid=\"SC:PUSHBUTTON:RID_SCPAGE_USERLISTS:BTN_COPY\"\>Copies the contents of the cells in the \<emph\>Copy list from\</emph\> box. If you select a reference to related rows and columns, the \<link href=\"text/shared/optionen/01060401.xhp\" name=\"Copy List\"\>\<emph\>Copy List\</emph\>\</link\> dialog appears after clicking the button. You can use this dialog to define if the reference is converted to sort lists by row or by column.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060400.xhp#hd_id3154684.9
msgid  "New/Discard"
msgstr "Nieuw/Verwerpen"

#: source/text/shared/optionen/01060400.xhp#par_id3153970.10
msgid  "\<ahelp hid=\"SC:PUSHBUTTON:RID_SCPAGE_USERLISTS:BTN_NEW\"\>Enters the contents of a new list into the \<emph\>Entries\</emph\> box.\</ahelp\> This button will change from \<emph\>New\</emph\> to \<emph\>Discard\</emph\>, which enables you to delete the new list."
msgstr ""

#: source/text/shared/optionen/01060400.xhp#hd_id3144760.11
msgid  "Add/Modify"
msgstr "Toevoegen/Wijzigen"

#: source/text/shared/optionen/01060400.xhp#par_id3145785.12
msgid  "\<ahelp hid=\"SC:PUSHBUTTON:RID_SCPAGE_USERLISTS:BTN_ADD\"\>Adds a new list into the \<emph\>Lists\</emph\> box.\</ahelp\> If you would like to edit this list in the \<emph\>Entries\</emph\> box, this button will change from \<emph\>Add\</emph\> to \<emph\>Modify\</emph\>, which enables you to include the newly modified list."
msgstr ""

#: source/text/shared/optionen/01060401.xhp#tit
msgid  "Copy List"
msgstr "Lijst kopiëren van"

#: source/text/shared/optionen/01060401.xhp#bm_id3153341
msgid  "\<bookmark_value\>sort lists; copying to in Calc\</bookmark_value\>"
msgstr ""

#: source/text/shared/optionen/01060401.xhp#hd_id3153341.1
msgid  "Copy List"
msgstr "Lijst kopiëren van"

#: source/text/shared/optionen/01060401.xhp#par_id3150772.2
msgid  "\<ahelp hid=\"SC_MODALDIALOG_RID_SCDLG_COLORROW\"\>Allows you to copy marked cells to a sort list.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060401.xhp#hd_id3147574.3
msgid  "List from"
msgstr "Lijst kopiëren"

#: source/text/shared/optionen/01060401.xhp#par_id3148563.4
msgid  "Choose between the options\<emph\> Rows\</emph\> and \<emph\>Columns\</emph\>. Cells without text will be ignored when copying."
msgstr ""

#: source/text/shared/optionen/01060401.xhp#hd_id3156343.5
msgid  "Rows"
msgstr "Rijen"

#: source/text/shared/optionen/01060401.xhp#par_id3148664.6
msgid  "\<ahelp hid=\"SC_RADIOBUTTON_RID_SCDLG_COLORROW_BTN_GROUP_ROWS\"\>Select the\<emph\> Rows \</emph\>option to summarize the contents of the selected rows in a list.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060401.xhp#hd_id3153525.7
msgid  "Columns"
msgstr "Kolommen"

#: source/text/shared/optionen/01060401.xhp#par_id3154216.8
msgid  "\<ahelp hid=\"SC_RADIOBUTTON_RID_SCDLG_COLORROW_BTN_GROUP_COLS\"\>Select the\<emph\> Columns \</emph\>option to summarize the contents of the selected columns in a list.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060500.xhp#tit
msgid  "Calculate"
msgstr "Berekenen"

#: source/text/shared/optionen/01060500.xhp#bm_id3149399
msgid  "\<bookmark_value\>references; iterative (Calc)\</bookmark_value\>         \<bookmark_value\>calculating;iterative references (Calc)\</bookmark_value\>         \<bookmark_value\>iterative references in spreadsheets\</bookmark_value\>         \<bookmark_value\>recursions in spreadsheets\</bookmark_value\>         \<bookmark_value\>dates; default (Calc)\</bookmark_value\>         \<bookmark_value\>dates; start 1900/01/01 (Calc)\</bookmark_value\>         \<bookmark_value\>dates; start 1904/01/01 (Calc)\</bookmark_value\>         \<bookmark_value\>case sensitivity;comparing cell contents (Calc)\</bookmark_value\>         \<bookmark_value\>decimal places displayed (Calc)\</bookmark_value\>         \<bookmark_value\>precision as shown (Calc)\</bookmark_value\>         \<bookmark_value\>values; rounded as shown (Calc)\</bookmark_value\>         \<bookmark_value\>rounding precision (Calc)\</bookmark_value\>         \<bookmark_value\>search criteria for database functions in cells\</bookmark_value\>         \<bookmark_value\>Excel; search criteria\</bookmark_value\>"
msgstr ""

#: source/text/shared/optionen/01060500.xhp#hd_id3145071.1
msgid  "\<link href=\"text/shared/optionen/01060500.xhp\" name=\"Calculate\"\>Calculate\</link\>"
msgstr ""

#: source/text/shared/optionen/01060500.xhp#par_id3147576.2
msgid  "\<ahelp hid=\".\"\>Defines the calculation settings for spreadsheets.\</ahelp\> Defines the behavior of spreadsheets with iterative references, the date settings, the number of decimal places, and if capitalization or lower cases are to be considered when searching within sheets."
msgstr ""

#: source/text/shared/optionen/01060500.xhp#hd_id3149399.3
msgid  "Iterative references"
msgstr "Terugkerende verwijzingen"

#: source/text/shared/optionen/01060500.xhp#par_id3155419.4
msgid  "In this section you can delimit the number of approximation steps carried out during iterative calculations. In addition, you can specify the degree of precision of the answer."
msgstr "Hier kunt u bepalen hoeveel benaderingsstappen er tijdens iteratieve berekeningen uitgevoerd worden. Daarnaast kunt u de mate van nauwkeurigheid van het antwoord specificeren."

#: source/text/shared/optionen/01060500.xhp#hd_id3154142.5
msgid  "Iterations"
msgstr "Iteraties"

#: source/text/shared/optionen/01060500.xhp#par_id3149795.6
msgid  "\<ahelp hid=\"SC:CHECKBOX:RID_SCPAGE_CALC:BTN_ITERATE\"\>Specifies whether formulas with iterative references (formulas that are continuously repeated until the problem is solved) are calculated after a specific number of repetitions.\</ahelp\> If the\<emph\> Iterations \</emph\>box is not marked, an iterative reference in the table will cause an error message."
msgstr ""

#: source/text/shared/optionen/01060500.xhp#par_id3148686.7
msgid  "         \<emph\>Example:\</emph\> calculating the cost of an item without the value-added tax."
msgstr ""

#: source/text/shared/optionen/01060500.xhp#par_id3156155.8
msgid  "Type the text 'Selling price' in cell A5, the text 'Net' in cell A6, and the text 'Value-added tax' in cell A7."
msgstr "Typ de tekst 'Verkoopprijs' in cel A5, de tekst 'Netto' in A6, en de tekst 'BTW' in A7."

#: source/text/shared/optionen/01060500.xhp#par_id3147530.61
msgid  "Now type a selling price (for example, 100) in cell B5. The net price should be shown in cell B6 and the value-added tax should be shown in cell B7. "
msgstr "Typ nu een verkoopprijs (bijv. 100) in cel B5. De nettoprijs moet in cel B6 en de BTW in B7 worden weergegeven. "

#: source/text/shared/optionen/01060500.xhp#par_id3153061.62
msgid  "You know that the value-added tax is calculated as 'net price times 15%' and that you arrive at the net price by deducting the value-added tax from the selling price. Type the formula \<item type=\"literal\"\>=B5-B7\</item\> in B6 to calculate the net price, and type the formula \<item type=\"literal\"\>=B6*0.15\</item\> in cell B7 to calculate the value-added tax."
msgstr ""

#: source/text/shared/optionen/01060500.xhp#par_id3154760.63
msgid  "Switch on the iterations to correctly calculate the formulas, otherwise a 'Circular reference' error message appears in the \<emph\>Status\</emph\> Bar."
msgstr ""

#: source/text/shared/optionen/01060500.xhp#par_id3154365.9
msgid  "A"
msgstr "A"

#: source/text/shared/optionen/01060500.xhp#par_id3145606.10
msgid  "B"
msgstr "B"

#: source/text/shared/optionen/01060500.xhp#par_id3149202.11
msgid  "5"
msgstr "5"

#: source/text/shared/optionen/01060500.xhp#par_id3151041.12
msgid  "Selling Price"
msgstr "Verkoopprijs"

#: source/text/shared/optionen/01060500.xhp#par_id3159149.13
msgid  "100"
msgstr "100"

#: source/text/shared/optionen/01060500.xhp#par_id3159254.14
msgid  "6"
msgstr "6"

#: source/text/shared/optionen/01060500.xhp#par_id3147317.15
msgid  "Net"
msgstr "Netto"

#: source/text/shared/optionen/01060500.xhp#par_id3147348.16
msgid  "=B5-B7"
msgstr "=B5-B7"

#: source/text/shared/optionen/01060500.xhp#par_id3154918.17
msgid  "7"
msgstr "7"

#: source/text/shared/optionen/01060500.xhp#par_id3153573.18
msgid  "Tax"
msgstr "BTW"

#: source/text/shared/optionen/01060500.xhp#par_id3154319.19
msgid  "=B6*0,15"
msgstr "=B6*0,15"

#: source/text/shared/optionen/01060500.xhp#hd_id3145750.22
msgid  "Steps"
msgstr "Stappen"

#: source/text/shared/optionen/01060500.xhp#par_id3152576.23
msgid  "\<ahelp hid=\"SC:NUMERICFIELD:RID_SCPAGE_CALC:ED_STEPS\"\>Sets the maximum number of iteration steps.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060500.xhp#hd_id3153728.24
msgid  "Minimum Change"
msgstr "Min. wijzigingswaarde"

#: source/text/shared/optionen/01060500.xhp#par_id3153139.25
msgid  "\<ahelp hid=\"SC:EDIT:RID_SCPAGE_CALC:ED_EPS\"\>Specifies the difference between two consecutive iteration step results. If the result of the iteration is lower than the minimum change value, then the iteration will stop.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060500.xhp#hd_id3147125.45
msgid  "Date"
msgstr "Datum"

#: source/text/shared/optionen/01060500.xhp#par_id3155416.46
msgid  "Select the start date for the internal conversion from days to numbers."
msgstr "Selecteer de begindatum voor de interne omzetting van dagen naar getallen."

#: source/text/shared/optionen/01060500.xhp#hd_id3147396.47
msgid  "12/30/1899 (default)"
msgstr "30.12.1899 (Standaard)"

#: source/text/shared/optionen/01060500.xhp#par_id3145646.48
msgid  "\<ahelp hid=\"SC:RADIOBUTTON:RID_SCPAGE_CALC:BTN_DATESTD\"\>Sets 12/30/1899 as day zero.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060500.xhp#hd_id3156283.49
msgid  "01/01/1900 (StarCalc 1.0)"
msgstr "01-01-1900 (StarCalc 1.0)"

#: source/text/shared/optionen/01060500.xhp#par_id3154018.50
msgid  "\<ahelp hid=\"SC:RADIOBUTTON:RID_SCPAGE_CALC:BTN_DATESC10\"\>Sets 1/1/1900 as day zero. Use this setting for StarCalc 1.0 spreadsheets containing date entries.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060500.xhp#hd_id3156181.51
msgid  "01/01/1904"
msgstr "01-01-1904"

#: source/text/shared/optionen/01060500.xhp#par_id3153948.52
msgid  "\<ahelp hid=\"SC:RADIOBUTTON:RID_SCPAGE_CALC:BTN_DATE1904\"\>Sets 1/1/1904 as day zero. Use this setting for spreadsheets that are imported in a foreign format.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060500.xhp#hd_id3153838.26
msgid  "Case sensitive"
msgstr "Hoofdletters/kleine letters"

#: source/text/shared/optionen/01060500.xhp#par_id3146793.27
msgid  "\<ahelp hid=\"SC:CHECKBOX:RID_SCPAGE_CALC:BTN_CASE\"\>Specifies whether to distinguish between upper and lower case in texts when comparing cell contents.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060500.xhp#par_id3153707.28
msgid  "         \<emph\>Example:\</emph\> Type the text 'Test' in cell A1; and the text 'test' in B1. Then type the formula \"=A1=B1\" in cell C1. If the \<emph\>Case sensitive\</emph\> box is marked, FALSE will appear in the cell; otherwise, TRUE will appear in the cell."
msgstr ""

#: source/text/shared/optionen/01060500.xhp#par_id3153965.29
msgid  "The EXACT text function is always case-sensitive, independent of the settings in this dialog."
msgstr "De tekstfunctie GELIJK is altijd hoofdlettergevoelig, onafhankelijk van de instellingen in dit dialoogvenster."

#: source/text/shared/optionen/01060500.xhp#hd_id3145231.53
msgid  "Decimal places"
msgstr "Cijfers achter de komma"

#: source/text/shared/optionen/01060500.xhp#par_id3149568.54
msgid  "\<ahelp hid=\"SC:NUMERICFIELD:RID_SCPAGE_CALC:ED_PREC\"\>Defines the number of decimals to be displayed for numbers with the \<emph\>Standard\</emph\> number format. The numbers are displayed as rounded numbers, but are not saved as rounded numbers.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060500.xhp#hd_id3145150.30
msgid  "Precision as shown"
msgstr "Precisie zoals weergegeven"

#: source/text/shared/optionen/01060500.xhp#par_id3150644.31
msgid  "\<ahelp hid=\"SC:CHECKBOX:RID_SCPAGE_CALC:BTN_CALC\"\>Specifies whether to make calculations using the rounded values displayed in the sheet. Charts will be shown with the displayed values. If the \<emph\>Precision as shown\</emph\> option is not marked, the displayed numbers are rounded, but they are calculated internally using the non-rounded number.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060500.xhp#hd_id3152581.32
msgid  "Search criteria = and <> must apply to whole cells"
msgstr "Zoekcriteria = en <> moeten op hele cellen toepasbaar zijn"

#: source/text/shared/optionen/01060500.xhp#par_id3149211.33
msgid  "\<ahelp hid=\"SC:CHECKBOX:RID_SCPAGE_CALC:BTN_MATCH\"\>Specifies that the search criteria you set for the Calc database functions must match the whole cell exactly. When the \<emph\>Search criteria = and <> must apply to whole cells\</emph\> box is marked, $[officename] Calc behaves exactly as MS Excel when searching cells in the database functions.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060500.xhp#par_id3148422.34
msgid  ".* in following position:"
msgstr ".* op de volgende positie:"

#: source/text/shared/optionen/01060500.xhp#par_id3156139.35
msgid  "Search result:"
msgstr "Zoekresultaat:"

#: source/text/shared/optionen/01060500.xhp#par_id3150979.36
msgid  "win"
msgstr "win"

#: source/text/shared/optionen/01060500.xhp#par_id3159239.37
msgid  "Finds win, but not win95, os2win, or upwind"
msgstr "Vindt win, maar niet win95, os2win of opwinden"

#: source/text/shared/optionen/01060500.xhp#par_id3153782.38
msgid  "win.*"
msgstr "win.*"

#: source/text/shared/optionen/01060500.xhp#par_id3151278.39
msgid  "Finds win and win95, but not os2win or upwind"
msgstr "Vindt win en win95, maar niet os2win of opwinden"

#: source/text/shared/optionen/01060500.xhp#par_id3155506.40
msgid  ".*win"
msgstr ".*win"

#: source/text/shared/optionen/01060500.xhp#par_id3150886.41
msgid  "Finds win and os2win, but not win95 or upwind"
msgstr "Vindt win en os2win, maar niet win95 of opwinden"

#: source/text/shared/optionen/01060500.xhp#par_id3147167.42
msgid  ".*win.*"
msgstr ".*win.*"

#: source/text/shared/optionen/01060500.xhp#par_id3152985.43
msgid  "Finds win, win95, os2win, and upwind"
msgstr "Vindt win, win95, os2win en opwinden"

#: source/text/shared/optionen/01060500.xhp#par_id3148814.44
msgid  "If \<emph\>Search criteria = and <> must apply to whole cells \</emph\>is not enabled, the \"win\" search pattern acts like \".*win.*\". The search pattern can be at any position within the cell when searching with the Calc database functions."
msgstr ""

#: source/text/shared/optionen/01060500.xhp#hd_id3156448.58
msgid  "Enable regular expressions in formulas"
msgstr "Reguliere expressies in formules mogelijk"

#: source/text/shared/optionen/01060500.xhp#par_id3155092.59
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_CALC_BTN_REGEX\"\>Specifies that regular expressions are enabled when searching and also for character string comparisons.\</ahelp\>\<switchinline select=\"appl\"\>\<caseinline select=\"CALC\"\> This relates to the \<link href=\"text/scalc/01/04060101.xhp\" name=\"database functions\"\>database functions\</link\>, and to VLOOKUP, HLOOKUP and SEARCH.\</caseinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/optionen/01060500.xhp#hd_id3156199.55
msgid  "Automatically find column and row labels"
msgstr "Rij- en kolomkoppen automatisch zoeken"

#: source/text/shared/optionen/01060500.xhp#par_id3153818.56
msgid  "\<ahelp hid=\"SC:CHECKBOX:RID_SCPAGE_CALC:BTN_LOOKUP\"\>Specifies that you can use the text in any cell as a label for the column below the text or the row to the right of the text. The text must consist of at least one word and must not contain any operators.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060500.xhp#par_id3151242.57
msgid  "         \<emph\>Example\</emph\>: Cell E5 contains the text \"Europe\". Below, in cell E6, is the value 100 and in cell E7 the value 200. If the \<emph\>Automatically find column and row labels\</emph\> box is marked, you can write the following formula in cell A1: =SUM(Europe)."
msgstr ""

#: source/text/shared/optionen/01060500.xhp#hd_id315634199
msgid  "Limit decimals for general number format"
msgstr "Begrens het aantal plaatsen achter de komma voor de algemene getalopmaak"

#: source/text/shared/optionen/01060500.xhp#par_id315343818
msgid  "\<ahelp hid=\".\"\>You can specify the maximum number of decimal places that are shown by default for cells with General number format. If not enabled, cells with General number format show as many decimal places as the column width allows.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060600.xhp#tit
msgid  "Changes"
msgstr "Wijzigingen"

#: source/text/shared/optionen/01060600.xhp#hd_id3159399.1
msgid  "\<link href=\"text/shared/optionen/01060600.xhp\" name=\"Changes\"\>Changes\</link\>"
msgstr ""

#: source/text/shared/optionen/01060600.xhp#par_id3155390.2
msgid  "\<ahelp hid=\"HID_SCPAGE_OPREDLINE\"\>The\<emph\> Changes \</emph\>dialog specifies various options for highlighting recorded changes in documents.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060600.xhp#par_id3156343.13
msgid  "To record changes to your work, choose \<link href=\"text/shared/01/02230000.xhp\" name=\"Edit - Changes\"\>\<emph\>Edit - Changes\</emph\>\</link\>."
msgstr ""

#: source/text/shared/optionen/01060600.xhp#hd_id3152812.3
msgid  "Color Definition for Changes"
msgstr "Kleur voor wijzigingen"

#: source/text/shared/optionen/01060600.xhp#par_id3150792.4
msgid  "Defines colors for recorded changes. If you select the \"By author\" entry, $[officename] will automatically set the color depending on the author who undertook the changes."
msgstr "Definieert kleuren voor vastgelegde wijzigingen. Als u het item Naar auteur selecteert, stelt $[officename] automatisch de kleur in afhankelijk van de auteur die de wijzigingen heeft aangebracht."

#: source/text/shared/optionen/01060600.xhp#hd_id3150400.5
msgid  "Changes"
msgstr "Wijzigingen"

#: source/text/shared/optionen/01060600.xhp#par_id3148451.6
msgid  "\<ahelp hid=\"SC:LISTBOX:RID_SCPAGE_OPREDLINE:CLB_CONTENT\"\>Specifies the color for changes of cell contents.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060600.xhp#hd_id3158410.7
msgid  "Deletions"
msgstr "Verwijderingen"

#: source/text/shared/optionen/01060600.xhp#par_id3147084.8
msgid  "\<ahelp hid=\"SC:LISTBOX:RID_SCPAGE_OPREDLINE:CLB_REMOVE\"\>Specifies the color to highlight deletions in a document.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060600.xhp#hd_id3154685.9
msgid  "Insertions"
msgstr "Invoegingen"

#: source/text/shared/optionen/01060600.xhp#par_id3151383.10
msgid  "\<ahelp hid=\"SC:LISTBOX:RID_SCPAGE_OPREDLINE:CLB_INSERT\"\>Specifies the color to highlight insertions in a document.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060600.xhp#hd_id3125863.11
msgid  "Moved entries"
msgstr "Verplaatsingen"

#: source/text/shared/optionen/01060600.xhp#par_id3159151.12
msgid  "\<ahelp hid=\"SC:LISTBOX:RID_SCPAGE_OPREDLINE:CLB_MOVE\"\>Specifies the color to highlight moved cell contents.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060700.xhp#tit
msgid  "Print"
msgstr "Afdrukken"

#: source/text/shared/optionen/01060700.xhp#hd_id3153311.1
msgid  "\<link href=\"text/shared/optionen/01060700.xhp\" name=\"Print\"\>Print\</link\>"
msgstr ""

#: source/text/shared/optionen/01060700.xhp#par_id3143267.2
msgid  "\<ahelp hid=\"\"\>Determines the printer settings for spreadsheets.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060700.xhp#par_id3155892.3
msgid  "\<emph\>\<switchinline select=\"sys\"\>\<caseinline select=\"MAC\"\>%PRODUCTNAME - Preferences\</caseinline\>\<defaultinline\>Tools - Options\</defaultinline\>\</switchinline\> - %PRODUCTNAME Calc - Print\</emph\> defines the settings for all spreadsheets. To define settings for the current document only, choose \<emph\>File - Print\</emph\>, then click the \<emph\>Options\</emph\> button."
msgstr ""

#: source/text/shared/optionen/01060700.xhp#hd_id3153542.4
msgid  "Pages"
msgstr "Pagina's"

#: source/text/shared/optionen/01060700.xhp#hd_id3156155.5
msgid  "Suppress output of empty pages"
msgstr "Weergave van lege pagina's onderdrukken"

#: source/text/shared/optionen/01060700.xhp#par_id3158430.6
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_PRINT_BTN_SKIPEMPTYPAGES\"\>Specifies that empty pages that have no cell contents or draw objects are not printed.\</ahelp\> Cell attributes such as borders or background colors are not considered cell contents. Empty pages are not counted for page numbering."
msgstr ""

#: source/text/shared/optionen/01060700.xhp#hd_id3150275.7
msgid  "Sheets"
msgstr "Bladen"

#: source/text/shared/optionen/01060700.xhp#hd_id3149784.8
msgid  "Print only selected sheets"
msgstr "Alleen geselecteerde bladen afdrukken"

#: source/text/shared/optionen/01060700.xhp#par_id3152349.9
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCPAGE_PRINT_BTN_SELECTEDSHEETS\"\>Specifies that only contents from selected sheets are printed, even if you specify a wider range in the \<emph\>File - Print\</emph\> dialog or in the \<emph\>Format - Print Ranges\</emph\> dialog. Contents from sheets that are not selected will not be printed.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01060700.xhp#par_id3153349.10
msgid  "To select multiple sheets, click on the sheet names on the bottom margin of the workspace while keeping the Ctrl key pressed."
msgstr "Klik op de werkbladnamen in de ondermarge van de werkruimte terwijl u de Ctrl-toets ingedrukt houdt, om meerdere werkbladen te selecteren."

#: source/text/shared/optionen/01070000.xhp#tit
msgid  "Presentation Options"
msgstr "Presentatie"

#: source/text/shared/optionen/01070000.xhp#hd_id3155805.1
msgid  "%PRODUCTNAME Impress Options"
msgstr "Opties voor %PRODUCTNAME Impress"

#: source/text/shared/optionen/01070000.xhp#par_id3146957.2
msgid  "\<variable id=\"allgemein\"\>\<ahelp hid=\".uno:SdEditOptions\"\>Defines various settings for newly created presentation documents, such as the contents to be displayed, the measurement unit used, if and how grid alignment is carried out.\</ahelp\>\</variable\>"
msgstr ""

#: source/text/shared/optionen/01070100.xhp#tit
msgid  "View"
msgstr "Lay-out"

#: source/text/shared/optionen/01070100.xhp#bm_id3147008
msgid  "\<bookmark_value\>rulers; visible in presentations\</bookmark_value\>\<bookmark_value\>moving; using guide lines in presentations\</bookmark_value\>\<bookmark_value\>guides; displaying when moving objects (Impress)\</bookmark_value\>\<bookmark_value\>control point display in presentations\</bookmark_value\>\<bookmark_value\>Bézier curves; control points in presentations\</bookmark_value\>"
msgstr ""

#: source/text/shared/optionen/01070100.xhp#hd_id3147000.1
msgid  "\<link href=\"text/shared/optionen/01070100.xhp\" name=\"View\"\>View\</link\>"
msgstr ""

#: source/text/shared/optionen/01070100.xhp#par_id3157898.2
msgid  "\<ahelp hid=\"HID_SD_OPTIONS_CONTENTS\"\>Specifies the available display modes.\</ahelp\> By selecting an alternative display, you can speed up the screen display while editing your presentation."
msgstr ""

#: source/text/shared/optionen/01070100.xhp#hd_id3148920.23
msgid  "Display"
msgstr "Weergeven"

#: source/text/shared/optionen/01070100.xhp#hd_id3155430.25
msgid  "Rulers visible"
msgstr "Linialen zichtbaar"

#: source/text/shared/optionen/01070100.xhp#par_id3147443.26
msgid  "\<ahelp hid=\"SD_CHECKBOX_TP_OPTIONS_CONTENTS_CBX_RULER\"\>Specifies whether to display the rulers at the top and to the left of the work area.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01070100.xhp#hd_id3145364.27
msgid  "Guides when moving"
msgstr "Hulplijnen bij verplaatsen"

#: source/text/shared/optionen/01070100.xhp#par_id3154147.28
msgid  "\<variable id=\"verschieb\"\>\<ahelp hid=\"SD_CHECKBOX_TP_OPTIONS_CONTENTS_CBX_HELPLINES\"\>Specifies whether to display guides when moving an object.\</ahelp\>\</variable\>"
msgstr ""

#: source/text/shared/optionen/01070100.xhp#par_id3150488.29
msgid  "\<variable id=\"vertext\"\>\<item type=\"productname\"\>%PRODUCTNAME\</item\> creates dotted guides that extend beyond the box containing the selected object and which cover the entire work area, helping you position the object. \</variable\>"
msgstr ""

#: source/text/shared/optionen/01070100.xhp#par_id3153365.30
msgid  "You also can use this function through the \<switchinline select=\"appl\"\>\<caseinline select=\"IMPRESS\"\>\<link href=\"text/shared/02/01171400.xhp\" name=\"Icon\"\>icon\</link\>\</caseinline\>\<caseinline select=\"DRAW\"\>\<link href=\"text/shared/02/01171400.xhp\" name=\"Icon\"\>icon\</link\>\</caseinline\>\<defaultinline\>icon\</defaultinline\>\</switchinline\> with the same name in the \<emph\>Options\</emph\> bar if a presentation or a drawing document is opened."
msgstr ""

#: source/text/shared/optionen/01070100.xhp#hd_id3155306.31
msgid  "All control points in Bézier editor"
msgstr "Alle besturingspunten in de Bézier-editor"

#: source/text/shared/optionen/01070100.xhp#par_id3153877.32
msgid  "\<ahelp hid=\"SD_CHECKBOX_TP_OPTIONS_CONTENTS_CBX_HANDLES_BEZIER\"\>Displays the control points of all Bézier points if you have previously selected a \<link href=\"text/shared/00/00000005.xhp#bezierobjekt\" name=\"Bézier curve\"\>Bézier curve\</link\>. If the \<emph\>All control points in Bézier\</emph\> editor option is not marked, only the control points of the selected Bézier points will be visible.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01070100.xhp#hd_id3149418.33
msgid  "Contour of each individual object"
msgstr "Contour van elk object"

#: source/text/shared/optionen/01070100.xhp#par_id3156284.34
msgid  "\<ahelp hid=\"SD_CHECKBOX_TP_OPTIONS_CONTENTS_CBX_MOVE_OUTLINE\"\>\<item type=\"productname\"\>%PRODUCTNAME\</item\> displays the contour line of each individual object when moving this object.\</ahelp\> The \<emph\>Contour of each individual object \</emph\>option enables you to see if single objects conflict with other objects in the target position. If you do not mark the \<emph\>Contour of each individual object \</emph\>option, \<item type=\"productname\"\>%PRODUCTNAME\</item\> only displays a square contour that includes all selected objects."
msgstr ""

#: source/text/shared/optionen/01070300.xhp#tit
msgid  "Grid"
msgstr "Raster"

#: source/text/shared/optionen/01070300.xhp#bm_id3163802
msgid  "\<bookmark_value\>snapping in presentations and drawings\</bookmark_value\>         \<bookmark_value\>points;reducing editing points when snapping (Impress/Draw)\</bookmark_value\>"
msgstr ""

#: source/text/shared/optionen/01070300.xhp#hd_id3147571.1
msgid  "\<link href=\"text/shared/optionen/01070300.xhp\" name=\"Grid\"\>Grid\</link\>"
msgstr ""

#: source/text/shared/optionen/01070300.xhp#par_id3152801.2
msgid  "\<ahelp hid=\"HID_SD_OPTIONS_SNAP\"\>Defines the grid settings for creating and moving objects.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01070300.xhp#par_id3149177.3
msgid  "If you have activated the snap grid but wish to move or create individual objects without snap positions, keep the Shift key pressed to deactivate this function for as long as needed."
msgstr "Als u het vangraster hebt geactiveerd maar individuele objecten wilt verplaatsen of maken zonder vangposities, houdt u de Shift-toets ingedrukt om deze functie te deactiveren zolang dat nodig is."

#: source/text/shared/optionen/01070300.xhp#hd_id3156346.4
msgid  "Snap"
msgstr "Vangen"

#: source/text/shared/optionen/01070300.xhp#hd_id3163802.8
msgid  "To snap lines"
msgstr "Aan vanglijnen"

#: source/text/shared/optionen/01070300.xhp#par_id3149516.9
msgid  "\<variable id=\"anlinie\"\>\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_GRID_CBX_SNAP_HELPLINES\"\>Snaps the edge of a dragged object to the nearest snap line when you release the mouse.\</ahelp\>\</variable\>"
msgstr ""

#: source/text/shared/optionen/01070300.xhp#par_id3154142.37
msgid  "You can also define this setting by using the \<switchinline select=\"appl\"\>\<caseinline select=\"IMPRESS\"\>\<link href=\"text/simpress/02/13140000.xhp\" name=\"Snap to Guides\"\>\<emph\>Snap to Guides\</emph\>\</link\>\</caseinline\>\<caseinline select=\"DRAW\"\>\<link href=\"text/simpress/02/13140000.xhp\" name=\"Snap to Guides\"\>\<emph\>Snap to Guides\</emph\>\</link\>\</caseinline\>\<defaultinline\>\<emph\>Snap to Guides\</emph\>\</defaultinline\>\</switchinline\> icon, which is available in the \<emph\>Options\</emph\> bar in a presentation or drawing document."
msgstr ""

#: source/text/shared/optionen/01070300.xhp#hd_id3154306.31
msgid  "To the page margins"
msgstr "Aan paginamarges"

#: source/text/shared/optionen/01070300.xhp#par_id3156024.11
msgid  "\<variable id=\"seitenrand\"\>\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_GRID_CBX_SNAP_BORDER\"\>Specifies whether to align the contour of the graphic object to the nearest page margin.\</ahelp\>\</variable\>"
msgstr ""

#: source/text/shared/optionen/01070300.xhp#par_id3149670.33
msgid  "\<variable id=\"seittext\"\>The cursor or a contour line of the graphics object must be in the snap range.\</variable\>"
msgstr ""

#: source/text/shared/optionen/01070300.xhp#par_id3148947.38
msgid  "In a presentation or drawing document, this function can also be accessed with the \<switchinline select=\"appl\"\>\<caseinline select=\"IMPRESS\"\>\<link href=\"text/simpress/02/13150000.xhp\" name=\"Snap to Page Margins\"\>\<emph\>Snap to Page Margins\</emph\>\</link\>\</caseinline\>\<caseinline select=\"DRAW\"\>\<link href=\"text/simpress/02/13150000.xhp\" name=\"Snap to Page Margins\"\>\<emph\>Snap to Page Margins\</emph\>\</link\>\</caseinline\>\<defaultinline\>\<emph\>Snap to Page Margins\</emph\>\</defaultinline\>\</switchinline\> icon in the \<emph\>Options\</emph\> bar."
msgstr ""

#: source/text/shared/optionen/01070300.xhp#hd_id3154365.12
msgid  "To object frame"
msgstr "Aan objectframe"

#: source/text/shared/optionen/01070300.xhp#par_id3148674.13
msgid  "\<variable id=\"rahmen\"\>\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_GRID_CBX_SNAP_FRAME\"\>Specifies whether to align the contour of the graphic object to the border of the nearest graphic object.\</ahelp\>\</variable\>"
msgstr ""

#: source/text/shared/optionen/01070300.xhp#par_id3147228.34
msgid  "\<variable id=\"rahmtext\"\>The cursor or a contour line of the graphics object must be in the snap range.\</variable\>"
msgstr ""

#: source/text/shared/optionen/01070300.xhp#par_id3148922.39
msgid  "In a presentation or drawing document, this function can also be accessed with the \<switchinline select=\"appl\"\>\<caseinline select=\"IMPRESS\"\>\<link href=\"text/simpress/02/13160000.xhp\" name=\"Snap to Object Border\"\>\<emph\>Snap to Object Border\</emph\>\</link\>\</caseinline\>\<caseinline select=\"DRAW\"\>\<link href=\"text/simpress/02/13160000.xhp\" name=\"Snap to Object Border\"\>\<emph\>Snap to Object Border\</emph\>\</link\>\</caseinline\>\<defaultinline\>\<emph\>Snap to Object Border\</emph\>\</defaultinline\>\</switchinline\> icon in the \<emph\>Options\</emph\> bar."
msgstr ""

#: source/text/shared/optionen/01070300.xhp#hd_id3155431.14
msgid  "To object points"
msgstr "Aan objectpunten"

#: source/text/shared/optionen/01070300.xhp#par_id3145271.15
msgid  "\<variable id=\"opunkte\"\>\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_GRID_CBX_SNAP_POINTS\"\>Specifies whether to align the contour of the graphic object to the points of the nearest graphic object.\</ahelp\>\</variable\>"
msgstr ""

#: source/text/shared/optionen/01070300.xhp#par_id3149483.35
msgid  "\<variable id=\"opunktetext\"\>This only applies if the cursor or a contour line of the graphics object is in the snap range.\</variable\>"
msgstr ""

#: source/text/shared/optionen/01070300.xhp#par_id3146146.40
msgid  "In a presentation or drawing document, this function can also be accessed with the \<switchinline select=\"appl\"\>\<caseinline select=\"IMPRESS\"\>\<link href=\"text/simpress/02/13170000.xhp\" name=\"Snap to Object Points\"\>\<emph\>Snap to Object Points\</emph\>\</link\>\</caseinline\>\<caseinline select=\"DRAW\"\>\<link href=\"text/simpress/02/13170000.xhp\" name=\"Snap to Object Points\"\>\<emph\>Snap to Object Points\</emph\>\</link\>\</caseinline\>\<defaultinline\>\<emph\>Snap to Object Points\</emph\>\</defaultinline\>\</switchinline\> icon in the \<emph\>Options\</emph\> bar."
msgstr ""

#: source/text/shared/optionen/01070300.xhp#hd_id3148645.16
msgid  "Snap range"
msgstr "Vangbereik"

#: source/text/shared/optionen/01070300.xhp#par_id3154145.17
msgid  "\<ahelp hid=\"SVX_METRICFIELD_RID_SVXPAGE_GRID_MTR_FLD_SNAP_AREA\"\>Defines the snap distance between the mouse pointer and the object contour. $[officename] Impress snaps to a snap point if the mouse pointer is nearer than the distance selected in the \<emph\>Snap range\</emph\> control.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01070300.xhp#hd_id3150872.19
msgid  "Snap position"
msgstr "Vangpositie"

#: source/text/shared/optionen/01070300.xhp#hd_id3154639.21
msgid  "When creating or moving objects"
msgstr "Tijdens het maken en verplaatsen van objecten"

#: source/text/shared/optionen/01070300.xhp#par_id3150417.22
msgid  "\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_GRID_CBX_ORTHO\"\>Specifies that graphic objects are restricted vertically, horizontally or diagonally (45°) when creating or moving them.\</ahelp\> You can temporarily deactivate this setting by pressing the Shift key."
msgstr ""

#: source/text/shared/optionen/01070300.xhp#hd_id3159345.23
msgid  "Extend edges"
msgstr "Langere zijden"

#: source/text/shared/optionen/01070300.xhp#par_id3154942.24
msgid  "\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_GRID_CBX_BIGORTHO\"\>Specifies that a square is created based on the longer side of a rectangle when the Shift key is pressed before you release the mouse button. This also applies to an ellipse (a circle will be created based on the longest diameter of the ellipse). When the\<emph\> Extend edges \</emph\>box is not marked, a square or a circle will be created based on the shorter side or diameter.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01070300.xhp#hd_id3149413.25
msgid  "When rotating"
msgstr "Tijdens het draaien"

#: source/text/shared/optionen/01070300.xhp#par_id3150717.26
msgid  "\<ahelp hid=\"SVX_METRICFIELD_RID_SVXPAGE_GRID_MTR_FLD_ANGLE\"\>Specifies that graphic objects can only be rotated within the rotation angle that you selected in the \<emph\>When rotating\</emph\> control.\</ahelp\> If you want to rotate an object outside the defined angle, press the Shift key when rotating. Release the key when the desired rotation angle is reached."
msgstr ""

#: source/text/shared/optionen/01070300.xhp#hd_id3154163.27
msgid  "Point reduction"
msgstr "Punten reduceren"

#: source/text/shared/optionen/01070300.xhp#par_id3156275.28
msgid  "\<ahelp hid=\"SVX_METRICFIELD_RID_SVXPAGE_GRID_MTR_FLD_BEZ_ANGLE\"\>Defines the angle for point reduction.\</ahelp\> When working with polygons, you might find it useful to reduce their editing points."
msgstr ""

#: source/text/shared/optionen/01070400.xhp#tit
msgid  "Print"
msgstr "Afdrukken"

#: source/text/shared/optionen/01070400.xhp#bm_id3155450
msgid  "\<bookmark_value\>printing; drawings defaults\</bookmark_value\>\<bookmark_value\>drawings; printing defaults\</bookmark_value\>\<bookmark_value\>pages;printing page names in presentations\</bookmark_value\>\<bookmark_value\>printing; dates in presentations\</bookmark_value\>\<bookmark_value\>dates; printing in presentations\</bookmark_value\>\<bookmark_value\>times; inserting when printing presentations\</bookmark_value\>\<bookmark_value\>printing; hidden pages of presentations\</bookmark_value\>\<bookmark_value\>hidden pages; printing in presentations\</bookmark_value\>\<bookmark_value\>printing; without scaling in presentations\</bookmark_value\>\<bookmark_value\>scaling; when printing presentations\</bookmark_value\>\<bookmark_value\>printing; fitting to pages in presentations\</bookmark_value\>\<bookmark_value\>fitting to pages; print settings in presentations\</bookmark_value\>\<bookmark_value\>printing; tiling pages in presentations\</bookmark_value\>"
msgstr ""

#: source/text/shared/optionen/01070400.xhp#hd_id3155419.1
msgid  "\<link href=\"text/shared/optionen/01070400.xhp\" name=\"Print\"\>Print\</link\>"
msgstr ""

#: source/text/shared/optionen/01070400.xhp#par_id3155341.2
msgid  "\<ahelp hid=\"HID_SD_PRINT_OPTIONS\"\>Specifies print settings within a drawing or presentation document.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01070400.xhp#hd_id3150486.13
msgid  "Print"
msgstr "Afdrukken"

#: source/text/shared/optionen/01070400.xhp#par_id3153092.14
msgid  "Defines additional elements to be printed on the page margin."
msgstr "Definieert aanvullende elementen die moeten worden afgedrukt in de paginamarge."

#: source/text/shared/optionen/01070400.xhp#hd_id3150104.15
msgid  "Page name"
msgstr "Paginanaam"

#: source/text/shared/optionen/01070400.xhp#par_id3154146.16
msgid  "\<ahelp hid=\"SD:CHECKBOX:TP_PRINT_OPTIONS:CBX_PAGENAME\"\>Specifies whether to print the page name.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01070400.xhp#hd_id3147214.17
msgid  "Date"
msgstr "Datum"

#: source/text/shared/optionen/01070400.xhp#par_id3152938.18
msgid  "\<ahelp hid=\"SD:CHECKBOX:TP_PRINT_OPTIONS:CBX_DATE\"\>Specifies whether to print the current date.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01070400.xhp#hd_id3149301.19
msgid  "Time"
msgstr "Tijd"

#: source/text/shared/optionen/01070400.xhp#par_id3156285.20
msgid  "\<ahelp hid=\"SD:CHECKBOX:TP_PRINT_OPTIONS:CBX_TIME\"\>Specifies whether to print the current time.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01070400.xhp#hd_id3154097.23
msgid  "Hidden pages"
msgstr "Verborgen pagina's"

#: source/text/shared/optionen/01070400.xhp#par_id3154792.24
msgid  "\<ahelp hid=\"SD:CHECKBOX:TP_PRINT_OPTIONS:CBX_HIDDEN_PAGES\"\>Specifies whether to print the pages that are currently hidden from the presentation.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01070400.xhp#hd_id3154686.43
msgid  "Quality"
msgstr "Kwaliteit"

#: source/text/shared/optionen/01070400.xhp#par_id3147229.54
msgid  "See also \<embedvar href=\"text/shared/guide/print_blackwhite.xhp#print_blackwhite\"/\>."
msgstr ""

#: source/text/shared/optionen/01070400.xhp#hd_id3159149.45
msgid  "Default"
msgstr "Standaard"

#: source/text/shared/optionen/01070400.xhp#par_id3145608.46
msgid  "\<ahelp hid=\"SD:RADIOBUTTON:TP_PRINT_OPTIONS:RBT_COLOR\"\>Specifies that you want to print in original colors.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01070400.xhp#hd_id3155131.50
msgid  "Grayscale"
msgstr "Grijswaarden"

#: source/text/shared/optionen/01070400.xhp#par_id3149260.51
msgid  "\<ahelp hid=\"SD_RADIOBUTTON_TP_PRINT_OPTIONS_RBT_GRAYSCALE\"\>Specifies that you want to print colors as grayscale.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01070400.xhp#hd_id3146975.52
msgid  "Black & white"
msgstr "Zwart-wit"

#: source/text/shared/optionen/01070400.xhp#par_id3159154.53
msgid  "\<ahelp hid=\"SD_RADIOBUTTON_TP_PRINT_OPTIONS_RBT_BLACKWHITE\"\>Specifies that you want to print the document in black and white.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01070400.xhp#hd_id3154015.21
msgid  "Page options"
msgstr "Paginaopties"

#: source/text/shared/optionen/01070400.xhp#par_id3154512.22
msgid  "Define additional options for printing the pages."
msgstr "Definieer extra opties voor het afdrukken van de pagina's."

#: source/text/shared/optionen/01070400.xhp#hd_id3151207.25
msgid  "Default"
msgstr "Standaard"

#: source/text/shared/optionen/01070400.xhp#par_id3153836.26
msgid  "\<ahelp hid=\"SD:RADIOBUTTON:TP_PRINT_OPTIONS:RBT_DEFAULT\"\>Specifies that you do not want to further scale pages when printing.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01070400.xhp#hd_id3153710.27
msgid  "Fit to page"
msgstr "Aanpassen aan pagina"

#: source/text/shared/optionen/01070400.xhp#par_id3148405.28
msgid  "\<ahelp hid=\"SD:RADIOBUTTON:TP_PRINT_OPTIONS:RBT_PAGESIZE\"\>Specifies whether to scale down objects that are beyond the margins of the current printer, so that they fit on the paper in the printer.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01070400.xhp#hd_id3155764.29
msgid  "Tile pages"
msgstr "Pagina's naast elkaar"

#: source/text/shared/optionen/01070400.xhp#par_id3154255.30
msgid  "\<ahelp hid=\"SD:RADIOBUTTON:TP_PRINT_OPTIONS:RBT_PAGETILE\"\>Specifies that pages are to be printed in tiled format. If the pages or slides are smaller than the paper, several pages or slides will be printed on one page of paper.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01070400.xhp#hd_id3150388.31
msgid  "Brochure"
msgstr "Brochure"

#: source/text/shared/optionen/01070400.xhp#par_id3147322.32
msgid  "\<ahelp hid=\"SD:RADIOBUTTON:TP_PRINT_OPTIONS:RBT_BOOKLET\"\>Select the\<emph\> Brochure \</emph\>option to print the document in brochure format.\</ahelp\> You can also decide if you want to print the front, the back or both sides of the brochure."
msgstr ""

#: source/text/shared/optionen/01070400.xhp#hd_id3145790.35
msgid  "Front"
msgstr "Voorkant"

#: source/text/shared/optionen/01070400.xhp#par_id3145766.36
msgid  "\<ahelp hid=\"SD:CHECKBOX:TP_PRINT_OPTIONS:CBX_FRONT\"\>Select\<emph\> Front \</emph\>to print the front of a brochure.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01070400.xhp#hd_id3145760.37
msgid  "Back"
msgstr "Achterkant"

#: source/text/shared/optionen/01070400.xhp#par_id3154118.38
msgid  "\<ahelp hid=\"SD:CHECKBOX:TP_PRINT_OPTIONS:CBX_BACK\"\>Select \<emph\>Back\</emph\> to print the back of a brochure.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01070400.xhp#hd_id3153704.41
msgid  "Paper tray from printer settings"
msgstr "Papierlade uit printerinstellingen"

#: source/text/shared/optionen/01070400.xhp#par_id3150380.42
msgid  "\<ahelp hid=\"SD:CHECKBOX:TP_PRINT_OPTIONS:CBX_PAPERBIN\"\>Determines that the paper tray to be used is the one defined in the printer setup.\</ahelp\>"
msgstr ""

