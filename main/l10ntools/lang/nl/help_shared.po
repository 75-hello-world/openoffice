#: source/text/shared/guide/numbering_stop.xhp#par_id0202200910470118
msgid  "Bullets and Numbering of paragraphs is supported only in Writer, Impress and Draw."
msgstr "Opsommingstekens en nummering van alinea's wordt alleen ondersteund in Writer, Impress en Draw."

#: source/text/shared/guide/numbering_stop.xhp#par_id3154288
msgid  "\<image id=\"img_id3153527\" src=\"res/commandimagelist/sc_removebullets.png\" width=\"0.222in\" height=\"0.222in\"\>\<alt id=\"alt_id3153527\"\>Icon\</alt\>\</image\>"
