msgid  "Always"
msgstr "Altijd"

#: source/text/shared/optionen/01040900.xhp#par_id3155628.11
msgid  "\<ahelp hid=\"SW_RADIOBUTTON_TP_OPTLOAD_PAGE_RB_ALWAYS\"\>Always updates links while loading a document.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01040900.xhp#hd_id3155449.5
msgid  "On request"
msgstr "Op aanvraag"

#: source/text/shared/optionen/01040900.xhp#par_id3153252.12
msgid  "\<ahelp hid=\"SW_RADIOBUTTON_TP_OPTLOAD_PAGE_RB_REQUEST\"\>Updates links only on request while loading a document.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01040900.xhp#hd_id3151384.6
msgid  "Never"
msgstr "Nooit"

#: source/text/shared/optionen/01040900.xhp#par_id3148946.13
msgid  "\<ahelp hid=\"SW_RADIOBUTTON_TP_OPTLOAD_PAGE_RB_NEVER\"\>Links are never updated while loading a document.\</ahelp\>"
