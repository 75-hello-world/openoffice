msgstr "Print \"De vierkantswortel van \";iInvoera;\" is\";iInvoerc"

#: source/text/sbasic/shared/03090301.xhp#par_id3155764.30
msgid  "iInputc=iInputb"
msgstr "iInvoerc=iInvoerb"

#: source/text/sbasic/shared/03090301.xhp#par_id3152960.31
msgid  "GoSub SquareRoot"
msgstr "GoSub VierkantsWortel"

#: source/text/sbasic/shared/03090301.xhp#par_id3147340.32
msgid  "Print \"The square root of\";iInputb;\" is\";iInputc"
msgstr "Print \"De vierkantswortel van\";iInvoerb;\" is\";iInvoerc"

#: source/text/sbasic/shared/03090301.xhp#par_id3166450.33
msgid  "Exit Sub"
msgstr "Exit Sub"

#: source/text/sbasic/shared/03090301.xhp#par_id3155176.34
msgid  "SquareRoot:"
msgstr "VierkantsWortel:"

#: source/text/sbasic/shared/03090301.xhp#par_id3149257.35
