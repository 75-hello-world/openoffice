msgstr "Sub Tonen(SamenvoegStr, DeArray)"

#: source/text/sbasic/shared/03120314.xhp#par_id3153948.37
msgid  " l = LBound(TheArray)"
msgstr " l = LBound(DeArray)"

#: source/text/sbasic/shared/03120314.xhp#par_id3146969.38
msgid  " u = UBound(TheArray)"
msgstr " u = UBound(DeArray)"

#: source/text/sbasic/shared/03120314.xhp#par_id3150752.39
msgid  " total$ = \"=============================\" + Chr$(13) + JoinStr + Chr$(13) + Chr$(13)"
msgstr " total$ = \"=============================\" + Chr$(13) + SamenvoegStr + Chr$(13) + Chr$(13)"

#: source/text/sbasic/shared/03120314.xhp#par_id3148916.40
msgid  " For i = l To u"
msgstr " For i = l To u"

#: source/text/sbasic/shared/03120314.xhp#par_id3154754.41
msgid  " total$ = total$ + TheArray(i) + Str(Len(TheArray(i))) + Chr$(13)"
msgstr " totaal$ = totaal$ + DeArray(i) + Str(Len(DeArray(i))) + Chr$(13)"

#: source/text/sbasic/shared/03120314.xhp#par_id3156054.42
msgid  " Next i"
msgstr " Next i"

#: source/text/sbasic/shared/03120314.xhp#par_id3147338.43
msgid  " MsgBox total$"
msgstr " MsgBox totaal$"

#: source/text/sbasic/shared/03120314.xhp#par_id3155960.44
msgid  "End Sub"
msgstr "End Sub"

