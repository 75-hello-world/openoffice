msgid  "REM Replace asterisks with spaces"
msgstr "Rem Vervangt asterisks door spaties"

#: source/text/sbasic/shared/03120304.xhp#par_id3146119.20
msgid  "RSet sVar = sExpr"
msgstr "RSet sVar = sExpr"

#: source/text/sbasic/shared/03120304.xhp#par_id3153365.21
msgid  "Print \">\"; sVar; \"<\""
msgstr "Print \">\"; sVar; \"<\""

#: source/text/sbasic/shared/03120304.xhp#par_id3149260.23
msgid  "sVar = String(5,\"*\")"
msgstr "sVar = String(5,\"*\")"

#: source/text/sbasic/shared/03120304.xhp#par_id3147436.24
msgid  "sExpr = \"123457896\""
msgstr "sExpr = \"123457896\""

#: source/text/sbasic/shared/03120304.xhp#par_id3146923.25
msgid  "RSet sVar = sExpr"
msgstr "RSet sVar = sExpr"

#: source/text/sbasic/shared/03120304.xhp#par_id3151114.26
msgid  "Print \">\"; sVar; \"<\""
msgstr "Print \">\"; sVar; \"<\""

#: source/text/sbasic/shared/03120304.xhp#par_id3155855.28
msgid  "sVar = String(40,\"*\")"
msgstr "sVar = String(40,\"*\")"

#: source/text/sbasic/shared/03120304.xhp#par_id3145253.29
msgid  "sExpr = \"SBX\""
msgstr "sExpr = \"SBX\""

#: source/text/sbasic/shared/03120304.xhp#par_id3151075.30
msgid  "REM Left-align \"SBX\" within the 40-character reference string"
msgstr "REM Lijnt \"SBX\" binnen de 40-teken referentietekenreeks links uit"

#: source/text/sbasic/shared/03120304.xhp#par_id3147126.31
msgid  "LSet sVar = sExpr"
msgstr "LSet sVar = sExpr"

#: source/text/sbasic/shared/03120304.xhp#par_id3154792.32
msgid  "Print \">\"; sVar; \"<\""
msgstr "Print \">\"; sVar; \"<\""

#: source/text/sbasic/shared/03120304.xhp#par_id3154942.34
msgid  "sVar = String(5,\"*\")"
msgstr "sVar = String(5,\"*\")"

#: source/text/sbasic/shared/03120304.xhp#par_id3155603.35
msgid  "sExpr = \"123456789\""
msgstr "sExpr = \"123456789\""

#: source/text/sbasic/shared/03120304.xhp#par_id3150716.36
msgid  "LSet sVar = sExpr"
msgstr "LSet sVar = sExpr"

#: source/text/sbasic/shared/03120304.xhp#par_id3146969.37
msgid  "Print \">\"; sVar; \"<\""
msgstr "Print \">\"; sVar; \"<\""

#: source/text/sbasic/shared/03120304.xhp#par_id3150749.38
msgid  "End Sub"
msgstr "End Sub"

