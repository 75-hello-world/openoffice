msgstr "Green (Kleur As Long)"

#: source/text/sbasic/shared/03010302.xhp#hd_id3154124.5
msgid  "Return value:"
msgstr "Teruggegeven waarde:"

#: source/text/sbasic/shared/03010302.xhp#par_id3153194.6
msgid  "Integer"
msgstr "Integer"

#: source/text/sbasic/shared/03010302.xhp#hd_id3154909.7
msgid  "Parameter:"
msgstr "Parameter:"

#: source/text/sbasic/shared/03010302.xhp#par_id3153770.8
