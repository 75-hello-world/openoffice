#: source/text/sbasic/shared/03010102.xhp#par_id3145171.2
msgid  "Displays a dialog box containing a message and returns a value."
msgstr "Toont een dialoogvenster met een bericht en retourneert een waarde."

#: source/text/sbasic/shared/03010102.xhp#hd_id3156281.3
msgid  "Syntax:"
msgstr "Syntaxis:"

#: source/text/sbasic/shared/03010102.xhp#par_id3154685.4
msgid  "MsgBox (Text As String [,Type As Integer [,Dialogtitle As String]])"
msgstr "MsgBox (Tekst As String [,Type As Integer [,Dialoogtitel As String]])"

#: source/text/sbasic/shared/03010102.xhp#hd_id3153771.5
msgid  "Return value:"
msgstr "Retourwaarde:"

#: source/text/sbasic/shared/03010102.xhp#par_id3146985.6
msgid  "Integer"
msgstr "Integer"

#: source/text/sbasic/shared/03010102.xhp#hd_id3153363.7
msgid  "Parameter:"
msgstr "Parameter:"

#: source/text/sbasic/shared/03010102.xhp#par_id3153727.8
msgid  "\<emph\>Text\</emph\>: String expression displayed as a message in the dialog box. Line breaks can be inserted with Chr$(13)."
msgstr ""

#: source/text/sbasic/shared/03010102.xhp#par_id3147317.9
msgid  "\<emph\>DialogTitle\</emph\>: String expression displayed in the title bar of the dialog. If omitted, the name of the respective application is displayed."
msgstr ""

#: source/text/sbasic/shared/03010102.xhp#par_id3153954.10
msgid  "\<emph\>Type\</emph\>: Any integer expression that specifies the dialog type and defines the number and type of buttons or icons displayed. \<emph\>Type\</emph\> represents a combination of bit patterns (dialog elements defined by adding the respective values):"
msgstr ""

#: source/text/sbasic/shared/03010102.xhp#par_id3154319.11
msgid  "\<emph\>Values\</emph\>"
msgstr ""

#: source/text/sbasic/shared/03010102.xhp#par_id3147397.12
msgid  "0 : Display OK button only."
msgstr "0 : Alleen knop OK weergeven."

#: source/text/sbasic/shared/03010102.xhp#par_id3145646.13
msgid  "1 : Display OK and Cancel buttons."
msgstr "1 : Knoppen OK en Annuleren weergeven."

#: source/text/sbasic/shared/03010102.xhp#par_id3149410.14
msgid  "2 : Display Abort, Retry, and Ignore buttons."
msgstr "2 : De knoppen Afbreken, Opnieuw proberen en Negeren weergeven."

#: source/text/sbasic/shared/03010102.xhp#par_id3151075.15
msgid  "3 : Display Yes, No, and Cancel buttons."
msgstr "3 : Knoppen Ja, Nee en Annuleren weergeven."

#: source/text/sbasic/shared/03010102.xhp#par_id3153878.16
msgid  "4 : Display Yes and No buttons."
msgstr "4 : Knoppen Ja en Nee weergeven."

#: source/text/sbasic/shared/03010102.xhp#par_id3155601.17
msgid  "5 : Display Retry and Cancel buttons."
msgstr "5 : Knoppen Opnieuw proberen en Annuleren weergeven."

#: source/text/sbasic/shared/03010102.xhp#par_id3150716.18
msgid  "16 : Add the Stop icon to the dialog."
msgstr "16 : Voeg het pictogram Stoppen aan het dialoogvenster toe."

#: source/text/sbasic/shared/03010102.xhp#par_id3153837.19
msgid  "32 : Add the Question icon to the dialog."
msgstr "32 : Voeg het pictogram Vraagteken aan het dialoogvenster toe."

#: source/text/sbasic/shared/03010102.xhp#par_id3150751.20
msgid  "48 : Add the Exclamation Point icon to the dialog."
msgstr "48 : Voeg het pictogram Uitroepteken aan het dialoogvenster toe."

#: source/text/sbasic/shared/03010102.xhp#par_id3146915.21
msgid  "64 : Add the Information icon to the dialog."
msgstr "64 : Voeg het pictogram Informatie aan het dialoogvenster toe."

#: source/text/sbasic/shared/03010102.xhp#par_id3145640.22
msgid  "128 : First button in the dialog as default button."
msgstr "128 : Eerste knop in het dialoogvenster als standaardknop."

#: source/text/sbasic/shared/03010102.xhp#par_id3153765.23
msgid  "256 : Second button in the dialog as default button."
msgstr "256 : Tweede knop in het dialoogvenster als standaardknop."

#: source/text/sbasic/shared/03010102.xhp#par_id3153715.24
msgid  "512 : Third button in the dialog as default button."
msgstr "512 : Derde knop in het dialoogvenster als standaardknop."

#: source/text/sbasic/shared/03010102.xhp#par_id3159267.25
msgid  "\<emph\>Return value:\</emph\>"
msgstr ""

#: source/text/sbasic/shared/03010102.xhp#par_id3145230.26
msgid  "1 : OK"
msgstr "1 : OK"

#: source/text/sbasic/shared/03010102.xhp#par_id3149567.27
msgid  "2 : Cancel"
msgstr "2 : Annuleren"

#: source/text/sbasic/shared/03010102.xhp#par_id4056825
msgid  "3 : Abort"
msgstr "3 : Afbreken"

#: source/text/sbasic/shared/03010102.xhp#par_id3155335.28
msgid  "4 : Retry"
msgstr "4 : Opnieuw proberen"

#: source/text/sbasic/shared/03010102.xhp#par_id3146918.29
msgid  "5 : Ignore"
msgstr "5 : Negeren"

#: source/text/sbasic/shared/03010102.xhp#par_id3155961.30
msgid  "6 : Yes"
msgstr "6 : Ja"

#: source/text/sbasic/shared/03010102.xhp#par_id3148488.31
msgid  "7 : No"
msgstr "7 : Nee"

#: source/text/sbasic/shared/03010102.xhp#hd_id3150090.40
msgid  "Example:"
msgstr "Voorbeeld:"

#: source/text/sbasic/shared/03010102.xhp#par_id3154120.41
msgid  "Sub ExampleMsgBox"
msgstr "Sub VoorbeeldMsgBox"

#: source/text/sbasic/shared/03010102.xhp#par_id3145131.42
msgid  "Dim sVar as Integer"
msgstr "Dim sVar as Integer"

#: source/text/sbasic/shared/03010102.xhp#par_id3151278.43
msgid  "sVar = MsgBox(\"Las Vegas\")"
msgstr "sVar = MsgBox(\"Las Vegas\")"

#: source/text/sbasic/shared/03010102.xhp#par_id3149034.44
msgid  "sVar = MsgBox(\"Las Vegas\",1)"
msgstr "sVar = MsgBox(\"Las Vegas\",1)"

#: source/text/sbasic/shared/03010102.xhp#par_id3166424.45
msgid  "sVar = MsgBox( \"Las Vegas\",256 + 16 + 2,\"Dialog title\")"
msgstr "sVar = MsgBox( \"Den Bosch\",256 + 16 + 2,\"Dialoogtitel\")"

#: source/text/sbasic/shared/03010102.xhp#par_id3152581.46
msgid  "end sub"
msgstr "end sub"

#: source/text/sbasic/shared/03010103.xhp#tit
msgid  "Print Statement [Runtime]"
msgstr "Print-instructie [Runtime]"

#: source/text/sbasic/shared/03010103.xhp#bm_id3147230
msgid  "\<bookmark_value\>Print statement\</bookmark_value\>"
msgstr ""

#: source/text/sbasic/shared/03010103.xhp#hd_id3147230.1
msgid  "\<link href=\"text/sbasic/shared/03010103.xhp\" name=\"Print Statement [Runtime]\"\>Print Statement [Runtime]\</link\>"
msgstr ""

#: source/text/sbasic/shared/03010103.xhp#par_id3156281.2
msgid  "Outputs the specified strings or numeric expressions to a dialog or to a file."
msgstr "Schrijft de gespecificeerde strings of numerieke uitdrukkingen weg naar een dialoogvenster of naar een bestand."

#: source/text/sbasic/shared/03010103.xhp#hd_id3145785.3
msgid  "Syntax:"
msgstr "Syntaxis:"

#: source/text/sbasic/shared/03010103.xhp#par_id3153188.4
msgid  "Print [#FileName,] Expression1[{;|,} [Spc(Number As Integer);] [Tab(pos As Integer);] [Expression2[...]] "
msgstr "Print [#BestandsNaam,] Expressie1[{;|,} [Spc(Nummer As Integer);] [Tab(pos As Integer);] [Expressie2[...]]"

#: source/text/sbasic/shared/03010103.xhp#hd_id3147348.5
msgid  "Parameter:"
msgstr "Parameter:"

#: source/text/sbasic/shared/03010103.xhp#par_id2508621
msgid  "\<emph\>FileName:\</emph\> Any numeric expression that contains the file number that was set by the Open statement for the respective file."
msgstr ""

#: source/text/sbasic/shared/03010103.xhp#par_id3163712.6
msgid  "\<emph\>Expression\</emph\>: Any numeric or string expression to be printed. Multiple expressions can be separated by a semicolon. If separated by a comma, the expressions are indented to the next tab stop. The tab stops cannot be adjusted."
msgstr ""

#: source/text/sbasic/shared/03010103.xhp#par_id3153092.7
msgid  "\<emph\>Number\</emph\>: Number of spaces to be inserted by the \<emph\>Spc\</emph\> function."
msgstr ""

#: source/text/sbasic/shared/03010103.xhp#par_id3145364.8
msgid  "\<emph\>Pos\</emph\>: Spaces are inserted until the specified position."
msgstr ""

#: source/text/sbasic/shared/03010103.xhp#par_id3154319.9
msgid  "If a semicolon or comma appears after the last expression to be printed, $[officename] Basic stores the text in an internal buffer and continues program execution without printing. When another Print statement without a semicolon or comma at the end is encountered, all text to be printed is printed at once."
msgstr "Als een puntkomma of komma verschijnt na de laatste expressie die moet worden afgedrukt, slaat $[officename] BASIC de tekst op in een interne buffer en vervolgt de programma-uitvoering zonder afdrukken. Wanneer een andere Print-instructie, zonder een puntkomma of komma aan het eind wordt bereikt, wordt alle tekst die moet worden afgedrukt in één keer afgedrukt."

#: source/text/sbasic/shared/03010103.xhp#par_id3145272.10
msgid  "Positive numeric expressions are printed with a leading space. Negative expressions are printed with a leading minus sign. If a certain range is exceeded for floating-point values, the respective numeric expression is printed in exponential notation."
msgstr "Positieve numerieke expressies worden afgedrukt met een voorafgaande spatie. Negatieve expressies worden afgedrukt met een voorafgaand minteken. Als een bepaald bereik wordt overschreden voor zwevende-kommawaarden, wordt de respectievelijke numerieke expressie afgedrukt in exponentiële notatie."

#: source/text/sbasic/shared/03010103.xhp#par_id3154011.11
msgid  "If the expression to be printed exceeds a certain length, the display will automatically wrap to the next line."
msgstr "Als de uitdrukking die moet worden afgedrukt groter is dan een bepaalde lengte, zal de weergave op het scherm automatisch doorlopen naar de volgende regel."

#: source/text/sbasic/shared/03010103.xhp#par_id3146969.12
msgid  "You can insert the Tab function, enclosed by semicolons, between arguments to indent the output to a specific position, or you can use the \<emph\>Spc\</emph\> function to insert a specified number of spaces."
msgstr ""

#: source/text/sbasic/shared/03010103.xhp#hd_id3146912.13
msgid  "Example:"
msgstr "Voorbeeld:"

#: source/text/sbasic/shared/03010103.xhp#par_id3153711.14
msgid  "Sub ExamplePrint"
msgstr "Sub VoorbeeldPrint"

#: source/text/sbasic/shared/03010103.xhp#par_id3153764.15
msgid  "Print \"ABC\""
msgstr "Print \"ABC\""

#: source/text/sbasic/shared/03010103.xhp#par_id3155764.16
msgid  "Print \"ABC\",\"123\""
msgstr "Print \"ABC\",\"123\""

#: source/text/sbasic/shared/03010103.xhp#par_id5484176
msgid  "i = FreeFile()"
msgstr "i = FreeFile()"

#: source/text/sbasic/shared/03010103.xhp#par_id2904141
msgid  "Open \<switchinline select=\"sys\"\>\<caseinline select=\"WIN\"\>\"C:\\\\Temp.txt\"\</caseinline\>\<defaultinline\>\"~/temp.txt\"\</defaultinline\>\</switchinline\> For Output As i"
msgstr ""

#: source/text/sbasic/shared/03010103.xhp#par_id36317
msgid  "Print #i, \"ABC\""
msgstr "Afdrukken #i, \"ABC\""

#: source/text/sbasic/shared/03010103.xhp#par_id7381817
msgid  "Close #i"
msgstr "Afsluiten #i"

#: source/text/sbasic/shared/03010103.xhp#par_id3147339.17
msgid  "end Sub"
msgstr "End Sub"

#: source/text/sbasic/shared/03010200.xhp#tit
msgid  "Functions for Screen Input"
msgstr "Functies voor Scherminvoer"

#: source/text/sbasic/shared/03010200.xhp#hd_id3149456.1
msgid  "\<link href=\"text/sbasic/shared/03010200.xhp\" name=\"Functions for Screen Input\"\>Functions for Screen Input\</link\>"
msgstr ""

#: source/text/sbasic/shared/03010200.xhp#par_id3150398.2
msgid  "This section describes Runtime functions used to control screen input."
msgstr "Het volgende beschrijft de functies die gebruikt worden om scherminvoer te beheren."

#: source/text/sbasic/shared/03010201.xhp#tit
msgid  "InputBox Function [Runtime]"
msgstr "InputBox-functie [Runtime]"

#: source/text/sbasic/shared/03010201.xhp#bm_id3148932
msgid  "\<bookmark_value\>InputBox function\</bookmark_value\>"
msgstr ""

#: source/text/sbasic/shared/03010201.xhp#hd_id3148932.1
msgid  "\<link href=\"text/sbasic/shared/03010201.xhp\" name=\"InputBox Function [Runtime]\"\>InputBox Function [Runtime]\</link\>"
msgstr ""

#: source/text/sbasic/shared/03010201.xhp#par_id3151262.2
msgid  "Displays a prompt in a dialog at which the user can input text. The input is assigned to a variable."
msgstr "Toont een prompt in een dialoogvenster waar de gebruiker tekst kan invoeren. De invoer wordt toegekend aan een variabele."

#: source/text/sbasic/shared/03010201.xhp#par_id3151100.3
msgid  "The \<emph\>InputBox\</emph\> statement is a convenient method of entering text through a dialog. Confirm the input by clicking OK or pressing Return. The input is returned as the function return value. If you close the dialog with Cancel, \<emph\>InputBox\</emph\> returns a zero-length string (\"\")."
msgstr ""

#: source/text/sbasic/shared/03010201.xhp#hd_id3152347.4
msgid  "Syntax:"
msgstr "Syntaxis:"

#: source/text/sbasic/shared/03010201.xhp#par_id3159201.5
msgid  "InputBox (Msg As String[, Title As String[, Default As String[, x_pos As Integer, y_pos As Integer]]]]) "
msgstr "InputBox (Meld As String[, Titel As String[, Standaard As String[, x_pos As Integer, y_pos As Integer]]]])"

#: source/text/sbasic/shared/03010201.xhp#hd_id3150713.6
msgid  "Return value:"
msgstr "Teruggegeven waarde:"

#: source/text/sbasic/shared/03010201.xhp#par_id3145090.7
msgid  "String"
msgstr "String"

#: source/text/sbasic/shared/03010201.xhp#hd_id3149346.8
msgid  "Parameter:"
msgstr "Parameter:"

#: source/text/sbasic/shared/03010201.xhp#par_id3153311.9
msgid  "\<emph\>Msg\</emph\>: String expression displayed as the message in the dialog box."
msgstr ""

#: source/text/sbasic/shared/03010201.xhp#par_id3145315.10
msgid  "\<emph\>Title\</emph\>: String expression displayed in the title bar of the dialog box."
msgstr ""

#: source/text/sbasic/shared/03010201.xhp#par_id3154307.11
msgid  "\<emph\>Default\</emph\>: String expression displayed in the text box as default if no other input is given."
msgstr ""

#: source/text/sbasic/shared/03010201.xhp#par_id3147573.12
msgid  "\<emph\>x_pos\</emph\>: Integer expression that specifies the horizontal position of the dialog. The position is an absolute coordinate and does not refer to the window of the office application."
msgstr ""

#: source/text/sbasic/shared/03010201.xhp#par_id3156024.13
msgid  "\<emph\>y_pos\</emph\>: Integer expression that specifies the vertical position of the dialog. The position is an absolute coordinate and does not refer to the window of the office application."
msgstr ""

#: source/text/sbasic/shared/03010201.xhp#par_id3153897.14
msgid  "If \<emph\>x_pos\</emph\> and \<emph\>y_pos\</emph\> are omitted, the dialog is centered on the screen. The position is specified in \<link href=\"text/sbasic/shared/00000002.xhp#twips\" name=\"twips\"\>twips\</link\>."
msgstr ""

#: source/text/sbasic/shared/03010201.xhp#hd_id3149456.15
msgid  "Example:"
msgstr "Voorbeeld:"

#: source/text/sbasic/shared/03010201.xhp#par_id3153379.16
msgid  "Sub ExampleInputBox"
msgstr "Sub VoorbeeldInputBox"

#: source/text/sbasic/shared/03010201.xhp#par_id3149656.17
msgid  "Dim sText As String"
msgstr "Dim sTekst As String"

#: source/text/sbasic/shared/03010201.xhp#par_id3154367.18
msgid  "sText = InputBox (\"Please enter a phrase:\",\"Dear User\")"
msgstr "sTekst = InputBox (\"Toets alstublieft een zin in:\",\"Beste gebruiker\")"

#: source/text/sbasic/shared/03010201.xhp#par_id3151042.19
msgid  "MsgBox ( sText , 64, \"Confirmation of phrase\")"
msgstr "MsgBox (sTekst, 64, \"Bevestiging van de zin\")"

#: source/text/sbasic/shared/03010201.xhp#par_id3150768.20
msgid  "End Sub"
msgstr "End Sub"

