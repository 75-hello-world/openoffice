msgstr ""

#: source/text/sbasic/shared/03090404.xhp#par_id3153126.2
msgid  "Ends a procedure or block."
msgstr "Beëindigt een procedure of blok."

#: source/text/sbasic/shared/03090404.xhp#hd_id3147264.3
msgid  "Syntax:"
msgstr "Syntaxis:"

#: source/text/sbasic/shared/03090404.xhp#par_id3148552.4
msgid  "End, End Function, End If, End Select, End Sub"
msgstr "End, End function, End If, End Select, End Sub "

#: source/text/sbasic/shared/03090404.xhp#hd_id3149456.5
msgid  "Parameters:"
msgstr "Parameters:"

#: source/text/sbasic/shared/03090404.xhp#par_id3150398.6
msgid  "Use the End statement as follows:"
msgstr "Gebruik de End-instructie als volgt:"

#: source/text/sbasic/shared/03090404.xhp#hd_id3154366.7
msgid  "Statement"
msgstr "Instructie"

#: source/text/sbasic/shared/03090404.xhp#par_id3151043.8
msgid  "End: Is not required, but can be entered anywhere within a procedure to end the program execution."
msgstr "End: Is niet nodig, maar kan overal binnen een procedure ingevoerd worden om de uitvoering van het programma te beëindigen."

#: source/text/sbasic/shared/03090404.xhp#par_id3145171.9
msgid  "End Function: Ends a \<emph\>Function\</emph\> statement."
msgstr ""

#: source/text/sbasic/shared/03090404.xhp#par_id3153192.10
msgid  "End If: Marks the end of a \<emph\>If...Then...Else\</emph\> block."
msgstr ""

#: source/text/sbasic/shared/03090404.xhp#par_id3148451.11
msgid  "End Select: Marks the end of a \<emph\>Select Case\</emph\> block."
msgstr ""

#: source/text/sbasic/shared/03090404.xhp#par_id3155131.12
msgid  "End Sub: Ends a \<emph\>Sub\</emph\> statement."
msgstr ""

#: source/text/sbasic/shared/03090404.xhp#hd_id3146120.13
msgid  "Example:"
msgstr "Voorbeeld:"

#: source/text/sbasic/shared/03090404.xhp#par_id3146985.14
msgid  "Sub ExampleRandomSelect"
msgstr "Sub VoorbeeldWillekeurSelect"

