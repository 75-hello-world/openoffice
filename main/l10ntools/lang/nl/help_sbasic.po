msgstr "< : Kleiner dan"

#: source/text/sbasic/shared/03110100.xhp#par_id3146795.11
msgid  "> : Greater than"
msgstr "> : Groter dan"

#: source/text/sbasic/shared/03110100.xhp#par_id3150541.12
msgid  "<= : Less than or equal to"
msgstr "<= : Kleiner dan of gelijk aan"

#: source/text/sbasic/shared/03110100.xhp#par_id3150400.13
msgid  ">= : Greater than or equal to"
msgstr ">= : Groter dan of gelijk aan"

#: source/text/sbasic/shared/03110100.xhp#par_id3148797.14
msgid  "<> : Not equal to"
msgstr "<> : Niet gelijk aan"

#: source/text/sbasic/shared/03110100.xhp#hd_id3154686.15
msgid  "Example:"
msgstr "Voorbeeld:"

