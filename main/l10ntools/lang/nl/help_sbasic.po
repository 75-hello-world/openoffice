msgstr "Do..."

#: source/text/sbasic/shared/03090201.xhp#par_id3149298.43
msgid  "statements"
msgstr "instructies"

#: source/text/sbasic/shared/03090201.xhp#par_id3145646.44
msgid  "If condition = True Then Exit Do"
msgstr "If voorwaarde = Waar Then Exit Do"

#: source/text/sbasic/shared/03090201.xhp#par_id3154490.45
msgid  "statements"
msgstr "instructies"

#: source/text/sbasic/shared/03090201.xhp#par_id3153159.46
msgid  "Loop..."
msgstr "Loop..."

#: source/text/sbasic/shared/03090201.xhp#hd_id3147396.47
msgid  "Example"
msgstr "Voorbeeld"

#: source/text/sbasic/shared/03090201.xhp#par_id3144764.49
msgid  "Sub ExampleDoLoop"
msgstr "Sub VoorbeeldDoLoop"

#: source/text/sbasic/shared/03090201.xhp#par_id3154791.50
msgid  "Dim sFile As String"
msgstr "Dim sBestand As String"

#: source/text/sbasic/shared/03090201.xhp#par_id3149401.51
msgid  "Dim sPath As String"
msgstr "Dim sPad As String"

#: source/text/sbasic/shared/03090201.xhp#par_id3155600.52
msgid  "sPath = \"c:\\\\\""
msgstr ""

#: source/text/sbasic/shared/03090201.xhp#par_id3150717.53
msgid  "sFile = Dir$( sPath ,22)"
msgstr "sBestand = Dir$( sPath ,22)"

#: source/text/sbasic/shared/03090201.xhp#par_id3146898.54
msgid  "If sFile <> \"\" Then"
msgstr "If sBestand <> \"\" Then"

#: source/text/sbasic/shared/03090201.xhp#par_id3156333.55
msgid  "Do"
msgstr "Do"

#: source/text/sbasic/shared/03090201.xhp#par_id3153947.56
msgid  "MsgBox sFile"
msgstr "MsgBox sBestand"

#: source/text/sbasic/shared/03090201.xhp#par_id3150327.57
msgid  "sFile = Dir$"
msgstr "sBestand = Dir$"

#: source/text/sbasic/shared/03090201.xhp#par_id3150749.58
msgid  "Loop Until sFile = \"\""
msgstr "Loop Until sBestand = \"\""

#: source/text/sbasic/shared/03090201.xhp#par_id3153765.59
msgid  "End If"
