msgstr ""

#: source/text/sbasic/shared/03080201.xhp#par_id3155555.2
msgid  "Returns the base of the natural logarithm (e = 2.718282) raised to a power."
msgstr "Geeft de basis van het natuurlijke logaritme (e = 2.718282) verhoogd tot een macht."

#: source/text/sbasic/shared/03080201.xhp#hd_id3150984.3
msgid  "Syntax:"
msgstr "Syntaxis:"

#: source/text/sbasic/shared/03080201.xhp#par_id3145315.4
msgid  "Exp (Number)"
msgstr "Exp (Getal) "

#: source/text/sbasic/shared/03080201.xhp#hd_id3154347.5
msgid  "Return value:"
msgstr "Teruggegeven waarde:"

#: source/text/sbasic/shared/03080201.xhp#par_id3149670.6
msgid  "Double"
msgstr "Double"

#: source/text/sbasic/shared/03080201.xhp#hd_id3154760.7
msgid  "Parameters:"
msgstr "Parameters:"

#: source/text/sbasic/shared/03080201.xhp#par_id3150793.8
msgid  "\<emph\>Number:\</emph\> Any numeric expression that specifies the power that you want to raise \"e\" to (the base of natural logarithms). The power must be for both single-precision numbers less than or equal to 88.02969 and double-precision numbers less than or equal to 709.782712893, since $[officename] Basic returns an Overflow error for numbers exceeding these values."
msgstr ""

#: source/text/sbasic/shared/03080201.xhp#hd_id3156280.9
msgid  "Example:"
msgstr "Voorbeeld:"

#: source/text/sbasic/shared/03080201.xhp#par_id3153193.10
msgid  "Sub ExampleLogExp"
msgstr "Sub VoorbeeldLogExp"

#: source/text/sbasic/shared/03080201.xhp#par_id3125864.11
msgid  "Dim dValue as Double"
msgstr "Dim dWaarde As Double"

#: source/text/sbasic/shared/03080201.xhp#par_id3145172.12
msgid  "const b1=12.345e12"
msgstr "Const b1=12.345e12"

#: source/text/sbasic/shared/03080201.xhp#par_id3159254.13
msgid  "const b2=1.345e34"
msgstr "Const b2=1.345e34"

#: source/text/sbasic/shared/03080201.xhp#par_id3147287.14
msgid  "dValue=Exp( Log(b1)+Log(b2) )"
msgstr "dWaarde=Exp(Log(b1)+Log(b2))"

#: source/text/sbasic/shared/03080201.xhp#par_id3161832.15
msgid  "MsgBox \"\" & dValue & chr(13) & (b1*b2) ,0,\"Multiplication by logarithm\""
msgstr "MsgBox \"\" & dWaarde & Chr(13) & (b1*b2) ,0,\"Vermenigvuldiging met logaritmen\""

#: source/text/sbasic/shared/03080201.xhp#par_id3151112.16
msgid  "end sub"
msgstr "End Sub"

