msgid "Column"
msgstr "Spalte"

#: 05120000.xhp#hd_id3154762.1.help.text
msgid ""
"<link href=\"text/swriter/01/05120000.xhp\" name=\"Column\">Column</link>"
msgstr ""
"<link href=\"text/swriter/01/05120000.xhp\" name=\"Kolonne\">Kolonne</link>"

#: 05120000.xhp#par_id3149052.2.help.text
msgid "Set the width of columns, or select, insert, and delete columns."
msgstr "Definer bredden af kolonner eller vælg, indsæt og slet kolonner."

#: 05120000.xhp#hd_id3146322.4.help.text
msgid ""
"<link href=\"text/swriter/01/05120100.xhp\" name=\"Width...\">Width...</link>"
msgstr ""
"<link href=\"text/swriter/01/05120100.xhp\" name=\"Bredde...\">Bredde...</"
"link>"

#: 05120000.xhp#hd_id3154558.6.help.text
msgid ""
"<link href=\"text/swriter/01/05120200.xhp\" name=\"Optimal width\">Optimal "
"width</link>"
msgstr ""
"<link href=\"text/swriter/01/05120200.xhp\" name=\"Optimal bredde\">Optimal "
"bredde</link>"

