msgctxt "05110200.xhp#hd_id3150010.1.help.text"
msgid "Optimal Height"
msgstr "Optimal højde"

#: 05110200.xhp#par_id3147436.2.help.text
msgid ""
"<variable id=\"zeilenhoehetext\"><ahelp hid=\".uno:SetOptimalRowHeight\" "
"visibility=\"visible\">Automatically adjusts row heights to match the "
"contents of the cells.</ahelp> This is the default setting for new tables.</"
"variable>"
msgstr ""
"<variable id=\"zeilenhoehetext\"><ahelp hid=\".uno:SetOptimalRowHeight\" "
"visibility=\"visible\">Justerer automatisk rækkehøjderne, så de passer til "
"indholdet af cellerne.</ahelp> Dette er standardindstillingen for nye "
"tabeller.</variable>"

#: 05110200.xhp#par_id3154765.3.help.text
msgid ""
"This option is only available if the table contains a row with a fixed "
"height."
msgstr ""
"Denne indstilling er kun tilgængelig, hvis tabellen indeholder en række med "
"fast højde."

#: 05110300.xhp#tit.help.text
