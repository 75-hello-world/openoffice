msgid  "{a + c} over 2 = m"
msgstr "{a + c} over 2 = m"

#: source/text/smath/guide/brackets.xhp#par_id3154703.6
msgid  "or"
msgstr "eller"

#: source/text/smath/guide/brackets.xhp#par_id3150297.5
