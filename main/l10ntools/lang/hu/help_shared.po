msgid  "{empty}"
msgstr "{üres}"

#: source/text/shared/02/01170102.xhp#par_id3154260.50
msgid  "The 1st column of the table is linked to the field specified under \<emph\>Data field\</emph\>."
msgstr ""

#: source/text/shared/02/01170102.xhp#par_id3156448.51
msgid  "1"
msgstr "1"

#: source/text/shared/02/01170102.xhp#par_id3154486.52
msgid  "The 2nd column of the table is linked to the field specified under \<emph\>Data field\</emph\>."
