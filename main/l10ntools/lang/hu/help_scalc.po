msgstr "<item type=\"input\">10</item>"

#: source/text/scalc/01/04060107.xhp#par_idN11B92
msgid  "\<item type=\"input\"\>11\</item\>"
msgstr "<item type=\"input\">11</item>"

#: source/text/scalc/01/04060107.xhp#par_idN11B98
