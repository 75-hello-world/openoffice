msgid  "\<item type=\"input\"\>Name\</item\>"
msgstr "<item type=\"input\">Név</item>"

#: source/text/scalc/01/04060101.xhp#par_id3158414.73
msgid  "\<item type=\"input\"\>Grade\</item\>"
msgstr "<item type=\"input\">Osztály</item>"

#: source/text/scalc/01/04060101.xhp#par_id3152820.74
msgid  "\<item type=\"input\"\>Age\</item\>"
msgstr "<item type=\"input\">Kor</item>"

#: source/text/scalc/01/04060101.xhp#par_id3154866.75
msgid  "\<item type=\"input\"\>Distance to School\</item\>"
msgstr "<item type=\"input\">Távolság az iskolától</item>"

#: source/text/scalc/01/04060101.xhp#par_id3150471.76
msgid  "\<item type=\"input\"\>Weight\</item\>"
msgstr "<item type=\"input\">Testsúly</item>"

#: source/text/scalc/01/04060101.xhp#par_id3153920.77
