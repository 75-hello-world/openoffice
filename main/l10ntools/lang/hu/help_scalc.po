msgid  "Example"
msgstr "Példa"

#: source/text/scalc/01/04060116.xhp#par_id3155592.57
msgid  "            \<item type=\"input\"\>=IMAGINARY(\"4+3j\")\</item\> returns 3."
msgstr "            Az <item type=\"input\">=IMAGINARY(\"4+3j\")</item> a 3 értéket adja vissza."

