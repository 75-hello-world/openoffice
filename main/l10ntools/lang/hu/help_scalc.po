msgstr "<item type=\"input\">1000</item>"

#: source/text/scalc/01/04060101.xhp#par_id3145353.68
msgid  "\<item type=\"input\"\>42\</item\>"
msgstr "<item type=\"input\">42</item>"

#: source/text/scalc/01/04060101.xhp#par_id3150662.69
msgid  "11"
msgstr "11"

#: source/text/scalc/01/04060101.xhp#par_id3150568.70
msgid  "12"
msgstr "12"

