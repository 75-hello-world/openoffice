msgstr "=SZÁM(B1) => HAMIS (MS-Excel: IGAZ)"

#: source/text/scalc/05/empty_cells.xhp#par_id9604480
msgid  "=ISTEXT(B1) => FALSE"
msgstr "=SZÖVEG.E(B1) => HAMIS"

#: source/text/scalc/05/empty_cells.xhp#par_id2298959
msgid  "=B1=0 => TRUE"
msgstr "=B1=0 => IGAZ"

#: source/text/scalc/05/empty_cells.xhp#par_id4653767
msgid  "=B1=\"\" => TRUE (MS-Excel: FALSE)"
msgstr "=B1=\"\" => IGAZ (MS-Excel: HAMIS)"

#: source/text/scalc/05/empty_cells.xhp#par_id8801538
msgid  "C1: =VLOOKUP(...) with empty cell result => displays empty (MS-Excel: displays 0)"
msgstr "A C1: =FKERES(...) üres cellával üresen jelenik meg (MS-Excel: 0 jelenik meg)"

#: source/text/scalc/05/empty_cells.xhp#par_id6746421
