#: source/text/schart/main0000.xhp#par_id0810200904362778
msgid  "\<ahelp hid=\".\" visibility=\"hidden\"\>Deletes all data labels.\</ahelp\>"
msgstr "<ahelp hid=\".\" visibility=\"hidden\">Xóa tất cả nhãn dữ liệu.</ahelp>"

