msgid ""
"            <emph>TotalPeriods</emph> is the total number of installment "
"periods."
msgstr "<emph>Total_periods</emph> шумораи ҳама давраҳо."

#: 04060103.xhp#par_id3153736.45.help.text
msgid ""
"                     <item type=\"input\">10,000 currency units</item> "
"                 "
msgstr "10,000 воҳид"

#: 04060103.xhp#par_id3153778.38.help.text
msgid ""
