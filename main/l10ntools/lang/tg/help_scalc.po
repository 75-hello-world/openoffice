msgctxt "04060101.xhp#par_id3148739.53.help.text"
msgid "<item type=\"input\">1</item>"
msgstr "1"

#: 04060101.xhp#par_id3148761.64.help.text
msgid "<item type=\"input\">Irene</item>"
msgstr "Irene"

