"Shift<switchinline select=\"sys\"><caseinline select=\"MAC\">+Command</caseinlin"
"e><defaultinline>+Ctrl</defaultinline></switchinline>+F11"
msgstr ""
"Maj<switchinline select=\"sys\"><caseinline select=\"MAC\">+Ordre</caseinline><d"
"efaultinline>+Ctrl</defaultinline></switchinline>+F11"

#: 01020000.xhp#par_id3153215.51.help.text
msgid "Updates the templates."
msgstr "Actualitza les plantilles."

#: 01020000.xhp#hd_id3150760.52.help.text
msgid "F12"
msgstr "F12"

#: 01020000.xhp#par_id3156321.53.help.text
