#: source/text/scalc/01/04060199.xhp#hd_id3156445.1
msgid  "Operators in $[officename] Calc"
msgstr "Operadors a l'$[officename] Calc"

#: source/text/scalc/01/04060199.xhp#par_id3155812.2
msgid  "You can use the following operators in $[officename] Calc:"
msgstr "Podeu utilitzar els operadors següents a l'$[officename] Calc:"

#: source/text/scalc/01/04060199.xhp#hd_id3153066.3
msgid  "Arithmetical Operators"
msgstr "Operadors aritmètics"

#: source/text/scalc/01/04060199.xhp#par_id3148601.4
msgid  "These operators return numerical results."
msgstr "Aquests operadors retornen resultats numèrics."

#: source/text/scalc/01/04060199.xhp#par_id3144768.5
msgid  "Operator"
msgstr "Operador"

#: source/text/scalc/01/04060199.xhp#par_id3157982.6
msgid  "Name"
msgstr "Nom"

#: source/text/scalc/01/04060199.xhp#par_id3159096.7
msgid  "Example"
msgstr "Exemple"

#: source/text/scalc/01/04060199.xhp#par_id3149126.8
msgid  "+ (Plus)"
msgstr "+ (més)"

#: source/text/scalc/01/04060199.xhp#par_id3150892.9
msgid  "Addition"
msgstr "Suma"

#: source/text/scalc/01/04060199.xhp#par_id3153247.10
msgid  "1+1"
msgstr "1+1"

