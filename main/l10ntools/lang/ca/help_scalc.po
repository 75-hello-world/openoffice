msgid "Tab1"
msgstr "Tab1"

#: 04060112.xhp#par_id3149820.91.help.text
msgctxt "04060112.xhp#par_id3149820.91.help.text"
msgid ""
"Table number in the upper-left corner of the cell area; numbering starts at "
"0."
msgstr ""
