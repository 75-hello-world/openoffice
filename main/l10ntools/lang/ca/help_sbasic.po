msgstr "Valor"

#: source/text/sbasic/shared/03020409.xhp#par_id3147349.11
msgid  "0 : Normal files."
msgstr "0 : Fitxers normals."

#: source/text/sbasic/shared/03020409.xhp#par_id3147434.12
msgid  "1 : Read-only files."
msgstr "1 : Fitxers només de lectura."

#: source/text/sbasic/shared/03020409.xhp#par_id3159154.15
msgid  "8 : Returns the name of the volume"
msgstr "8 : Torna només el nom del volum"

#: source/text/sbasic/shared/03020409.xhp#par_id3145271.16
msgid  "16 : Returns the name of the directory only."
msgstr "16 : Torna només el nom del directori."

#: source/text/sbasic/shared/03020409.xhp#par_id3153953.17
msgid  "32 : File was changed since last backup (Archive bit)."
msgstr "32 : El fitxer s'ha canviat des de l'última còpia de seguretat (bit d'arxiu)."

