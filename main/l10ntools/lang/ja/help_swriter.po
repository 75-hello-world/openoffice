#: source/text/swriter/guide/text_emphasize.xhp#tit
msgid  "Emphasizing Text"
msgstr "テキストを強調表示する"

#: source/text/swriter/guide/text_emphasize.xhp#bm_id3149820
msgid  "\<bookmark_value\>text; emphasizing\</bookmark_value\>      \<bookmark_value\>emphasizing text\</bookmark_value\>"
