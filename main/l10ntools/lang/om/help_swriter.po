msgid "Edit"
msgstr "Gulaali"

#: 04070200.xhp#par_id3156281.20.help.text
msgid ""
"<ahelp hid=\"SW:MENUBUTTON:TP_ENV_FMT:BTN_SEND_EDIT\">Click and choose the "
