msgstr "<link href=\"text/swriter/01/04070200.xhp\" name=\"Format\">Dhangii</link>"

#: 04070200.xhp#par_id3149295.2.help.text
msgid ""
"<ahelp hid=\"HID_ENV_FMT\">Specifies the layout and the dimension of the "
"envelope.</ahelp>"
msgstr "<ahelp hid=\"HID_ENV_FMT\">Teessumaaf roga batergaa ifteessa.</ahelp>"

