msgstr "Definice barvy nebo obrázku na pozadí"

#: background.xhp#bm_id3149346.help.text
msgid ""
"<bookmark_value>backgrounds;text objects</bookmark_value><bookmark_value>wor"
"ds;backgrounds</bookmark_value><bookmark_value>paragraphs; backgrounds</book"
"mark_value><bookmark_value>text;backgrounds</bookmark_value><bookmark_value>"
"tables; backgrounds</bookmark_value><bookmark_value>cells; backgrounds</book"
"mark_value><bookmark_value>backgrounds;selecting</bookmark_value>"
msgstr ""
