msgstr ""
"<link href=\"text/simpress/01/03100000.xhp\" name=\"Slide Sorter\">Ordenação de "
"diapositivos</link>"

#: 03100000.xhp#par_id3154492.2.help.text
msgid ""
"<ahelp hid=\"HID_SD_BTN_SLIDE\">Displays miniature versions of the "
"slides.</ahelp>"
msgstr ""
"<ahelp hid=\"HID_SD_BTN_SLIDE\">Apresenta versões em miniatura dos "
"diapositivos.</ahelp>"

