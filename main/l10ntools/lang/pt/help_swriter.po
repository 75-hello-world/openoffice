msgstr ""
"<bookmark_value>formatar estilos; importar</bookmark_value>      "
"<bookmark_value>estilos; importar a partir de outros "
"ficheiros</bookmark_value>      <bookmark_value>importar;estilos de outros "
"ficheiros</bookmark_value>      <bookmark_value>carregar;estilos de outros "
"ficheiros</bookmark_value>"

