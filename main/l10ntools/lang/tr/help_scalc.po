msgid "Page Styles"
msgstr "Sayfa Biçemleri"

#: 05100000.xhp#par_id3147003.9.help.text
msgid ""
"<ahelp hid=\".uno:PageStyle\">Displays the Page Styles available for indirect "
"page formatting.</ahelp>"
msgstr ""
"<ahelp hid=\".uno:PageStyle\">Dolaylı Sayfa biçemleme için uygun olan sayfa "
"stillerini görüntüler.</ahelp>"

