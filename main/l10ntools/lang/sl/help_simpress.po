msgid ""
"<ahelp hid=\".\" visibility=\"hidden\">Applies the master page to all "
"slides.</ahelp>"
msgstr ""
"<ahelp hid=\".\" visibility=\"hidden\">Uporabi glavno stran na vseh "
"prosojnicah.</ahelp>"

