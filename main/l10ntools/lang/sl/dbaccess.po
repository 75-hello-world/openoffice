#: source/ext/adabas/AdabasNewDb.src#DLG_ADABAS_NEWDB.String.Text
msgid  "Create New Adabas D Database"
msgstr "Ustvari novo zbirko podatkov Adabas D"

#: source/ext/adabas/AdabasNewDb.src#STR_ADABAS_NO_PARAM_SET.String.Text
msgid  "No entry could be written in the configuration file."
msgstr "V konfiguracijsko datoteko ni bilo mogoče zapisati nobenega vnosa."

#: source/ext/adabas/AdabasNewDb.src#STR_ADABAS_NO_DB_CREATED.String.Text
msgid  "An error occurred as the DB files were being created."
msgstr "Pri ustvarjanju datotek zbirke podatkov je prišlo do napake."

#: source/ext/adabas/AdabasNewDb.src#STR_ADABAS_NO_SYSTEM_TABLES.String.Text
msgid  "An error occurred as the system tables were being created."
msgstr "Pri ustvarjanju sistemskih tabel je prišlo do napake."

#: source/ext/adabas/AdabasNewDb.src#STR_ADABAS_NO_BACKUP_FILE.String.Text
msgid  "The backup file entered does not exist."
msgstr "Vnesena datoteka varnostne kopije ne obstaja."

#: source/ext/adabas/AdabasNewDb.src#INFO_STR_MAX_FILE_LENGTH.InfoBox.Message
msgid  "The fully qualified file cannot be longer than \'#\' characters."
msgstr ""

#: source/ext/adabas/AdabasNewDb.src#STR_NO_SAME_USER.String.Text
msgid  "The administrator and the control user cannot have the same name!"
msgstr "Skrbnik in nadzorni uporabnik ne moreta imeti istega imena!"

#: source/ext/adabas/AdabasNewDb.src#STR_DEVSPACE_NO_FILE.String.Text
msgid  "No name has been entered for the $name$ file!"
msgstr "Za datoteko $name$ niste vnesli imena!"

#: source/ext/adabas/AdabasNewDb.src#STR_NO_PERMISSION.String.Text
msgid  "You have to have administrator rights to create a new database!"
msgstr "Za ustvarjanje nove zbirke podatkov potrebujete skrbniške pravice!"

#: source/ext/adabas/AdabasNewDb.src#STR_FILE_OR_DIR_UNKNOWN.String.Text
msgid  "The specified file name or directory does not exist."
msgstr "Navedeno ime datoteke ali mape ne obstaja."

#: source/ext/adabas/AdabasNewDb.src#STR_ERROR_IN_ADABAS_INSTALLATION.String.Text
msgid  "No new database can be created.\nPlease check your Adabas D installation, especially the variables \"DBROOT\" and \"DBWORK\"."
msgstr "Nove zbirke podatkov ni mogoče ustvariti.\nPreverite namestitev Adabas D, še posebej spremenljivki \"DBROOT\" in \"DBWORK\"."

#: source/ext/adabas/AdabasNewDb.src#STR_STAT_WARNING.String.Text
msgid  "Warning!"
msgstr "Opozorilo!"

#: source/ext/adabas/AdabasNewDb.src#STR_ADABAS_CREATEFILES.String.Text
msgid  "Creating database files."
msgstr "Ustvarjanje datotek zbirke podatkov."

#: source/ext/adabas/AdabasNewDb.src#STR_ADABAS_CREATEPARAM_FILE.String.Text
msgid  "Creating database configuration file."
msgstr "Ustvarjanje konfiguracijske datoteke zbirke podatkov."

#: source/ext/adabas/AdabasNewDb.src#STR_ADABAS_START_DATABASE.String.Text
msgid  "Starting database for the first time."
msgstr "Prvi zagon zbirke podatkov."

#: source/ext/adabas/AdabasNewDb.src#STR_ADABAS_DB_EXISTS.String.Text
msgid  "The name entered already exists."
msgstr "Vneseno ime že obstaja."

#: source/ext/adabas/AdabasNewDb.src#STR_ADABAS_NO_DEFAULTNAME.String.Text
msgid  "No default names could be set for the database files. The total length is more than 40 characters."
msgstr "Datotekam zbirke podatkov ni mogoče nastaviti privzetih imen. Skupna dolžina presega 40 znakov."

#: source/ext/adabas/AdabasNewDb.src#STR_ADABAS_NO_ACCESSRIGHTS.String.Text
msgid  "No default names could be set for the database files. There is no write access."
msgstr "Datotekam zbirke podatkov ni mogoče nastaviti privzetih imen. Ni pravic za pisanje."

#: source/ext/adabas/AdabasNewDb.src#STR_ADABAS_CANNOT_CONVERT.String.Text
msgid  "The current database need to be converted. Please insert control user  and password."
msgstr "Trenutno zbirko podatkov je potrebno pretvoriti. Vnesite nadzornega uporabnika in geslo."

#: source/ext/adabas/AdabasNewDb.src#STR_ADABAS_PASSWORD_ILLEGAL.String.Text
msgid  "Spaces are not permitted in the password. Please enter a new password."
msgstr "V geslu presledki niso dovoljeni. Vnesite novo geslo."

#: source/ext/macromigration/macromigration.src#DLG_MACRO_MIGRATION.ModalDialog.Text
msgid  "Database Document Macro Migration"
msgstr "Migracija makrov dokumenta zbirke podatkov"

#: source/ext/macromigration/macromigration.src#DLG_MACRO_MIGRATION.STR_STATE_CLOSE_SUB_DOCS.String.Text
msgid  "Prepare"
msgstr "Pripravi"

#: source/ext/macromigration/macromigration.src#DLG_MACRO_MIGRATION.STR_STATE_BACKUP_DBDOC.String.Text
msgid  "Backup Document"
msgstr "Naredi varnostno kopijo dokumenta"

#: source/ext/macromigration/macromigration.src#DLG_MACRO_MIGRATION.STR_STATE_MIGRATE.String.Text
