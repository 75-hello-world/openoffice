msgid "Options"
msgstr "Opciones"

#: 05060200.xhp#hd_id3147397.33.help.text
msgid "Gaps"
msgstr "Espacios"

#: 05060200.xhp#hd_id3148790.30.help.text
msgid "In Background"
msgstr "En el fondo"

