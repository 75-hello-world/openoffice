msgstr ""
"<link href=\"text/swriter/01/05090200.xhp\" name=\"Columnas\">Columnas</link>"

#: 05090200.xhp#hd_id3154476.9.help.text
msgid "Column width"
msgstr "Ancho de columna"

#: 05090200.xhp#hd_id3155182.13.help.text
