msgid ""
"<image id=\"img_id3145295\" src=\"svx/res/rotate3d.png\" width=\"5.64mm\" "
"height=\"5.64mm\"><alt id=\"alt_id3145295\">Icon</alt></image>"
msgstr ""
"<image id=\"img_id3145295\" src=\"svx/res/rotate3d.png\" width=\"5.64mm\" "
