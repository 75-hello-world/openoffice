msgid ""
"<ahelp hid=\".\" visibility=\"hidden\">Applies the master page to all "
"slides.</ahelp>"
msgstr ""
"<ahelp hid=\".\" visibility=\"hidden\">Aplica a página mestra a todos os "
"slides.</ahelp>"

#: taskpanel.xhp#par_idN10762.help.text
msgid ""
"<ahelp hid=\".\" visibility=\"hidden\">Applies the master page or the slide "
"design to the selected slides.</ahelp>"
msgstr ""
"<ahelp hid=\".\" visibility=\"hidden\">Aplica a página mestre ou o modelo do "
"slide aos slides selecionados.</ahelp>"

