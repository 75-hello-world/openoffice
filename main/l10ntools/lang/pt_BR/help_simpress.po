msgid ""
"<ahelp hid=\".\" visibility=\"hidden\">Resizes the preview of the master "
"pages.</ahelp>"
msgstr ""
"<ahelp hid=\".\" visibility=\"hidden\">Dimensiona a visualização das páginas "
"mestres.</ahelp>"

