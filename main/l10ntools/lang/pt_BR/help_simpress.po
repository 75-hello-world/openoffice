msgstr "Navegar"

#: 06070000.xhp#par_id3156061.58.help.text
msgid ""
"<ahelp hid=\"SD:PUSHBUTTON:TP_ANIMATION:BTN_SEARCH\">Locate the file you want "
"to open.</ahelp>"
msgstr ""
"<ahelp hid=\"SD:PUSHBUTTON:TP_ANIMATION:BTN_SEARCH\">Localize o arquivo que "
"deseja abrir.</ahelp>"

