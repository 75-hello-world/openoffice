msgstr "<image id=\"img_id3150508\" src=\"res/commandimagelist/sc_defaultnumbering.png\" width=\"0.222inch\" height=\"0.222inch\"><alt id=\"alt_id3150508\">Ícone</alt></image>"

#: source/text/swriter/02/02110000.xhp#par_id3147525.4
msgid  "Numbering On/Off"
msgstr "Ativar / Desativar numeração"

#: source/text/swriter/02/02110000.xhp#par_id3147549
msgid  "\<link href=\"text/shared/01/06050000.xhp\" name=\"Bullets and Numbering\"\>Bullets and Numbering\</link\>"
msgstr "<link href=\"text/shared/01/06050000.xhp\" name=\"Bullets and Numbering\">Marcadores e numeração</link>"

