msgstr "<link href=\"text/shared/guide/background.xhp\">Marcas d'água</link>"

#: source/text/scalc/guide/background.xhp#par_id3156180.30
msgid  "\<link href=\"text/shared/01/05030600.xhp\" name=\"Background tab page\"\>\<emph\>Background\</emph\> tab page\</link\>"
msgstr "<link href=\"text/shared/01/05030600.xhp\" name=\"Background tab page\">Página da guia <emph>Plano de fundo</emph></link>"

