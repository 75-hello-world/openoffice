msgid "Document Name\tStatus"
msgstr "Nome documento\tStato"

#: docrecovery.src#RID_SVXPAGE_DOCRECOVERY_RECOVER.BTN_RECOV_NEXT.pushbutton.text
msgid "~Start Recovery >"
msgstr "~Avvia ripristino >"

