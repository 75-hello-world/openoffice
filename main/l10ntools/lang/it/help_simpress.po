msgid "Delete Current Image"
msgstr "Cancella immagine attuale"

#: 06050000.xhp#par_id3149710.43.help.text
msgid ""
"<ahelp hid=\"SD_IMAGEBUTTON_FLT_WIN_ANIMATION_BTN_REMOVE_BITMAP\">Deletes "
"current image from the animation sequence.</ahelp>"
msgstr ""
"<ahelp hid=\"SD_IMAGEBUTTON_FLT_WIN_ANIMATION_BTN_REMOVE_BITMAP\">Elimina "
"l'immagine dalla sequenza di animazione.</ahelp>"

#: 06050000.xhp#par_id3152926.help.text
msgid ""
"<image id=\"img_id3153210\" src=\"sd/res/del1bmp.png\" width=\"0.1665in\" "
"height=\"0.1665in\"><alt id=\"alt_id3153210\">Icon</alt></image>"
msgstr ""
"<image id=\"img_id3153210\" src=\"sd/res/del1bmp.png\" width=\"0.1665in\" "
"height=\"0.1665in\"><alt id=\"alt_id3153210\">Icona</alt></image>"

