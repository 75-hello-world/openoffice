msgid "Previous Slide"
msgstr "Pagina precedente"

#: 02110000.xhp#par_id3148768.15.help.text
msgid ""
"<ahelp hid=\"HID_SD_NAVIGATOR_TBI_PREV\">Moves back one slide in the slide "
"show.</ahelp>"
msgstr ""
"<ahelp hid=\"HID_SD_NAVIGATOR_TBI_PREV\">Torna indietro di una diapositiva "
