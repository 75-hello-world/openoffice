#: 02110000.xhp#par_id3150473.16.help.text
msgctxt "02110000.xhp#par_id3150473.16.help.text"
msgid "Previous Slide"
msgstr "Pagina precedente"

#: 02110000.xhp#hd_id3155369.17.help.text
msgctxt "02110000.xhp#hd_id3155369.17.help.text"
msgid "Next Slide"
msgstr "Pagina successiva"

#: 02110000.xhp#par_id3153920.18.help.text
msgid ""
"<ahelp hid=\"HID_SD_NAVIGATOR_TBI_NEXT\">Move forward one slide in the slide "
"show.</ahelp>"
msgstr ""
"<ahelp hid=\"HID_SD_NAVIGATOR_TBI_NEXT\">Avanza di una diapositiva nella "
"presentazione.</ahelp>"

#: 02110000.xhp#par_id3083279.help.text
msgid ""
"<image id=\"img_id3083286\" src=\"res/commandimagelist/sc_nextrecord.png\" "
