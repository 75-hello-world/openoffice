msgstr "1° trimestre"

#: 12080500.xhp#par_id3155854.15.help.text
msgid "April"
msgstr "Aprile"

#: 12080500.xhp#par_id3148575.16.help.text
msgid "May"
msgstr "Maggio"

#: 12080500.xhp#par_id3145271.17.help.text
msgid "June"
msgstr "Giugno"

#: 12080500.xhp#par_id3145648.18.help.text
msgid "2nd Quarter"
msgstr "2° trimestre"

#: 12080500.xhp#par_id3153876.19.help.text
