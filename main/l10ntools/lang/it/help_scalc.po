msgid "Toggle"
msgstr "Commuta"

#: 02110000.xhp#hd_id3149381.11.help.text
msgctxt "02110000.xhp#hd_id3149381.11.help.text"
msgid "Contents"
msgstr "Contenuti"

#: 02110000.xhp#par_id3150051.13.help.text
msgid ""
"<ahelp hid=\"HID_SC_NAVIPI_ZOOM\">Allows you to hide/show the "
"contents.</ahelp>"
msgstr ""
"<ahelp hid=\"HID_SC_NAVIPI_ZOOM\">Consente di nascondere/mostrare i "
"contenuti.</ahelp>"

#: 02110000.xhp#par_id3155597.help.text
msgid ""
"<image id=\"img_id3154738\" src=\"sw/imglst/sc20233.png\" width=\"0.2228in\" "
"height=\"0.2228in\"><alt id=\"alt_id3154738\">Icon</alt></image>"
msgstr ""
"<image id=\"img_id3154738\" src=\"sw/imglst/sc20233.png\" width=\"0.2228in\" "
"height=\"0.2228in\"><alt id=\"alt_id3154738\">Icona</alt></image>"

