msgid  "Type of cell content, 0 == Double, 1 == String"
msgstr "Tipo di contenuto della cella, 0 == double, 1 == string"

#: source/text/scalc/01/04060112.xhp#par_id3155182.210
msgid  "24"
msgstr "24"

