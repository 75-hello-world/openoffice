#: 04060116.xhp#bm_id3154959.help.text
msgid "<bookmark_value>IMABS function</bookmark_value>"
msgstr "<bookmark_value>COMP.MODULO, funzione</bookmark_value>"

#: 04060116.xhp#hd_id3154959.44.help.text
msgid "IMABS"
msgstr "COMP.MODULO"

#: 04060116.xhp#par_id3149895.45.help.text
msgid ""
"<ahelp hid=\"HID_AAI_FUNC_IMABS\">The result is the absolute value of a "
"complex number.</ahelp>"
msgstr ""
"<ahelp hid=\"HID_AAI_FUNC_IMABS\">Restituisce il valore assoluto di un numero "
"complesso.</ahelp>"

#: 04060116.xhp#hd_id3155382.46.help.text
msgctxt "04060116.xhp#hd_id3155382.46.help.text"
msgid "Syntax"
msgstr "Sintassi"

#: 04060116.xhp#par_id3151302.47.help.text
msgid "IMABS(\"ComplexNumber\")"
msgstr "COMP.MODULO(\"Numero complesso\")"

#: 04060116.xhp#par_id3153974.48.help.text
msgid ""
"<variable id=\"complex\"><emph>ComplexNumber</emph> is a complex number that "
"is entered in the form \"x+yi\" or \"x+yj\".</variable>"
msgstr ""
"<variable id=\"complex\"><emph>Numero complesso</emph> è un numero complesso "
"nella forma \"x+yi\" o \"x+yj\".</variable>"

#: 04060116.xhp#hd_id3149697.49.help.text
msgctxt "04060116.xhp#hd_id3149697.49.help.text"
msgid "Example"
msgstr "Esempio"

#: 04060116.xhp#par_id3143222.50.help.text
msgid "            <item type=\"input\">=IMABS(\"5+12j\")</item> returns 13."
msgstr "<item type=\"input\">=COMP.MODULO(\"5+12j\")</item> restituisce 13."

#: 04060116.xhp#bm_id3145357.help.text
msgid "<bookmark_value>IMAGINARY function</bookmark_value>"
msgstr "<bookmark_value>COMP.IMMAGINARIO, funzione</bookmark_value>"

#: 04060116.xhp#hd_id3145357.51.help.text
msgid "IMAGINARY"
msgstr "COMP.IMMAGINARIO"

#: 04060116.xhp#par_id3146965.52.help.text
msgid ""
"<ahelp hid=\"HID_AAI_FUNC_IMAGINARY\">The result is the imaginary coefficient "
"of a complex number.</ahelp>"
msgstr ""
"<ahelp hid=\"HID_AAI_FUNC_IMAGINARY\">Restituisce il coefficiente "
"dell'immaginario di un numero complesso.</ahelp>"

#: 04060116.xhp#hd_id3153555.53.help.text
msgctxt "04060116.xhp#hd_id3153555.53.help.text"
msgid "Syntax"
msgstr "Sintassi"

#: 04060116.xhp#par_id3155522.54.help.text
msgid "IMAGINARY(\"ComplexNumber\")"
msgstr "COMP.IMMAGINARIO(\"Numero complesso\")"

#: 04060116.xhp#hd_id3151193.56.help.text
msgctxt "04060116.xhp#hd_id3151193.56.help.text"
msgid "Example"
msgstr "Esempio"

#: 04060116.xhp#par_id3155592.57.help.text
msgid "            <item type=\"input\">=IMAGINARY(\"4+3j\")</item> returns 3."
msgstr "<item type=\"input\">=COMP.IMMAGINARIO(\"4+3j\")</item> restituisce 3."

#: 04060116.xhp#bm_id3146106.help.text
msgid "<bookmark_value>IMPOWER function</bookmark_value>"
msgstr "<bookmark_value>COMP.POTENZA, funzione</bookmark_value>"

#: 04060116.xhp#hd_id3146106.58.help.text
msgid "IMPOWER"
msgstr "COMP.POTENZA"

#: 04060116.xhp#par_id3147245.59.help.text
msgid ""
"<ahelp hid=\"HID_AAI_FUNC_IMPOWER\">The result is the integer power of a "
"complex number.</ahelp>"
msgstr ""
"<ahelp hid=\"HID_AAI_FUNC_IMPOWER\">Restituisce un numero complesso elevato ad "
"una potenza intera.</ahelp>"

#: 04060116.xhp#hd_id3150954.60.help.text
msgctxt "04060116.xhp#hd_id3150954.60.help.text"
msgid "Syntax"
msgstr "Sintassi"

#: 04060116.xhp#par_id3147501.61.help.text
msgid "IMPOWER(\"ComplexNumber\"; Number)"
msgstr "COMP.POTENZA(\"Numero complesso\"; Numero)"

#: 04060116.xhp#par_id3155743.63.help.text
msgid "            <emph>Number</emph> is the exponent."
msgstr "<emph>Potenza</emph> è l'esponente."

#: 04060116.xhp#hd_id3149048.64.help.text
msgctxt "04060116.xhp#hd_id3149048.64.help.text"
msgid "Example"
msgstr "Esempio"

#: 04060116.xhp#par_id3151393.65.help.text
msgid ""
"            <item type=\"input\">=IMPOWER(\"2+3i\";2)</item> returns -5+12i."
msgstr "<item type=\"input\">=COMP.POTENZA(\"2+3i\"; 2)</item> restituisce -5+12i."

#: 04060116.xhp#bm_id3148748.help.text
msgid "<bookmark_value>IMARGUMENT function</bookmark_value>"
msgstr "<bookmark_value>COMP.ARGOMENTO, funzione</bookmark_value>"

#: 04060116.xhp#hd_id3148748.66.help.text
msgid "IMARGUMENT"
msgstr "COMP.ARGOMENTO"

#: 04060116.xhp#par_id3151341.67.help.text
msgid ""
"<ahelp hid=\"HID_AAI_FUNC_IMARGUMENT\">The result is the argument (the phi "
"angle) of a complex number.</ahelp>"
msgstr ""
"<ahelp hid=\"HID_AAI_FUNC_IMARGUMENT\">Restituisce l'argomento (l'angolo fi) "
"di un numero complesso.</ahelp>"

#: 04060116.xhp#hd_id3150533.68.help.text
msgctxt "04060116.xhp#hd_id3150533.68.help.text"
msgid "Syntax"
msgstr "Sintassi"

#: 04060116.xhp#par_id3156402.69.help.text
msgid "IMARGUMENT(\"ComplexNumber\")"
msgstr "COMP.ARGOMENTO(\"Numero complesso\")"

#: 04060116.xhp#hd_id3153019.71.help.text
msgctxt "04060116.xhp#hd_id3153019.71.help.text"
msgid "Example"
msgstr "Esempio"

#: 04060116.xhp#par_id3159125.72.help.text
msgid ""
"            <item type=\"input\">=IMARGUMENT(\"3+4j\")</item> returns 0.927295."
msgstr "<item type=\"input\">=COMP.ARGOMENTO(\"3+4j\")</item> restituisce 0,927295."

#: 04060116.xhp#bm_id3149146.help.text
msgid "<bookmark_value>IMCOS function</bookmark_value>"
msgstr "<bookmark_value>COMP.COS, funzione</bookmark_value>"

#: 04060116.xhp#hd_id3149146.73.help.text
msgid "IMCOS"
msgstr "COMP.COS"

#: 04060116.xhp#par_id3149725.74.help.text
msgid ""
"<ahelp hid=\"HID_AAI_FUNC_IMCOS\">The result is the cosine of a complex "
"number.</ahelp>"
msgstr ""
"<ahelp hid=\"HID_AAI_FUNC_IMCOS\">Restituisce il coseno di un numero "
"complesso.</ahelp>"

#: 04060116.xhp#hd_id3159116.75.help.text
msgctxt "04060116.xhp#hd_id3159116.75.help.text"
msgid "Syntax"
msgstr "Sintassi"

#: 04060116.xhp#par_id3147415.76.help.text
msgid "IMCOS(\"ComplexNumber\")"
msgstr "COMP.COS(\"Numero complesso\")"

#: 04060116.xhp#hd_id3152980.78.help.text
msgctxt "04060116.xhp#hd_id3152980.78.help.text"
msgid "Example"
msgstr "Esempio"

#: 04060116.xhp#par_id3157901.79.help.text
msgid ""
"            <item type=\"input\">=IMCOS(\"3+4j\") </item>returns -27.03-3.85i "
"(rounded)."
msgstr ""
"<item type=\"input\">=COMP.COS(\"3+4j\")</item> restituisce -27,03-3,85j "
"(arrotondato)."

#: 04060116.xhp#bm_id3150024.help.text
msgid "<bookmark_value>IMDIV function</bookmark_value>"
msgstr "<bookmark_value>COMP.DIV, funzione</bookmark_value>"

#: 04060116.xhp#hd_id3150024.80.help.text
msgid "IMDIV"
msgstr "COMP.DIV"

#: 04060116.xhp#par_id3145825.81.help.text
msgid ""
"<ahelp hid=\"HID_AAI_FUNC_IMDIV\">The result is the division of two complex "
"numbers.</ahelp>"
msgstr ""
"<ahelp hid=\"HID_AAI_FUNC_IMDIV\">Restituisce il quoziente di due numeri "
"complessi.</ahelp>"

#: 04060116.xhp#hd_id3150465.82.help.text
msgctxt "04060116.xhp#hd_id3150465.82.help.text"
msgid "Syntax"
msgstr "Sintassi"

#: 04060116.xhp#par_id3146942.83.help.text
msgid "IMDIV(\"Numerator\"; \"Denominator\")"
msgstr "COMP.DIV(\"Numeratore\";\"Denominatore\")"

#: 04060116.xhp#par_id3150741.84.help.text
msgid ""
"            <emph>Numerator</emph>, <emph>Denominator</emph> are complex "
"numbers that are entered in the form \"x+yi\" or \"x+yj\"."
msgstr ""
"<emph>Numeratore</emph>, <emph>Denominatore</emph> sono numeri complessi "
"nella forma \"x+yi\" o \"x+yj\"."

#: 04060116.xhp#hd_id3151229.85.help.text
msgctxt "04060116.xhp#hd_id3151229.85.help.text"
msgid "Example"
msgstr "Esempio"

#: 04060116.xhp#par_id3148440.86.help.text
msgid ""
"            <item type=\"input\">=IMDIV(\"-238+240i\";\"10+24i\")</item> returns "
"5+12i."
msgstr ""
"<item type=\"input\">=COMP.DIV(\"-238+240i\"; \"10+24i\")</item> restituisce "
"5+12i."

#: 04060116.xhp#bm_id3153039.help.text
msgid "<bookmark_value>IMEXP function</bookmark_value>"
msgstr "<bookmark_value>COMP.EXP, funzione</bookmark_value>"

#: 04060116.xhp#hd_id3153039.87.help.text
msgid "IMEXP"
msgstr "COMP.EXP"

#: 04060116.xhp#par_id3144741.88.help.text
msgid ""
"<ahelp hid=\"HID_AAI_FUNC_IMEXP\">The result is the power of e and the complex "
"number.</ahelp> The constant e has a value of approximately "
"2.71828182845904."
msgstr ""
"<ahelp hid=\"HID_AAI_FUNC_IMEXP\">Restituisce la potenza di e e il numero "
"complesso.</ahelp> La costante ha un valore approssimativo di "
"2,71828182845904."

#: 04060116.xhp#hd_id3145591.89.help.text
msgctxt "04060116.xhp#hd_id3145591.89.help.text"
msgid "Syntax"
msgstr "Sintassi"

#: 04060116.xhp#par_id3154810.90.help.text
msgid "IMEXP(\"ComplexNumber\")"
msgstr "COMP.EXP(\"Numero complesso\")"

#: 04060116.xhp#hd_id3148581.92.help.text
msgctxt "04060116.xhp#hd_id3148581.92.help.text"
msgid "Example"
msgstr "Esempio"

#: 04060116.xhp#par_id3149253.93.help.text
msgid ""
"            <item type=\"input\">=IMEXP(\"1+j\") </item>returns 1.47+2.29j "
"(rounded)."
msgstr ""
"<item type=\"input\">=COMP.EXP(\"1+j\")</item> restituisce 1,47+2,29j "
"(arrotondato)."

#: 04060116.xhp#bm_id3149955.help.text
msgid "<bookmark_value>IMCONJUGATE function</bookmark_value>"
msgstr "<bookmark_value>COMP.CONIUGATO, funzione</bookmark_value>"

#: 04060116.xhp#hd_id3149955.94.help.text
msgid "IMCONJUGATE"
msgstr "COMP.CONIUGATO"

#: 04060116.xhp#par_id3155263.95.help.text
msgid ""
"<ahelp hid=\"HID_AAI_FUNC_IMCONJUGATE\">The result is the conjugated complex "
"complement to a complex number.</ahelp>"
msgstr ""
"<ahelp hid=\"HID_AAI_FUNC_IMCONJUGATE\">Restituisce il complesso coniugato di "
"un numero complesso.</ahelp>"

#: 04060116.xhp#hd_id3148750.96.help.text
msgctxt "04060116.xhp#hd_id3148750.96.help.text"
msgid "Syntax"
msgstr "Sintassi"

#: 04060116.xhp#par_id3153082.97.help.text
msgid "IMCONJUGATE(\"ComplexNumber\")"
msgstr "COMP.CONIUGATO(\"Numero complesso\")"

#: 04060116.xhp#hd_id3153326.99.help.text
msgctxt "04060116.xhp#hd_id3153326.99.help.text"
msgid "Example"
msgstr "Esempio"

#: 04060116.xhp#par_id3149688.100.help.text
msgid "            <item type=\"input\">=IMCONJUGATE(\"1+j\")</item> returns 1-j."
msgstr "<item type=\"input\">=COMP.CONIUGATO(\"1+j\")</item> restituisce 1-j."

#: 04060116.xhp#bm_id3150898.help.text
msgid "<bookmark_value>IMLN function</bookmark_value>"
msgstr "<bookmark_value>COMP.LN, funzione</bookmark_value>"

#: 04060116.xhp#hd_id3150898.101.help.text
msgid "IMLN"
msgstr "COMP.LN"

#: 04060116.xhp#par_id3146853.102.help.text
msgid ""
"<ahelp hid=\"HID_AAI_FUNC_IMLN\">The result is the natural logarithm (to the "
"base e) of a complex number.</ahelp> The constant e has a value of "
"approximately 2.71828182845904."
msgstr ""
"<ahelp hid=\"HID_AAI_FUNC_IMLN\">Restituisce il logaritmo naturale (in base e) "
"di un numero complesso.</ahelp> La costante ha un valore approssimativo di "
"2,71828182845904."

#: 04060116.xhp#hd_id3150008.103.help.text
msgctxt "04060116.xhp#hd_id3150008.103.help.text"
msgid "Syntax"
msgstr "Sintassi"

#: 04060116.xhp#par_id3155954.104.help.text
msgid "IMLN(\"ComplexNumber\")"
msgstr "COMP.LN(\"Numero complesso\")"

#: 04060116.xhp#hd_id3153565.106.help.text
msgctxt "04060116.xhp#hd_id3153565.106.help.text"
msgid "Example"
msgstr "Esempio"

#: 04060116.xhp#par_id3153736.107.help.text
msgid ""
"            <item type=\"input\">=IMLN(\"1+j\")</item> returns 0.35+0.79j "
"(rounded)."
msgstr ""
"<item type=\"input\">=COMP.LN(\"1+j\")</item> restituisce 0,35+0,79j "
"(arrotondato)."

#: 04060116.xhp#bm_id3155929.help.text
msgid "<bookmark_value>IMLOG10 function</bookmark_value>"
msgstr "<bookmark_value>COMP.LOG10, funzione</bookmark_value>"

#: 04060116.xhp#hd_id3155929.108.help.text
msgid "IMLOG10"
msgstr "COMP.LOG10"

#: 04060116.xhp#par_id3149882.109.help.text
msgid ""
"<ahelp hid=\"HID_AAI_FUNC_IMLOG10\">The result is the common logarithm (to the "
"base 10) of a complex number.</ahelp>"
msgstr ""
"<ahelp hid=\"HID_AAI_FUNC_IMLOG10\">Restituisce il logaritmo comune (in base "
"10) di un numero complesso.</ahelp>"

#: 04060116.xhp#hd_id3154327.110.help.text
msgctxt "04060116.xhp#hd_id3154327.110.help.text"
msgid "Syntax"
msgstr "Sintassi"

#: 04060116.xhp#par_id3150128.111.help.text
msgid "IMLOG10(\"ComplexNumber\")"
msgstr "COMP.LOG10(\"Numero complesso\")"

#: 04060116.xhp#hd_id3149003.113.help.text
msgctxt "04060116.xhp#hd_id3149003.113.help.text"
msgid "Example"
msgstr "Esempio"

#: 04060116.xhp#par_id3151021.114.help.text
msgid ""
"            <item type=\"input\">=IMLOG10(\"1+j\")</item> returns 0.15+0.34j "
"(rounded)."
msgstr ""
"<item type=\"input\">=COMP.LOG10(\"1+j\")</item> restituisce 0,15+0,34j "
"(arrotondato)."

#: 04060116.xhp#bm_id3155623.help.text
msgid "<bookmark_value>IMLOG2 function</bookmark_value>"
msgstr "<bookmark_value>COMP.LOG2, funzione</bookmark_value>"

#: 04060116.xhp#hd_id3155623.115.help.text
msgid "IMLOG2"
msgstr "COMP.LOG2"

#: 04060116.xhp#par_id3150932.116.help.text
msgid ""
"<ahelp hid=\"HID_AAI_FUNC_IMLOG2\">The result is the binary logarithm of a "
"complex number.</ahelp>"
msgstr ""
"<ahelp hid=\"HID_AAI_FUNC_IMLOG2\">Restituisce il logaritmo in base 2 di un "
"numero complesso.</ahelp>"

#: 04060116.xhp#hd_id3153046.117.help.text
msgctxt "04060116.xhp#hd_id3153046.117.help.text"
msgid "Syntax"
msgstr "Sintassi"

#: 04060116.xhp#par_id3145355.118.help.text
msgid "IMLOG2(\"ComplexNumber\")"
msgstr "COMP.LOG2(\"Numero complesso\")"

#: 04060116.xhp#hd_id3148768.120.help.text
msgctxt "04060116.xhp#hd_id3148768.120.help.text"
msgid "Example"
msgstr "Esempio"

#: 04060116.xhp#par_id3149536.121.help.text
msgid ""
"            <item type=\"input\">=IMLOG2(\"1+j\")</item> returns 0.50+1.13j "
"(rounded)."
msgstr ""
"<item type=\"input\">=COMP.LOG2(\"1+j\")</item> restituisce 0,50+1,13j "
"(arrotondato)."

#: 04060116.xhp#bm_id3145626.help.text
msgid "<bookmark_value>IMPRODUCT function</bookmark_value>"
msgstr "<bookmark_value>COMP.PRODOTTO, funzione</bookmark_value>"

#: 04060116.xhp#hd_id3145626.122.help.text
msgid "IMPRODUCT"
msgstr "COMP.PRODOTTO"

#: 04060116.xhp#par_id3153545.123.help.text
msgid ""
"<ahelp hid=\"HID_AAI_FUNC_IMPRODUCT\">The result is the product of up to 29 "
"complex numbers.</ahelp>"
msgstr ""
"<ahelp hid=\"HID_AAI_FUNC_IMPRODUCT\">Restituisce il prodotto di più numeri "
"complessi (fino a 29).</ahelp>"

#: 04060116.xhp#hd_id3149388.124.help.text
msgctxt "04060116.xhp#hd_id3149388.124.help.text"
msgid "Syntax"
msgstr "Sintassi"

#: 04060116.xhp#par_id3149027.125.help.text
msgid "IMPRODUCT(\"ComplexNumber\"; \"ComplexNumber1\"; ...)"
msgstr "COMP.PRODOTTO(\"Numero complesso\"; \"Numero complesso 1\"; ...)"

#: 04060116.xhp#hd_id3153228.127.help.text
msgctxt "04060116.xhp#hd_id3153228.127.help.text"
msgid "Example"
msgstr "Esempio"

#: 04060116.xhp#par_id3155815.128.help.text
msgid ""
"            <item type=\"input\">=IMPRODUCT(\"3+4j\";\"5-3j\")</item> returns "
"27+11j."
msgstr ""
"<item type=\"input\">=COMP.PRODOTTO(\"3+4j\"; \"5-3j\")</item> restituisce 27+11j."

#: 04060116.xhp#bm_id3147539.help.text
msgid "<bookmark_value>IMREAL function</bookmark_value>"
msgstr "<bookmark_value>COMP.PARTE.REALE, funzione</bookmark_value>"

#: 04060116.xhp#hd_id3147539.129.help.text
msgid "IMREAL"
msgstr "COMP.PARTE.REALE"

#: 04060116.xhp#par_id3155372.130.help.text
msgid ""
"<ahelp hid=\"HID_AAI_FUNC_IMREAL\">The result is the real coefficient of a "
"complex number.</ahelp>"
msgstr ""
"<ahelp hid=\"HID_AAI_FUNC_IMREAL\">Restituisce la parte reale di un numero "
"complesso.</ahelp>"

