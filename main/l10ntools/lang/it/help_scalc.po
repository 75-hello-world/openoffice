msgctxt "csv_files.xhp#par_idN107F4.help.text"
msgid "Click <item type=\"menuitem\">OK</item>."
msgstr "Fate clic su <item type=\"menuitem\">OK</item>."

#: csv_files.xhp#par_idN107FC.help.text
