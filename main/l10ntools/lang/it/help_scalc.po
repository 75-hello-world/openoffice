msgstr "3"

#: source/text/scalc/01/04060106.xhp#par_id3165956.476
msgid  "COUNTA"
msgstr "CONTA.VALORI"

#: source/text/scalc/01/04060106.xhp#par_id3165983.477
msgid  "4"
msgstr "4"

#: source/text/scalc/01/04060106.xhp#par_id3166006.478
msgid  "MAX"
msgstr "MAX"

#: source/text/scalc/01/04060106.xhp#par_id3166033.479
msgid  "5"
msgstr "5"

#: source/text/scalc/01/04060106.xhp#par_id3166056.480
msgid  "MIN"
msgstr "MIN"

#: source/text/scalc/01/04060106.xhp#par_id3143316.481
msgid  "6"
msgstr "6"

#: source/text/scalc/01/04060106.xhp#par_id3143339.482
msgid  "PRODUCT"
msgstr "PRODOTTO"

#: source/text/scalc/01/04060106.xhp#par_id3143366.483
msgid  "7"
msgstr "7"

#: source/text/scalc/01/04060106.xhp#par_id3143389.484
msgid  "STDEV"
msgstr "DEV.ST"

#: source/text/scalc/01/04060106.xhp#par_id3143416.485
msgid  "8"
msgstr "8"

#: source/text/scalc/01/04060106.xhp#par_id3143439.486
msgid  "STDEVP"
msgstr "DEV.ST.POP"

#: source/text/scalc/01/04060106.xhp#par_id3143466.487
msgid  "9"
msgstr "9"

#: source/text/scalc/01/04060106.xhp#par_id3143489.488
msgid  "SUM"
msgstr "SOMMA"

#: source/text/scalc/01/04060106.xhp#par_id3143516.489
msgid  "10"
msgstr "10"

#: source/text/scalc/01/04060106.xhp#par_id3143539.490
msgid  "VAR"
msgstr "VAR"

