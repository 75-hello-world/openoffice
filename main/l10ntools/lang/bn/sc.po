#: scfuncs.src#RID_SC_FUNCTION_DESCRIPTIONS2.SC_OPCODE_BASE.4.string.text
msgctxt "scfuncs.src#RID_SC_FUNCTION_DESCRIPTIONS2.SC_OPCODE_BASE.4.string.text"
msgid "radix"
msgstr "সংখ্যা পদ্ধতির পরিচায়ক"

