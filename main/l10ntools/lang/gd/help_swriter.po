msgstr ""

#: 06100000.xhp#par_id3152946.20.help.text
msgid ""
"<ahelp hid=\"SW:RADIOBUTTON:DLG_SORTING:RB_DN3\">Sorts in descending order "
"(for example, 9, 8, 7 or z, y, x).</ahelp>"
msgstr ""

