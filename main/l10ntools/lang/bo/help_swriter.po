#: indices_form.xhp#hd_id3155855.38.help.text
msgid ""
"<variable id=\"indices_form\"><link href=\"text/swriter/guide/indices_form.xhp\" "
"name=\"Formatting an Index or a Table of Contents\">Formatting an Index or a "
"Table of Contents</link></variable>"
msgstr ""
