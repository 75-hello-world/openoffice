msgid  "Entry of the ranges to be converted..."
msgstr "བརྗེ་རྩིས་ཀྱི་ས་ཁོངས་ནང་བཅུག..."

#: source/euro/euro.src#STATUSLINE + 4.String.Text
msgid  "Sheet protection for each sheet will be restored..."
msgstr "རེའུ་མིག་རེའི་སྲུང་སྐྱོབ་སླང་ཡང་བསྐྱར་གསོ་བྱེད།..."

#: source/euro/euro.src#STATUSLINE + 5.String.Text
msgid  "Conversion of the currency units in the cell templates..."
msgstr "དྲ་མིག་བཟོ་ལྟའི་ནང་དངུལ་ལོའི་ཚན་པའི་བརྗེ་སྒྱུར།..."

#: source/euro/euro.src#MESSAGES.String.Text
msgid  "~Finish"
msgstr "ལེགས་སྒྲུབ།(~F)"

#: source/euro/euro.src#MESSAGES + 1.String.Text
msgid  "Select directory"
msgstr "དཀར་ཆག་གཅིག་འདེམས་རོགས།"

#: source/euro/euro.src#MESSAGES + 2.String.Text
msgid  "Select file"
msgstr "ཁྱེད་རང་གི་ཡིག་ཆ་གཅིག་འདེམས་རོག"

#: source/euro/euro.src#MESSAGES + 3.String.Text
msgid  "Select target directory"
msgstr "དམིགས་འབེན་དཀར་ཆག་གཅིག་འདེམས་རོག"

#: source/euro/euro.src#MESSAGES + 4.String.Text
msgid  "non-existant"
msgstr "གནས་མེད་་པ།"

#: source/euro/euro.src#MESSAGES + 5.String.Text
msgid  "Euro Converter"
msgstr "ཡོ་སྒོར་བརྗེ་རྩིས་ཆས།"

#: source/euro/euro.src#MESSAGES + 6.String.Text
msgid  "Should protected spreadsheets be temporarily unprotected?"
msgstr "སྲུང་སྐྱོབ་ཐོབཔའི་རེའུ་མིག་གནས་སྐབས་སྲུང་སྐྱོབ་རྩིས་མེད་གཏོང་ངམ།"

#: source/euro/euro.src#MESSAGES + 7.String.Text
msgid  "Enter the password to unprotect the table %1TableName%1"
msgstr "གསང་ཨང་གཅུག་རོགས་ རེའུ་མིག%1TableName%1སྲུང་སྐྱོབ་འཆིང་རྒྱབ་རྩེས་མེད་ཐོངས།"

#: source/euro/euro.src#MESSAGES + 8.String.Text
msgid  "Wrong Password!"
msgstr "གསང་ཨང་ཕན་མེད།"

#: source/euro/euro.src#MESSAGES + 9.String.Text
msgid  "Protected Sheet"
msgstr "སྲུང་སྐྱོབ་ཐོབ་པའི་རེའུ་མིག"

#: source/euro/euro.src#MESSAGES + 10.String.Text
msgid  "Warning!"
msgstr "མཐའ་ཚིག"

#: source/euro/euro.src#MESSAGES + 11.String.Text
msgid  "Protection for the sheets will not be removed."
msgstr "རེའུ་མིག་གི་སྲུང་སྐྱོབ་རྩིས་མེད་གཏོང་ཐབས་བྲལ།"

#: source/euro/euro.src#MESSAGES + 12.String.Text
msgid  "Sheet cannot be unprotected"
msgstr "རེའུ་མིག་གི་སྲུང་སྐྱོབ་རྩིས་མེད་གཏོང་ཐབས་བྲལ།"

#: source/euro/euro.src#MESSAGES + 13.String.Text
msgid  "The Wizard cannot edit this document as cell formats cannot be modified in documents containing protected spreadsheets."
msgstr "རང་འགུལ་ཡིག་ཆའི་ལས་རོགས་ཀྱིས་ཡིག་ཚགས་འདེ་རྩོམ་སྒྲིག་བྱེད་ཐབས་བྲལ་ཡིག་ཚགས་ནང་སྲུང་སྐྱོབ་ཐོབ་པའི་ལས་ཁྲ་ཚུད་པའི་སྐབས་དྲ་མིག་གི་བའི་བཟོ་ལྟ་བཟོ་བཅོས་བྱེད་ཐབས་བྲལ་པའི་རྐྱེན་གྱི་་ཡིན།"

#: source/euro/euro.src#MESSAGES + 14.String.Text
msgid  "Please note that the Euro Converter will, otherwise, not be able to edit this document!"
msgstr "དོ་སྣང་བྱེད། ད་ལྟ་མིན་ན་ཡོ་སྒོར་བརྗེ་རྩིས་ཆས་ཀྱི་ཡིག་ཚགས་འདི་རྩོམ་སྒྲིག་བྱེད་ཐབས་བྲལ།"

#: source/euro/euro.src#MESSAGES + 15.String.Text
msgid  "Please choose a currency to be converted first!"
msgstr "སྔོན་ལ་སྒྱུར་བརྗེའི་བྱེད་པའི་དངུལ་ལོར་འདེམས་རོགས།"

