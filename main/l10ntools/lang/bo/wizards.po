#: source/euro/euro.src#MESSAGES + 17.String.Text
msgid  "OK"
msgstr "གཏན་འཕེལ།"

#: source/euro/euro.src#MESSAGES + 18.String.Text
msgid  "Cancel"
msgstr "རྩེས་མེད།"

#: source/euro/euro.src#MESSAGES + 19.String.Text
msgid  "Please select a %PRODUCTNAME Calc document for editing!"
msgstr "ཁྱེད་རང་གྱི་རྩོམ་སྒྲིག་བྱེད་པའི་ %PRODUCTNAME Calc ཡིག་ཚགས་འདེམས་རོག"

#: source/euro/euro.src#MESSAGES + 20.String.Text
msgid  "'<1>' is not a directory!"
msgstr "'<1>' དཀར་ཆག་གཅིག་མ་རེད།"

