msgstr "TANH"

#: 04060106.xhp#par_id3165462.457.help.text
msgid ""
"<ahelp hid=\"HID_FUNC_TANHYP\">Returns the hyperbolic tangent of a number.</"
"ahelp>"
msgstr ""
"<ahelp hid=\"HID_FUNC_TANHYP\">숫자의 쌍곡선 탄젠트를 구합니다.</ahelp>"

#: 04060106.xhp#hd_id3165480.458.help.text
msgctxt "04060106.xhp#hd_id3165480.458.help.text"
msgid "Syntax"
msgstr "구문"

