msgid  "---"
msgstr "---"

#: source/text/shared/01/06040100.xhp#par_id3154690.38
msgid  "0.5pt single underline"
msgstr "0,5pt 아래 단일 선"

#: source/text/shared/01/06040100.xhp#par_id3154472.39
msgid  "___"
msgstr "___"

#: source/text/shared/01/06040100.xhp#par_id3149266.40
msgid  "1.0pt single underline"
msgstr "1,0pt 아래 단일 선"

#: source/text/shared/01/06040100.xhp#par_id3147580.41
msgid  "==="
msgstr "==="

#: source/text/shared/01/06040100.xhp#par_id3145364.42
msgid  "1.1pt double underline"
msgstr "1,1pt 아래 이중 선"

#: source/text/shared/01/06040100.xhp#par_id3148647.43
msgid  "***"
