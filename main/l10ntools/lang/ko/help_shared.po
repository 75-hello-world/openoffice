msgstr ""

#: source/text/shared/autopi/01100400.xhp#hd_id3149233.6
msgid  "Portrait"
msgstr "세로"

#: source/text/shared/autopi/01100400.xhp#par_id3154285.14
msgid  "\<ahelp hid=\"HID_DLGREPORT_4_PORTRAIT\"\>Selects a portrait page orientation for the report.\</ahelp\>"
msgstr ""

#: source/text/shared/autopi/01100400.xhp#par_id3148491.15
