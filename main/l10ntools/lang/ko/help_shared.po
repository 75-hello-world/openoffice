msgid  "\<image src=\"res/sc10714.png\" id=\"img_id3153894\"\>\<alt id=\"alt_id3153894\"\>Icon\</alt\>\</image\>"
msgstr ""

#: source/text/shared/02/12100000.xhp#par_id3147143.3
msgid  "Sort Order"
msgstr "정렬 순서"

#: source/text/shared/02/12100100.xhp#tit
msgid  "Sort Order"
msgstr "정렬 순서"

#: source/text/shared/02/12100100.xhp#bm_id3147000
msgid  "\<bookmark_value\>sorting; databases\</bookmark_value\>\<bookmark_value\>databases; sorting\</bookmark_value\>"
msgstr ""

