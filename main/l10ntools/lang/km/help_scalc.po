msgstr "<item type=\"input\">បាន​ទទួល</item>"

#: source/text/scalc/01/04060118.xhp#par_id3153277.310
msgid  "2"
msgstr "២"

#: source/text/scalc/01/04060118.xhp#par_id3154052.203
