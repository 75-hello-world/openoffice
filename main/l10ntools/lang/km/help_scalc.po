msgid "Left"
msgstr "ឆ្វេង"

#: 02140600.xhp#hd_id3154762.35.help.text
msgid "Start Value"
msgstr "តម្លៃ​ចាប់ផ្តើម"

#: 02140600.xhp#hd_id3154957.31.help.text
msgid "Month"
msgstr "ខែ"

#: 02140600.xhp#hd_id3155738.8.help.text
