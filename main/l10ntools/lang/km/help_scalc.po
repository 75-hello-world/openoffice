msgstr "<image id=\"img_id3145785\" src=\"sc/res/imglst/apptbx/sc26049.png\" width=\"5.64mm\" height=\"5.64mm\"><alt id=\"alt_id3145785\">រូប​តំណាង​ </alt></image>"

