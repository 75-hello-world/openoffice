msgstr "<image id=\"img_id3153287\" src=\"res/commandimagelist/sc_group.png\" width=\"0.222inch\" height=\"0.222inch\"><alt id=\"alt_id3153287\">រូប​តំណាង​</alt></image>"

#: source/text/scalc/00/00000412.xhp#par_id3150214.36
msgid  "Group"
msgstr "ដាក់​ជា​ក្រុម"

#: source/text/scalc/00/00000412.xhp#par_id3146781.37
msgid  "Choose \<emph\>Data - Group and Outline - Ungroup\</emph\>"
msgstr "ជ្រើស <emph>ទិន្នន័យ - ក្រុម និង​គ្រោង - ដោះក្រុម</emph>"

#: source/text/scalc/00/00000412.xhp#par_id3150892.52
msgid  "\<switchinline select=\"sys\"\>\<caseinline select=\"MAC\"\>Command\</caseinline\>\<defaultinline\>Ctrl\</defaultinline\>\</switchinline\>+F12"
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><defaultinline>បញ្ជា(Ctrl)</defaultinline></switchinline>+F12"

#: source/text/scalc/00/00000412.xhp#par_id3155097.38
msgid  "On \<emph\>Tools\</emph\> bar, click"
msgstr "លើ​របារ <emph>ឧបករណ៍</emph> ចុច"

#: source/text/scalc/00/00000412.xhp#par_id3150048
msgid  "\<image id=\"img_id3155914\" src=\"res/commandimagelist/sc_ungroup.png\" width=\"0.222inch\" height=\"0.222inch\"\>\<alt id=\"alt_id3155914\"\>Icon\</alt\>\</image\>"
msgstr "<image id=\"img_id3155914\" src=\"res/commandimagelist/sc_ungroup.png\" width=\"0.222inch\" height=\"0.222inch\"><alt id=\"alt_id3155914\">រូប​តំណាង </alt></image>"

#: source/text/scalc/00/00000412.xhp#par_id3153555.39
msgid  "Ungroup"
msgstr "បំបែក​ក្រុម"

#: source/text/scalc/00/00000412.xhp#par_id3153008.40
msgid  "\<variable id=\"dnglagl\"\>Choose \<emph\>Data - Group and Outline - AutoOutline\</emph\>\</variable\>"
msgstr "<variable id=\"dnglagl\">ជ្រើស <emph>ទិន្នន័យ - ក្រុម និង​គ្រោង - គ្រោង​ស្វ័យ​ប្រវត្តិ</emph></variable>"

#: source/text/scalc/00/00000412.xhp#par_id3154709.41
msgid  "\<variable id=\"dnglef\"\>Choose \<emph\>Data - Group and Outline - Remove\</emph\>\</variable\>"
msgstr "<variable id=\"dnglef\">ជ្រើស <emph>ទិន្នន័យ - ក្រុម និង​គ្រោង - យកចេញ</emph></variable>"

#: source/text/scalc/00/00000412.xhp#par_id1774346
msgid  "\<variable id=\"dngdrill\"\>Choose \<emph\>Data - Group and Outline - Show Details\</emph\> (for some pivot tables)\</variable\>"
msgstr "<variable id=\"dngdrill\">ជ្រើស <emph>ទិន្នន័យ - ក្រុម និង​គ្រោង - បង្ហាញ​សេចក្ដី​លម្អិត</emph> (សម្រាប់​តារាង​​អ្នក​ជំនួយ​ការ​​ទិន្នន័យ) </variable>"

#: source/text/scalc/00/00000412.xhp#par_id3155759.42
msgid  "\<variable id=\"dndtpt\"\>Choose \<emph\>Data - Pivot Table\</emph\>\</variable\>"
msgstr "<variable id=\"dndtpt\">ជ្រើស​ <emph>ទិន្នន័យ - អ្នក​ជំនួយ​ការ​ទិន្នន័យ​</emph></variable>"

#: source/text/scalc/00/00000412.xhp#par_id3154625.43
msgid  "\<variable id=\"dndpa\"\>Choose \<emph\>Data - Pivot Table - Create\</emph\>\</variable\>"
msgstr "<variable id=\"dndpa\">ជ្រើស​ <emph>ទិន្នន័យ - អ្នក​ជំនួយ​ការ​ទិន្នន័យ​ - ចាប់ផ្តើម​</emph></variable>"

#: source/text/scalc/00/00000412.xhp#par_id3147558.53
msgid  "\<variable id=\"dndq\"\>Choose \<emph\>Data - Pivot Table - Create\</emph\>, in the Select Source dialog choose the option \<emph\>Data source registered in $[officename]\</emph\>.\</variable\>"
msgstr "<variable id=\"dndq\">ជ្រើស <emph>ទិន្នន័យ - អ្នក​ជំនួយ​ការ​​ទិន្នន័យ - ចាប់ផ្តើម</emph> ក្នុង​ប្រអប់ \"ជ្រើស​ប្រភព\" ជ្រើស​ជម្រើស <emph>ប្រភព​ទិន្នន័យ​ត្រូវ​បាន​ចុះ​ឈ្មោះ​ក្នុង​ $[officename]</emph> ។</variable>"

#: source/text/scalc/00/00000412.xhp#par_id3153297.50
msgid  "Choose \<emph\>Data - Pivot Table - Create\</emph\>, in the Select Source dialog choose the option \<emph\>Current selection\</emph\>."
msgstr "ជ្រើស​ <emph>ទិន្នន័យ​ - អ្នក​ជំនួយ​ការ​ទិន្នន័យ​ - ចាប់ផ្តើម​</emph> ក្នុង​ប្រអប់ \"ជ្រើស​ប្រភព\" ជ្រើស​ជម្រើស​ <emph>ជម្រើស​បច្ចុប្បន្ន​</emph> ។"

#: source/text/scalc/00/00000412.xhp#par_id3145118.54
msgid  "Choose \<emph\>Data - Pivot Table - Create\</emph\>, in the Select Source dialog choose the option \<emph\>Data source registered in $[officename]\</emph\>, click \<emph\>OK\</emph\> to see \<emph\>Select Data Source\</emph\> dialog."
msgstr "ជ្រើស <emph>ទិន្នន័យ - អ្នក​ជំនួយ​ការ​ទិន្នន័យ - ចាប់ផ្តើម</emph> ក្នុង​ប្រអប់ \"ជ្រើស​ប្រភព\" ជ្រើស​ជម្រើស <emph>ប្រភព​ទិន្នន័យ​ត្រូវ​បាន​ចុះ​ឈ្មោះ​ក្នុង​ $[officename]</emph> ចុច <emph>យល់​ព្រម​</emph> ដើម្បី​មើល​ប្រអប់ <emph>ជ្រើស​ប្រភព​ទិន្នន័យ</emph> ។"

#: source/text/scalc/00/00000412.xhp#par_id3153294.44
msgid  "\<variable id=\"dndpak\"\>Choose \<emph\>Data - Pivot Table - Refresh\</emph\>\</variable\>"
msgstr "<variable id=\"dndpak\">ជ្រើស​ <emph>ទិន្នន័យ​ - អ្នក​ជំនួយ​ការ​ទិន្នន័យ​ - ធ្វើ​ឲ្យ​ស្រស់​</emph></variable>"

#: source/text/scalc/00/00000412.xhp#par_id3151344.45
msgid  "\<variable id=\"dndploe\"\>Choose \<emph\>Data - Pivot Table - Delete\</emph\>\</variable\>"
msgstr "<variable id=\"dndploe\">ជ្រើស​ <emph>ទិន្នន័យ​ - អ្នក​ជំនួយ​ការ​ទិន្នន័យ​ - លុប</emph></variable>"

#: source/text/scalc/00/00000412.xhp#par_id3150397.46
msgid  "\<variable id=\"dndakt\"\>Choose \<emph\>Data - Refresh Range\</emph\>\</variable\>"
msgstr "<variable id=\"dndakt\">ជ្រើស <emph>ទិន្នន័យ - ធ្វើ​ឲ្យ​ជួរ​ស្រស់</emph></variable>"

#: source/text/scalc/00/00000412.xhp#par_idN10B8F
msgid  "\<variable id=\"grouping\"\>Choose \<emph\>Data - Group and Outline - Group\</emph\>\</variable\>"
msgstr "<variable id=\"grouping\">ជ្រើស <emph>ទិន្នន័យ - ក្រុម និង​គ្រោង - ក្រុម</emph></variable>"

#: source/text/scalc/01/01120000.xhp#tit
msgid  "Page Preview"
msgstr "មើល​ទំព័រ​ជា​មុន"

#: source/text/scalc/01/01120000.xhp#hd_id1918698
msgid  "\<link href=\"text/scalc/01/01120000.xhp\"\>Page Preview\</link\>"
msgstr "<link href=\"text/scalc/01/01120000.xhp\">មើល​ទំព័រ​ជាមុន</link>"

#: source/text/scalc/01/01120000.xhp#par_id3831598
msgid  "\<ahelp hid=\".uno:PrintPreview\"\>Displays a preview of the printed page or closes the preview.\</ahelp\>"
msgstr "<ahelp hid=\".uno:PrintPreview\">បង្ហាញ​ការមើល​ជា​មុន​នៃ​ទំព័រ​ដែលបាន​បោះពុម្ព ឬ​បិទ​ការ​មើល​ជាមុន ។</ahelp>"

#: source/text/scalc/01/01120000.xhp#par_id3145847
msgid  "Use the icons on the \<emph\>Page Preview Bar\</emph\> to scroll through the pages of the document or to print the document."
msgstr "ប្រើ​រូបតំណាង​លើ <emph>របារ​មើល​ទំព័រ​ជាមុន</emph> ដើម្បី​រមូរ​តាម​ទំព័រ​ឯកសារ ឬ​ដើម្បី​បោះពុម្ព​ឯកសារ ។"

#: source/text/scalc/01/01120000.xhp#par_id9838862
msgid  "You can also press \<switchinline select=\"sys\"\>\<caseinline select=\"MAC\"\>Command\</caseinline\>\<defaultinline\>Ctrl\</defaultinline\>\</switchinline\>+Page Up and Ctrl+Page Down keys to scroll through the pages."
msgstr "អ្នក​ក៏អាច​ចុច <switchinline select=\"sys\"><caseinline select=\"MAC\">ពាក្យ​បញ្ជា</caseinline><defaultinline>បញ្ជា (Ctrl)</defaultinline></switchinline>+ទំព័រ​លើ (Page Up) និង​បញ្ជា (Ctrl)+ទំព័រ​ក្រោម ដើម្បី​រមូរ​កាត់​ទំព័រ ។"

#: source/text/scalc/01/01120000.xhp#par_id7211828
msgid  "You cannot edit your document while you are in the page preview."
msgstr "អ្នក​មិនអាច​កែសម្រួល​ឯកសារ​របស់អ្នក នៅ​ខណៈ​ដែល​អ្នក​នៅក្នុង​ការ​មើល​ទំព័រ​ជាមុន​នោះទេ ។"

#: source/text/scalc/01/01120000.xhp#par_id460829
msgid  "\<ahelp hid=\".\" visibility=\"hidden\"\>To exit the page preview, click the \<emph\>Close Preview\</emph\> button.\</ahelp\>"
msgstr "<ahelp hid=\".\" visibility=\"hidden\">ដើម្បី​ចេញ​ពី​ការមើល​ទំព័រ​ជាមុន គឺ​ត្រូវ​ចុច​លើ​ប៊ូតុង <emph>បិទ​ការ​មើល​ជា​មុន</emph> ។</ahelp>"

#: source/text/scalc/01/01120000.xhp#par_id3155829
msgid  "\<link href=\"text/scalc/main0210.xhp\" name=\"Page View Object Bar\"\>Page View Object Bar\</link\>"
msgstr "<link href=\"text/scalc/main0210.xhp\" name=\"Page View Object Bar\">របារ​វត្ថុ​ទិដ្ឋភាព​ទំព័រ</link>"

#: source/text/scalc/01/02110000.xhp#tit
msgid  "Navigator"
msgstr "កម្មវិធី​រុករក​"

#: source/text/scalc/01/02110000.xhp#bm_id3150791
msgid  "\<bookmark_value\>Navigator;for sheets\</bookmark_value\>\<bookmark_value\>navigating;in spreadsheets\</bookmark_value\>\<bookmark_value\>displaying; scenario names\</bookmark_value\>\<bookmark_value\>scenarios;displaying names\</bookmark_value\>"
msgstr "<bookmark_value>កម្មវិធី​រុករក សម្រាប់​សន្លឹក</bookmark_value><bookmark_value>ការរុករុក​ នៅក្នុង​សៀវភៅ​បញ្ជី</bookmark_value><bookmark_value>ការបង្ហាញ ឈ្មោះ​ឆាក</bookmark_value><bookmark_value>ឆាក ឈ្មោះ​បង្ហាញ</bookmark_value>"

#: source/text/scalc/01/02110000.xhp#hd_id3150791.1
msgid  "\<link href=\"text/scalc/01/02110000.xhp\" name=\"Navigator\"\>Navigator\</link\>"
msgstr "<link href=\"text/scalc/01/02110000.xhp\" name=\"Navigator\">កម្មវិធី​រុករក</link>"

#: source/text/scalc/01/02110000.xhp#par_id3156422.2
msgid  "\<ahelp hid=\".uno:Navigator\"\>Activates and deactivates the Navigator.\</ahelp\> The Navigator is a \<link href=\"text/shared/00/00000005.xhp#andocken\" name=\"dockable window\"\>dockable window\</link\>."
msgstr "<ahelp hid=\".uno:Navigator\">ធ្វើ​ឲ្យកម្មវិធី​រុករក​សកម្ម និង​អសកម្ម ។</ahelp> កម្មវិធី​រុករក​គឺ​ជា <link href=\"text/shared/00/00000005.xhp#andocken\" name=\"dockable window\">បង្អួច​ដែល​អាច​ចូល​ផែ​បាន</link> ។"

#: source/text/scalc/01/02110000.xhp#par_id3145271.40
msgid  "Choose \<emph\>View - Navigator\</emph\> to display the Navigator."
msgstr "ជ្រើស <emph>មើល - កម្មវិធី​រុករក</emph> ដើម្បី​បង្ហាញ​កម្មវិធី​រុករក ។"

#: source/text/scalc/01/02110000.xhp#hd_id3159155.4
msgid  "Column"
msgstr "ជួរ​ឈរ"

#: source/text/scalc/01/02110000.xhp#par_id3146984.5
msgid  "\<ahelp hid=\"HID_SC_NAVIPI_COL\"\>Enter the column letter. Press Enter to reposition the cell cursor to the specified column in the same row.\</ahelp\>"
msgstr "<ahelp hid=\"HID_SC_NAVIPI_COL\">បញ្ចូល​តួអក្សរ​ជួរ​ឈរ ។ សង្កត់ បញ្ចូល ដើម្បី​ដាក់​ទីតាំង​ទស្សន៍​ទ្រនិច​ក្រឡា​ឡើងវិញ ទៅ​ជួរ​ឈរ​ដែលបាន​បញ្ជាក់​ក្នុង​ជួរដេក​ដូចគ្នា ។</ahelp>"

#: source/text/scalc/01/02110000.xhp#hd_id3147126.6
msgid  "Row"
msgstr "ជួរ​ដេក"

#: source/text/scalc/01/02110000.xhp#par_id3149958.7
msgid  "\<ahelp hid=\"HID_SC_NAVIPI_ROW\"\>Enter a row number. Press Enter to reposition the cell cursor to the specified row in the same column.\</ahelp\>"
msgstr "<ahelp hid=\"HID_SC_NAVIPI_ROW\">បញ្ចូល​លេខ​ជួរ​ឈរ ។ សង្កត់ បញ្ចូល ដើម្បី​ដាក់ទីតាំង​ទស្សន៍​ទ្រនិច​ក្រឡា ទៅ​ជួរ​ដេក​ដែលបាន​បញ្ជាក់​ក្នុង​ជួរ​ឈរ​ដូចគ្នា ។</ahelp>"

#: source/text/scalc/01/02110000.xhp#hd_id3150717.8
msgid  "Data Range"
msgstr "ជួរ​ទិន្នន័យ"

#: source/text/scalc/01/02110000.xhp#par_id3150752.10
msgid  "\<ahelp hid=\"HID_SC_NAVIPI_DATA\"\>Specifies the current data range denoted by the position of the cell cursor.\</ahelp\>"
msgstr "<ahelp hid=\"HID_SC_NAVIPI_DATA\">បញ្ជាក់​លម្អិត​ជួរ​ទិន្នន័យ​បច្ចុប្បន្ន ដែល​បាន​ចង្អុល​បង្ហាញ​ដោយ​ទីតាំង​នៃ​ទស្សន៍​ទ្រនិច​ក្រឡា ។</ahelp>"

#: source/text/scalc/01/02110000.xhp#par_id3159264
msgid  "\<image id=\"img_id3147338\" src=\"res/commandimagelist/sc_grid.png\" width=\"0.2228in\" height=\"0.2228in\"\>\<alt id=\"alt_id3147338\"\>Icon\</alt\>\</image\>"
msgstr "<image id=\"img_id3147338\" src=\"res/commandimagelist/sc_grid.png\" width=\"0.2228in\" height=\"0.2228in\"><alt id=\"alt_id3147338\">រូបតំណាង</alt></image>"

#: source/text/scalc/01/02110000.xhp#par_id3146919.9
msgid  "Data Range"
msgstr "ជួរ​ទិន្នន័យ"

#: source/text/scalc/01/02110000.xhp#hd_id3148488.14
msgid  "Start"
msgstr "ចាប់ផ្តើម"

#: source/text/scalc/01/02110000.xhp#par_id3150086.16
msgid  "\<ahelp hid=\"HID_SC_NAVIPI_UP\"\>Moves to the cell at the beginning of the current data range, which you can highlight using the \<emph\>Data Range\</emph\> button.\</ahelp\>"
msgstr "<ahelp hid=\"HID_SC_NAVIPI_UP\">ផ្លាស់ទី​ទៅ​ក្រឡា​នៅ​ដើម​នៃ​ជួរ​ទិន្នន័យ​បច្ចុប្បន្ន ដែល​អ្នក​អាច​បន្លិច​ដោយ​ប្រើ​ប៊ូតុង <emph>ជួរ​ទិន្នន័យ</emph> ។</ahelp>"

#: source/text/scalc/01/02110000.xhp#par_id3152994
msgid  "\<image id=\"img_id3150515\" src=\"sw/imglst/sc20186.png\" width=\"0.2228in\" height=\"0.2228in\"\>\<alt id=\"alt_id3150515\"\>Icon\</alt\>\</image\>"
msgstr "<image id=\"img_id3150515\" src=\"sw/imglst/sc20186.png\" width=\"0.2228in\" height=\"0.2228in\"><alt id=\"alt_id3150515\">រូបតំណាង</alt></image>"

#: source/text/scalc/01/02110000.xhp#par_id3154372.15
msgid  "Start"
msgstr "ចាប់ផ្តើម"

#: source/text/scalc/01/02110000.xhp#hd_id3146982.17
msgid  "End"
msgstr "ចុង (End)"

#: source/text/scalc/01/02110000.xhp#par_id3152985.19
msgid  "\<ahelp hid=\"HID_SC_NAVIPI_DOWN\"\>Moves to the cell at the end of the current data range, which you can highlight using the \<emph\>Data Range\</emph\> button.\</ahelp\>"
msgstr "<ahelp hid=\"HID_SC_NAVIPI_DOWN\">ផ្លាស់ទី​ទៅ​ក្រឡា​នៅ​ចុង​នៃ​ជួរ​ទិន្នន័យ​បច្ចុប្បន្ន ដែល​អ្នក​អាច​បន្លិច​ដោយ​ប្រើ​ប៊ូតុង  <emph>ជួរ​ទិន្នន័យ</emph> ។</ahelp>"

