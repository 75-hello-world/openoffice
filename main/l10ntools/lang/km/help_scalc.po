msgstr ""

#: source/text/scalc/01/04060116.xhp#hd_id3159117
msgid  "Syntax"
msgstr "វាក្យ​សម្ពន្ធ​​"

#: source/text/scalc/01/04060116.xhp#par_id3147416
msgid  "IMCSC(\"ComplexNumber\")"
