msgstr ""

#: source/text/shared/guide/language_select.xhp#tit
msgid  "Selecting the Document Language"
msgstr "ការ​ជ្រើស​ភាសា​ឯកសារ"

#: source/text/shared/guide/language_select.xhp#bm_id3083278
