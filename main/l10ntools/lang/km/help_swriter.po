msgstr ""
"<variable id=\"loeschentext\"><ahelp "
"hid=\".uno:DeleteRows\">លុប​ជួរដេក​ដែល​បានជ្រើស​ពី​តារាង ។</ahelp></variable>"

#: 05110500.xhp#tit.help.text
msgctxt "05110500.xhp#tit.help.text"
msgid "Delete"
msgstr "លុប"

