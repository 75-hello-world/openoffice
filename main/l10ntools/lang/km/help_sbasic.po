msgid  "\<variable id=\"err1\"\>1 An exception occurred\</variable\>"
msgstr "<variable id=\"err1\">មាន​ករណី​លើកលែង ១ </variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3155095.38
msgid  "\<variable id=\"err2\"\>2 Syntax error\</variable\>"
msgstr "<variable id=\"err2\">2​ កំហុស​វាក្យ​សម្ពន្ធ​មិន​ជាក់លាក់</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3149126.39
msgid  "\<variable id=\"err3\"\>3 Return without Gosub\</variable\>"
msgstr "<variable id=\"err3\">3 Return ដោយ​គ្មាន Gosub</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3153976.40
msgid  "\<variable id=\"err4\"\>4 Incorrect entry; please retry\</variable\>"
msgstr "<variable id=\"err4\">4 មិន​ធ្វើ​វិញ​ពី​ដើម​ដំបូង </variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3150891.41
msgid  "\<variable id=\"err5\"\>5 Invalid procedure call\</variable\>"
msgstr "<variable id=\"err5\">5 បែបបទ​ហៅ​មិន​ត្រឹមត្រូវ</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3159227.42
msgid  "\<variable id=\"err6\"\>6 Overflow\</variable\>"
msgstr "<variable id=\"err6\">6 លើស​ចំណុះ</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3154649.43
msgid  "\<variable id=\"err7\"\>7 Not enough memory\</variable\>"
msgstr "<variable id=\"err7\">7 អស់​អង្គ​ចងចាំ</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3150050.44
msgid  "\<variable id=\"err8\"\>8 Array already dimensioned\</variable\>"
msgstr "<variable id=\"err8\">8 អារេ​មាន​វិមាត្រ​រួច​ហើយ</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3148900.45
msgid  "\<variable id=\"err9\"\>9 Index out of defined range\</variable\>"
msgstr "<variable id=\"err9\">9 ទីតាំង​ធាតុ​មិន​ស្ថិត​ក្នុង​ជួរ</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3153806.46
msgid  "\<variable id=\"err10\"\>10 Duplicate definition\</variable\>"
msgstr "<variable id=\"err10\">10 ការ​កំណត់​​ជាន់​គ្នា</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3146963.47
msgid  "\<variable id=\"err11\"\>11 Division by zero\</variable\>"
msgstr "<variable id=\"err11\">11 ចែក​នឹង​សូន្យ</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3153013.48
msgid  "\<variable id=\"err12\"\>12 Variable not defined\</variable\>"
msgstr "<variable id=\"err12\">12 អថេរ​មិន​ត្រូវ​បាន​កំណត់ </variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3155593.49
msgid  "\<variable id=\"err13\"\>13 Data type mismatch\</variable\>"
msgstr "<variable id=\"err13\">13 ប្រភេទ​មិន​ត្រូវ​គូ</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3151197.50
msgid  "\<variable id=\"err14\"\>14 Invalid parameter\</variable\>"
msgstr "<variable id=\"err14\">14 ប៉ារ៉ាម៉ែត្រ​មិន​ត្រឹមត្រូវ</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3154710.51
msgid  "\<variable id=\"err18\"\>18 Process interrupted by user\</variable\>"
msgstr "<variable id=\"err18\">18 មាន​ការ​រំខាន​ពី​អ្នក​ប្រើ</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3147504.52
msgid  "\<variable id=\"err20\"\>20 Resume without error\</variable\>"
msgstr "<variable id=\"err20\">20 បន្ត​​ដោយ​គ្មាន​កំហុស</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3145319.53
msgid  "\<variable id=\"err28\"\>28 Not enough stack memory\</variable\>"
msgstr "<variable id=\"err28\">28  អស់​ទំហំ​ជង់</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3146110.54
msgid  "\<variable id=\"err35\"\>35 Sub-procedure or function procedure not defined\</variable\>"
msgstr "<variable id=\"err35\">35 Sub ឬ Function មិន​ត្រូវ​បាន​កំណត់</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3147246.55
