msgstr "<link href=\"text/sbasic/shared/02/11030000.xhp\" name=\"Run\">រត់</link>"

#: source/text/sbasic/shared/02/11030000.xhp#par_id3159201.2
msgid  "\<ahelp hid=\".uno:RunBasic\"\>Runs the first macro of the current module.\</ahelp\>"
msgstr "<ahelp hid=\".uno:RunBasic\">រត់​ម៉ាក្រូ​ដំបូង​របស់​ម៉ូឌុល​បច្ចុប្បន្ន ។</ahelp>"

#: source/text/sbasic/shared/02/11030000.xhp#par_id3156410
msgid  "\<image id=\"img_id3153311\" src=\"res/commandimagelist/sc_runbasic.png\" width=\"0.423cm\" height=\"0.423cm\"\>\<alt id=\"alt_id3153311\"\>Icon\</alt\>\</image\>"
