msgid  "String"
msgstr "ខ្សែ​អក្សរ"

#: source/text/sbasic/shared/03120309.xhp#hd_id3148798.8
msgid  "Parameters:"
msgstr "ប៉ារ៉ាម៉ែត្រ ៖"

#: source/text/sbasic/shared/03120309.xhp#par_id3151380.9
msgid  "\<emph\>Text: \</emph\>Any string expression."
msgstr "<emph>អត្ថបទ ៖ </emph>កន្សោម​ខ្សែ​អក្សរ​ណា​មួយ ។"

#: source/text/sbasic/shared/03120309.xhp#hd_id3151041.10
msgid  "Example:"
msgstr "ឧទាហរណ៍ ៖"

#: source/text/sbasic/shared/03120309.xhp#par_id3148673.11
msgid  "Sub ExampleSpaces"
msgstr "Sub ExampleSpaces"

#: source/text/sbasic/shared/03120309.xhp#par_id3156281.12
msgid  "Dim sText2 as String,sText as String,sOut as String"
msgstr "Dim sText2 as String,sText as String,sOut as String"

#: source/text/sbasic/shared/03120309.xhp#par_id3154125.13
msgid  "sText2 = \" <*Las Vegas*> \""
msgstr "sText2 = \" <*Las Vegas*> \""

#: source/text/sbasic/shared/03120309.xhp#par_id3155131.15
msgid  "sOut = \"'\"+sText2 +\"'\"+ Chr(13)"
msgstr "sOut = \"'\"+sText2 +\"'\"+ Chr(13)"

#: source/text/sbasic/shared/03120309.xhp#par_id3161833.16
msgid  "sText = Ltrim(sText2) REM sText = \"<*Las Vegas*> \""
msgstr "sText = Ltrim(sText2) REM sText = <*Las Vegas*> \""

#: source/text/sbasic/shared/03120309.xhp#par_id3147317.17
msgid  "sOut = sOut + \"'\"+sText +\"'\" + Chr(13)"
msgstr "sOut = sOut + \"'\"+sText +\"'\" + Chr(13)"

#: source/text/sbasic/shared/03120309.xhp#par_id3151112.18
msgid  "sText = Rtrim(sText2) REM sText = \" <*Las Vegas*>\""
msgstr "sText = Rtrim(sText2) REM sText = \" <*Las Vegas*> \""

#: source/text/sbasic/shared/03120309.xhp#par_id3149664.19
msgid  "sOut = sOut +\"'\"+ sText +\"'\" + Chr(13)"
msgstr "sOut = sOut +\"'\"+ sText +\"'\" + Chr(13)"

#: source/text/sbasic/shared/03120309.xhp#par_id3152576.20
msgid  "sText = Trim(sText2) REM sText = \"<*Las Vegas*>\""
msgstr "sText = Trim(sText2)  REM sText = \" <*Las Vegas*> \""

#: source/text/sbasic/shared/03120309.xhp#par_id3153729.21
msgid  "sOut = sOut +\"'\"+ sText +\"'\""
msgstr "sOut = sOut +\"'\"+ sText +\"'\""

#: source/text/sbasic/shared/03120309.xhp#par_id3145749.22
msgid  "MsgBox sOut"
msgstr "MsgBox sOut"

#: source/text/sbasic/shared/03120309.xhp#par_id3146922.23
msgid  "end sub"
msgstr "end sub"

