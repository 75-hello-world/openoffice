msgid  "Space (n As Long)"
msgstr "Space (n As Long)"

#: source/text/sbasic/shared/03120201.xhp#hd_id3147242.5
msgid  "Return value:"
msgstr "តម្លៃ​ត្រឡប់ ៖"

#: source/text/sbasic/shared/03120201.xhp#par_id3149233.6
msgid  "String"
msgstr "ខ្សែ​អក្សរ"

#: source/text/sbasic/shared/03120201.xhp#hd_id3156152.7
msgid  "Parameters:"
msgstr "ប៉ារ៉ាម៉ែត្រ ៖"

#: source/text/sbasic/shared/03120201.xhp#par_id3143228.8
msgid  "\<emph\>n:\</emph\> Numeric expression that defines the number of spaces in the string. The maximum allowed value of n is 65535."
msgstr "<emph>n ៖</emph> កន្សោម​លេខ​ដែល​កំណត់​ចំនួន​ដកឃ្លា​ក្នុង​ខ្សែអក្សរ ។ អនុញ្ញាត​ឲ្យ​មាន​តម្លៃ n ច្រើន​បំផុត​ត្រឹម 65535 ។"

#: source/text/sbasic/shared/03120201.xhp#hd_id3154760.9
msgid  "Example:"
msgstr "ឧទាហរណ៍ ៖"

#: source/text/sbasic/shared/03120201.xhp#par_id3147560.10
msgid  "Sub ExampleSpace"
msgstr "Sub ExampleSpace"

#: source/text/sbasic/shared/03120201.xhp#par_id3149670.11
msgid  "Dim sText As String,sOut As String"
msgstr "Dim sText As String,sOut As String"

#: source/text/sbasic/shared/03120201.xhp#par_id3154938.12
msgid  "DIm iLen As Integer"
msgstr "DIm iLen As Integer"

#: source/text/sbasic/shared/03120201.xhp#par_id3153525.13
msgid  "iLen = 10"
msgstr "iLen = 10"

#: source/text/sbasic/shared/03120201.xhp#par_id3151211.14
msgid  "sText = \"Las Vegas\""
msgstr "sText = \"Las Vegas\""

#: source/text/sbasic/shared/03120201.xhp#par_id3156282.15
msgid  "sOut = sText & Space(iLen) & sText & Chr(13) &_"
msgstr "sOut  = sText & Space(iLen) &  sText & Chr(13) &_"

#: source/text/sbasic/shared/03120201.xhp#par_id3144760.16
msgid  "sText & Space(iLen*2) & sText & Chr(13) &_"
msgstr "sText & Space(iLen*2) &  sText & Chr(13) &_"

#: source/text/sbasic/shared/03120201.xhp#par_id3159149.17
msgid  "sText & Space(iLen*4) & sText & Chr(13)"
msgstr "sText & Space(iLen*4) & sText & Chr(13)"

#: source/text/sbasic/shared/03120201.xhp#par_id3154216.18
msgid  "msgBox sOut,0,\"Info:\""
msgstr "msgBox sOut,0,\"Info:\""

#: source/text/sbasic/shared/03120201.xhp#par_id3158409.19
msgid  "End Sub"
msgstr "End Sub"

#: source/text/sbasic/shared/03120202.xhp#tit
