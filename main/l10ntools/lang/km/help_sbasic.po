#: source/text/sbasic/shared/00000003.xhp#par_id3149890.77
msgid  "\<variable id=\"err93\"\>93 Invalid string pattern\</variable\>"
msgstr "<variable id=\"err93\">93 ខ្សែ​អក្សរ​លំនាំ​មិន​ត្រឹមត្រូវ</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3146942.78
msgid  "\<variable id=\"err94\"\>94 Use of zero not permitted\</variable\>"
msgstr "<variable id=\"err94\">94 ប្រើ Null មិន​បាន​ត្រឹមត្រូវ</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469429
msgid  "\<variable id=\"err250\"\>250 DDE Error\</variable\>"
msgstr "<variable id=\"err250\">កំហុស ២៥០ DDE </variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469428
msgid  "\<variable id=\"err280\"\>280 Awaiting response to DDE connection\</variable\>"
msgstr "<variable id=\"err280\">មាន​ចម្លើយ​តប​ដែល​រងចាំ ២៨០ ​​​ចំពោះ​ការ​តភ្ជាប់ DDE </variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469427
msgid  "\<variable id=\"err281\"\>281 No DDE channels available\</variable\>"
msgstr "<variable id=\"err281\">គ្មាន DDE ២៨១ ឆានែល​ដែល​អាច​រក​បាន </variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469426
msgid  "\<variable id=\"err282\"\>282 No application responded to DDE connect initiation\</variable\>"
msgstr "<variable id=\"err282\">គ្មាន​កម្មវិធី ២៨២ ​ដែល​បាន​ឆ្លើយ​តប​ចំណោះ​ការ​ចាប់ផ្ដើម​តភ្ជាប់ DDE </variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469425
msgid  "\<variable id=\"err283\"\>283 Too many applications responded to DDE connect initiation\</variable\>"
msgstr "<variable id=\"err283\">២៨៣ កម្មវិធី​ជា​ច្រើន​ដែល​បាន​ឆ្លើយ​តប​ចំពោះ​ការ​ចាប់ផ្ដើម​តភ្ជាប់ DDE </variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469424
msgid  "\<variable id=\"err284\"\>284 DDE channel locked\</variable\>"
msgstr "<variable id=\"err284\">ឆានែល DDE ២៨៤ ដែល​ជាប់​សោ</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469423
msgid  "\<variable id=\"err285\"\>285 External application cannot execute DDE operation\</variable\>"
msgstr "<variable id=\"err285\">កម្មវិធី​ខាងក្រៅ​ ២៨៥ ដែល​មិន​អាច​ប្រតិបត្តិ​ DDE</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469422
msgid  "\<variable id=\"err286\"\>286 Timeout while waiting for DDE response\</variable\>"
msgstr "<variable id=\"err286\">អស់​ពេលវេលា ២៨៦ ខណៈ​ពេល​រង់ចាំ​ចម្លើយ​តប​របស់ DDE </variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469421
msgid  "\<variable id=\"err287\"\>287 user pressed ESCAPE during DDE operation\</variable\>"
msgstr "<variable id=\"err287\">អ្នក​ប្រើ ២៨៧ ​​បាន​​ចុច​គ្រាប់ចុច​គេច (ESCAPE) អំឡុង​ពេល​ប្រតិបត្តិ DDE </variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469420
msgid  "\<variable id=\"err288\"\>288 External application busy\</variable\>"
msgstr "<variable id=\"err288\">​កម្មវិធី​ខាងក្រៅ ២៨៨ ​រវល់</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469419
msgid  "\<variable id=\"err289\"\>289 DDE operation without data\</variable\>"
msgstr "<variable id=\"err289\">​ប្រតិបត្តិការ DDE ២៨៩ គ្មាន​ទិន្នន័យ​ឡើយ</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469418
msgid  "\<variable id=\"err290\"\>290 Data are in wrong format\</variable\>"
msgstr "<variable id=\"err290\">ទិន្នន័យ ២៩០ ដែល​​មាន​ទ្រង់ទ្រាយ​ខុស</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469417
msgid  "\<variable id=\"err291\"\>291 External application has been terminated\</variable\>"
msgstr "<variable id=\"err291\">កម្មវិធី​ខាងក្រៅ​ ២៩១ ត្រូវ​បាន​បញ្ចប់​ដំណើរការ</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469416
msgid  "\<variable id=\"err292\"\>292 DDE connection interrupted or modified\</variable\>"
msgstr "<variable id=\"err292\">ការ​តភ្ជាប់ DDE ២៩២ ​ដែល​បាន​​ផ្អាក ឬ​​មិន​បាន​កែប្រែ</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469415
msgid  "\<variable id=\"err293\"\>293 DDE method invoked with no channel open\</variable\>"
msgstr "<variable id=\"err293\">វិធីសាស្ត្រ DDE ២៩៣ ដែល​បាន​ប្រើ​​ដោយ​គ្មាន​ឆានែល​បើក</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469414
msgid  "\<variable id=\"err294\"\>294 Invalid DDE link format\</variable\>"
msgstr "<variable id=\"err294\">២៩៤ ទ្រង់ទ្រាយ​តំណ DDE មិន​ត្រឹមត្រូវ</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469413
msgid  "\<variable id=\"err295\"\>295 DDE message has been lost\</variable\>"
msgstr "<variable id=\"err295\"> សារ DDE ២៩៥ ត្រូវ​បាន​បាត់បង់</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469412
msgid  "\<variable id=\"err296\"\>296 Paste link already performed\</variable\>"
msgstr "<variable id=\"err296\">តំណ​បិទភ្ជាប់ ២៩៦ ​បាន​ប្រតិបត្តិ​រួច​ហើយ</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469411
msgid  "\<variable id=\"err297\"\>297 Link mode cannot be set due to invalid link topic\</variable\>"
msgstr "<variable id=\"err297\">របៀប​តំណ ២៩៧ មិន​អាច​ត្រូវ​បាន​កំណត់ ដោយ​សារ​តែ​ប្រធានបទ​តំណ​មិន​ត្រឹមត្រូវ</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id31469410
msgid  "\<variable id=\"err298\"\>298 DDE requires the DDEML.DLL file\</variable\>"
msgstr "<variable id=\"err298\">ទាមទារ DDE ២៩៨ ឯកសារ DDEML.DLL </variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3150028.79
msgid  "\<variable id=\"err323\"\>323 Module cannot be loaded; invalid format\</variable\>"
msgstr "<variable id=\"err323\">323 មិន​អាច​ផ្ទុក​ម៉ូឌុល</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3148434.80
msgid  "\<variable id=\"err341\"\>341 Invalid object index\</variable\>"
msgstr "<variable id=\"err341\">341  សន្ទស្សន៍​វត្ថុ​មិន​ត្រឹមត្រូវ</variable>"

#: source/text/sbasic/shared/00000003.xhp#par_id3143219.81
msgid  "\<variable id=\"err366\"\>366 Object is not available\</variable\>"
msgstr "<variable id=\"err366\">366 គ្មាន​ទិដ្ឋភាព ឬ​ឯកសារ​សកម្ម</variable>"

