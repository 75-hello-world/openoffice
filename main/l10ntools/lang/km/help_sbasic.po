msgstr "vOut = A > B Eqv B > D REM returns -1"

#: source/text/sbasic/shared/03060200.xhp#par_id3149412.18
msgid  "vOut = (B > D Eqv B > A) REM returns -1"
msgstr "vOut = A > B Eqv B > D REM returns -1"

#: source/text/sbasic/shared/03060200.xhp#par_id3149959.19
msgid  "vOut = B Eqv A REM returns -3"
msgstr "vOut = A > B Eqv B > D REM returns -1"

#: source/text/sbasic/shared/03060200.xhp#par_id3145646.20
