msgstr ""
"<ahelp hid=\"SD_LISTBOX_FLT_WIN_ANIMATION_LB_LOOP_COUNT\">កំណត់​ចំនួន​ពេល​វេលា"
"​ដែល​ចលនា​​ដំណើរការ ។​</ahelp> "
"ប្រសិន​បើ​អ្នក​ចង់​ឲ្យ​ចលនា​​ដំណើរ​ការ​ជា​និរន្ត​ ជ្រើស "
"<emph>អតិ​បរមា​</emph> ។ "

#: 06050000.xhp#par_id3154675.14.help.text
msgid ""
"<ahelp hid=\"SD_IMAGEBUTTON_FLT_WIN_ANIMATION_BTN_LAST\">Jumps to the last "
"image in the animation sequence.</ahelp>"
msgstr ""
"<ahelp hid=\"SD_IMAGEBUTTON_FLT_WIN_ANIMATION_BTN_LAST\">លោត​ទៅ​រូបភាព​ចុង​ក្រ"
"ោយ​ក្នុង​លំដាប់​ចលនា ។</ahelp>"

#: 06050000.xhp#par_id3155329.29.help.text
msgid ""
"<ahelp hid=\"SD_RADIOBUTTON_FLT_WIN_ANIMATION_RBT_BITMAP\">Combines images "
"into a single image.</ahelp>"
msgstr ""
"<ahelp hid=\"SD_RADIOBUTTON_FLT_WIN_ANIMATION_RBT_BITMAP\">ផ្សំ​រូប​ភាព​ទាំង​ន"
"េះ​ជា​រូប​ភាពតែ​មួយ​ ។​</ahelp>"

#: 06050000.xhp#par_id3155434.37.help.text
msgid "Adds or removes objects from your animation."
msgstr "បន្ថែម​ ឬ​ យក​វត្ថុ​ចេញ​ពី​ចលនា​របស់​អ្នក​ ។"

#: 06050000.xhp#par_id3155444.86.help.text
