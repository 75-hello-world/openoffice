msgid  "\<image id=\"img_id3152778\" src=\"res/commandimagelist/sc_arc.png\" width=\"0.222inch\" height=\"0.222inch\"\>\<alt id=\"alt_id3152778\"\>Icon\</alt\>\</image\>"
