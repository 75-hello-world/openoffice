msgid  "Apply"
msgstr "Aplikatu"

#: source/text/simpress/01/03152000.xhp#par_idN10800
msgid  "\<ahelp hid=\"sd:PushButton:RID_SD_TABPAGE_HEADERFOOTER:BT_APPLY\"\>Applies the current settings to the selected slides.\</ahelp\>"
msgstr "<ahelp hid=\"sd:PushButton:RID_SD_TABPAGE_HEADERFOOTER:BT_APPLY\">Uneko ezarpenak hautatutako diapositibetan aplikatzen ditu.</ahelp>"

