msgstr "Diapositibak koloretan, gris-eskalan edo zuri-beltzean bistaratzen ditu."

#: source/text/simpress/01/03180000.xhp#hd_id3149123.3
msgid  "Color"
msgstr "Kolorea"

#: source/text/simpress/01/03180000.xhp#par_id3154757.4
msgid  "\<ahelp hid=\".uno:OutputQualityColor\"\>Shows slides in color.\</ahelp\>"
msgstr "<ahelp hid=\".uno:OutputQualityColor\">Diapositibak koloretan erakusten ditu.</ahelp>"

#: source/text/simpress/01/03180000.xhp#hd_id3155333.5
