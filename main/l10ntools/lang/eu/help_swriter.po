msgid ""
"<ahelp hid=\".uno:UpdateAll\" visibility=\"visible\">Updates all links, fields, "
"indexes, tables of contents, and page formatting in the current "
"document.</ahelp>"
msgstr ""
"<ahelp hid=\".uno:UpdateAll\" visibility=\"visible\">Uneko dokumentuko esteka, "
"eremu, indize, aurkibide eta orrialde-formatu guztiak eguneratzen "
"ditu.</ahelp>"

