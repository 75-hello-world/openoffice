msgid ""
"<ahelp hid=\".uno:UpdateAllLinks\" visibility=\"visible\">Updates the links in "
"the current document.</ahelp>"
msgstr ""
"<ahelp hid=\".uno:UpdateAllLinks\" visibility=\"visible\">Uneko dokumentuko "
"estekak eguneratzen ditu.</ahelp>"

#: 06210000.xhp#tit.help.text
msgid "Links"
msgstr "Estekak"

