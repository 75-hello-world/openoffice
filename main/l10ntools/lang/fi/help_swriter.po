msgid "Text"
msgstr "Teksti"

#: 05170000.xhp#par_id3149026.11.help.text
msgid ""
"<ahelp hid=\"SFX2_CHECKBOX_DLG_NEW_FILE_CB_TEXT_STYLE\">Loads the paragraph "
"and the character styles from the selected document into the current "
"document.</ahelp>"
msgstr ""
"<ahelp hid=\"SFX2_CHECKBOX_DLG_NEW_FILE_CB_TEXT_STYLE\">Ladataan kappale- ja "
"merkkityylit valitusta asiakirjasta käsiteltävään asiakirjaan.</ahelp>"

