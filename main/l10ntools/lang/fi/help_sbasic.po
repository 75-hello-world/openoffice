msgid  "\<emph\>Integer:\</emph\> Integer variable (-32768 - 32767)"
msgstr ""

#: source/text/sbasic/shared/03102100.xhp#par_id3150045.21
msgid  "\<emph\>Long:\</emph\> Long integer variable (-2.147.483.648 - 2.147.483.647)"
msgstr ""

