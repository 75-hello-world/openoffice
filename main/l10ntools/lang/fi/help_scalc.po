msgstr ""
"<ahelp hid=\"SC:CHECKBOX:RID_SCDLG_NAMES:BTN_CRITERIA\">Merkintä määrää, että "
"valittua aluetta käytetään <link href=\"text/scalc/01/12040300.xhp\" "
"name=\"erityssuodatus\">erityssuodatuksen</link> ehdoille.</ahelp>"

#: 04070100.xhp#hd_id3159267.21.help.text
msgid "Repeat column"
msgstr "Toista sarake"

#: 04070100.xhp#par_id3149565.22.help.text
msgid ""
"<ahelp hid=\"SC:CHECKBOX:RID_SCDLG_NAMES:BTN_COLHEADER\">Defines the area as a "
"repeating column.</ahelp>"
msgstr ""
"<ahelp hid=\"SC:CHECKBOX:RID_SCDLG_NAMES:BTN_COLHEADER\">Merkintä määrää "
"alueen toistuvaksi sarakkeeksi.</ahelp>"

#: 04070100.xhp#hd_id3153966.23.help.text
msgid "Repeat row"
msgstr "Toista rivi"

#: 04070100.xhp#par_id3150300.24.help.text
msgid ""
"<ahelp hid=\"SC:CHECKBOX:RID_SCDLG_NAMES:BTN_ROWHEADER\">Defines the area as a "
"repeating row.</ahelp>"
msgstr ""
"<ahelp hid=\"SC:CHECKBOX:RID_SCDLG_NAMES:BTN_ROWHEADER\">Merkintä määrää "
"alueen toistuvaksi riviksi.</ahelp>"

#: 04070100.xhp#hd_id3155112.27.help.text
