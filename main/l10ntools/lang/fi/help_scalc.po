msgid  "IMCOS"
msgstr "IMCOS"

#: source/text/scalc/01/04060116.xhp#par_id3149725.74
msgid  "\<ahelp hid=\"HID_AAI_FUNC_IMCOS\"\>The result is the cosine of a complex number.\</ahelp\>"
msgstr "<ahelp hid=\"HID_AAI_FUNC_IMCOS\">Tulos on kompleksiluvun kosini.</ahelp>"

#: source/text/scalc/01/04060116.xhp#hd_id3159116.75
msgid  "Syntax"
msgstr "Syntaksi"

#: source/text/scalc/01/04060116.xhp#par_id3147415.76
msgid  "IMCOS(\"ComplexNumber\")"
msgstr "IMCOS(\"kompleksiluku\")"

#: source/text/scalc/01/04060116.xhp#hd_id3152980.78
msgid  "Example"
msgstr "Esimerkki"

#: source/text/scalc/01/04060116.xhp#par_id3157901.79
msgid  "            \<item type=\"input\"\>=IMCOS(\"3+4j\") \</item\>returns -27.03-3.85j (rounded)."
msgstr "<item type=\"input\">=IMCOS(\"3+4i\") </item>antaa tuloksen -27.03-3.85i (pyöristettynä)."

