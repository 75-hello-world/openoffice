msgstr ""
"<variable id=\"loeschentext\"><ahelp hid=\".uno:DeleteColumns\">Poistetaan "
"taulukon valitut sarakkeet.</ahelp></variable>"

#: 05120500m.xhp#par_id3156385.3.help.text
msgid "This command is only available if the cursor is in a table."
msgstr "Tämä komento on käytettävissä vain, kun kohdistin on taulukossa."

#: 05120500m.xhp#par_id3155328.115.help.text
msgctxt "05120500m.xhp#par_id3155328.115.help.text"
msgid "On <emph>Table</emph> Bar, click"
msgstr "Napsauta <emph>Taulukko</emph>-palkissa"

#: 05120500m.xhp#par_id3153600.help.text
msgid ""
"<image id=\"img_id3153607\" src=\"res/commandimagelist/sc_deletecolumns.png\" "
