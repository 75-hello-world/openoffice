msgstr "A"

#: source/text/scalc/01/04060107.xhp#par_id3150117.261
msgid  "B"
msgstr "B"

#: source/text/scalc/01/04060107.xhp#par_id3155325.262
msgid  "C"
msgstr "C"

#: source/text/scalc/01/04060107.xhp#par_id3153104.263
msgid  "1"
msgstr "१"

#: source/text/scalc/01/04060107.xhp#par_id3146996.264
msgid  "\<item type=\"input\"\>7\</item\>"
msgstr ""

#: source/text/scalc/01/04060107.xhp#par_id3150529.265
msgid  "\<item type=\"input\"\>31\</item\>"
msgstr ""

#: source/text/scalc/01/04060107.xhp#par_id3148831.266
