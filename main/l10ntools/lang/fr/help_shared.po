msgstr ""

#: source/text/shared/explorer/database/05010000.xhp#par_id3152363.2
msgid  "In the \<emph\>Table Design\</emph\> window you define new tables or edit the structure of a table."
msgstr ""

#: source/text/shared/explorer/database/05010000.xhp#par_id3146957.3
msgid  "The window has its own menu bar. It also contains the following new command: \<link href=\"text/shared/explorer/database/05010100.xhp\" name=\"Index Design\"\>\<emph\>Index Design\</emph\>\</link\>"
msgstr ""

#: source/text/shared/explorer/database/05010000.xhp#hd_id3152551.4
msgid  "Table definition area"
msgstr "Zone de définition de la table"

#: source/text/shared/explorer/database/05010000.xhp#par_id3153681.5
msgid  "\<ahelp hid=\"HID_TABDESIGN_BACKGROUND\"\>This area is where you define the table structure.\</ahelp\>"
msgstr ""

#: source/text/shared/explorer/database/05010000.xhp#hd_id3153031.6
msgid  "Field Name"
msgstr "Nom de champ"

#: source/text/shared/explorer/database/05010000.xhp#par_id3156113.7
msgid  "\<ahelp hid=\"HID_TABDESIGN_NAMECELL\"\>Specifies the name of the data field. Note the database restrictions, such as the length of the name, special characters and spaces.\</ahelp\>"
msgstr ""

#: source/text/shared/explorer/database/05010000.xhp#hd_id3147618.8
msgid  "Field type"
msgstr "Type de champ"

#: source/text/shared/explorer/database/05010000.xhp#par_id3154897.9
msgid  "\<ahelp hid=\"HID_TABDESIGN_TYPECELL\"\>Specifies the field type.\</ahelp\>"
msgstr ""

#: source/text/shared/explorer/database/05010000.xhp#hd_id3156119.10
msgid  "Description"
msgstr "Description"

#: source/text/shared/explorer/database/05010000.xhp#par_id3145315.11
msgid  "\<ahelp hid=\"HID_TABDESIGN_COMMENTCELL\"\>Specifies an optional description.\</ahelp\>"
msgstr ""

#: source/text/shared/explorer/database/05010000.xhp#par_id3155630.12
msgid  "The row headers contain the following context menu commands:"
msgstr "Le menu contextuel des en-têtes de colonne contient les commandes suivantes :"

#: source/text/shared/explorer/database/05010000.xhp#hd_id3156330.13
msgid  "Cut"
msgstr "Couper"

#: source/text/shared/explorer/database/05010000.xhp#par_id3159157.14
msgid  "Cuts the selected row to the clipboard."
msgstr "Coupe la ligne sélectionnée dans le presse-papiers."

#: source/text/shared/explorer/database/05010000.xhp#hd_id3159177.15
msgid  "Copy"
msgstr "Copier"

#: source/text/shared/explorer/database/05010000.xhp#par_id3148685.16
msgid  "Copies the selected row to the clipboard."
msgstr "Copie la ligne sélectionnée dans le presse-papiers."

#: source/text/shared/explorer/database/05010000.xhp#hd_id3156327.17
msgid  "Paste"
msgstr "Coller"

#: source/text/shared/explorer/database/05010000.xhp#par_id3152472.18
msgid  "Pastes the content of the clipboard."
msgstr "Colle le contenu du presse-papiers."

#: source/text/shared/explorer/database/05010000.xhp#hd_id3144511.19
msgid  "Delete"
msgstr "Supprimer"

