msgid  "Chart options"
msgstr "Options des diagrammes"

#: source/text/shared/optionen/01110000.xhp#hd_id3145345.1
msgid  "Chart options"
msgstr "Options des diagrammes"

#: source/text/shared/optionen/01110000.xhp#par_id3149182.2
msgid  "\<variable id=\"farbe\"\>\<ahelp hid=\"\" visibility=\"visible\"\>Defines the general settings for charts.\</ahelp\>\</variable\>"
msgstr ""

#: source/text/shared/optionen/01110100.xhp#tit
msgid  "Default colors"
msgstr "Couleurs par défaut"

#: source/text/shared/optionen/01110100.xhp#bm_id3154751
msgid  "\<bookmark_value\>charts; colors\</bookmark_value\>\<bookmark_value\>colors;charts\</bookmark_value\>"
msgstr ""

