#: source/text/shared/01/06020000.xhp#par_id3159233.6
msgid  "\<ahelp hid=\".\"\>Displays the current word, or the related term that you selected by double-clicking a line in the Alternatives list. You can also type text directly in this box to look up your text.\</ahelp\>"
msgstr ""

#: source/text/shared/01/06020000.xhp#hd_id3647571.5
msgid  "Arrow left"
msgstr "Flèche vers la gauche"

#: source/text/shared/01/06020000.xhp#par_id369233.6
msgid  "\<ahelp hid=\".\"\>Recalls the previous contents of the \"Current word\" text box.\</ahelp\>"
msgstr ""

#: source/text/shared/01/06020000.xhp#hd_id3154983.7
msgid  "Alternatives"
msgstr "Alternatives"

#: source/text/shared/01/06020000.xhp#par_id3149182.8
msgid  "\<ahelp hid=\".\"\>Click an entry in the Alternatives list to copy the related term to the \"Replace with\" text box. Double-click an entry to copy the related term to the \"Current word\" text box and to look up that term.\</ahelp\>"
