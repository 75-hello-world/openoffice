msgstr "Icône format de fichier"

#: source/text/shared/autopi/webwizard04.xhp#par_idN10585
msgid  "\<ahelp hid=\"34241\"\>Displays the file format icon on the index page.\</ahelp\>"
msgstr ""

#: source/text/shared/autopi/webwizard04.xhp#par_idN10588
msgid  "No. of pages"
msgstr "N° des pages"

#: source/text/shared/autopi/webwizard04.xhp#par_idN1058C
msgid  "\<ahelp hid=\"34242\"\>Displays the number of pages in your site on the index page.\</ahelp\>"
msgstr ""

#: source/text/shared/autopi/webwizard04.xhp#par_idN1058F
msgid  "Size in KB"
msgstr "Taille (Ko)"

#: source/text/shared/autopi/webwizard04.xhp#par_idN10593
msgid  "\<ahelp hid=\"34243\"\>Displays the file size in kilobytes on the index page.\</ahelp\>"
msgstr ""

