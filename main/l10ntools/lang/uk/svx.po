msgid "Recovery in progress"
msgstr "Триває відновлення"

#: docrecovery.src#RID_SVXPAGE_DOCRECOVERY_RECOVER.STR_NOTRECOVYET.string.text
msgid "Not recovered yet"
msgstr "Ще не відновлено"

