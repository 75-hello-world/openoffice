msgid "Original document recovered"
msgstr "Оригінальний документ відновлено"

#: docrecovery.src#RID_SVXPAGE_DOCRECOVERY_RECOVER.STR_RECOVFAILED.string.text
msgid "Recovery failed"
msgstr "Не вдається відновити"

