msgstr ""
"<ahelp hid=\"SC:EDIT:RID_SCDLG_DBNAMES:ED_DBAREA\">Visar det markerade "
"cellområdet.</ahelp>"

#: 12010000.xhp#hd_id3153188.10.help.text
msgctxt "12010000.xhp#hd_id3153188.10.help.text"
msgid "Add/Modify"
msgstr "Ändra/Lägg till"

#: 12010000.xhp#par_id3153726.11.help.text
msgid ""
"<ahelp hid=\"SC:PUSHBUTTON:RID_SCDLG_DBNAMES:BTN_ADD\">Adds the selected cell "
"range to the database range list, or modifies an existing database "
"range.</ahelp>"
msgstr ""
"<ahelp hid=\"SC:PUSHBUTTON:RID_SCDLG_DBNAMES:BTN_ADD\">Lägger till det "
"markerade cellområdet i listan med databasområden, eller ändrar ett "
"befintligt databasområde.</ahelp>"

