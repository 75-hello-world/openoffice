msgid "COS"
msgstr "COS"

#: 04060106.xhp#par_id3148803.150.help.text
msgid ""
"<ahelp hid=\"HID_FUNC_COS\">Returns the cosine of the given angle (in "
"radians).</ahelp>"
msgstr ""
"<ahelp hid=\"HID_FUNC_COS\">Returnerar cosinus för en given vinkel (i "
"radianer).</ahelp>"

#: 04060106.xhp#hd_id3150779.151.help.text
msgctxt "04060106.xhp#hd_id3150779.151.help.text"
msgid "Syntax"
msgstr "Syntax"

