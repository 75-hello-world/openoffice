msgid "Example"
msgstr "Exempel"

#: 04060116.xhp#par_id3150963.149.help.text
msgid ""
"            <item type=\"input\">=IMSUB(\"13+4j\";\"5+3j\")</item> returns 8+j."
msgstr ""
"             <item type=\"input\">=IMDIFF(\"13+4j\";\"5+3j\")</item> returnerar "
"8+j."

