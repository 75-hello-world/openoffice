msgstr "Alla diagram"

#: 06220000.xhp#hd_id3155959.1.help.text
msgid ""
"<link href=\"text/swriter/01/06220000.xhp\" name=\"All Charts\">All Charts</"
"link>"
msgstr ""
"<link href=\"text/swriter/01/06220000.xhp\" name=\"Alla diagram\">Alla "
"diagram</link>"

#: 06220000.xhp#par_id3150344.2.help.text
msgid ""
"<ahelp hid=\".uno:UpdateCharts\" visibility=\"visible\">Updates the charts "
