#: 06990000.xhp#tit.help.text
msgid "Update"
msgstr "Uppdatera"

#: 06990000.xhp#bm_id3154704.help.text
msgid "<bookmark_value>updating; text documents</bookmark_value>"
msgstr "<bookmark_value>Uppdatera; textdokument</bookmark_value>"

#: 06990000.xhp#hd_id3154704.1.help.text
msgid ""
"<link href=\"text/swriter/01/06990000.xhp\" name=\"Update\">Update</link>"
msgstr ""
"<link href=\"text/swriter/01/06990000.xhp\" name=\"Uppdatera\">Uppdatera</"
"link>"

