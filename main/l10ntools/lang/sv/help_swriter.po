msgstr "Radera"

#: 05110500.xhp#hd_id3149502.1.help.text
msgid ""
"<link href=\"text/swriter/01/05110500.xhp\" name=\"Delete\">Delete</link>"
msgstr ""
"<link href=\"text/swriter/01/05110500.xhp\" name=\"Radera\">Radera</link>"

#: 05110500.xhp#par_id3149050.2.help.text
msgid ""
"<variable id=\"loeschentext\"><ahelp hid=\".uno:DeleteRows\">Deletes the "
"selected row(s) from the table.</ahelp></variable>"
msgstr ""
"<variable id=\"loeschentext\"><ahelp hid=\".uno:DeleteRows\">Tar bort den "
"markerade raden eller de markerade raderna från tabellen.</ahelp></variable>"

#: 05120000.xhp#tit.help.text
msgctxt "05120000.xhp#tit.help.text"
msgid "Column"
msgstr "Kolumn"

#: 05120000.xhp#hd_id3154762.1.help.text
msgid ""
"<link href=\"text/swriter/01/05120000.xhp\" name=\"Column\">Column</link>"
msgstr ""
"<link href=\"text/swriter/01/05120000.xhp\" name=\"Kolumn\">Kolumn</link>"

#: 05120000.xhp#par_id3149052.2.help.text
msgid "Set the width of columns, or select, insert, and delete columns."
msgstr "Ange bredden på kolumner, eller markera, infoga och ta bort kolumner."

#: 05120000.xhp#hd_id3146322.4.help.text
msgid ""
"<link href=\"text/swriter/01/05120100.xhp\" name=\"Width...\">Width...</link>"
msgstr ""
"<link href=\"text/swriter/01/05120100.xhp\" name=\"Bredd...\">Bredd...</link>"

