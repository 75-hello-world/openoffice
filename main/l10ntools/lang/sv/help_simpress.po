msgstr "Navigera med tangentbordet i Bildsortering"

#: source/text/simpress/04/01020000.xhp#par_idN110AA
msgid  "Shortcut Keys"
msgstr "Kortkommandon"

#: source/text/simpress/04/01020000.xhp#par_idN110B0
msgid  "\<emph\>Effect\</emph\>"
msgstr "<emph>Effekt</emph>"

#: source/text/simpress/04/01020000.xhp#hd_id3153254.135
msgid  "Escape"
msgstr "Escape-tangenten"

#: source/text/simpress/04/01020000.xhp#par_id3153268.136
msgid  "Sets the focus to the first slide."
msgstr "Flyttar fokus till första bilden."

#: source/text/simpress/04/01020000.xhp#hd_id3154811.137
msgid  "Arrow key"
msgstr "piltangenter"

#: source/text/simpress/04/01020000.xhp#par_id3145073.138
msgid  "Sets the focus to the next slide."
msgstr "Flyttar fokus till nästa bild."

#: source/text/simpress/04/01020000.xhp#hd_id3145086.139
