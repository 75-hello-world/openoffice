msgstr "Första sidan"

#: source/text/simpress/01/02110000.xhp#hd_id3156061.14
msgid  "Previous Slide"
msgstr "Föregående sida"

#: source/text/simpress/01/02110000.xhp#par_id3148768.15
msgid  "\<ahelp hid=\"HID_SD_NAVIGATOR_TBI_PREV\"\>Moves back one slide in the slide show.\</ahelp\>"
msgstr "<ahelp hid=\"HID_SD_NAVIGATOR_TBI_PREV\">Går en sida bakåt i presentationen.</ahelp>"

