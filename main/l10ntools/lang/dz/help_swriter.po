#: mm_seladdblo.xhp#par_idN10539.help.text
msgctxt "mm_seladdblo.xhp#par_idN10539.help.text"
msgid "Select Address Block"
msgstr "ཁ་བྱང་སྡེབ་ཚན་སེལ་འཐུ་འབད་"

#: mm_seladdblo.xhp#par_idN1053D.help.text
msgid ""
"Select, edit, or delete an address block layout for <link "
"href=\"text/swriter/01/mailmerge03.xhp\">mail merge</link>."
msgstr ""
"<link href=\"text/swriter/01/mailmerge03.xhp\">ཡིག་འཕྲིན་མཉམ་བསྡོམས་</link>དེ་"
"གི་དོན་ལུ་  ཁ་བྱང་སྡེབ་ཚན་སྒྲིག་བཀོད་ཅིག་ སེལ་འཐུའབད་ནི་དང་ ཞུན་དག་འབད་ནི་ "
"ཡང་ན་ བཏོན་གཏང་།"

#: mm_seladdblo.xhp#par_idN1054E.help.text
msgid "Select the address block which you want to use"
msgstr "ཁྱོད་ཀྱིས་ལག་ལེན་འཐབ་དགོ་མནོ་མི་ ཁ་བྱང་སྡེབ་ཚན་དེ་སེལ་འཐུ་འབད་"

#: mm_seladdblo.xhp#par_idN10552.help.text
msgid ""
"<ahelp hid=\".\">Select the block in the list that you want to use for mail "
"merge addresses, and click <emph>OK</emph>.</ahelp>"
msgstr ""
"<ahelp "
"hid=\".\">ཡིག་འཕྲིན་མཉམ་བསྡོམས་ཁ་བྱང་ཚུ་གི་དོན་ལུ་ལག་ལེན་འཐབ་དགོ་མནོ་མི་ "
"སྡེབ་ཚད་དེ་ཐོ་ཡིག་ནང་ལུ་སེལ་འཐུ་འབད་ཞིནམ་ལས་ དེ་ལས་  and click "
"<emph>བཏུབ་</emph>ལུ་ཨེབ་གཏང་འབད།</ahelp>"

#: mm_seladdblo.xhp#par_idN10555.help.text
msgid "Never include country/region"
msgstr "རྒྱལ་ཁབ་/ལུང་ཕྱོགས་ཚུ་ནམ་ཡང་གྲངས་སུ་མི་བཙུགས་"

#: mm_seladdblo.xhp#par_idN10559.help.text
msgid ""
"<ahelp hid=\".\">Excludes country or regional information from the address "
"block.</ahelp>"
msgstr ""
"<ahelp hid=\".\">ཁ་བྱང་སྡེབ་ཚན་དེ་ནང་ལས་ རྒྱལ་ཁབ་ ཡང་ན་ ལུང་ཕྱོགས་བརྡ་དོན་དེ་ "
"གྲངས་ལས་བཏོན་གཏངམ་ཨིན།</ahelp>"

#: mm_seladdblo.xhp#par_idN1055C.help.text
msgid "Always include country/region"
msgstr "ཨ་རྟག་རང་ རྒྱལ་གབ་/ལུང་ཕྱོགས་ཚུ་གྲངས་སུ་བཙུགས་དགོ་"

#: mm_seladdblo.xhp#par_idN10560.help.text
msgid ""
"<ahelp hid=\".\">Includes country or regional information in the address "
"block.</ahelp>"
msgstr ""
"<ahelp hid=\".\">རྒྱལ་ཁབ་ ཡང་ན་ ལུང་ཕྱོགས་ཀྱི་བརྡ་དོན་ཚུ་ "
"ཁ་བྱང་སྡེབ་ཚན་གྱི་གྲངས་སུ་བཙུགསཔ་ཨིན།</ahelp>"

#: mm_seladdblo.xhp#par_idN10563.help.text
msgid "Only include country/region if it is not:"
msgstr "འདི་མེན་པ་ཅིན་ རྒྱལ་ཁབ་/ལུང་ཕྱོགས་རྐྱངམ་ཅིག་གྲངས་སུ་བཙུགས:"

#: mm_seladdblo.xhp#par_idN10567.help.text
msgid ""
"<ahelp hid=\".\">Only includes country or regional information in the address "
"block if the value differs from the value that you enter in the text "
"box.</ahelp>"
msgstr ""
"<ahelp hid=\".\">ཁྱོད་ཀྱིས་ ཚིག་ཡིག་སྒྲོམ་ནང་ལུ་ཐོ་བཀོད་འབད་མི་གནས་གོང་དེ་ལས་ "
"སོ་སོ་འགྱུར་བ་ཅིན་ ཁ་བྱང་སྡེབ་ཚན་དེ་ནང་ལུ་ རྒྱལ་ཁབ་ཡང་ན་ "
"ལུང་ཕྱོགས་བརྡ་དོན་ཚུ་རྐྱངམ་ཅིག་གྲངས་སུ་བཙུགསཔ་ཨིན།</ahelp>"

#: mm_seladdblo.xhp#par_idN10651.help.text
msgid ""
"<ahelp hid=\".\">Enter the country/region string that shall not be "
"printed.</ahelp>"
msgstr ""
"<ahelp hid=\".\">དཔར་བསྐྲུན་མ་འབད་བར་བཞག་ནི་ཨིན་མི་ "
"རྒྱལ་ཁབ་/ལུང་ཕྱོགས་ཀྱི་ཡིག་རྒྱུན་དེ་ཐོ་བཀོད་འབད།</ahelp>"

