"<switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><def"
"aultinline>Ctrl</defaultinline></switchinline> + /"
msgstr ""
"<switchinline select=\"sys\"><caseinline select=\"MAC\">བརྡ་བཀོད་ "
"</caseinline><defaultinline>ཚད་འཛིན་ </defaultinline></switchinline> + /"

#: 01020000.xhp#par_id3149949.181.help.text
msgid "where (/) is the division sign on the numeric key pad"
msgstr "ཨང་གྲངས་ཀྱི་ལྡེ་མིག་གདན་གུ་ལུ་བགོ་རྩིས་རྟགས་འདི་ (/) ཨིན་ས།"

#: 01020000.xhp#par_id3150144.182.help.text
msgid "Selects the matrix formula range that contains the cursor."
msgstr "ནང་ན་འོད་རྟགས་ཤོམ་ཏེ་ཡོད་པའི་ མེ་ཊིགསི་མན་ངག་ཁྱབ་ཚད་ སེལ་འཐུ་འབདཝ་ཨིན།"

#: 01020000.xhp#par_id8163396.help.text
msgid ""
"<switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><def"
