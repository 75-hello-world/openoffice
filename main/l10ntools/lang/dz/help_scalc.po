msgid "ISTEXT"
msgstr "ISTEXT"

#: 04060104.xhp#par_id3145368.102.help.text
msgid ""
"<ahelp hid=\"HID_FUNC_ISTTEXT\">Returns TRUE if the cell contents refer to "
"text.</ahelp>"
msgstr ""
"<ahelp hid=\"HID_FUNC_ISTTEXT\"> ནང་ཐིག་ནང་དོན་ཚུ་ "
"ཚིག་ཡིག་ལུ་གཞི་བསྟུན་འབདཝ་ཨིན་པ་ཅིན་ TRUE འདི་སླར་ལོག་འབདཝ་ཨིན། </ahelp>"

#: 04060104.xhp#par_id6779686.help.text
msgctxt "04060104.xhp#par_id6779686.help.text"
msgid "If an error occurs, the function returns FALSE."
msgstr ""

#: 04060104.xhp#hd_id3154332.103.help.text
msgctxt "04060104.xhp#hd_id3154332.103.help.text"
msgid "Syntax"
msgstr "ཚིག་སྦྱོར།"

#: 04060104.xhp#par_id3148649.104.help.text
msgid "ISTEXT(Value)"
msgstr "ISTEXT(གནས་གོང་།)"

#: 04060104.xhp#par_id3150417.105.help.text
msgid ""
"<emph>Value</emph> is a value, number, Boolean value, or an error value to "
"be tested."
msgstr ""
"<emph> གནས་གོང་ </emph> འདི་བརྟག་དཔྱད་འབད་ནི་ཨིན་པའི་ གནས་གོང་  ཨང་ "
"བུ་ལིན་གནས་གོང་ ཡང་ན་གནས་གོང་འཛོལ་བ་ཨིན། "

#: 04060104.xhp#hd_id3149239.106.help.text
msgctxt "04060104.xhp#hd_id3149239.106.help.text"
msgid "Example"
msgstr "དཔེར་བརྗོད།"

#: 04060104.xhp#par_id3144756.107.help.text
msgid ""
"<item type=\"input\">=ISTEXT(D9)</item> returns TRUE if cell D9 contains the "
"text <item type=\"input\">abcdef</item>."
