msgstr "ཉེན་སྐྱོབ།"

#: 05020600.xhp#hd_id3125864.9.help.text
msgid "Hide all"
msgstr "ཆ་མཉམ་སྦ།"

#: 05020600.xhp#par_id3153768.10.help.text
msgid ""
"<ahelp hid=\"SC:TRISTATEBOX:RID_SCPAGE_PROTECTION:BTN_HIDE_ALL\">Hides "
"formulas and contents of the selected cells.</ahelp>"
msgstr ""
"<ahelp hid=\"SC:TRISTATEBOX:RID_SCPAGE_PROTECTION:BTN_HIDE_ALL\"> "
"སེལ་འཐུ་གྲུབ་ཡོད་པའི་ནང་ཐིག་ཚུ་གི་མན་ངག་དང་ནང་དོན་ཚུ་སྦ་བཞགཔ་ཨིན། </ahelp>"

#: 05020600.xhp#hd_id3153190.5.help.text
