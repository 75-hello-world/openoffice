msgid  "This area lists the options for deleting contents."
msgstr "Esta àrea llista les opcions per suprimir els continguts."

#: source/text/scalc/01/02150000.xhp#hd_id3146975.5
msgid  "Delete All"
msgstr "Suprimeix-ho tot"

#: source/text/scalc/01/02150000.xhp#par_id3154729.6
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCDLG_DELCONT_BTN_DELALL\"\>Deletes all content from the selected cell range.\</ahelp\>"
msgstr ""

#: source/text/scalc/01/02150000.xhp#hd_id3156286.7
msgid  "Text"
msgstr "Text"

#: source/text/scalc/01/02150000.xhp#par_id3154015.8
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCDLG_DELCONT_BTN_DELSTRINGS\"\>Deletes text only. Formats, formulas, numbers and dates are not affected.\</ahelp\>"
msgstr ""

#: source/text/scalc/01/02150000.xhp#hd_id3153840.9
msgid  "Numbers"
msgstr "Nombres"

#: source/text/scalc/01/02150000.xhp#par_id3148405.10
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCDLG_DELCONT_BTN_DELNUMBERS\"\>Deletes numbers only. Formats and formulas remain unchanged.\</ahelp\>"
msgstr ""

#: source/text/scalc/01/02150000.xhp#hd_id3155764.11
msgid  "Date & time"
msgstr "Data i hora"

#: source/text/scalc/01/02150000.xhp#par_id3149567.12
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCDLG_DELCONT_BTN_DELDATETIME\"\>Deletes date and time values. Formats, text, numbers and formulas remain unchanged.\</ahelp\>"
msgstr ""

#: source/text/scalc/01/02150000.xhp#hd_id3154703.13
msgid  "Formulas"
msgstr "Fórmules"

#: source/text/scalc/01/02150000.xhp#par_id3148485.14
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCDLG_DELCONT_BTN_DELFORMULAS\"\>Deletes formulas. Text, numbers, formats, dates and times remain unchanged.\</ahelp\>"
msgstr ""

#: source/text/scalc/01/02150000.xhp#hd_id3150300.15
msgid  "Comments"
msgstr "Comentaris"

#: source/text/scalc/01/02150000.xhp#par_id3154658.16
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCDLG_DELCONT_BTN_DELNOTES\"\>Deletes comments added to cells. All other elements remain unchanged.\</ahelp\>"
msgstr ""

#: source/text/scalc/01/02150000.xhp#hd_id3155112.17
msgid  "Formats"
msgstr "Formats"

#: source/text/scalc/01/02150000.xhp#par_id3146134.18
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCDLG_DELCONT_BTN_DELATTRS\"\>Deletes format attributes applied to cells. All cell content remains unchanged.\</ahelp\>"
msgstr ""

#: source/text/scalc/01/02150000.xhp#hd_id3150088.19
msgid  "Objects"
msgstr "Objectes"

#: source/text/scalc/01/02150000.xhp#par_id3152990.20
msgid  "\<ahelp hid=\"SC_CHECKBOX_RID_SCDLG_DELCONT_BTN_DELOBJECTS\"\>Deletes objects. All cell content remains unchanged.\</ahelp\>"
msgstr ""

#: source/text/scalc/01/02160000.xhp#tit
