msgid  "\<image id=\"img_id3145410\" src=\"res/commandimagelist/sc_freeline.png\" width=\"0.222inch\" height=\"0.222inch\"\>\<alt id=\"alt_id3145410\"\>Icon\</alt\>\</image\>"
msgstr "<image id=\"img_id3145410\" src=\"res/commandimagelist/sc_freeline.png\" width=\"0.222inch\" height=\"0.222inch\"><alt id=\"alt_id3145410\">Symbol</alt></image>"

