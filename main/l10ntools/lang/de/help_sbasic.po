msgid  "Dim sDate as String"
msgstr "Dim sDate as String"

#: source/text/sbasic/shared/03090101.xhp#par_id3155855.14
msgid  "Dim sToday as String"
msgstr "Dim sToday as String"

#: source/text/sbasic/shared/03090101.xhp#par_id3154490.16
msgid  "sDate = InputBox(\"Enter the expiration date (MM.DD.YYYY)\")"
msgstr "sDate = InputBox(\"Bitte geben Sie das Verfalldatum ein (TT.MM.JJJJ)\")"

#: source/text/sbasic/shared/03090101.xhp#par_id3154943.17
msgid  "sDate = Right$(sDate, 4) + Mid$(sDate, 4, 2) + Left$(sDate, 2)"
msgstr "sDate = Right$(sDate, 4) +  Mid$(sDate, 4, 2)  + Left$(sDate, 2)"

