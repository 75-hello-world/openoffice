msgid  "Dim lValue As Long"
msgstr "Dim lValue As Long"

#: source/text/sbasic/shared/03090403.xhp#par_id3145647.19
msgid  "lValue = 5000"
msgstr "lValue = 5000"

#: source/text/sbasic/shared/03090403.xhp#par_id3145801.20
msgid  "MyMessageBeep( lValue )"
msgstr "MyMessageBeep( lValue )"

#: source/text/sbasic/shared/03090403.xhp#par_id3145253.21
msgid  "FreeLibrary(\"user32.dll\" )"
msgstr "FreeLibrary(\"user32.dll\" )"

#: source/text/sbasic/shared/03090403.xhp#par_id3149402.22
msgid  "End Sub"
msgstr "End Sub"

