msgid  "Syntax:"
msgstr "Syntax:"

#: source/text/sbasic/shared/03120401.xhp#par_id3146957.5
msgid  "InStr ([Start As Long,] Text1 As String, Text2 As String[, Compare])"
msgstr "InStr ([Anfang As Integer,] Text1 As String, Text2 As String[, Vergleich])"

#: source/text/sbasic/shared/03120401.xhp#hd_id3148538.6
msgid  "Return value:"
msgstr "Rückgabewert:"

