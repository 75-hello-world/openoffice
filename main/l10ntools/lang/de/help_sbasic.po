msgid  "sToday = Right$(sToday, 4)+ Mid$(sToday, 4, 2) + Left$(sToday, 2)"
msgstr "sToday = Right$(sToday, 4)+ Mid$(sToday, 4, 2)  + Left$(sToday, 2)"

#: source/text/sbasic/shared/03090101.xhp#par_id3154792.20
msgid  "If sDate < sToday Then"
msgstr "If sDate < sToday Then"

#: source/text/sbasic/shared/03090101.xhp#par_id3155601.21
msgid  "MsgBox \"The expiration date has passed\""
msgstr "MsgBox \"Das Verfalldatum ist abgelaufen\""

#: source/text/sbasic/shared/03090101.xhp#par_id3146972.22
msgid  "ElseIf sDate > sToday Then"
msgstr "ElseIf sDate > sToday Then"

#: source/text/sbasic/shared/03090101.xhp#par_id3146912.23
msgid  "MsgBox \"The expiration date has not yet passed\""
msgstr "MsgBox \"Das Verfalldatum ist noch nicht abgelaufen\""

#: source/text/sbasic/shared/03090101.xhp#par_id3153710.24
msgid  "Else"
msgstr "Else"

#: source/text/sbasic/shared/03090101.xhp#par_id3154754.25
msgid  "MsgBox \"The expiration date is today\""
msgstr "MsgBox \"Das Verfalldatum läuft heute ab\""

#: source/text/sbasic/shared/03090101.xhp#par_id3154361.26
msgid  "End If"
msgstr "End If"

#: source/text/sbasic/shared/03090101.xhp#par_id3148405.28
