msgstr "Case 6, 7, 8"

#: source/text/sbasic/shared/03090404.xhp#par_id3148618.21
msgid  "Print \"Number from 6 to 8\""
msgstr "Print \"Zahl von 6,7, oder  8\""

#: source/text/sbasic/shared/03090404.xhp#par_id3153144.22
msgid  "Case Is > 8 And iVar < 11"
msgstr "Case Is > 8 And iVar < 11"

#: source/text/sbasic/shared/03090404.xhp#par_id3147436.23
msgid  "Print \"Greater than 8\""
msgstr "Print \"Größer als 8\""

#: source/text/sbasic/shared/03090404.xhp#par_id3155418.24
msgid  "Case Else"
msgstr "Case Else"

#: source/text/sbasic/shared/03090404.xhp#par_id3150418.25
msgid  "Print \"Outside range 1 to 10\""
msgstr "Print \"Außerhalb von 1 bis 10\""

#: source/text/sbasic/shared/03090404.xhp#par_id3156285.26
msgid  "End Select"
msgstr "End Select"

#: source/text/sbasic/shared/03090404.xhp#par_id3149582.27
msgid  "end sub"
msgstr "end sub"

