msgstr "Hinzufügen"

#: mm_cusaddlis.xhp#par_idN10564.help.text
msgid "<ahelp hid=\".\">Inserts a new text field.</ahelp>"
msgstr "<ahelp hid=\".\">Fügt ein neues Textfeld ein.</ahelp>"

