"<bookmark_value>Text; verbergen</bookmark_value>      "
"<bookmark_value>Bereiche; ausblenden</bookmark_value>      "
"<bookmark_value>Absätze; verbergen</bookmark_value>      "
"<bookmark_value>verbergen;mit Bedingungen</bookmark_value>      "
"<bookmark_value>Variablem; um Text zu verbergen</bookmark_value>"

#: hidden_text.xhp#hd_id3148675.38.help.text
msgid "To Hide a Section"
msgstr "So verbergen Sie einen Bereich:"

#: hidden_text.xhp#hd_id3148856.1.help.text
msgid ""
"<variable id=\"hidden_text\"><link href=\"text/swriter/guide/hidden_text.xhp\" "
"name=\"Hiding Text\">Hiding Text</link> </variable>"
msgstr ""
"<variable id=\"hidden_text\"><link href=\"text/swriter/guide/hidden_text.xhp\" "
"name=\"Verstecken von Text\">Verstecken von Text</link></variable>"

#: hidden_text.xhp#hd_id3154256.33.help.text
