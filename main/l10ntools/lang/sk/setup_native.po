#: source/mac/macinstall.ulf#OKLabel
msgid  "Ok"
msgstr "Ok"

#: source/mac/macinstall.ulf#InstallLabel
msgid  "Install"
msgstr "Inštalovať"

#: source/mac/macinstall.ulf#AbortLabel
msgid  "Abort"
msgstr "Prerušiť"

#: source/mac/macinstall.ulf#IntroText1
msgid  "Welcome to the [FULLPRODUCTNAME] Installation Wizard"
msgstr "Vitajte v sprievodcovi inštaláciou [FULLPRODUCTNAME]"

#: source/mac/macinstall.ulf#IntroText2
msgid  "This installation will update your installed versions of [PRODUCTNAME]"
msgstr "Inštalácia aktualizuje vašu nainštalovanú verziu [PRODUCTNAME]"

#: source/mac/macinstall.ulf#IntroText3
msgid  "This might take a moment."
msgstr "Toto môže chvíľu trvať."

#: source/mac/macinstall.ulf#ChooseMyOwnText
msgid  "Not listed (choose location in an extra step)"
msgstr "Nie je v zozname (zvoľte umiestnenie v ďalšom kroku)"

#: source/mac/macinstall.ulf#ListPromptText
msgid  "Choose [PRODUCTNAME] [PRODUCTVERSION] installation for which you want to install the [FULLPRODUCTNAME]"
msgstr "Zvoľte inštaláciu [PRODUCTNAME] [PRODUCTVERSION] pre ktorú chcete inštalovať [FULLPRODUCTNAME]"

#: source/mac/macinstall.ulf#ChooseManualText
msgid  "Point the dialog to your [PRODUCTNAME] [PRODUCTVERSION] installation."
msgstr "Označte v dialógovom okne vašu inštaláciu [PRODUCTNAME] [PRODUCTVERSION]."

#: source/mac/macinstall.ulf#ListOKLabelText
msgid  "Install"
msgstr "Inštalovať"

#: source/mac/macinstall.ulf#ListCancelLabel
msgid  "Abort"
msgstr "Prerušiť"

#: source/mac/macinstall.ulf#AppInvalidText1
msgid  "This is not a valid [PRODUCTNAME] [PRODUCTVERSION] installation."
msgstr "Toto nie je planá inštalácia [PRODUCTNAME] [PRODUCTVERSION]."

#: source/mac/macinstall.ulf#AppInvalidText2
msgid  "Run the installer again and choose a valid [PRODUCTNAME] [PRODUCTVERSION] installation"
msgstr "Spustite znovu inštalátor a zvoľte platnú inštaláciu [PRODUCTNAME] [PRODUCTVERSION]."

#: source/mac/macinstall.ulf#StartInstallText1
msgid  "Click Install to start the installation"
msgstr "Kliknite na Inštalovať pre začatie inštalácie."

#: source/mac/macinstall.ulf#StartInstallText2
msgid  "Installation might take a minute..."
msgstr "Inštalácie môže trvať niekoľko minút..."

#: source/mac/macinstall.ulf#IdentifyQText
msgid  "Installation failed, most likely your account does not have the necessary privileges."
msgstr "Inštalácia zlyhala, váš účet pravdepodobne nemá dostatočné práva."

#: source/mac/macinstall.ulf#IdentifyQText2
msgid  "Do you want to identify as administrator and try again?"
msgstr "Chcete sa prihlásiť ako administrátor a skúsiť znova?"

#: source/mac/macinstall.ulf#IdentifyYES
msgid  "Yes, identify"
msgstr "Áno, prihlásiť"

#: source/mac/macinstall.ulf#IdentifyNO
msgid  "No, abort installation"
msgstr "Nie, prerušiť inštaláciu"

#: source/mac/macinstall.ulf#InstallFailedText
msgid  "Installation failed."
msgstr "Inštalácia zlyhala."

#: source/mac/macinstall.ulf#InstallCompleteText
msgid  "Installation of [PRODUCTNAME] language pack completed."
msgstr "Inštalácia jazykového balíčka [PRODUCTNAME] dokončená."

#: source/mac/macinstall.ulf#InstallCompleteText2
msgid  "Call '[PRODUCTNAME]-Preferences-Language Settings-Languages' to change the user interface language."
msgstr "Vyberte '[PRODUCTNAME]-Možnosti-Nastavenia jazyka-Jazyky' na zmenu jazyka používateľského rozhrania."

#: source/mac/macinstall.ulf#InstallCompleteTextPatch
msgid  "Installation of [FULLPRODUCTNAME] completed"
msgstr "Inštalácia [FULLPRODUCTNAME] dokončená."
