msgid  "substituted"
msgstr "nahradené"

#: source/resource/strings.src#RID_STR_CONTROL_SUBSTITUTED_EPXPLAIN.String.Text
msgid  "An error occurred while this control was being loaded. It was therefore replaced with a placeholder."
msgstr "Objavila sa chyba pri načítaní tohto ovládacieho prvku, ktorý bol preto nahradený len zástupcom."

#: source/resource/strings.src#RID_STR_READERROR.String.Text
msgid  "Error reading data from database"
msgstr "Chyba pri čítaní­ dát z databázy"

#: source/resource/strings.src#RID_STR_CONNECTERROR.String.Text
msgid  "Connection failed"
msgstr "Spojenie prerušené"

