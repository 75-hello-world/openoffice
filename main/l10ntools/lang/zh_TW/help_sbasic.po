msgstr "Sub Sample1"

#: source/text/sbasic/guide/sample_code.xhp#par_id3155335.163
msgid  "BasicLibraries.LoadLibrary("Tools")"
msgstr ""

#: source/text/sbasic/guide/sample_code.xhp#par_id3163808.137
msgid  "oDialog1 = LoadDialog("Standard", "Dialog1")"
msgstr ""

