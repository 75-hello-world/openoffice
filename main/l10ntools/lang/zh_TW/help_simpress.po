#: main0102.xhp#hd_id3153726.1.help.text
msgid "<link href=\"text/simpress/main0102.xhp\" name=\"Edit\">Edit</link>"
msgstr "<link href=\"text/simpress/main0102.xhp\" name=\"編輯\">編輯</link>"

#: main0102.xhp#par_id3151075.2.help.text
msgid ""
