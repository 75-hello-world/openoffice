msgid "Apply"
msgstr "Прилагане"

#: 05060201.xhp#hd_id3147091.6.help.text
msgid "Wrap Region"
msgstr "Работна площ"

#: 05060201.xhp#par_id3147217.7.help.text
msgid ""
"<ahelp hid=\"HID_CONTDLG_WORKPLACE\">Deletes the custom contour. Click here, "
"and then click in the preview area.</ahelp>"
msgstr ""
"<ahelp hid=\"HID_CONTDLG_WORKPLACE\">Изтрива зададения контур. Щракнете тук, "
"после щракнете в областта за мострата.</ahelp>"

#: 05060201.xhp#par_id3147579.help.text
msgid ""
"<image id=\"img_id3147585\" src=\"svx/res/cd02.png\" width=\"0.222inch\" "
"height=\"0.222inch\"><alt id=\"alt_id3147585\">Icon</alt></image>"
msgstr ""
"<image id=\"img_id3147585\" src=\"svx/res/cd02.png\" width=\"0.222inch\" "
"height=\"0.222inch\"><alt id=\"alt_id3147585\">Икона</alt></image>"

#: 05060201.xhp#par_id3153351.8.help.text
msgid "Work area"
msgstr "Работна област"

#: 05060201.xhp#hd_id3149170.9.help.text
