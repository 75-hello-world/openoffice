msgstr "<image id=\"img_id2405774\" src=\"res/commandimagelist/sc_pararighttoleft.png\" width=\"0.1665in\" height=\"0.1665in\"><alt id=\"alt_id2405774\">икона отдясно наляво</alt></image>"

#: source/text/swriter/main0202.xhp#par_id3147625.15
