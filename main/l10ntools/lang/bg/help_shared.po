msgid  "\<link href=\"text/shared/optionen/01010800.xhp\" name=\"View\"\>View\</link\>"
msgstr ""

#: source/text/shared/optionen/01010800.xhp#par_id3155630.2
msgid  "\<ahelp hid=\"HID_OFA_TP_VIEW\"\>Specifies view options.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010800.xhp#hd_id3145171.45
msgid  "Scaling"
msgstr "Мащабиране"

#: source/text/shared/optionen/01010800.xhp#par_id3158407.46
msgid  "\<ahelp hid=\"OFFMGR:METRICFIELD:OFA_TP_VIEW:MF_SCALING\"\>Uses percentile scaling for font size in user interface elements, such as dialogs and icon labels.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010800.xhp#par_id3159154.67
msgid  "The \<emph\>Scaling\</emph\> setting does not affect the font size of text in a document."
msgstr ""

#: source/text/shared/optionen/01010800.xhp#hd_id3149123.112
msgid  "Icon size and style"
msgstr "Размер и стил на иконите"

#: source/text/shared/optionen/01010800.xhp#par_id3153947.113
msgid  "\<ahelp hid=\"OFFMGR_LISTBOX_OFA_TP_VIEW_LB_BIG\"\>Specifies the display size of toolbar icons.\</ahelp\>\<switchinline select=\"sys\"\>\<caseinline select=\"WIN\"\>The \<emph\>Automatic\</emph\> option uses the font size settings of your operating system for menus.\</caseinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/optionen/01010800.xhp#par_id4664754
msgid  "\<ahelp hid=\".\" visibility=\"hidden\"\>Select the icon style for icons in toolbars and dialogs.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010800.xhp#par_idN10738
msgid  "Use system font for user interface"
msgstr "Използване на системния шрифт за потребителския интерфейс"

#: source/text/shared/optionen/01010800.xhp#par_idN1073C
msgid  "\<ahelp hid=\"svx_CheckBox_OFA_TP_VIEW_CB_SYSTEM_FONT\"\>Specifies to use the system font to display all menus and dialogs. Else another installed font is used.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010800.xhp#hd_id3400982
msgid  "Screen font antialiasing"
msgstr "Изглаждане на екранните шрифтове"

#: source/text/shared/optionen/01010800.xhp#par_id6649372
msgid  "\<ahelp hid=\".\"\>Select to smooth the screen appearance of text.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010800.xhp#par_id4743797
msgid  "\<ahelp hid=\".\"\>Enter the smallest font size to apply antialiasing.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010800.xhp#par_idN107A3
msgid  "Menu"
msgstr "Меню"

#: source/text/shared/optionen/01010800.xhp#hd_id3156056.91
msgid  "Icons in menus"
msgstr "Икони в менютата"

#: source/text/shared/optionen/01010800.xhp#par_id3155766.92
msgid  "\<ahelp hid=\"OFFMGR_CHECKBOX_OFA_TP_VIEW_CB_MENU_ICONS\"\>Displays icons next to the corresponding menu items. Select from \"Automatic\", \"Hide\" and \"Show\". \"Automatic\" displays icons according to system settings and themes.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010800.xhp#hd_id3149262.63
msgid  "Show preview of fonts"
msgstr "Показване на мостра от шрифтовете"

#: source/text/shared/optionen/01010800.xhp#par_id3155415.64
msgid  "\<ahelp hid=\"OFFMGR:CHECKBOX:OFA_TP_VIEW:CB_FONT_SHOW\"\>Displays the names of selectable fonts in the corresponding font, for example, fonts in the Font box on the \<emph\>Formatting\</emph\> bar.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010800.xhp#hd_id3151319.68
msgid  "Show font history"
msgstr "Показване на хронология на шрифтовете"

#: source/text/shared/optionen/01010800.xhp#par_id3153513.69
msgid  "\<ahelp hid=\"OFFMGR:CHECKBOX:OFA_TP_VIEW:CB_FONT_HISTORY\"\>Lists the last five fonts that you used in the current document at the top of the list in the \<emph\>Font Name\</emph\> box on the \<emph\>Formatting\</emph\> bar.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010800.xhp#par_idN10962
msgid  "\<ahelp hid=\".\" visibility=\"hidden\"\>Specifies whether to restore the last used document view. Many view properties valid when the document was last saved will be restored.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010800.xhp#par_idN10AC8
msgid  "Graphics output"
msgstr "Графичен изход"

#: source/text/shared/optionen/01010800.xhp#par_idN10A1D
msgid  "\<ahelp hid=\"20201\"\>Press Shift+Ctrl+R to restore or refresh the view of the current document.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010800.xhp#par_idN10AD4
msgid  "Use hardware acceleration"
msgstr "Използване на хардуерно ускоряване"

#: source/text/shared/optionen/01010800.xhp#par_idN10AD8
msgid  "\<ahelp hid=\".\"\>Directly accesses hardware features of the graphical display adapter to improve the screen display.\</ahelp\> The support for hardware acceleration is not available for all operating systems and platform distributions of %PRODUCTNAME."
msgstr ""

#: source/text/shared/optionen/01010800.xhp#hd_id1208200812004470
msgid  "Use Anti-Aliasing"
msgstr "Изглаждане на назъбванията"

#: source/text/shared/optionen/01010800.xhp#par_id1208200812004444
msgid  "\<ahelp hid=\".\"\>When supported, you can enable and disable anti-aliasing of graphics. With anti-aliasing enabled, the display of most graphical objects looks smoother and with less artifacts.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010800.xhp#hd_id101920091058114
msgid  "Selection"
msgstr "Селекция"

#: source/text/shared/optionen/01010800.xhp#hd_id1019200910581166
msgid  "Transparency"
msgstr "Прозрачност"

#: source/text/shared/optionen/01010800.xhp#par_id1019200910581186
msgid  "\<ahelp hid=\".\"\>If enabled, the text selection in Writer and the cell selection in Calc will be shown using a transparent color. If not enabled, the selection will be shown by inverted colors.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010800.xhp#hd_id1019200910581266
msgid  "Transparency level"
msgstr "Степен на прозрачност"

#: source/text/shared/optionen/01010800.xhp#par_id1019200910581220
msgid  "\<ahelp hid=\".\"\>Select the transparency level for transparent selections. The default value is 75%. You can select values from 10% to 90%.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010800.xhp#hd_id3166432.47
msgid  "Mouse positioning"
msgstr "Позициониране на мишката"

#: source/text/shared/optionen/01010800.xhp#par_id3155530.48
msgid  "\<ahelp hid=\"OFFMGR:LISTBOX:OFA_TP_VIEW:LB_MOUSEPOS\"\>Specifies if and how the mouse pointer will be positioned in newly opened dialogs.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010800.xhp#hd_id3146982.93
msgid  "Middle mouse button"
msgstr "Среден бутон на мишката"

#: source/text/shared/optionen/01010800.xhp#par_id3150521.94
msgid  "\<ahelp hid=\"OFFMGR_LISTBOX_OFA_TP_VIEW_LB_MOUSEMIDDLE\"\>Defines the function of the middle mouse button.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010800.xhp#par_id3152889.97
msgid  "               \<emph\>Automatic scrolling\</emph\> - dragging while pressing the middle mouse button shifts the view."
msgstr ""

#: source/text/shared/optionen/01010800.xhp#par_id3155810.98
msgid  "               \<emph\>Paste clipboard\</emph\> - pressing the middle mouse button inserts the contents of the \"Selection clipboard\" at the cursor position. "
msgstr ""

#: source/text/shared/optionen/01010800.xhp#par_id3148703.99
msgid  "The \"Selection clipboard\" is independent of the normal clipboard that you use by \<emph\>Edit - Copy/Cut /Insert\</emph\> or the respective keyboard shortcuts. Clipboard and \"Selection clipboard\" can contain different contents at the same time."
msgstr ""

#: source/text/shared/optionen/01010800.xhp#par_id3148870.100
msgid  "                  \<emph\>Clipboard\</emph\>               "
msgstr ""

#: source/text/shared/optionen/01010800.xhp#par_id3145076.101
msgid  "                  \<emph\>Selection clipboard\</emph\>               "
msgstr ""

#: source/text/shared/optionen/01010800.xhp#par_id3156030.102
msgid  "                  \<emph\>Copy content\</emph\>               "
msgstr ""

#: source/text/shared/optionen/01010800.xhp#par_id3150110.103
msgid  "Edit - Copy Ctrl+C."
msgstr "Редактиране - Копиране Ctrl+C."

#: source/text/shared/optionen/01010800.xhp#par_id3149588.104
msgid  "Select text, table, object."
msgstr "Изберете текст, таблица или обект."

#: source/text/shared/optionen/01010800.xhp#par_id3149331.105
msgid  "                  \<emph\>Paste content\</emph\>               "
msgstr ""

#: source/text/shared/optionen/01010800.xhp#par_id3156337.106
msgid  "Edit - Paste Ctrl+V pastes at the cursor position."
msgstr "Редактиране - Поставяне Ctrl+V поставя в позицията на курсора."

#: source/text/shared/optionen/01010800.xhp#par_id3151127.107
msgid  "Clicking the middle mouse button pastes at the mouse pointer position."
msgstr "Щракването със средния бутон на мишката поставя в позицията на показалеца на мишката."

#: source/text/shared/optionen/01010800.xhp#par_id3159206.108
msgid  "                  \<emph\>Pasting into another document\</emph\>               "
msgstr ""

#: source/text/shared/optionen/01010800.xhp#par_id3148974.109
msgid  "No effect on the clipboard contents."
msgstr "Няма ефект върху съдържанието на клипборда."

#: source/text/shared/optionen/01010800.xhp#par_id3152870.110
msgid  "The last marked selection is the content of the selection clipboard."
msgstr "Съдържанието на клипборда за селекции е последната маркирана селекция."

#: source/text/shared/optionen/01010900.xhp#tit
msgid  "Print Options"
msgstr "Настройки за печатане"

#: source/text/shared/optionen/01010900.xhp#bm_id3147323
msgid  "\<bookmark_value\>printing; colors in grayscale\</bookmark_value\>\<bookmark_value\>grayscale printing\</bookmark_value\>\<bookmark_value\>colors; printing in grayscale\</bookmark_value\>\<bookmark_value\>printing; warnings\</bookmark_value\>\<bookmark_value\>paper size warning\</bookmark_value\>"
msgstr ""

#: source/text/shared/optionen/01010900.xhp#hd_id3148946.1
msgid  "\<link href=\"text/shared/optionen/01010900.xhp\" name=\"Print Options\"\>Print Options\</link\>"
msgstr ""

#: source/text/shared/optionen/01010900.xhp#par_id3150359.2
msgid  "\<ahelp hid=\".\"\>Specifies the print setting options.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010900.xhp#hd_id3148451.56
msgid  "Reduce print data"
msgstr "Редуциране на данните за печат"

#: source/text/shared/optionen/01010900.xhp#par_id3154910.68
msgid  "You can reduce the amount of data to be sent to the printer. Reducing the print data increases the print speed because the print files are smaller. This makes it easier for printers with a smaller memory to print. Reducing print data can result in slightly lower print quality."
msgstr "Можете да намалите количеството на изпращаните към принтера данни. Редуцирането на данните за печат ускорява печатането, тъй като принтерните файлове са по-малки. Така се печата по-лесно на принтери с по-малка памет. Редуцирането на данните може леко да влоши качеството на печата."

#: source/text/shared/optionen/01010900.xhp#hd_id3147085.57
msgid  "Settings for"
msgstr "Настройки за"

#: source/text/shared/optionen/01010900.xhp#par_id3158407.69
msgid  "\<ahelp hid=\"SFX2_RADIOBUTTON_TP_COMMONPRINTOPTIONS_RB_PRINTFILEOUTPUT\"\>Specifies whether the print settings apply to direct printing or to printing to a file.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010900.xhp#hd_id3145786.58
msgid  "Reduce transparency"
msgstr "Редуциране на прозрачността"

#: source/text/shared/optionen/01010900.xhp#par_id3159154.70
msgid  "\<ahelp hid=\"SFX2_CHECKBOX_TP_COMMONPRINTOPTIONS_CB_REDUCETRANSPARENCY\"\>If you mark this field the transparent objects will be printed like normal, non-transparent objects, depending on your selection in the following two option buttons.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010900.xhp#par_id3156444.71
msgid  "Transparency cannot be output directly to a printer. The areas of the document in which transparency is to be visible must therefore always be calculated as bitmaps and sent to the printer. Depending on the size of the bitmaps and the print resolution a large amount of data may result."
msgstr "Прозрачността не може да бъде изведена пряко на принтер. Затова областите от документа, в които има прозрачност, трябва да се изчислят като растерни изображения и да се изпратят към принтера. В зависимост от размера на растерните изображения и разделителната способност на печатане може да се получи голямо количество данни."

#: source/text/shared/optionen/01010900.xhp#hd_id3147441.59
msgid  "Automatically"
msgstr "Автоматично"

#: source/text/shared/optionen/01010900.xhp#par_id3150488.72
msgid  "\<ahelp hid=\"SFX2_RADIOBUTTON_TP_COMMONPRINTOPTIONS_RB_REDUCETRANSPARENCY_AUTO\"\>Specifies that the transparency is only printed if the transparent area covers less than a quarter of the entire page.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010900.xhp#hd_id3149417.73
msgid  "No transparency"
msgstr "Без прозрачност"

#: source/text/shared/optionen/01010900.xhp#par_id3153878.74
msgid  "\<ahelp hid=\"SFX2_RADIOBUTTON_TP_COMMONPRINTOPTIONS_RB_REDUCETRANSPARENCY_NONE\"\>With this option transparency is never printed.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010900.xhp#hd_id3149960.60
msgid  "Reduce bitmaps"
msgstr "Редуциране на растерните изображения"

#: source/text/shared/optionen/01010900.xhp#par_id3148455.75
msgid  "\<ahelp hid=\"SFX2_CHECKBOX_TP_COMMONPRINTOPTIONS_CB_REDUCEBITMAPS\"\>Specifies that bitmaps are printed with reduced quality. The resolution can only be reduced and not increased.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010900.xhp#hd_id3149400.61
msgid  "High/normal print quality"
msgstr "Висококачествен печат/Печат с обикновено качество"

#: source/text/shared/optionen/01010900.xhp#par_id3154510.76
msgid  "\<ahelp hid=\"SFX2_RADIOBUTTON_TP_COMMONPRINTOPTIONS_RB_REDUCEBITMAPS_NORMAL\"\>High print quality corresponds to a resolution of 300dpi. Normal print quality corresponds to a resolution of 200dpi. \</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010900.xhp#hd_id3146969.62
msgid  "Resolution"
msgstr "Разделителна способност"

#: source/text/shared/optionen/01010900.xhp#par_id3154270.77
msgid  "\<ahelp hid=\"SFX2_LISTBOX_TP_COMMONPRINTOPTIONS_LB_REDUCEBITMAPS_RESOLUTION\"\>Specifies the maximum print quality in dpi. The resolution can only be reduced and not increased.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010900.xhp#hd_id3146789.63
msgid  "Include transparent objects"
msgstr "Включване на прозрачните обекти"

#: source/text/shared/optionen/01010900.xhp#par_id3150749.78
msgid  "\<ahelp hid=\"SFX2_CHECKBOX_TP_COMMONPRINTOPTIONS_CB_REDUCEBITMAPS_TRANSPARENCY\"\>If this field is marked, the reduction in print quality for bitmaps also applies to the transparent areas of objects.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010900.xhp#hd_id3154362.64
msgid  "Reduce gradients"
msgstr "Редуциране на градиентите"

#: source/text/shared/optionen/01010900.xhp#par_id3148914.79
msgid  "\<ahelp hid=\"SFX2_CHECKBOX_TP_COMMONPRINTOPTIONS_CB_REDUCEGRADIENTS\"\>If this field is marked, gradients are printed with reduced quality.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010900.xhp#hd_id3155766.65
msgid  "Gradient stripes"
msgstr "Ивици в градиентите"

#: source/text/shared/optionen/01010900.xhp#par_id3156382.80
msgid  "\<ahelp hid=\"SFX2_NUMERICFIELD_TP_COMMONPRINTOPTIONS_NF_REDUCEGRADIENTS_STEPCOUNT\"\>Specifies the maximum number of gradient stripes for printing.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010900.xhp#hd_id3146313.66
msgid  "Intermediate color"
msgstr "Междинен цвят"

#: source/text/shared/optionen/01010900.xhp#par_id3145230.81
msgid  "\<ahelp hid=\"SFX2_RADIOBUTTON_TP_COMMONPRINTOPTIONS_RB_REDUCEGRADIENTS_COLOR\"\>Specifies that gradients are only printed in a single intermediate color.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010900.xhp#hd_id3147323.67
msgid  "Convert colors to grayscale"
msgstr "Печат в степени на сивото"

#: source/text/shared/optionen/01010900.xhp#par_id3145150.82
msgid  "\<ahelp hid=\"SFX2_CHECKBOX_TP_COMMONPRINTOPTIONS_CB_CONVERTTOGREYSCALES\"\>Specifies that all colors are printed only as grayscale.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010900.xhp#hd_id3150646.50
msgid  "Printer warnings"
msgstr "Предупреждения на принтера"

#: source/text/shared/optionen/01010900.xhp#par_id3154022.51
msgid  "\<ahelp hid=\"SFX2_CHECKBOX_TP_COMMONPRINTOPTIONS_CB_CONVERTTOGREYSCALES\"\>Defines which warnings appear before printing begins.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01010900.xhp#hd_id3147003.52
msgid  "Paper size"
msgstr "Размер на хартията"

#: source/text/shared/optionen/01010900.xhp#par_id3150206.53
msgid  "\<ahelp hid=\"SFX2_CHECKBOX_TP_COMMONPRINTOPTIONS_CB_PAPERSIZE\"\>Mark this check box if a certain paper size is needed for printing the current document.\</ahelp\> If the paper size used in the document is not provided by the current printer, you will receive an error message."
msgstr ""

#: source/text/shared/optionen/01010900.xhp#hd_id3155581.54
msgid  "Paper orientation"
msgstr "Ориентация на хартията"

#: source/text/shared/optionen/01010900.xhp#par_id3153231.55
msgid  "\<ahelp hid=\"SFX2_CHECKBOX_TP_COMMONPRINTOPTIONS_CB_PAPERORIENTATION\"\>Mark this check box if you need a certain paper orientation for printing the current document.\</ahelp\> If the format used by the current document is not available from the printer, an error message will appear."
msgstr ""

#: source/text/shared/optionen/01010900.xhp#hd_id3149531.83
msgid  "Transparency"
msgstr "Прозрачност"

#: source/text/shared/optionen/01010900.xhp#par_id3152778.84
msgid  "\<ahelp hid=\"SFX2_CHECKBOX_TP_COMMONPRINTOPTIONS_CB_TRANSPARENCY\"\>Mark this check box if you always want to be warned if transparent objects are contained in the document.\</ahelp\> If you print such a document, a dialog appears in which you can select if the transparency is to be printed in this print instruction."
msgstr ""

#: source/text/shared/optionen/01011000.xhp#tit
msgid  "Memory"
msgstr "Памет"

#: source/text/shared/optionen/01011000.xhp#bm_id3153881
msgid  "\<bookmark_value\>undoing; number of steps\</bookmark_value\>\<bookmark_value\>graphics; cache\</bookmark_value\>\<bookmark_value\>cache for graphics\</bookmark_value\>\<bookmark_value\>Quickstarter\</bookmark_value\>"
msgstr ""

#: source/text/shared/optionen/01011000.xhp#hd_id3157909.25
msgid  "\<link href=\"text/shared/optionen/01011000.xhp\" name=\"Memory\"\>Memory\</link\>"
msgstr ""

#: source/text/shared/optionen/01011000.xhp#par_id3154307.26
msgid  "\<ahelp hid=\".\"\>Defines the settings for the graphics cache and the number of steps you can undo.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01011000.xhp#hd_id3155390.1
msgid  "Undo"
msgstr "Отмяна"

#: source/text/shared/optionen/01011000.xhp#par_id3145673.2
msgid  "Defines the maximum number of reverse steps allowed."
msgstr "Определя максималния брой отменени стъпки."

#: source/text/shared/optionen/01011000.xhp#hd_id3153881.3
msgid  "Number of steps"
msgstr "Брой стъпки"

#: source/text/shared/optionen/01011000.xhp#par_id3148685.4
msgid  "\<ahelp hid=\"OFFMGR_NUMERICFIELD_OFA_TP_MEMORY_ED_UNDO\"\>You can specify the number of steps which can be undone by selecting a number from the list.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01011000.xhp#hd_id3147530.10
msgid  "Graphics cache"
msgstr "Графичен кеш"

#: source/text/shared/optionen/01011000.xhp#par_id3145069.11
msgid  "The graphics cache saves the graphics contained in a document in your computer's main memory. This means that the attributes of a graphic stored in the cache do not have to be re-calculated if you return to the page containing the graphic after scrolling through a document."
msgstr "Кешът за графики съхранява графиките от документа в основната памет на компютъра. Това означава, че атрибутите на графика, съхранявана в кеша, не е необходимо да се преизчисляват, ако се върнете към съдържащата я страница след превъртане на документа."

#: source/text/shared/optionen/01011000.xhp#hd_id3154924.12
msgid  "Use for $[officename] (MB)"
msgstr "Използване за $[officename] (МБ)"

#: source/text/shared/optionen/01011000.xhp#par_id3152813.13
msgid  "\<ahelp hid=\"OFFMGR_NUMERICFIELD_OFA_TP_MEMORY_NF_GRAPHICCACHE\"\>Specifies the total cache size for all graphics.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01011000.xhp#hd_id3150359.14
msgid  "Memory per object (MB)"
msgstr "Памет за един обект (МБ)"

#: source/text/shared/optionen/01011000.xhp#par_id3148797.15
msgid  "\<ahelp hid=\"OFFMGR_NUMERICFIELD_OFA_TP_MEMORY_NF_GRAPHICOBJECTCACHE\"\>Specifies that objects which are larger than the selected megabytes will not be placed in the cache.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01011000.xhp#hd_id3148920.16
msgid  "Remove from memory after (hh:mm)"
msgstr "Премахване от паметта след (чч:мм)"

#: source/text/shared/optionen/01011000.xhp#par_id3148674.17
msgid  "\<ahelp hid=\"OFFMGR_TIMEFIELD_OFA_TP_MEMORY_TF_GRAPHICOBJECTTIME\"\>Specifies the time that each graphic remains in the cache in hours and minutes.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01011000.xhp#hd_id3156212.18
msgid  "Cache for inserted objects"
msgstr "Кеш за вмъкнати обекти"

#: source/text/shared/optionen/01011000.xhp#hd_id3147085.19
msgid  "Number of objects"
msgstr "Брой обекти"

#: source/text/shared/optionen/01011000.xhp#par_id3153192.20
msgid  "\<ahelp hid=\"OFFMGR_NUMERICFIELD_OFA_TP_MEMORY_NF_OLECACHE\"\>Choose the maximum number of OLE objects that are pooled in the cache.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01011000.xhp#hd_id3145171.21
msgid  "$[officename] Quickstarter"
msgstr "$[officename] Quickstarter"

#: source/text/shared/optionen/01011000.xhp#hd_id3155429.22
msgid  "\<switchinline select=\"sys\"\>\<caseinline select=\"WIN\"\>Load $[officename] during system start-up \</caseinline\>\<defaultinline\>Enable systray Quickstarter\</defaultinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/optionen/01011000.xhp#par_id3152940.23
msgid  "\<ahelp hid=\"OFFMGR_CHECKBOX_OFA_TP_MEMORY_CB_QUICKLAUNCH\"\>Mark this check box if you want $[officename] to enable quickstart. This option is available if the Quickstart module has been installed.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01012000.xhp#tit
msgid  "Appearance"
msgstr "Външен вид"

#: source/text/shared/optionen/01012000.xhp#bm_id3153527
msgid  "\<bookmark_value\>colors; appearance\</bookmark_value\>\<bookmark_value\>options; appearance\</bookmark_value\>\<bookmark_value\>appearance options\</bookmark_value\>"
msgstr ""

#: source/text/shared/optionen/01012000.xhp#hd_id3153527.1
msgid  "\<link href=\"text/shared/optionen/01012000.xhp\" name=\"Appearance\"\>Appearance\</link\>"
msgstr ""

#: source/text/shared/optionen/01012000.xhp#par_id3145120.2
msgid  "\<ahelp hid=\".\"\>Sets the colors for the $[officename] user interface.\</ahelp\> You can save the current settings as color scheme and load them later."
msgstr ""

#: source/text/shared/optionen/01012000.xhp#hd_id3154046.3
msgid  "Color scheme"
msgstr "Цветова схема"

#: source/text/shared/optionen/01012000.xhp#par_id3152349.4
msgid  "Save and delete color schemes."
msgstr "Записване и изтриване на цветови схеми."

#: source/text/shared/optionen/01012000.xhp#hd_id3153252.5
msgid  "Scheme"
msgstr "Схема"

#: source/text/shared/optionen/01012000.xhp#par_id3153541.6
msgid  "\<ahelp hid=\"SVX_LISTBOX_RID_SVXPAGE_COLORCONFIG_LB_COLORSCHEME\"\>Selects the color scheme you want to use.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01012000.xhp#hd_id3153665.7
msgid  "Save"
msgstr "Съхраняване"

#: source/text/shared/optionen/01012000.xhp#par_id3145609.8
msgid  "\<ahelp hid=\"SVX_PUSHBUTTON_RID_SVXPAGE_COLORCONFIG_PB_SAVESCHEME\"\>Saves the current settings as a color scheme that you can reload later.\</ahelp\> The name is added to the \<emph\>Scheme\</emph\> box."
msgstr ""

#: source/text/shared/optionen/01012000.xhp#hd_id3149669.9
msgid  "Name of color scheme"
msgstr "Име на цветова схема"

#: source/text/shared/optionen/01012000.xhp#par_id3159413.10
msgid  "\<ahelp hid=\"HID_OPTIONS_COLORCONFIG_NAME_SCHEME\"\>Enter a name for the color scheme.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01012000.xhp#hd_id3152811.11
msgid  "Delete"
msgstr "Изтриване"

#: source/text/shared/optionen/01012000.xhp#par_id3150400.12
msgid  "\<ahelp hid=\"SVX_PUSHBUTTON_RID_SVXPAGE_COLORCONFIG_PB_DELETESCHEME\"\>Deletes the color scheme shown in the \<emph\>Scheme\</emph\> box. You cannot delete the Default scheme.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01012000.xhp#hd_id3148672.13
msgid  "Scheme"
msgstr "Схема"

#: source/text/shared/optionen/01012000.xhp#par_id3149204.14
msgid  "\<ahelp hid=\"HID_OPTIONS_COLORCONFIG_COLORLIST_WIN\"\>Select the colors for the user interface elements.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01012000.xhp#par_id3150769.15
msgid  "To apply a color to a user interface element, ensure that the checkbox in front of the name is marked. To hide a user interface element, clear the check box."
msgstr "За да приложите цвят върху елемент на интерфейса, уверете се, че е отметнато полето пред името. За да скриете елемент на интерфейса, махнете отметката."

#: source/text/shared/optionen/01012000.xhp#par_id3155430.18
msgid  "Some user interface elements cannot be hidden."
msgstr "Някои елементи на потребителския интерфейс не могат да бъдат скрити."

#: source/text/shared/optionen/01012000.xhp#par_id3145365.20
msgid  "In order to enhance cursor visibility, if the user sets the application background color between 40% and 60% gray, it is automatically changed to 40% gray."
msgstr "За да се подобри видимостта на курсора, ако потребителят избере фонов цвят на приложението между 40% и 60% сиво, той автоматично се сменя на 40% сиво."

#: source/text/shared/optionen/01012000.xhp#par_id3153144.16
msgid  "The \<emph\>Automatic\</emph\> color setting changes the user interface element to the preset color from the color scheme."
msgstr ""

#: source/text/shared/optionen/01012000.xhp#par_id3154921.17
msgid  "The color settings for \"Visited links\" and \"Unvisited links\" only apply to documents created after the settings are applied."
msgstr "Настройките за цвят „Посетени връзки“ и „Непосетени връзки“ важат само за документи, създадени след като настройките са били приложени."

#: source/text/shared/optionen/01013000.xhp#tit
msgid  "Accessibility"
msgstr "Достъпност"

#: source/text/shared/optionen/01013000.xhp#bm_id3159411
msgid  "\<bookmark_value\>disabled persons\</bookmark_value\>\<bookmark_value\>text colors for better accessibility\</bookmark_value\>\<bookmark_value\>animations; accessibility options\</bookmark_value\>\<bookmark_value\>Help tips; hiding\</bookmark_value\>\<bookmark_value\>high contrast mode\</bookmark_value\>\<bookmark_value\>accessibility; options\</bookmark_value\>\<bookmark_value\>options; accessibility\</bookmark_value\>"
msgstr ""

#: source/text/shared/optionen/01013000.xhp#hd_id3159411.1
msgid  "\<link href=\"text/shared/optionen/01013000.xhp\" name=\"Accessibility\"\>Accessibility\</link\>"
msgstr ""

#: source/text/shared/optionen/01013000.xhp#par_id3149827.2
msgid  "\<ahelp hid=\".\"\>Sets options that make \<item type=\"productname\"\>%PRODUCTNAME\</item\> programs more accessible for users with reduced sight, limited dexterity or other disabilities.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01013000.xhp#hd_id3166460.9
msgid  "Miscellaneous options"
msgstr "Разни настройки"

#: source/text/shared/optionen/01013000.xhp#par_id3152996.10
msgid  "Sets accessibility options."
msgstr "Задава настройките за достъпност."

#: source/text/shared/optionen/01013000.xhp#hd_id3154750.23
msgid  "\<switchinline select=\"sys\"\>\<caseinline select=\"UNIX\"\>\</caseinline\>\<defaultinline\>Support assistive technology tools (program restart required)\</defaultinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/optionen/01013000.xhp#par_id3155628.24
msgid  "\<switchinline select=\"sys\"\>\<caseinline select=\"UNIX\"\>\</caseinline\>\<defaultinline\>\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_ACCESSIBILITYCONFIG_CB_ACCESSIBILITY_TOOL\"\>Allows you to use assistive tools, such as external screen readers, Braille devices or speech recognition input devices. The Java Runtime Environment must be installed on your computer before you can enable assistive support.\</ahelp\>\</defaultinline\>\</switchinline\>"
msgstr ""

#: source/text/shared/optionen/01013000.xhp#hd_id3154047.27
msgid  "Use text selection cursor in read-only text document"
msgstr "Маркиране на текст в документи, отворени само за четене"

#: source/text/shared/optionen/01013000.xhp#par_id3149164.28
msgid  "\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_ACCESSIBILITYCONFIG_CB_TEXTSELECTION\"\>Displays cursor in read-only documents.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01013000.xhp#hd_id3147531.15
msgid  "Allow animated graphics"
msgstr "Разрешаване на анимирани картинки"

#: source/text/shared/optionen/01013000.xhp#par_id3145069.16
msgid  "\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_ACCESSIBILITYCONFIG_CB_ANIMATED_GRAPHICS\"\>Previews animated graphics, such as GIF images, in \<item type=\"productname\"\>%PRODUCTNAME\</item\>.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01013000.xhp#hd_id3147264.17
msgid  "Allow animated text"
msgstr "Разрешаване на анимиран текст"

#: source/text/shared/optionen/01013000.xhp#par_id3149656.18
msgid  "\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_ACCESSIBILITYCONFIG_CB_ANIMATED_TEXTS\"\>Previews animated text, such as blinking and scrolling, in \<item type=\"productname\"\>%PRODUCTNAME\</item\>.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01013000.xhp#hd_id3153381.11
msgid  "Help tips disappear after"
msgstr "Съветите на помощта изчезват след "

#: source/text/shared/optionen/01013000.xhp#par_id3150868.12
msgid  "\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_ACCESSIBILITYCONFIG_CB_TIPHELP\"\>Hides the \<link href=\"text/shared/main0108.xhp\" name=\"Help tips\"\>Help tips\</link\> after the number of seconds that you enter.\</ahelp\> Otherwise, Help tips are displayed until you press Escape or move the cursor."
msgstr ""

#: source/text/shared/optionen/01013000.xhp#hd_id3151043.13
msgid  "Seconds"
msgstr "Секунди"

#: source/text/shared/optionen/01013000.xhp#par_id3148672.14
msgid  "\<ahelp hid=\"SVX_NUMERICFIELD_RID_SVXPAGE_ACCESSIBILITYCONFIG_NF_TIPHELP\"\>Enter the number of seconds to display the Help tips.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01013000.xhp#hd_id3149809.3
msgid  "Options for high contrast appearance"
msgstr "Настройки за изглед с висок контраст"

#: source/text/shared/optionen/01013000.xhp#par_id3153106.4
msgid  "High contrast is an operating system setting that changes the system color scheme to improve readability. You can decide how \<item type=\"productname\"\>%PRODUCTNAME\</item\> uses the high contrast settings of the operating system."
msgstr ""

#: source/text/shared/optionen/01013000.xhp#par_id3125863.31
msgid  "Cell borders and shadows are always shown in text color when high contrast mode is active. The cell background color is ignored then."
msgstr "В режима с висок контраст кантовете на клетки и сенките винаги се показват с цвета на текста. Фоновият цвят на клетките се пренебрегва."

#: source/text/shared/optionen/01013000.xhp#hd_id3155132.29
msgid  "Automatically detect high contrast mode of operating system"
msgstr "Автоматично засичане на контрастен режим на операционната система"

#: source/text/shared/optionen/01013000.xhp#par_id3153768.30
msgid  "\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_ACCESSIBILITYCONFIG_CB_AUTO_DETECT_HC\"\>Switches \<item type=\"productname\"\>%PRODUCTNAME\</item\> into high contrast mode when the system background color is very dark.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01013000.xhp#hd_id3154918.19
msgid  "Use automatic font color for screen display"
msgstr "Автоматичен цвят на шрифта за екранна визуализация"

#: source/text/shared/optionen/01013000.xhp#par_id3146984.20
msgid  "\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_ACCESSIBILITYCONFIG_CB_AUTOMATIC_FONT_COLOR\"\>Displays fonts in \<item type=\"productname\"\>%PRODUCTNAME\</item\> using the system color settings. This option only affects the screen display.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01013000.xhp#hd_id3153091.7
msgid  "Use system colors for page previews"
msgstr "Използване на системните цветове в мострите на страници"

#: source/text/shared/optionen/01013000.xhp#par_id3146923.8
msgid  "\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_ACCESSIBILITYCONFIG_CB_PAGE_PREVIEWS\"\>Applies the high contrast settings of the operating system to page previews.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01014000.xhp#tit
msgid  "Network Identity"
msgstr "Мрежова самоличност"

#: source/text/shared/optionen/01014000.xhp#bm_id3153681
msgid  "\<bookmark_value\>network identity options\</bookmark_value\>\<bookmark_value\>options; network identity\</bookmark_value\>\<bookmark_value\>single sign on options\</bookmark_value\>\<bookmark_value\>LDAP server; sign on options\</bookmark_value\>\<bookmark_value\>remote configurations\</bookmark_value\>\<bookmark_value\>Configuration Manager\</bookmark_value\>"
msgstr ""

#: source/text/shared/optionen/01014000.xhp#hd_id3153681.1
msgid  "\<link href=\"text/shared/optionen/01014000.xhp\" name=\"Network Identity\"\>Network Identity\</link\>"
msgstr ""

#: source/text/shared/optionen/01014000.xhp#par_id3153562.2
msgid  "\<ahelp hid=\".\"\>Sets the options for a remote configuration access to your personal $[officename] settings stored on an LDAP server.\</ahelp\> To see this tab page and to use this feature, remote configuration must be active: You need an account on an LDAP server that is running and configured to store $[officename] user settings."
msgstr ""

#: source/text/shared/optionen/01014000.xhp#par_id3149797.7
msgid  "Using remote configuration, you can start a copy of $[officename] on any computer in the network with your own user data and personal configuration."
msgstr "Чрез отдалеченото конфигуриране можете да стартирате $[officename] с произволен компютър от мрежата, използвайки собствените си данни за потребител и личната си конфигурация."

#: source/text/shared/optionen/01014000.xhp#hd_id3155388.3
msgid  "Authentication Method"
msgstr "Метод за идентификация"

#: source/text/shared/optionen/01014000.xhp#par_id3147335.8
msgid  "\<ahelp hid=\"SVX_LISTBOX_RID_SVXPAGE_SSO_LB_MECHANISM\"\>Choose your security mechanism for access to the LDAP server.\</ahelp\> Choices are Simple or Kerberos."
msgstr ""

#: source/text/shared/optionen/01014000.xhp#hd_id3153881.4
msgid  "User name"
msgstr "Потребителско име"

#: source/text/shared/optionen/01014000.xhp#par_id3148943.9
msgid  "\<ahelp hid=\"SVX_EDIT_RID_SVXPAGE_SSO_ED_USERNAME\"\>Using \"simple\" authentication method, you enter your user name on the LDAP server here.\</ahelp\> Using \"Kerberos\", you just see your user name, but cannot edit it."
msgstr ""

#: source/text/shared/optionen/01014000.xhp#hd_id3153061.5
msgid  "Password"
msgstr "Парола"

#: source/text/shared/optionen/01014000.xhp#par_id3156343.10
msgid  "\<ahelp hid=\"SVX_EDIT_RID_SVXPAGE_SSO_ED_PASSWORD\"\>Using \"simple\" authentication method, enter your password for access to the LDAP server here.\</ahelp\> Using \"Kerberos\", this box is not active."
msgstr ""

#: source/text/shared/optionen/01014000.xhp#hd_id3146795.6
msgid  "Save password"
msgstr "Съхраняване на паролата"

#: source/text/shared/optionen/01014000.xhp#par_id3150358.11
msgid  "\<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_SSO_CB_PASSWORD\"\>Using \"simple\" authentication method, check this box to make your password persistent.\</ahelp\> If the password is persistent and the same user starts $[officename] later on, the user name and password will not be requested again. Using \"Kerberos\", this box is not active."
msgstr ""

#: source/text/shared/optionen/01014000.xhp#par_id3154939.12
msgid  "The password will be stored encrypted in a file called .ssop in your \<switchinline select=\"sys\"\>\<caseinline select=\"UNIX\"\>home directory \</caseinline\>\<defaultinline\>\"My Documents\" folder\</defaultinline\>\</switchinline\>."
msgstr ""

#: source/text/shared/optionen/01020000.xhp#tit
msgid  "Load/Save options"
msgstr "Настройки за зареждане и съхраняване"

#: source/text/shared/optionen/01020000.xhp#hd_id3147291.1
msgid  "Load/Save options"
msgstr "Настройки за зареждане и съхраняване"

#: source/text/shared/optionen/01020000.xhp#par_id3146957.2
msgid  "\<variable id=\"laden\"\>\<ahelp hid=\"\" visibility=\"visible\"\>Specifies general Load/Save settings. \</ahelp\>\</variable\>"
msgstr ""

#: source/text/shared/optionen/01020100.xhp#tit
msgid  "Proxy"
msgstr "Прокси"

#: source/text/shared/optionen/01020100.xhp#bm_id3147577
msgid  "\<bookmark_value\>settings; proxies\</bookmark_value\>\<bookmark_value\>proxy settings\</bookmark_value\>"
msgstr ""

#: source/text/shared/optionen/01020100.xhp#hd_id3156414.1
msgid  "\<link href=\"text/shared/optionen/01020100.xhp\" name=\"Proxy\"\>Proxy\</link\>"
msgstr ""

#: source/text/shared/optionen/01020100.xhp#par_id3145090.32
msgid  "\<link href=\"text/shared/00/00000002.xhp#proxy\" name=\"Proxy servers\"\>Proxy servers\</link\> for accessing the Internet can be set up manually as needed."
msgstr ""

#: source/text/shared/optionen/01020100.xhp#hd_id3147577.3
msgid  "Settings"
msgstr "Настройки"

#: source/text/shared/optionen/01020100.xhp#par_id3153748.4
msgid  "Defines the settings for the \<link href=\"text/shared/00/00000002.xhp#proxy\" name=\"proxy server\"\>proxy server\</link\>."
msgstr ""

#: source/text/shared/optionen/01020100.xhp#hd_id3151110.8
msgid  "Proxy server"
msgstr "Прокси сървър"

#: source/text/shared/optionen/01020100.xhp#par_id3147275.9
msgid  "\<ahelp hid=\"SVX:LISTBOX:RID_SVXPAGE_INET_PROXY:LB_PROXYMODE\"\>Specifies the type of proxy definition.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01020100.xhp#hd_id3147574.10
msgid  "None"
msgstr "Няма"

#: source/text/shared/optionen/01020100.xhp#par_id3148685.11
msgid  "Accesses the Internet without a proxy server. Allows you to set up a connection directly on your computer to an Internet provider that does not use a proxy."
msgstr "Достъп до Интернет без прокси сървър. Позволява ви да настроите пряка връзка от компютъра си до интернет доставчик без използване на прокси сървър."

#: source/text/shared/optionen/01020100.xhp#hd_id3150984.14
msgid  "Manual"
msgstr "Ръчно"

#: source/text/shared/optionen/01020100.xhp#par_id3156155.15
msgid  "Lets you enter the proxy server manually. Specify the proxy servers in accordance with your Internet service. Ask your system administrator for the proxies and ports to enter."
msgstr "Позволява ви да въведете ръчно данните за прокси сървър. Задайте ги в зависимост от използваната услуга за достъп до Интернет. Попитайте системния си администратор за правилните стойности, които трябва да въведете."

#: source/text/shared/optionen/01020100.xhp#par_id3148943.29
msgid  "Type server names without the protocol prefix. For example, type www.example.com, not http://www.example.com."
msgstr "Въвеждайте имената на сървъри без представката за протокол, например www.example.com, а не http://www.example.com."

#: source/text/shared/optionen/01020100.xhp#par_idN10684
msgid  "System"
msgstr "Системен"

#: source/text/shared/optionen/01020100.xhp#par_idN10688
msgid  "On Windows or UNIX systems using GNOME or KDE, this option tells %PRODUCTNAME to use the system settings. You must restart %PRODUCTNAME to initiate this setting."
msgstr "В Windows или UNIX с GNOME или KDE тази алтернатива указва на %PRODUCTNAME да използва системните настройки. За да я задействате, трябва да рестартирате %PRODUCTNAME."

#: source/text/shared/optionen/01020100.xhp#hd_id3148948.16
msgid  "HTTP proxy"
msgstr "Прокси за HTTP"

#: source/text/shared/optionen/01020100.xhp#par_id3154923.17
msgid  "\<ahelp hid=\"SVX:EDIT:RID_SVXPAGE_INET_PROXY:ED_HTTP_PROXY\"\>Type the name of the proxy server for \<link href=\"text/shared/00/00000002.xhp#http\" name=\"HTTP\"\>HTTP\</link\>.\</ahelp\> Type the port in the right-hand field."
msgstr ""

#: source/text/shared/optionen/01020100.xhp#hd_id9971922
msgid  "HTTPS proxy"
msgstr "Прокси за HTTPS"

#: source/text/shared/optionen/01020100.xhp#par_id5319071
msgid  "\<ahelp hid=\".\"\>Type the name of the proxy server for HTTPS. Type the port in the right-hand field.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01020100.xhp#hd_id3150543.33
msgid  "FTP proxy"
msgstr "Прокси за FTP"

#: source/text/shared/optionen/01020100.xhp#par_id3154138.34
msgid  "\<ahelp hid=\"SVX_EDIT_RID_SVXPAGE_INET_PROXY_ED_FTP_PROXY\"\>Type the name of the proxy server for \<link href=\"text/shared/00/00000002.xhp#ftp\" name=\"FTP\"\>FTP\</link\>.\</ahelp\> Type the port in the right-hand field."
msgstr ""

#: source/text/shared/optionen/01020100.xhp#hd_id3125863.24
msgid  "No proxy for"
msgstr "Без прокси за"

#: source/text/shared/optionen/01020100.xhp#par_id3151178.25
msgid  "\<ahelp hid=\"SVX:EDIT:RID_SVXPAGE_INET_PROXY:ED_NOPROXYFOR\"\>Specifies the names of the servers that do not require any proxy servers, separated by semicolons.\</ahelp\> These are servers addressed in your local network, and servers used for video and audio streaming, for example."
msgstr ""

#: source/text/shared/optionen/01020100.xhp#par_id3145171.26
msgid  "You can also use placeholders for the names of hosts and domains. For example, type *.sun.com to address all the hosts in the sun.com domain without proxy."
msgstr "Можете да използвате заместващи знаци за имената на хостове и домейни. Например въведете *.sun.com, за да адресирате всички хостове от домейна sun.com без прокси."

#: source/text/shared/optionen/01020100.xhp#hd_id3149808.27
msgid  "Port"
msgstr "Порт"

#: source/text/shared/optionen/01020100.xhp#par_id3153666.28
msgid  "\<ahelp hid=\"SVX:EDIT:RID_SVXPAGE_INET_PROXY:ED_SOCKS_PORT\"\>Type the port for the corresponding proxy server.\</ahelp\> The maximum value of a port number is fixed at 65535."
msgstr ""

#: source/text/shared/optionen/01020200.xhp#tit
msgid  "Search"
msgstr "Търсене"

#: source/text/shared/optionen/01020200.xhp#bm_id3154515
msgid  "\<bookmark_value\>search engines; selecting\</bookmark_value\>"
msgstr ""

#: source/text/shared/optionen/01020200.xhp#hd_id3155892.1
msgid  "\<link href=\"text/shared/optionen/01020200.xhp\" name=\"Search\"\>Search\</link\>"
msgstr ""

#: source/text/shared/optionen/01020200.xhp#par_id3153662.2
msgid  "\<ahelp hid=\"HID_OPTIONS_SEARCH\"\>Use this tab page to define the settings for searching in the Internet.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01020200.xhp#hd_id3154306.7
msgid  "Search in"
msgstr "Търсене в"

#: source/text/shared/optionen/01020200.xhp#par_id3157909.8
msgid  "Specifies the search engine and its corresponding configuration."
msgstr "Задава търсещата машина и съответната й конфигурация."

#: source/text/shared/optionen/01020200.xhp#hd_id3154515.9
msgid  "Search engines"
msgstr "Търсещи машини"

#: source/text/shared/optionen/01020200.xhp#par_id3153880.10
msgid  "\<ahelp hid=\"SVX:LISTBOX:RID_SVXPAGE_INET_SEARCH:LB_SEARCH\"\>Displays the available search engines.\</ahelp\> Select a search engine to display its configuration in the adjacent text and option boxes."
msgstr ""

#: source/text/shared/optionen/01020200.xhp#hd_id3150276.11
msgid  "Name"
msgstr "Име"

#: source/text/shared/optionen/01020200.xhp#par_id3155449.12
msgid  "\<ahelp hid=\"SVX:EDIT:RID_SVXPAGE_INET_SEARCH:ED_SEARCH_NAME\"\>Displays the name of the selected search engine.\</ahelp\> This is the name that will be shown in the sub-menu of the \<emph\>Search\</emph\> icon on the Hyperlink bar. "
msgstr ""

#: source/text/shared/optionen/01020200.xhp#hd_id3151245.13
msgid  "Type"
msgstr "Тип"

#: source/text/shared/optionen/01020200.xhp#par_id3154758.14
msgid  "\<ahelp hid=\"SVX:RADIOBUTTON:RID_SVXPAGE_INET_SEARCH:RB_EXACT\"\>Sets the advanced search preferences (and, or, exact) as well as the way the results are defined and displayed.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01020200.xhp#par_id3148663.15
msgid  "The links \<emph\>And, Or\</emph\> and \<emph\>Exactly\</emph\> are available."
msgstr ""

#: source/text/shared/optionen/01020200.xhp#hd_id3150358.17
msgid  "Prefix"
msgstr "Префикс"

#: source/text/shared/optionen/01020200.xhp#par_id3154138.18
msgid  "\<ahelp hid=\"SVX:EDIT:RID_SVXPAGE_INET_SEARCH:ED_URL\"\>This text box displays the URL and the first part of the query command for a search machine. A URL can also be typed directly.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01020200.xhp#hd_id3153193.22
msgid  "Suffix"
msgstr "Суфикс"

#: source/text/shared/optionen/01020200.xhp#par_id3153970.23
msgid  "The search terms are passed between the prefix and the suffix.\<ahelp hid=\"SVX:EDIT:RID_SVXPAGE_INET_SEARCH:ED_POSTFIX\"\>The suffix provides the command sequence, sent at the end of a search.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01020200.xhp#hd_id3147086.24
msgid  "Separator"
msgstr "Разделител"

#: source/text/shared/optionen/01020200.xhp#par_id3150870.25
msgid  "\<ahelp hid=\"SVX:EDIT:RID_SVXPAGE_INET_SEARCH:ED_SEPARATOR\"\>If you type more than one word while running a search, these words will be separated by the character specified here.\</ahelp\> Most search engines require the \"+\" character."
msgstr ""

#: source/text/shared/optionen/01020200.xhp#par_id3149560.27
msgid  "To determine which prefixes, separators and suffixes to use when defining a new search engine:"
msgstr "За да определите какви префикси, разделители и суфикси да използвате, когато задавате нова търсеща машина:"

#: source/text/shared/optionen/01020200.xhp#par_id3159255.28
msgid  "Activate the search engine and perform a search with at least two words."
msgstr "Отворете сайта за търсене и извършете търсене за поне две думи."

#: source/text/shared/optionen/01020200.xhp#par_id3147434.29
msgid  "Copy the URL from your browser's \<emph\>URL\</emph\> field and paste it into a blank document."
msgstr ""

#: source/text/shared/optionen/01020200.xhp#par_id3148647.30
msgid  "Change the conditions for the search in the search engine, if it offers you the choice."
msgstr "Променете условията на търсенето в търсещата машина, ако тя позволява."

#: source/text/shared/optionen/01020200.xhp#par_id3147317.31
msgid  "Again, copy the contents of the \<emph\>URL\</emph\> field."
msgstr ""

#: source/text/shared/optionen/01020200.xhp#par_id3147213.32
msgid  "Compare the URLs with the sample URLs in the fields of this dialog and make a note of the prefixes, suffixes and separators used."
msgstr "Сравнете URL адресите с примерните URL от полетата на този диалогов прозорец и забележете какви префикс, суфикс и разделител са използвани."

#: source/text/shared/optionen/01020200.xhp#hd_id3147348.33
msgid  "Case match"
msgstr "Регистър на буквите"

#: source/text/shared/optionen/01020200.xhp#par_id3154918.34
msgid  "\<ahelp hid=\"SVX:LISTBOX:RID_SVXPAGE_INET_SEARCH:ED_CASE\"\>Determines the case match for running a search.\</ahelp\> The list contains the following entries: \<emph\>None\</emph\>, \<emph\>Upper\</emph\> and \<emph\>Lower\</emph\>. The search terms entered are converted according to your selection."
msgstr ""

#: source/text/shared/optionen/01020200.xhp#par_id3149262.35
msgid  "Search engines handle queries differently according to the way they are constructed. For example, some are case sensitive and some are not. Because of this, it is best to select \<emph\>None\</emph\> as the case match option."
msgstr ""

#: source/text/shared/optionen/01020200.xhp#hd_id3156283.44
msgid  "New"
msgstr "Нов"

#: source/text/shared/optionen/01020200.xhp#par_id3149413.45
msgid  "\<ahelp hid=\"SVX:PUSHBUTTON:RID_SVXPAGE_INET_SEARCH:PB_NEW\"\>Use this button to include a new search engine.\</ahelp\>\<emph\>New\</emph\> deletes all important entries of the entry mask such as \"Name\" and \"Suffix\". Enter the new search engine together with all of the necessary details. As soon as you accept your entries with \<emph\>Add\</emph\>, the new search engine will appear in the search engine list under \<emph\>Search in\</emph\>. Close the dialog with the \<emph\>OK\</emph\> button."
msgstr ""

#: source/text/shared/optionen/01020200.xhp#hd_id3145254.36
msgid  "Add"
msgstr "Добавяне"

#: source/text/shared/optionen/01020200.xhp#par_id3144767.37
msgid  "\<ahelp hid=\"SVX:PUSHBUTTON:RID_SVXPAGE_INET_SEARCH:PB_ADD\"\>Click this button to add a new configuration to the list.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01020200.xhp#hd_id3155603.38
msgid  "Modify"
msgstr "Промяна"

#: source/text/shared/optionen/01020200.xhp#par_id3151320.39
msgid  "\<ahelp hid=\"SVX:PUSHBUTTON:RID_SVXPAGE_INET_SEARCH:PB_CHANGE\"\>Click this button to apply the modifications made to an existing configuration.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01020200.xhp#hd_id3149122.40
msgid  "Delete"
msgstr "Изтриване"

#: source/text/shared/optionen/01020200.xhp#par_id3154270.41
msgid  "\<ahelp hid=\"SVX:PUSHBUTTON:RID_SVXPAGE_INET_SEARCH:PB_DELETE\"\>Click this button to delete the selected search engine from the list. There will be no security response.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01020300.xhp#tit
msgid  "E-mail"
msgstr "Е-поща"

#: source/text/shared/optionen/01020300.xhp#par_idN1054D
msgid  "\<link href=\"text/shared/optionen/01020300.xhp\"\>E-mail\</link\>"
msgstr ""

#: source/text/shared/optionen/01020300.xhp#par_idN1056B
msgid  "On UNIX systems, specifies the e-mail program to use when you send the current document as e-mail."
msgstr "В системи с UNIX задава програмата за електронна поща, използвана за изпращане на текущия документ."

#: source/text/shared/optionen/01020300.xhp#par_idN10576
msgid  "E-mail program"
msgstr "Програма за е-поща"

#: source/text/shared/optionen/01020300.xhp#par_idN1057A
msgid  "\<ahelp hid=\"703252484\"\>Enter the e-mail program path and name.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01020300.xhp#par_idN10591
msgid  "..."
msgstr "..."

#: source/text/shared/optionen/01020300.xhp#par_idN10595
msgid  "\<ahelp hid=\"703255045\"\>Opens a file dialog to select the e-mail program.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030000.xhp#tit
msgid  "Internet options"
msgstr "Настройки за Интернет"

#: source/text/shared/optionen/01030000.xhp#hd_id3154926.1
msgid  "Internet options"
msgstr "Настройки за Интернет"

#: source/text/shared/optionen/01030000.xhp#par_id3154894.2
msgid  "\<variable id=\"laden\"\>\<ahelp hid=\".\"\>Specifies Internet settings.\</ahelp\>\</variable\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#tit
msgid  "Security"
msgstr "Сигурност"

#: source/text/shared/optionen/01030300.xhp#bm_id2322153
msgid  "\<bookmark_value\>macros;selecting security warnings\</bookmark_value\>\<bookmark_value\>security;options for documents with macros\</bookmark_value\>\<bookmark_value\>macros;security\</bookmark_value\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#hd_id3147588.1
msgid  "\<link href=\"text/shared/optionen/01030300.xhp\" name=\"Security\"\>Security\</link\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#par_id3153255.2
msgid  "\<ahelp hid=\".\"\>Defines the security options for saving documents, for web connections, and for opening documents that contain macros.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#par_idN10640
msgid  "Options"
msgstr "Настройки"

#: source/text/shared/optionen/01030300.xhp#par_idN10644
msgid  "\<ahelp hid=\".\"\>Opens the \"Security options and warning\" dialog.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#par_id5616626
msgid  "The Security options and warnings dialog contains the following controls:"
msgstr "Диалоговият прозорец Настройки и предупреждения за сигурността съдържа следните елементи за управление:"

#: source/text/shared/optionen/01030300.xhp#par_idN10647
msgid  "When saving or sending"
msgstr "При съхраняване или изпращане"

#: source/text/shared/optionen/01030300.xhp#par_idN1064B
msgid  "\<ahelp hid=\"703988739\"\>Select to see a warning dialog when you try to save or send a document that contains recorded changes, versions, or comments.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#par_idN1064E
msgid  "When printing"
msgstr "При печат"

#: source/text/shared/optionen/01030300.xhp#par_idN10652
msgid  "\<ahelp hid=\"703988740\"\>Select to see a warning dialog when you try to print a document that contains recorded changes or comments.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#par_idN10655
msgid  "When signing"
msgstr "При подписване"

#: source/text/shared/optionen/01030300.xhp#par_idN10659
msgid  "\<ahelp hid=\"703988741\"\>Select to see a warning dialog when you try to sign a document that contains recorded changes, versions, fields, references to other sources (for example linked sections or linked pictures), or comments.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#par_idN1065C
msgid  "When creating PDF files"
msgstr "При създаване на PDF файлове"

#: source/text/shared/optionen/01030300.xhp#par_idN10660
msgid  "\<ahelp hid=\"703988742\"\>Select to see a warning dialog when you try to export a document to PDF format that displays recorded changes in Writer, or that displays comments.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#par_idN10663
msgid  "Remove personal information on saving"
msgstr "Премахване на личните данни при съхраняване"

#: source/text/shared/optionen/01030300.xhp#par_idN10667
msgid  "\<ahelp hid=\"703988743\"\>Select to always remove user data from the file properties. If this option is not selected, you can still remove the personal information for the current document with the \<emph\>Reset\</emph\> button on \<emph\>File - Properties - General\</emph\>.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#par_idN1067C
msgid  "Recommend password protection on saving"
msgstr "Препоръчване на защита с парола при съхраняване"

#: source/text/shared/optionen/01030300.xhp#par_idN10680
msgid  "\<ahelp hid=\"703988744\"\>Select to always enable the \<emph\>Save with password\</emph\> option in the file save dialogs. Deselect the option to save files by default without password.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#hd_id1972106
msgid  "Ctrl-click required to follow hyperlinks"
msgstr "Проследяване на хипервръзки с Ctrl+щракване"

#: source/text/shared/optionen/01030300.xhp#par_id79042
msgid  "\<ahelp hid=\".\"\>If enabled, you must hold down the Ctrl key while clicking a hyperlink to follow that link. If not enabled, a click opens the hyperlink.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#hd_id4076357
msgid  "Passwords for web connections"
msgstr "Пароли за връзки с Уеб"

#: source/text/shared/optionen/01030300.xhp#par_id8231757
msgid  "You can enter a master password to enable easy access to sites that require a user name and password."
msgstr "Можете да въведете главна парола, за да улесните дотъпа до сайтове, които изискват потребителско име и парола."

#: source/text/shared/optionen/01030300.xhp#hd_id3168736
msgid  "Persistently save passwords protected by a master password"
msgstr "Постоянно съхранение на паролите, защитени с главна парола"

#: source/text/shared/optionen/01030300.xhp#par_id1909848
msgid  "\<ahelp hid=\".\"\>If enabled, %PRODUCTNAME will securely store all passwords that you use to access files from web servers. You can retrieve the passwords from the list after you enter the master password.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#hd_id3901791
msgid  "Master Password"
msgstr "Главна парола"

#: source/text/shared/optionen/01030300.xhp#par_id4571977
msgid  "\<ahelp hid=\".\"\>Opens the Enter Master Password dialog.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#par_id5216223
msgid  "\<ahelp hid=\".\" visibility=\"hidden\"\>Enter the master password.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#par_id7067171
msgid  "\<ahelp hid=\".\" visibility=\"hidden\"\>Enter the master password again.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#par_id7499313
msgid  "\<ahelp hid=\".\" visibility=\"hidden\"\>Enter the master password to continue.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#hd_id3283486
msgid  "Connections"
msgstr "Връзки"

#: source/text/shared/optionen/01030300.xhp#par_id3472090
msgid  "\<ahelp hid=\".\"\>Asks for the master password. If master password is correct, shows the Stored Web Connection Information dialog.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#par_id3289590
msgid  "The Stored Web Connection Information dialog shows a list of web sites and user names that you entered previously. You can select any entry and remove it from the list. You can view the password for the selected entry."
msgstr "Диалоговият прозорец Съхранена информация за връзка с Уеб показва списък на уебсайтове и потребителски имена, които сте въвеждали по-рано. Можете да изберете някой от записите и да го премахнете от списъка. Освен това можете да видите паролата за избрания запис."

#: source/text/shared/optionen/01030300.xhp#par_id7499008
msgid  "\<ahelp hid=\".\" visibility=\"hidden\"\>Removes the selected entry from the list.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#par_id7021088
msgid  "\<ahelp hid=\".\" visibility=\"hidden\"\>Removes all entries from the list.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#par_id1981261
msgid  "\<ahelp hid=\".\" visibility=\"hidden\"\>Opens a dialog where you can view and change the password for the selected entry.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030300.xhp#par_idN10687
msgid  "Macro security"
msgstr "Сигурност на макросите"

#: source/text/shared/optionen/01030300.xhp#par_idN1068B
msgid  "Adjust the security level for executing macros and specify trusted macro authors."
msgstr "Определя нивото на защита при изпълняване на макроси и авторите на макроси, на които имате доверие."

#: source/text/shared/optionen/01030300.xhp#par_idN1068E
msgid  "Macro Security"
msgstr "Сигурност на макросите"

#: source/text/shared/optionen/01030300.xhp#par_idN10692
msgid  "\<ahelp hid=\"703992332\"\>Opens the \<emph\>Macro Security\</emph\> dialog.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030500.xhp#tit
msgid  "HTML compatibility"
msgstr "Съвместимост за HTML"

#: source/text/shared/optionen/01030500.xhp#bm_id3155132
msgid  "\<bookmark_value\>$[officename] Basic scripts in HTML documents\</bookmark_value\>\<bookmark_value\>HTML;compatibility settings\</bookmark_value\>"
msgstr ""

#: source/text/shared/optionen/01030500.xhp#hd_id3153821.1
msgid  "\<link href=\"text/shared/optionen/01030500.xhp\" name=\"HTML compatibility\"\>HTML compatibility\</link\>"
msgstr ""

#: source/text/shared/optionen/01030500.xhp#par_id3156326.2
msgid  "\<ahelp hid=\"OFFMGR:TABPAGE:RID_OFAPAGE_HTMLOPT\"\>Defines settings for HTML pages.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030500.xhp#hd_id3154897.3
msgid  "Font sizes"
msgstr "Размери на шрифта"

#: source/text/shared/optionen/01030500.xhp#par_id3145673.4
msgid  "\<ahelp hid=\"OFFMGR:NUMERICFIELD:RID_OFAPAGE_HTMLOPT:NF_SIZE7\"\>Use the spin buttons \<emph\>Size 1\</emph\> to \<emph\>Size 7\</emph\> to define the respective font sizes for the HTML <font size=1> to <font size=7> tags.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030500.xhp#hd_id3148943.5
msgid  "Import"
msgstr "Импортиране"

#: source/text/shared/optionen/01030500.xhp#par_id3151385.6
msgid  "Defines the settings for importing HTML documents."
msgstr "Определя настройките за импортиране на документи във формат HTML."

#: source/text/shared/optionen/01030500.xhp#hd_id6065248
msgid  "Use 'English (USA)' locale for numbers"
msgstr "Използване на локал „Английски (САЩ)“ за числа"

#: source/text/shared/optionen/01030500.xhp#par_id8023926
msgid  "When importing numbers from an HTML page, the decimal separator and the thousands separator characters differ according to the locale of the HTML page. The clipboard however contains no information about the locale. For example, the characters \"1.000\" copied from a German Web page most possibly mean \"one thousand\" because the period is the thousands separator in a German locale. If copied from an English Web page, the same characters stand for the number 1 as in \"one dot zero zero zero\"."
msgstr "При импортиране на числа от страница на HTML, дробният разделител и разделителят на хилядите се различават според локала на страницата. Клипбордът обаче не съдържа информация за локала. Например знаците „1.000“, копирани от немска уебстраница, най-вероятно означават „хиляда“, тъй като в немския локал точката е разделител на хилядите. Ако са копирани от уебстраница на английски, същите знаци означават числото 1."

#: source/text/shared/optionen/01030500.xhp#par_id7658314
msgid  "\<ahelp hid=\".\"\>If not checked, numbers will be interpreted according to the setting in \<emph\>Language Settings - Language of - Locale setting\</emph\> in the Options dialog box. If checked, numbers will be interpreted as 'English (USA)' locale.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030500.xhp#hd_id3145068.7
msgid  "Import unknown HTML tags as fields"
msgstr "~Импортиране непознатите HTML етикети като полета"

#: source/text/shared/optionen/01030500.xhp#par_id3149295.8
msgid  "\<ahelp hid=\"OFFMGR:CHECKBOX:RID_OFAPAGE_HTMLOPT:CB_UNKNOWN_TAGS\"\>Mark this check box if you want \<link href=\"text/shared/00/00000002.xhp#tags\" name=\"tags\"\>tags\</link\> that are not recognized by $[officename] to be imported as fields.\</ahelp\> For an opening tag, an HTML_ON field will be created with the value of the tag name. For a closing tag, an HTML_OFF will be created. These fields will be converted to tags in the HTML export."
msgstr ""

#: source/text/shared/optionen/01030500.xhp#hd_id3148797.43
msgid  "Ignore font settings"
msgstr "Игнориране на настройките за шрифт"

#: source/text/shared/optionen/01030500.xhp#par_id3149202.44
msgid  "\<ahelp hid=\"OFFMGR:CHECKBOX:RID_OFAPAGE_HTMLOPT:CB_IGNORE_FONTNAMES\"\>Mark this check box to ignore all font settings when importing. The fonts that were defined in the HTML Page Style will be the fonts that will be used. \</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030500.xhp#hd_id3151177.9
msgid  "Export"
msgstr "Експортиране"

#: source/text/shared/optionen/01030500.xhp#par_id3150449.10
msgid  "\<ahelp hid=\"OFFMGR:LISTBOX:RID_OFAPAGE_HTMLOPT:LB_EXPORT\"\>Defines the settings for exporting HTML documents.\</ahelp\> To export in HTML format select the \<emph\>HTML Document\</emph\> file type in the \<link href=\"text/shared/01/01070000.xhp\" name=\"Save As\"\>\<emph\>Save As\</emph\>\</link\> dialog. You can find further instructions in the \<link href=\"text/shared/00/00000020.xhp\" name=\"Import and export filters\"\>Import and export filters\</link\> description."
msgstr ""

#: source/text/shared/optionen/01030500.xhp#par_id3145606.42
msgid  "To optimize the HTML export, select a browser or HTML standard from the Export box. If \"$[officename] Writer\" is selected, specific $[officename] Writer instructions are exported."
msgstr "За да оптимизирате експортирания код на HTML, изберете браузър или стандарт за HTML от полето Експортиране. Ако е  избрано „$[officename] Writer“, ще бъдат експортирани специфични за $[officename] Writer инструкции."

#: source/text/shared/optionen/01030500.xhp#hd_id3155132.16
msgid  "$[officename] Basic"
msgstr "$[officename] Basic"

#: source/text/shared/optionen/01030500.xhp#par_id3146120.17
msgid  "\<ahelp hid=\"OFFMGR:CHECKBOX:RID_OFAPAGE_HTMLOPT:CB_STARBASIC\"\>Mark this check box to include the $[officename] Basic instructions when exporting to HTML format.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030500.xhp#par_id3150872.29
msgid  "You must activate this option before you create the $[officename] Basic Script, since otherwise it will not be inserted. $[officename] Basic Scripts must be located in the header of the HTML document. Once you have created the macro in the $[officename] Basic IDE, it appears in the source text of the HTML document in the header."
msgstr "Тази настройка трябва да бъде включена, преди да създадете скрипт на $[officename] Basic, защото в противен случай той няма да бъде вмъкнат. Скриптовете на $[officename] Basic трябва да се намират в заглавния блок на документа на HTML. След като създадете макроса в развойната среда на $[officename] Basic, той се показва в изходния код на документа на HTML, в заглавния блок."

#: source/text/shared/optionen/01030500.xhp#hd_id3149664.45
msgid  "Display warning"
msgstr "Показване на предупреждение"

#: source/text/shared/optionen/01030500.xhp#par_id3150420.46
msgid  "\<ahelp hid=\"OFFMGR:CHECKBOX:RID_OFAPAGE_HTMLOPT:CB_STARBASIC_WARNING\"\>If this field is marked, when exporting to HTML a warning is shown that %PRODUCTNAME Basic macros will be lost.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030500.xhp#hd_id3154729.28
msgid  "Print layout"
msgstr "Оформление за печат"

#: source/text/shared/optionen/01030500.xhp#par_id3145254.39
msgid  "\<ahelp hid=\".\"\>If you mark this field, the print layout of the current document is exported as well.\</ahelp\> It can be read by $[officename], Netscape Navigator, and MS Internet Explorer."
msgstr ""

#: source/text/shared/optionen/01030500.xhp#par_id3156276.41
msgid  "The HTML filter supports CSS2 (Cascading Style Sheets Level 2) for printing documents. These capabilities are only effective if print layout export is activated."
msgstr "Филтърът за HTML поддържа CSS2 (Cascading Style Sheets Level 2) за печатане на документи. Тези възможности са в сила само ако е включено експортирането на оформление за печат."

#: source/text/shared/optionen/01030500.xhp#hd_id3144764.24
msgid  "Copy local graphics to Internet"
msgstr "Копиране на локалните графики в Интернет"

#: source/text/shared/optionen/01030500.xhp#par_id3149379.25
msgid  "\<ahelp hid=\"OFFMGR:CHECKBOX:RID_OFAPAGE_HTMLOPT:CB_LOCAL_GRF\"\>Mark this check box to automatically upload the embedded pictures to the Internet server when uploading using FTP. Use the \<emph\>Save As\</emph\> dialog to save the document and enter a complete FTP URL as the file name in the Internet.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01030500.xhp#hd_id3152960.48
msgid  "Character set"
msgstr "Знаков набор"

#: source/text/shared/optionen/01030500.xhp#par_id3149018.49
msgid  "\<ahelp hid=\"OFFMGR_LISTBOX_RID_OFAPAGE_HTMLOPT_LB_CHARSET\"\>Select the appropriate character set for the export\</ahelp\>."
msgstr ""

#: source/text/shared/optionen/01040000.xhp#tit
msgid  "Text Document Options"
msgstr "Настройки за текстов документ"

#: source/text/shared/optionen/01040000.xhp#hd_id3155628.1
msgid  "%PRODUCTNAME Writer Options"
msgstr "Настройки на %PRODUCTNAME Writer"

#: source/text/shared/optionen/01040000.xhp#par_id3145315.2
msgid  "\<variable id=\"optionentextdokument\"\>\<ahelp hid=\".uno:SwEditOptions\"\>These settings determine the way text documents that are created in $[officename] are handled. It is also possible to define settings for the current text document.\</ahelp\>\</variable\> The global settings are automatically saved."
msgstr ""

#: source/text/shared/optionen/01040000.xhp#hd_id3159399.5
msgid  "\<link href=\"text/shared/optionen/01040300.xhp\" name=\"Basic Fonts (Western)\"\>Basic Fonts (Western)\</link\>"
msgstr ""

#: source/text/shared/optionen/01040000.xhp#par_id3151385.3
msgid  "Specifies the settings for the basic fonts."
msgstr "Задава настройките за основните шрифтове."

#: source/text/shared/optionen/01040000.xhp#hd_id3148563.6
msgid  "\<link href=\"text/shared/optionen/01040300.xhp\" name=\"Basic Fonts (Asian)\"\>Basic Fonts (Asian)\</link\>"
msgstr ""

#: source/text/shared/optionen/01040000.xhp#par_id3147304.4
msgid  "Specifies the settings for the basic Asian fonts if Asian language support has been activated under \<emph\>\<switchinline select=\"sys\"\>\<caseinline select=\"MAC\"\>%PRODUCTNAME - Preferences\</caseinline\>\<defaultinline\>Tools - Options\</defaultinline\>\</switchinline\> - Language Settings - Languages.\</emph\>"
msgstr ""

#: source/text/shared/optionen/01040000.xhp#hd_id3149294.7
msgid  "\<link href=\"text/shared/optionen/01040300.xhp\" name=\"Basic Fonts (CTL)\"\>Basic Fonts (CTL)\</link\>"
msgstr ""

#: source/text/shared/optionen/01040000.xhp#par_id3150792.8
msgid  "Specifies the settings for basic fonts for complex text layout languages if their support has been activated under \<emph\>\<switchinline select=\"sys\"\>\<caseinline select=\"MAC\"\>%PRODUCTNAME - Preferences\</caseinline\>\<defaultinline\>Tools - Options\</defaultinline\>\</switchinline\> - Language Settings - Languages\</emph\>."
msgstr ""

#: source/text/shared/optionen/01040200.xhp#tit
msgid  "View"
msgstr "Изглед"

#: source/text/shared/optionen/01040200.xhp#bm_id3156346
msgid  "\<bookmark_value\>guides; showing when moving frames (Writer)\</bookmark_value\>\<bookmark_value\>simple handles (Writer)\</bookmark_value\>\<bookmark_value\>handles; displaying (Writer)\</bookmark_value\>\<bookmark_value\>large handles (Writer)\</bookmark_value\>\<bookmark_value\>scrollbars; horizontal and vertical (Writer)\</bookmark_value\>\<bookmark_value\>horizontal scrollbars (Writer)\</bookmark_value\>\<bookmark_value\>vertical scrollbars (Writer)\</bookmark_value\>\<bookmark_value\>smooth scrolling (Writer)\</bookmark_value\>\<bookmark_value\>displaying; pictures and objects (Writer)\</bookmark_value\>\<bookmark_value\>pictures; displaying in Writer (Writer)\</bookmark_value\>\<bookmark_value\>objects; displaying in text documents\</bookmark_value\>\<bookmark_value\>displaying; tables (Writer)\</bookmark_value\>\<bookmark_value\>tables in text; displaying\</bookmark_value\>\<bookmark_value\>limits of tables (Writer)\</bookmark_value\>\<bookmark_value\>borders;table boundaries (Writer)\</bookmark_value\>\<bookmark_value\>boundaries of tables (Writer)\</bookmark_value\>\<bookmark_value\>showing; drawings and controls (Writer)\</bookmark_value\>\<bookmark_value\>drawings; showing (Writer)\</bookmark_value\>\<bookmark_value\>controls; showing (Writer)\</bookmark_value\>\<bookmark_value\>fields;displaying field codes (Writer)\</bookmark_value\>\<bookmark_value\>displaying; comments in text documents\</bookmark_value\>"
msgstr ""

#: source/text/shared/optionen/01040200.xhp#hd_id3145090.1
msgid  "\<link href=\"text/shared/optionen/01040200.xhp\" name=\"View\"\>View\</link\>"
msgstr ""

#: source/text/shared/optionen/01040200.xhp#par_id3147088.2
msgid  "\<ahelp hid=\".\"\>Defines the default settings for displaying objects in your text documents and also the default settings for the window elements.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01040200.xhp#hd_id3145072.3
msgid  "Guides"
msgstr "Помощни линии"

#: source/text/shared/optionen/01040200.xhp#par_id3153527.4
msgid  "Specifies settings that refer to the representation of boundaries."
msgstr "Задава настройки, свързани с изобразяването на граници."

#: source/text/shared/optionen/01040200.xhp#hd_id3156346.9
msgid  "Guides while moving"
msgstr "Помощни линии при местене"

#: source/text/shared/optionen/01040200.xhp#par_id3146798.10
msgid  "\<ahelp hid=\"SW_CHECKBOX_TP_CONTENT_OPT_CB_CROSS\"\>Displays guides around the frames when frames are moved. You can select the\<emph\> Guides while moving \</emph\>option to show the exact position of the object using lineal values.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01040200.xhp#hd_id3155420.23
msgid  "Simple handles"
msgstr "Прости манипулатори"

#: source/text/shared/optionen/01040200.xhp#par_id3156327.24
msgid  "\<ahelp hid=\"SW_CHECKBOX_TP_CONTENT_OPT_CB_HANDLE\"\>Displays the handles (the eight points on a delimiting rectangle) as simple squares without 3D effect.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01040200.xhp#hd_id3149763.32
msgid  "Large handles"
msgstr "Големи манипулатори"

#: source/text/shared/optionen/01040200.xhp#par_id3153665.33
msgid  "\<ahelp hid=\"SW_CHECKBOX_TP_CONTENT_OPT_CB_BIGHANDLE\"\>Displays larger sized handles (the eight points on a delimiting rectangle).\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01040200.xhp#hd_id3149416.11
msgid  "View"
msgstr "Изглед"

#: source/text/shared/optionen/01040200.xhp#par_id3155922.12
msgid  "Specifies whether scrollbars and rulers are displayed."
msgstr "Указва дали да се показват лентите с плъзгачи и скалите."

#: source/text/shared/optionen/01040200.xhp#hd_id3145609.13
msgid  "Horizontal scrollbar"
msgstr "Хоризонтален плъзгач"

#: source/text/shared/optionen/01040200.xhp#par_id3154347.14
msgid  "\<ahelp hid=\"SW_CHECKBOX_TP_CONTENT_OPT_CB_HSCROLL\"\>Displays the horizontal scrollbar.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01040200.xhp#hd_id3148663.15
msgid  "Vertical scrollbar"
msgstr "Вертикален плъзгач"

#: source/text/shared/optionen/01040200.xhp#par_id3150541.16
msgid  "\<ahelp hid=\"SW_CHECKBOX_TP_CONTENT_OPT_CB_VSCROLL\"\>Displays the vertical scrollbar.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01040200.xhp#hd_id3148798.65
msgid  "Ruler"
msgstr "Скала"

#: source/text/shared/optionen/01040200.xhp#par_id3154126.66
msgid  "\<ahelp hid=\"SW_CHECKBOX_TP_CONTENT_OPT_CB_ANY_RULER\"\>Enables the rulers. Use the following two check boxes to select the ruler(s) to be shown.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01040200.xhp#hd_id3159149.17
msgid  "Horizontal ruler"
msgstr "Хоризонтална скала"

#: source/text/shared/optionen/01040200.xhp#par_id3149202.18
msgid  "\<ahelp hid=\"SW_LISTBOX_TP_CONTENT_OPT_LB_HMETRIC\"\>Displays the horizontal ruler. Select the desired measurement unit from the corresponding list.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01040200.xhp#hd_id3153104.19
msgid  "Vertical ruler"
msgstr "Вертикална скала"

#: source/text/shared/optionen/01040200.xhp#par_id3147287.20
msgid  "\<ahelp hid=\"SW_LISTBOX_TP_CONTENT_OPT_LB_VMETRIC\"\>Displays the vertical ruler. Select the desired measurement unit from the corresponding list.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01040200.xhp#hd_id3152460.62
msgid  "Right-aligned"
msgstr "Дясно подравняване"

#: source/text/shared/optionen/01040200.xhp#par_id3151116.61
msgid  "\<ahelp hid=\"SW_CHECKBOX_TP_CONTENT_OPT_CB_VRULER_RIGHT\"\>Aligns the vertical ruler with the right border.\</ahelp\>"
msgstr ""

#: source/text/shared/optionen/01040200.xhp#hd_id3155414.27
msgid  "Smooth scroll"
msgstr "Плавно превъртане"

#: source/text/shared/optionen/01040200.xhp#par_id3153364.28
msgid  "\<ahelp hid=\"SW_CHECKBOX_TP_CONTENT_OPT_CB_SMOOTH_SCROLL\"\>Activates the smooth page scrolling function. \</ahelp\> The scrolling speed depends of the area and of the color depth display."
msgstr ""

#: source/text/shared/optionen/01040200.xhp#hd_id3153091.34
msgid  "Display"
msgstr "Визуализиране"

#: source/text/shared/optionen/01040200.xhp#par_id3154920.35
msgid  "Defines which document elements are displayed."
msgstr "Определя кои елементи на документите да се показват."

#: source/text/shared/optionen/01040200.xhp#hd_id3153143.36
msgid  "Graphics and objects"
