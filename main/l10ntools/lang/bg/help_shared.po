msgid  "Start Center"
msgstr "Начален екран"

#: source/text/shared/guide/startcenter.xhp#bm_id0820200802500562
msgid  "\<bookmark_value\>backing window\</bookmark_value\>      \<bookmark_value\>start center\</bookmark_value\>"
