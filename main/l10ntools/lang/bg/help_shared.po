msgstr "Помощник за заявки - Обзор"

#: source/text/shared/explorer/database/querywizard08.xhp#par_idN10543
msgid  "\<link href=\"text/shared/explorer/database/querywizard08.xhp\"\>Query Wizard - Overview\</link\>"
msgstr ""

#: source/text/shared/explorer/database/querywizard08.xhp#par_idN10553
msgid  "Enter a name of the query, and specify whether you want to display or to modify the query after the Wizard is finished."
msgstr "Въведете име на заявката и укажете дали след края на помощника искате да видите резултатите от нея, или да я променяте."

#: source/text/shared/explorer/database/querywizard08.xhp#par_idN10556
msgid  "Name of the query"
msgstr "Име на заявка"

#: source/text/shared/explorer/database/querywizard08.xhp#par_idN1055A
msgid  "\<ahelp hid=\".\"\>Enter the name of the query.\</ahelp\>"
msgstr ""

#: source/text/shared/explorer/database/querywizard08.xhp#par_idN1055D
msgid  "Display query"
msgstr "Показване на заявка"

#: source/text/shared/explorer/database/querywizard08.xhp#par_idN10561
msgid  "\<ahelp hid=\".\"\>Select to save and display the query.\</ahelp\>"
msgstr ""

#: source/text/shared/explorer/database/querywizard08.xhp#par_idN10564
msgid  "Modify query"
msgstr "Редактиране на заявка"

#: source/text/shared/explorer/database/querywizard08.xhp#par_idN10568
msgid  "\<ahelp hid=\".\"\>Select to save the query and open it for editing.\</ahelp\>"
