msgstr "sText = Rtrim(sText2) REM sText = \" <*Лас Вегас*>\""

#: source/text/sbasic/shared/03120311.xhp#par_id3150449.19
msgid  "sOut = sOut +\"'\"+ sText +\"'\" + Chr(13)"
msgstr "sOut = sOut +\"'\"+ sText +\"'\" + Chr(13)"

#: source/text/sbasic/shared/03120311.xhp#par_id3149562.20
msgid  "sText = Trim(sText2) REM sText = \"<*Las Vegas*>\""
msgstr "sText = Trim(sText2) REM sText = \"<*Лас Вегас*>\""

#: source/text/sbasic/shared/03120311.xhp#par_id3161831.21
msgid  "sOut = sOut +\"'\"+ sText +\"'\""
msgstr "sOut = sOut +\"'\"+ sText +\"'\""

