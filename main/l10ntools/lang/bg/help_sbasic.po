msgstr "Dim A As Variant"

#: source/text/sbasic/shared/03104200.xhp#par_id3153525.10
msgid  "A = Array(\"Fred\",\"Tom\",\"Bill\")"
msgstr "A = Array(\"Иван\", \"Петър\", \"Асен\")"

#: source/text/sbasic/shared/03104200.xhp#par_id3150792.11
msgid  "Msgbox A(2)"
msgstr "MsgBox A(2)"

#: source/text/sbasic/shared/03104300.xhp#tit
msgid  "DimArray Function [Runtime]"
msgstr "Функция DimArray [динамична]"

