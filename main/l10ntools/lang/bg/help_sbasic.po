msgid  "Dim sDatf(10) as String"
msgstr "Dim sDatf(10) as String"

#: source/text/sbasic/shared/03102200.xhp#par_id3155414.12
msgid  "print isarray(sdatf())"
msgstr "Print IsArray(sDatf())"

#: source/text/sbasic/shared/03102200.xhp#par_id3153727.13
msgid  "end Sub"
msgstr "End Sub"

