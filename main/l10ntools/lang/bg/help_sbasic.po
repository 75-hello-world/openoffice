msgstr "операторен блок"

#: source/text/sbasic/shared/03090201.xhp#par_id3145090.8
msgid  "Loop"
msgstr "Loop"

#: source/text/sbasic/shared/03090201.xhp#par_id3154749.9
msgid  "or"
msgstr "или"

#: source/text/sbasic/shared/03090201.xhp#par_id3150503.10
msgid  "Do"
msgstr "Do"

#: source/text/sbasic/shared/03090201.xhp#par_id3149762.11
msgid  "statement block"
msgstr "операторен блок"

