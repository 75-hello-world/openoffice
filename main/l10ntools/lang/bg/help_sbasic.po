msgid  "Parameters:"
msgstr "Параметри:"

#: source/text/sbasic/shared/03020411.xhp#par_id3153750.6
msgid  "\<emph\>Text:\</emph\> Any string expression that specifies the name and path of the directory to be created. You can also use \<link href=\"text/sbasic/shared/00000002.xhp\" name=\"URL notation\"\>URL notation\</link\>."
msgstr "<emph>Text:</emph> низов израз, задаващ името и пътя на създаваната директория. Може да бъде във вид на <link href=\"text/sbasic/shared/00000002.xhp\" name=\"URL\">URL</link>."

#: source/text/sbasic/shared/03020411.xhp#par_id3153311.7
msgid  "If the path is not determined, the directory is created in the current directory."
msgstr "Ако пътят не е определен, директорията се създава в текущата директория."

#: source/text/sbasic/shared/03020411.xhp#hd_id3155388.8
msgid  "Example:"
msgstr "Пример:"

#: source/text/sbasic/shared/03020411.xhp#par_id3148473.9
msgid  "Sub ExampleFileIO"
msgstr "Sub ExampleFileIO"

#: source/text/sbasic/shared/03020411.xhp#par_id3149762.10
msgid  "' Example for functions of the file organization"
msgstr "' Пример за функциите за управление на файлове"

#: source/text/sbasic/shared/03020411.xhp#par_id3145610.11
msgid  "Const sFile1 as String = \"file://c|/autoexec.bat\""
msgstr "Const sFile1 As String = \"file://c|/autoexec.bat\""

#: source/text/sbasic/shared/03020411.xhp#par_id3147264.12
msgid  "Const sDir1 as String = \"file://c|/Temp\""
msgstr "Const sDir1 As String = \"file://c|/Temp\""

#: source/text/sbasic/shared/03020411.xhp#par_id3149669.13
msgid  "Const sSubDir1 as String =\"Test\""
msgstr "Const sSubDir1 As String =\"Test\""

#: source/text/sbasic/shared/03020411.xhp#par_id3148663.14
msgid  "Const sFile2 as String = \"Copied.tmp\""
msgstr "Const sFile2 As String = \"Copied.tmp\""

#: source/text/sbasic/shared/03020411.xhp#par_id3154071.15
msgid  "Const sFile3 as String = \"Renamed.tmp\""
msgstr "Const sFile3 As String = \"Renamed.tmp\""

#: source/text/sbasic/shared/03020411.xhp#par_id3150792.16
msgid  "Dim sFile as String"
msgstr "Dim sFile As String"

#: source/text/sbasic/shared/03020411.xhp#par_id3154366.17
msgid  "sFile = sDir1 + \"/\" + sSubDir1"
msgstr "sFile = sDir1 + \"/\" + sSubDir1"

#: source/text/sbasic/shared/03020411.xhp#par_id3149204.18
msgid  "ChDir( sDir1 )"
msgstr "ChDir(sDir1)"

#: source/text/sbasic/shared/03020411.xhp#par_id3154217.19
msgid  "If Dir(sSubDir1,16)=\"\" then ' Does the directory exist ?"
msgstr "If Dir(sSubDir1, 16) = \"\" Then ' Съществува ли директорията?"

#: source/text/sbasic/shared/03020411.xhp#par_id3156423.20
msgid  "MkDir sSubDir1"
msgstr "MkDir sSubDir1"

#: source/text/sbasic/shared/03020411.xhp#par_id3147228.21
msgid  "MsgBox sFile,0,\"Create directory\""
msgstr "MsgBox sFile, 0, \"Създаване на директория\""

#: source/text/sbasic/shared/03020411.xhp#par_id3153970.22
msgid  "End If"
msgstr "End If"

#: source/text/sbasic/shared/03020411.xhp#par_id3148451.24
msgid  "sFile = sFile + \"/\" + sFile2"
msgstr "sFile = sFile + \"/\" + sFile2"

#: source/text/sbasic/shared/03020411.xhp#par_id3155132.25
msgid  "FileCopy sFile1 , sFile"
msgstr "FileCopy sFile1 , sFile"

#: source/text/sbasic/shared/03020411.xhp#par_id3153770.26
msgid  "MsgBox fSysURL(CurDir()),0,\"Current directory\""
msgstr "MsgBox fSysURL(CurDir()), 0, \"Текуща директория\""

#: source/text/sbasic/shared/03020411.xhp#par_id3159154.27
msgid  "MsgBox sFile & Chr(13) & FileDateTime( sFile ),0,\"Creation time\""
msgstr "MsgBox sFile & Chr(13) & FileDateTime(sFile), 0, \"Час на създаване\""

#: source/text/sbasic/shared/03020411.xhp#par_id3149484.28
msgid  "MsgBox sFile & Chr(13)& FileLen( sFile ),0,\"File length\""
msgstr "MsgBox sFile & Chr(13) & FileLen(sFile), 0, \"Дължина на файла\""

#: source/text/sbasic/shared/03020411.xhp#par_id3152885.29
msgid  "MsgBox sFile & Chr(13)& GetAttr( sFile ),0,\"File attributes\""
msgstr "MsgBox sFile & Chr(13) & GetAttr(sFile), 0, \"Атрибути на файла\""

#: source/text/sbasic/shared/03020411.xhp#par_id3152596.30
