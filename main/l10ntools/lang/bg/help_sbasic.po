msgid  "Dim lVar As Long"
msgstr "Dim lVar As Long"

#: source/text/sbasic/shared/03010301.xhp#par_id3148576.12
msgid  "lVar = rgb(128,0,200)"
msgstr "lVar = RGB(128, 0, 200)"

#: source/text/sbasic/shared/03010301.xhp#par_id3154012.13
msgid  "MsgBox \"The color \" & lVar & \" consists of:\" & Chr(13) &_"
msgstr "MsgBox \"Цветът \" & lVar & \" съдържа:\" & Chr(13) &_"

#: source/text/sbasic/shared/03010301.xhp#par_id3148645.14
msgid  "\"red= \" & Red(lVar) & Chr(13)&_"
msgstr "\"червено= \" & Red(lVar) & Chr(13)&_"

#: source/text/sbasic/shared/03010301.xhp#par_id3159155.15
msgid  "\"green= \" & Green(lVar) & Chr(13)&_"
msgstr "\"зелено= \" & Green(lVar) & Chr(13)&_"

#: source/text/sbasic/shared/03010301.xhp#par_id3147319.16
msgid  "\"blue= \" & Blue(lVar) & Chr(13) , 64,\"colors\""
msgstr "\"синьо= \" & Blue(lVar) & Chr(13), 64, \"Цветове\""

#: source/text/sbasic/shared/03010301.xhp#par_id3147434.17
msgid  "End Sub"
msgstr "End Sub"

