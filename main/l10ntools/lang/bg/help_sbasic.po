msgid  "Dim siW1 As Single"
msgstr "Dim siW1 As Single"

#: source/text/sbasic/shared/03080601.xhp#par_id3125864.13
msgid  "Dim siW2 As Single"
msgstr "Dim siW2 As Single"

#: source/text/sbasic/shared/03080601.xhp#par_id3145786.14
msgid  "siW1 = Int(InputBox$ (\"Please enter the first amount\",\"Value input\"))"
msgstr "siW1 = Int(InputBox$ (\"Моля, въведете първото количество\",\"Входна стойност\"))"

#: source/text/sbasic/shared/03080601.xhp#par_id3149561.15
msgid  "siW2 = Int(InputBox$ (\"Please enter the second amount\",\"Value input\"))"
msgstr "siW2 = Int(InputBox$ (\"Моля, въведете второто количество\",\"Входна стойност\"))"

#: source/text/sbasic/shared/03080601.xhp#par_id3145750.16
msgid  "Print \"The difference is \"; Abs(siW1 - siW2)"
msgstr "Print \"Разликата е \"; Abs(siW1 - siW2)"

#: source/text/sbasic/shared/03080601.xhp#par_id3147319.17
msgid  "End Sub"
msgstr "End Sub"

