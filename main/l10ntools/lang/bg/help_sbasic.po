msgstr "case 2"

#: source/text/sbasic/shared/03030105.xhp#par_id3153952.18
msgid  "sDay=\"Monday\""
msgstr "sDay=\"Понеделник\""

#: source/text/sbasic/shared/03030105.xhp#par_id3149666.19
msgid  "case 3"
msgstr "case 3"

#: source/text/sbasic/shared/03030105.xhp#par_id3153157.20
msgid  "sDay=\"Tuesday\""
msgstr "sDay=\"Вторник\""

#: source/text/sbasic/shared/03030105.xhp#par_id3154730.21
msgid  "case 4"
msgstr "case 4"

#: source/text/sbasic/shared/03030105.xhp#par_id3154942.22
msgid  "sDay=\"Wednesday\""
msgstr "sDay=\"Сряда\""

#: source/text/sbasic/shared/03030105.xhp#par_id3145799.23
msgid  "case 5"
msgstr "case 5"

#: source/text/sbasic/shared/03030105.xhp#par_id3155416.24
msgid  "sDay=\"Thursday\""
msgstr "sDay=\"Четвъртък\""

#: source/text/sbasic/shared/03030105.xhp#par_id3150716.25
msgid  "case 6"
msgstr "case 6"

#: source/text/sbasic/shared/03030105.xhp#par_id3154015.26
msgid  "sDay=\"Friday\""
msgstr "sDay=\"Петък\""

#: source/text/sbasic/shared/03030105.xhp#par_id3146971.27
msgid  "case 7"
msgstr "case 7"

#: source/text/sbasic/shared/03030105.xhp#par_id3153707.28
msgid  "sDay=\"Saturday\""
msgstr "sDay=\"Събота\""

#: source/text/sbasic/shared/03030105.xhp#par_id3155065.29
msgid  "End Select"
msgstr "End Select"

#: source/text/sbasic/shared/03030105.xhp#par_id3148993.30
msgid  "msgbox \"\" + sDay,64,\"Today is\""
