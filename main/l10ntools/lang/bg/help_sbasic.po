#: source/text/sbasic/shared/03030106.xhp#tit
msgid  "Year Function [Runtime]"
msgstr "Функция Year [динамична]"

#: source/text/sbasic/shared/03030106.xhp#bm_id3148664
msgid  "\<bookmark_value\>Year function\</bookmark_value\>"
msgstr "<bookmark_value>Year, функция</bookmark_value>"

#: source/text/sbasic/shared/03030106.xhp#hd_id3148664.1
msgid  "\<link href=\"text/sbasic/shared/03030106.xhp\" name=\"Year Function [Runtime]\"\>Year Function [Runtime]\</link\>"
msgstr "<link href=\"text/sbasic/shared/03030106.xhp\" name=\"Функция Year [динамична]\">Функция Year [динамична]</link>"

#: source/text/sbasic/shared/03030106.xhp#par_id3149655.2
msgid  "Returns the year from a serial date number that is generated by the DateSerial or the DateValue function."
msgstr "Връща годината по зададено серийно число на дата, генерирано от функцията DateSerial или DateValue. "

#: source/text/sbasic/shared/03030106.xhp#hd_id3154125.3
msgid  "Syntax:"
msgstr "Синтаксис:"

#: source/text/sbasic/shared/03030106.xhp#par_id3147229.4
msgid  "Year (Number)"
msgstr "Year (Number)"

#: source/text/sbasic/shared/03030106.xhp#hd_id3154685.5
msgid  "Return value:"
msgstr "Връщана стойност:"

#: source/text/sbasic/shared/03030106.xhp#par_id3153970.6
msgid  "Integer"
