msgid "Meaning"
msgstr "Betydning"

#: fields_userdata.xhp#par_id3150508.96.help.text
msgid "== or EQ"
msgstr "== eller EQ"

#: fields_userdata.xhp#par_id3150531.97.help.text
msgid "equals"
msgstr "er lik"

#: fields_userdata.xhp#par_id3150725.98.help.text
