msgid "Left"
msgstr "Venstre"

#: 05060200.xhp#par_id3150678.36.help.text
msgid ""
"<ahelp hid=\"SW:METRICFIELD:TP_FRM_WRAP:ED_LEFT_MARGIN\">Enter the amount of "
"space that you want between the left edge of the object and the "
"text.</ahelp>"
msgstr ""
"<ahelp hid=\"SW:METRICFIELD:TP_FRM_WRAP:ED_LEFT_MARGIN\">Velg hvor stor "
"avstand det skal være mellom venstre kant av objektet og teksten.</ahelp>"

